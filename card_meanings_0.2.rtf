47564  \u45817 \u49888 \u50640 \u44172  \u49892 \u51228 \u47196  \u49345 \u52376 \u47484  \u48155 \u50520 \u44144 \u45208 , \u49345 \u52376 \u47484  \u51500 \u45796 \u44256  \u49373 \u44033 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47560 \u51648 \u47561 \u51004 \u47196 , \u49345 \u49345  \u49549  \u44256 \u53685 \u51077 \u45768 \u45796 . \u49892 \u51228 \u47196  \u51068 \u50612 \u45208 \u51648  \u50506 \u51008  \u51068 \u50640  \u45824 \u54620  \u44145 \u51221 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51060 \u48324 \u51012  \u53685 \u48372 \u54624 \u44620 \u48400  \u44145 \u51221  \u54624  \u49688 \u46020  \u51080 \u44256 , \u44256 \u48177 \u54664 \u45796 \u44032  \u52264 \u51060 \u45716  \u49345 \u49345 , \u45796 \u47480  \u51060 \u49457 \u44284  \u48148 \u46988 \u51012  \u54588 \u45716  \u49345 \u49345  \u46321 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u44288 \u44228 \u50640  \u48520 \u50504 \u44048 \u51012  \u45712 \u45184 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51060  \u46496 \u45208 \u44040 \u44620 \u48400  \u46160 \u47157 \u44592 \u46020  \u54616 \u44256 , \u48148 \u46988 \u51012  \u54588 \u45716  \u44148  \u50500 \u45772 \u51648  \u44145 \u51221 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45572 \u44400 \u44032 \u45716  \u51665 \u52265 \u51012  \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \'93\u44284 \u50672  \u51060  \u44288 \u44228 \u44032  \u44148 \u44053 \u54620  \u44288 \u44228 \u51064 \u51648 \'94 \u46104 \u46028 \u50500 \u48380  \u49884 \u44592 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u45208 \u49257 \u45768 \u45796 . \u45796 \u47564  \u51116 \u54924 \u50752  \u48324 \u44060 \u47196 , \u49345 \u45824 \u48169 \u51060  \u45817 \u49888 \u51012  \u44618 \u44172  \u49373 \u44033 \u54616 \u45716  \u46319  \u54633 \u45768 \u45796 . \u51060 \u48324 \u51032  \u50500 \u54548 \u51060 \u45208  \u52628 \u50613  \u46321 \u51012  \u54844 \u51088 \u49436  \u44273 \u50489 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u44900 \u45208  \u53360  \u44256 \u53685 \u51012  \u44202 \u50632 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u49892 \u50672 \u51032  \u50500 \u54548 \u51060  \u50672 \u46973 \u51004 \u47196  \u51060 \u50612 \u51648 \u51648 \u45716  \u50506 \u51012  \u44163  \u44057 \u49845 \u45768 \u45796 . \u54844 \u51088 \u49436  \u51060 \u47088  \u49345 \u49345 , \u51200 \u47088  \u49345 \u49345 \u51012  \u54616 \u47728  \u44161 \u47673 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u49548 \u46300 9 \u51649 \u50629 \u50868  \u54644 \u49437 \u51649 \u50629 \u50640  \u45824 \u54644  \u44284 \u46020 \u54620  \u48520 \u50504 \u44048 \u51012  \u45712 \u45180 \u44256  \u44228 \u49884 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u50629 \u47924 \u44032  \u45320 \u47924  \u47566 \u44144 \u45208 , \u48120 \u47000 \u44032  \u48520 \u54869 \u49892 \u54616 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \'93\u51667 \u45580 \u47532 \u44256  \u51080 \u45796 \u45716 \'94 \u45712 \u45196 \u51012  \u48155 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u52293 \u51076 \u51648 \u45716  \u50629 \u47924 \u47484  \u45817 \u48516 \u44036  \u49340 \u44032 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51649 \u51109 \u50640 \u49436 \u45716 , \u45733 \u47141  \u48150 \u51032  \u51068 \u51012  \u47585 \u50500  \u49828 \u53944 \u47112 \u49828 \u47484  \u49900 \u54616 \u44172  \u48155 \u51012  \u49688  \u51080 \u51004 \u47728  \u49324 \u50629 \u50640 \u49436 \u45716 , \u44284 \u44048 \u54620  \u53804 \u51088 \u45208  \u54869 \u51109 \u51012  \u49340 \u44032 \u50556  \u44192 \u49845 \u45768 \u45796 . \u47560 \u51020  \u44288 \u47532 \u44032  \u53945 \u55176  \u51473 \u50836 \u54620  \u49884 \u44592 \u51077 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51216 \u52832  \u49688  \u50630 \u