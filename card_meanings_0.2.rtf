   \uc0\u50756 \u46300 6 \'96 \u49849 \u47532  \u50756 \u46300 6 \u44536 \u47548 \u51032  \u51032 \u48120 \u51452 \u48320 \u51064 \u46308 \u50640 \u44172  \u52845 \u52268 \u51012  \u48155 \u45716  \u49345 \u54889 \u54665 \u46041 \u50640  \u46384 \u47480  \u48372 \u49345 \u51012  \u50619 \u44172  \u46104 \u45716  \u49345 \u54889 \u48376 \u51064 \u51032  \u50629 \u51201 , \u49457 \u44284 \u47484  \u51088 \u46993 \u49828 \u47101 \u44172  \u50668 \u44592 \u45716  \u51473  (\u54841 \u51008  \u51088 \u46993 )\u44288 \u49900 \u51032  \u52488 \u51216 \u51060  \u48376 \u51064 \u51012  \u54693 \u54644 \u51080 \u44592 \u50640 , \u49548 \u47928  \u54140 \u51648 \u44592  \u51339 \u51008  \u49884 \u44592 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u45796 \u47480  \u49324 \u46988 \u46308 \u50640 \u44172  \u52629 \u54616  \u48155 \u45716 \u50756 \u46300 6\u45716  \u45796 \u47480  \u49324 \u46988 \u46308 \u50640 \u44172  \u52629 \u54616  \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u45817 \u49888 \u51060  \u54924 \u49324 \u50640  \u53360  \u44277 \u51012  \u49464 \u50892 , \u46041 \u47308 \u46308 \u50640 \u44172  \u51064 \u51221  \u48155 \u45716  \u49345 \u54889 \u51677 \u49324 \u46993 \u51060 \u50632 \u45796 \u47732 , \u51677 \u49324 \u46993 \u51060  \u51060 \u47364 \u51256 \u49436  \u52828 \u44396 \u46308 \u51032  \u52629 \u54616 \u47484  \u48155 \u45716  \u49345 \u54889 \u52964 \u54540 \u51060 \u46972 \u47732 , \u54532 \u47196 \u54252 \u51592 \u47484  \u48155 \u50500  \u44263  \u44208 \u54844 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u52852 \u46300  \u49549  \u47568 \u51012  \u53444  \u51064 \u47932 \u51008  \u49324 \u46988 \u46308 \u47196  \u46168 \u47084 \u49912 \u51064  \u52292 , \u49849 \u47532 \u47484  \u47564 \u45181 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47672 \u47532  \u50948 \u50640 \u45716  \u50900 \u44228 \u44288 \u51060  \u54616 \u45208 , \u50756 \u46300  \u50948 \u50640 \u45716  \u54868 \u54872 \u51060  \u54616 \u45208  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 \u45716  \u54665 \u46041 \u51012  \u49345 \u51669 \u54616 \u45716  \u50836 \u49548 \u51077 \u45768 \u45796 . \u50756 \u46300  \u50948 \u50640  \u54868 \u54872 \u51060  \u44152 \u47140 \u51080 \u45716  \u44536 \u47548 \u51008  \u44536 \u51032  \u54665 \u46041 \u51060  \u52629 \u54616  \u48155 \u51012  \u47564 \u54620  \u44163 \u51060 \u50632 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644 \u49436 \'85\u52629 \u54616  \u48155 \u51012  \u49688  \u50630 \u45796 \u47732  \u8594  \u51060 \u47364 \u51648 \u51648  \u50506 \u49845 \u45768 \u45796  \u50668 \u44592 \u49436  \u54645 \u49900  \u54252 \u51064 \u53944 \u45716  \u51452 \u48320 \u50640 \u49436  [\u52629 \u54616 \u47484  \u48155 \u51012  \u49688  \u51080 \u45716 \u51648 ] \u50668 \u48512 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  \u48520 \u47452  \u44288 \u44228 \u50640 \u49436 , \'91\u49345 \u45824 \u48169 \u44284  \u45236 \u44032  \u51060 \u50612 \u51656  \u49688  \u51080 \u51012 \u44620 \u50836 ?\'92 \u46972 \u44256  \u47932 \u50632 \u45796 \u47732 , \'91\u50500 \u47560  \u51060 \u50612 \u51648 \u44592  \u55192 \u46308  \u44163 \'92 \u51060 \u46972 \u44256  \u45813 \u48320 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u12302 \u50756 \u46300  6 \u51032 \u48120 \u50752  \u54644 \u49437 \u12303 <\u51068 \u44284  \u50672 \u50528 > \'96 tarotfun.jp\u45796 \u47480  \u49345 \u54889 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u51201 \u50857 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796  :\u51452 \u48320 \u50640 \u49436  \u50416 \u47112 \u44592 \u46972 \u47728 , \u54756 \u50612 \u51648 \u46972 \u44256  \u54664 \u45912  \u49324 \u46988  \u8594  \u54980 \u47144 \u54620  \u47560 \u51020 \u51004 \u47196  \u51060 \u48324 \u51677 \u49324 \u46993 \u54664 \u51648 \u47564 , \u54217 \u54032 \u51060  \u50504  \u51339 \u50520 \u45912  \u49324 \u46988  \u8594  \u47560 \u51020  \u51221 \u47532 \u54616 \u44256  \u49352 \u47196 \u50868  \u49324 \u46993 \u51012  \u52286 \u51020 \u49345 \u45824 \u48169 \u51008  \u54872 \u49849  \u51060 \u48324 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u51012 \u44620 ? \u8594  \u50500 \u47560 \u46020  NO. \u51452 \u48320 \u50640 \u49436  \u52629 \u54616  \u48155 \u44592  \u55192 \u46310 .\u45236 \u44032  \u54924 \u49324 \u47484  \u46496 \u45208 \u47732  \u49324 \u46988 \u46308 \u51060  \u51339 \u50500 \u54624 \u44620 ? \u8594  \u54217 \u49548 \u50640  \u47560 \u52272 \u51060  \u51110 \u50520 \u45796 \u47732  YES\u54644 \u49437  2. \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u46020 \u50880 \u51012  \u48155 \u45716 \u50756 \u46300 6\u51008  \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u46020 \u50880 \u51012  \u48155 \u50500 , \u50896 \u54616 \u45716  \u44163 \u51012  \u49457 \u52712 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u46907 \u54633 \u45768 \u45796 .\u49688 \u48708 \u54617 \u50640 \u49436  \u49707 \u51088 6\u51008  \u51648 \u50896 \u44284  \u48372 \u54840 \u47484  \u51032 \u48120 \u54633 \u45768 \u45796 .[\u52280 \u44256 \u51088 \u47308 ] \u45224 \u51088 \u44032  \u50416 \u44256  \u51080 \u45716  \u50900 \u44228 \u44288 \u44284  \u50756 \u46300  \u50948 \u50640  \u44152 \u47140  \u51080 \u45716  \u54868 \u54872 \u51008  \u47784 \u46160  \'91\u49324 \u46988 \u46308 \u51060  \u44152 \u50612 \u51456  \u44163 \'92\u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51452 \u48320  \u49324 \u46988 \u51032  \u46020 \u50880 \u51012  \u48155 \u50500  \u51068 \u51088 \u47532 \u47484  \u50619 \u45716  \u49345 \u54889 \u51452 \u48320 \u50640  \u49548 \u44060 \u54021 \u51012  \u49884 \u53020 \u51452 \u45716  \u49324 \u46988 \u51060  \u47566 \u50500 \u51664 \u47784 \u51076 \u51060 \u45208  \u46041 \u50500 \u47532  \u54876 \u46041 \u51004 \u47196  \u51060 \u49457 \u51012  \u47564 \u45216  \u44592 \u54924 \u44032  \u47566 \u50500 \u51664 \u51648 \u51064 \u51032  \u46020 \u50880 \u51004 \u47196  \u51116 \u54924 \u47484  \u49884 \u46020 \u54616 \u45716  \u49345 \u54889 \u8251  \u51064 \u44592 \u44032  \u47566 \u50500 \u51256 \u49436  \u45796 \u49548  \u44144 \u47564 \u54644 \u51656  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51060 \u49457 \u51012  \u47564 \u45216  \u44592 \u54924 \u44032  \u47566 \u50500 \u51256 \u49436  \u45576 \u51060  \u45458 \u50500 \u51648 \u45716  \u49884 \u44592 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51452 \u48320  \u49324 \u46988  \u45909 \u51012  \u47566 \u51060  \u48372 \u51648 \u47564 , \u44048 \u49324 \u54632 \u51012  \u47803  \u45712 \u45180 \u44144 \u45208  \u51088 \u50500  \u46020 \u52712 \u50640  \u48736 \u51656  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u45224 \u46308 \u51060  \u48372 \u44592 \u50640  \u54633 \u45817 \u54620  \u44208 \u44284 \u47484  \u50619 \u51020 \u50756 \u46300 6\u51008  \u45224 \u46308 \u51060  \u48372 \u44592 \u50640  \u54633 \u45817 \u54620  \u44208 \u44284 \u47484  \u50619 \u45716 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \'91\u47924 \u51312 \u44148  \u49849 \u47532 \u54620 \u45796 \'92\u45716  \u51096 \u47803 \u46108  \u47568 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u54633 \u44201 \u50868 \u50640 \u49436 , \'93\u50752  \u50756 \u46300 6\u51060  \u46516 \u45348 ? \u54633 \u44201 !!!\'94 \u51060  \u50500 \u45768 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50526 \u49436  \u49324 \u50857 \u54664 \u45912  \u44536 \u47548 \u51012  \u45796 \u49884  \u44032 \u51256 \u50724 \u44192 \u49845 \u45768 \u45796 . \u51060 \u48120 \u51648  \u49549  \u49444 \u47749 \u52376 \u47100 , \u50756 \u46300 6\u51008  [\u54665 \u46041 \u50640  \u46384 \u47480  \u8594  \u44208 \u44284 \u50752  \u49324 \u46988 \u46308 \u51032  \u51064 \u51221 ]\u51012  \u48155 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u50668 \u44592 \u49436  \u51473 \u50836 \u54620  \u54252 \u51064 \u53944 \u45716 \'85\u49457 \u44284 \u50640  \u45824 \u54620  \u54032 \u45800 , \u48372 \u49345 \u51008  \u47784 \u46160  \u49324 \u46988 \u46308 \u51060  \u51452 \u44592  \u46412 \u47928 \u50640 , \u45432 \u47141 \u44284  \u48372 \u49345 \u51008  \u44144 \u47532 \u44032  \u47680  \u49688  \u51080 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u45432 \u47141 \u51060  \u44284 \u49548 \u54217 \u44032  \u46104 \u44144 \u45208 , \u44284 \u45824 \u54217 \u44032  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\'93\u45224 \u46308 \u51060  \u48372 \u44592 \u50640 \'94 \u50676 \u49900 \u55176  \u54664 \u45796 \u47732  \u8594  \u44032 \u49104 \u55176  \u54633 \u44201 \'93\u45224 \u46308 \u51060  \u48372 \u44592 \u50640 \'94 \u51312 \u44552  \u50528 \u47588 \u54620  \u45432 \u47141 \u51060 \u47732  \u8594  \u50528 \u47588 \u54620  \u44208 \u44284 \'93\u45224 \u46308 \u51060  \u48372 \u44592 \u50640 \'94 \u50676 \u49900 \u55176  \u50504  \u54664 \u45796 \u47732  \u8594  \u48520 \u54633 \u44201 \u46384 \u46972 \u49436 , \u45208 \u45716  \u50676 \u49900 \u55176  \u50504  \u54664 \u45796 \u44256  \u49373 \u44033 \u54664 \u45716 \u45936  \u54633 \u44201 \u54620 \u45796 \u44144 \u45208 , \u45208 \u45716  \u50676 \u49900 \u55176  \u54664 \u45796 \u44256  \u49373 \u44033 \u54664 \u45716 \u45936  \u48520 \u54633 \u44201 \u54616 \u45716  \u51068 \u51060  \u48156 \u49373 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u51020  \u44172 \u49884 \u47932 \u50640  \u44536  \u49324 \u47168 \u44032  \u51096  \u45208 \u50752 \u51080 \u49845 \u45768 \u45796 .\u46907  \u51088 \u52404 \u45716  \u51339 \u44256  \u44557 \u51221 \u51201 \u51064 \u45936 , \u49892 \u51228 \u47196  \u51339 \u44172  \u48156 \u54788 \u46108  \u51201 \u51060  \u50630 \u45796 . \u53945 \u55176  \u54633 \u44201 \u50868 \u50640 \u49436  \u46888 \u47732 , \u45236 \u44032  \u51648 \u44552 \u44620 \u51648  \u48376  \u44221 \u50864 \u45716  \u48177 \u54140  \u48520 \u54633 .. \u50780 \u51068 \u44620 ????\u51652 \u51676  6\u50756 \u46300  \u47960 \u44620  \'96 \u46356 \u49884 \u51064 \u49324 \u51060 \u46300  \u53440 \u47196 \u52852 \u46300  \u44068 \u47084 \u47532 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51088 \u50500 \u46020 \u52712 \u50640  \u48736 \u51652 \u50756 \u46300 6\u45716  \u44144 \u47564 \u54616 \u44144 \u45208  \u51088 \u50500 \u46020 \u52712 \u50640  \u48736 \u51652  \u49324 \u46988 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u44536 \u47548 \u51012  \u48372 \u47732 , \u47568 \u51012  \u53444  \u45224 \u51088 \u45716  \u45796 \u47480  \u49324 \u46988 \u46308 \u51012  \'91\u45236 \u47140 \u45796  \u48372 \u44256 \'92 \u51080 \u51004 \u47728 , \u53553  \u46608 \u54620  \u45458 \u44172  \u52824 \u53020 \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49104 \u47564  \u50500 \u45768 \u46972 , \u50756 \u46300 \u47484  \u49688 \u51649 \u51004 \u47196  \u45458 \u44172  \u46308 \u50612  \u48376 \u51064 \u51032  \u54665 \u46041 \u51012  \u44284 \u49884 \u54616 \u45716  \u46319 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u49345 \u45824 \u48169 \u51012  \u47564 \u45216  \u46412  \u48376 \u51064 \u51012  \u44049 \u51060 \u46972 \u44256  \u49373 \u44033 \u54616 \u45716  \u49324 \u46988 \u50672 \u46973 \u51012  \u48155 \u50520 \u51012  \u46412 , \u45236 \u44032  \u49345 \u45824 \u48169 \u51012  \u51060 \u44220 \u45796 \u45716  \u49900 \u47532 \u48376 \u51064 \u51032  \u50808 \u47784 \u44032  \u51096 \u45228 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u49324 \u46988 \u50756 \u46300 6\u51060  \u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50728 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u48376 \u51064 \u51012  \'93\u47568 \u51012  \u53444  \u45224 \u51088 \'94 \u51221 \u46020 \u47196  \u49373 \u44033 \u54616 \u44256  \u51080 \u45716  \u44163 \u51077 \u45768 \u45796 . \u45208 \u47476 \u49884 \u49884 \u49828 \u53944  \u49457 \u54693 \u51060  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . (\u8251  \u49324 \u46988 \u51012  \u53440 \u47196 \u47196  \u54032 \u45800 \u54616 \u45716  \u44163 \u51008  \u45208 \u49244  \u54665 \u46041 \u51077 \u45768 \u45796 . \u44032 \u48333 \u44172  \u48372 \u49464 \u50836 )\u54644 \u49437  2. \u51452 \u48320  \u49324 \u46988 \u46308 \u51060  \u52280 \u44204 \u54616 \u45716 \u50756 \u46300 6\u51008  \u51452 \u48320  \u49324 \u46988 \u46308 \u51060  \u52280 \u44204 \u54616 \u44144 \u45208 , \u49548 \u47928 \u50640  \u55081 \u49912 \u51060 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50668 \u47084  \u49324 \u46988 \u46308 \u50640 \u44172  \u46168 \u47084 \u49912 \u50668  \u51080 \u49845 \u45768 \u45796 . \u44288 \u49900 \u51032  \u52488 \u51216 \u51060  \u47784 \u46160  \u44536 \u47484  \u54693 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u44032  \u47924 \u50631 \u51012  \u54616 \u46304 , \u51452 \u48320 \u51064 \u46308 \u51008  \u46020 \u50880 \u51012  \u51452 \u47140  \u54624  \u44163 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u47532 \u46377 \u50640 \u49436 \u45716 \'85\u54756 \u50612 \u51652  \u50672 \u51064 \u51012  \u44536 \u47532 \u50892  \u54616 \u45716  \u44163  \u44057 \u45796  \u8594  \u51648 \u51064 \u46308 \u51060  \u45208 \u49436 \u49436  \u51116 \u54924 \u47484  \u46020 \u50752 \u51468 \u54840 \u44048  \u51080 \u45716  \u49345 \u45824 \u44032  \u51080 \u51020  \u8594  \u51648 \u51064 \u46308 \u51060  \u45796  \u47568 \u54644 \u51468 \u51064 \u44592 \u44032  \u47566 \u50500 \u49436  \u8594  \u49324 \u49548 \u54620  \u54665 \u46041 \u50640  \u51452 \u48320  \u49324 \u46988 \u46308 \u51060  \u52280 \u44204 \u54632 \u51064 \u44592 \u44032  \u47566 \u50500 \u49436 , \u51088 \u50672 \u49828 \u47112  \u51648 \u51064 \u51012  \u45132 \u50612 \u47784 \u51004 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45208 \u44592 \u46020  \u54616 \u51648 \u47564 .. \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u48376 \u51064 \u51032  \u54665 \u46041 , \u50629 \u51201 \u51012  \u46496 \u48268 \u47532 \u45716  \u49345 \u54889 \u50640 \u49436  \u45208 \u53440 \u45208 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51593 , \u49548 \u47928 \u51012  \u45256 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612 \u49436  :\u54756 \u50612 \u51652  \u50672 \u51064 \u50640 \u44172  \u50672 \u46973  \u50740  \u8594  \u48148 \u47196  \u52828 \u44396 \u46308 \u50640 \u44172  \u49548 \u47928 \u51012  \u54140 \u46888 \u47548 \u44144 \u48400 . \u45236 \u44032  \u44536 \u47088 \u45796 \u44256  \u54664 \u51094 \u50500 .\u50756 \u46300 6 \u50672 \u50528 \u50868  \u54644 \u49437 \u47564 \u50557  \u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u51060 \u49457 \u46308 \u50640 \u44172  \u51064 \u44592 \u44032  \u47566 \u50500 \u51648 \u45716  \u49884 \u44592 \u44032  \u46104 \u44192 \u49845 \u45768 \u45796 . \u54841 \u51008  \u45572 \u44400 \u44032 \u51032  \u46020 \u50880 \u51012  \u48155 \u50500  \u50672 \u51064 \u51012  \u47564 \u46308  \u49688  \u51080 \u45716  \u46412 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u49548 \u44060 \u54021 \u51077 \u45768 \u45796 . \u47564 \u45208 \u45716  \u49324 \u46988 \u46308 \u46020  \u45817 \u49888 \u51012  \u44900  \u44316 \u52270 \u44172  \u49373 \u44033 \u54624  \u46319  \u54616 \u47728 , \u51088 \u50500 \u46020 \u52712 \u45208  \u54728 \u49464 \u47484  \u53945 \u55176  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u50756 \u46300 6\u51060  \u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50728 \u45796 \u47732 , \u53356 \u44172  3\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u51060  \u51452 \u48320  \u51648 \u51064 \u46308 \u50640 \u44172 \u46020  \u51088 \u46993 \u54624 \u47564 \u54620  \u49324 \u46988 \u51060 \u46972 \u45716  \u49549 \u47560 \u51020 \u51077 \u45768 \u45796 . \u44536 \u47000 \u49436  \u45817 \u49888 \u44284  \u47564 \u45212 \u45796 \u45716  \u49324 \u49892 , \u50672 \u46973 \u51012  \u51452 \u44256 \u48155 \u45716 \u45796 \u45716  \u49324 \u49892 \u51012  \u46496 \u48268 \u47160 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u48376 \u51064 \u51060  \u51060 \u44220 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u49549 \u47560 \u51020 \u51077 \u45768 \u45796 . \u45236 \u44032  \u44844 \u49512 \u45796  \u54841 \u51008  \u49345 \u45824 \u48169 \u51060  \u47676 \u51200  \u50672 \u46973 \u54664 \u45796 . \u49464  \u48264 \u51704 , \u51452 \u48320  \u51060 \u49457 \u46308 \u50640 \u44172  \u46168 \u47084 \u49912 \u50668  \u45817 \u49888 \u51012  \u49373 \u44033 \u52824 \u46020  \u47803 \u54616 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972  \u54644 \u49437 \u51032  \u48169 \u54693 \u49457 \u51060  \u45804 \u46972 \u51648 \u44172  \u46121 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732 , \u45817 \u49888 \u44284  \u54632 \u44760 \u54616 \u45716  \u49692 \u44036 \u51060  \'91\u49849 \u47532 \u44048 \'92\u51012  \u51452 \u47728 , \u45817 \u49888 \u51012  \u51088 \u46993 \u49828 \u47101 \u44172  \u50668 \u44592 \u45716 \u46319  \u54633 \u45768 \u45796 . \u48376 \u51064  \u52828 \u44396 \u46308 \u50640 \u44172 \u46020  \u54217 \u49548 \u50640  \u51088 \u46993 \u51012  \u47566 \u51060  \u54664 \u51012  \u44163 \u51077 \u45768 \u45796 . \u45224 \u46308 \u51060  \u50864 \u47532  \u52964 \u54540 \u51012  \u48512 \u47084 \u50892 \u54616 \u44256  \u51080 \u45796 \u44256  \u49373 \u44033 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44288 \u44228 \u45716  \u44557 \u51221 \u51201 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u47564 \u50557 , \u50756 \u46300 6\u51060  \u44288 \u44228 \u51032  \u51109 \u50528 \u47932 \u47196  \u45208 \u50772 \u45796 \u47732 , \u45817 \u49888 \u51012  \u53944 \u47196 \u54588 \u50752 \u51060 \u54532 (\u54841 \u51008  \u53944 \u47196 \u54588  \u54728 \u51592 \u48264 \u46300 ) \u52196 \u51004 \u47196  \u50668 \u44592 \u44144 \u45208 , \u48376 \u51064 \u51012  \u44049 \u51060 \u46972 \u44256  \u49373 \u44033 \u54624  \u49688  \u51080 \u51012  \u46319 \u54633 \u45768 \u45796 . \u45320  \u47568 \u44256 \u46020  \u50668 \u51088 (\u45224 \u51088 )\u45716  \u47566 \u50500 \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u47140 \u44256 , \u50672 \u46973 \u54616 \u51648  \u50506 \u51012  \u46319 \u54633 \u45768 \u45796 . \u51060 \u48324 \u54620  \u54980  \u55192 \u46308 \u50612 \u54616 \u45716  \u45817 \u49888 \u51032  \u47784 \u49845 \u50640 \u49436  \'91\u49849 \u47532 \u44048 \'92\u51012  \u45712 \u44808 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45768 \u44032  \u51060 \u47111 \u44172  \u55192 \u46308 \u50612 \u54616 \u45768  \u45320 \u47924  \u53685 \u53132 \u54616 \u45796  \u51312 \u44552 \u47564  \u45908  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u49548 \u46300  5\u48264  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u47484  \u51469 \u45768 \u45796 . \u51060 \u48120  \u44536 \u45716  \u51648 \u51064 \u46308 \u44284  \u50612 \u50872 \u47532 \u47728 , \u45796 \u47480  \u44275 \u51004 \u47196  \'91\u51060 \u46041 \u54616 \u45716  \u51473 \'92\u51077 \u45768 \u45796 . \u51116 \u54924 \u45716  \u44592 \u45824 \u54616 \u51648  \u50506 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 .\u50756 \u46300 6 \u51649 \u50629 \u50868  \u54644 \u49437 \u50676 \u49900 \u55176  \u51068 \u54620  \u44163 \u51012  \u51064 \u51221 \u48155 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u49900 \u54032  \u52852 \u46300 \u50752  \u45804 \u47532  \u54032 \u45800 \u51032  \u51452 \u52404 \u45716  \u49324 \u46988 \u51060 \u48064 \u47196 , \u44536  \u51064 \u51221 \u44284  \u48372 \u49345 \u51060  \u45796 \u49548  \u51452 \u44288 \u51201 \u51068  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45432 \u47141 \u50640  \u48708 \u54644  \u45908  \u47566 \u51008  \u44163 \u51012  \u48155 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \u51649 \u51109 \u51064 \u51060 \u46972 \u47732 , \u49457 \u44284 \u47484  \u51064 \u51221 \u48155 \u50500  \u51452 \u48320 \u51064 \u46308 \u50640 \u44172  \u52629 \u54616 \u47484  \u48155 \u45716  \u44536 \u47548 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u49849 \u51652 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \'91\u52629 \u54616 \u47484  \u48155 \u51004 \u47728  \u51060 \u46041 \'92\u54616 \u44256  \u51080 \u51004 \u48064 \u47196 , \u51060 \u51649 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 .\u50756 \u46300 6 \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u46020  \u47560 \u52268 \u44032 \u51648 \u47196 , \u45817 \u49888 \u51060  \u54620  \u45432 \u47141 \u46308 \u51060  \'91\u51452 \u48320 \u51064 \u51032  \u52629 \u54616 \u47484  \u48155 \u51012  \u47564 \u53372 \'92 \u51339 \u51008  \u49457 \u44284 \u47484  \u44144 \u46160 \u44172  \u46121 \u45768 \u45796 . \u45796 \u47564  \u51068 \u48512  \u49324 \u46988 \u46308 \u51032  \u49884 \u44592 , \u51656 \u53804 \u47484  \u49332  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \'91\u45236 \u44032  \u50620 \u47560 \u45208  \u48268 \u50632 \u45796 \'92\u45716  \u47568 \u51008  \u45817 \u48516 \u44036  \u54616 \u51648  \u50506 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 . \u49688 \u51061 \u51012  \u46496 \u48268 \u47160 \u45796 \u44036  \u46024  \u48716 \u47140 \u45804 \u46972 \u45716  \u49324 \u46988 \u51060  \u44553 \u44201 \u55176  \u45720 \u50612 \u45208 \u44172  \u46121 \u45768 \u45796 .    \
