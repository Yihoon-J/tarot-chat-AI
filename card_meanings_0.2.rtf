u51648  \u50506 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 . \u44368 \u47896 \u54620  \u49549 \u51076 \u49688 \u44032  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u45817 \u50672 \u55176  \u48537 \u51012  \u44144 \u46972  \u49373 \u44033 \u54664 \u45912  \u45824 \u54617 , \u51649 \u51109 \u50640 \u49436  \u46504 \u50612 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51032  \u49892 \u47141 \u51060  \'91\u47564 \u52380 \u54616 \u50640  \u46300 \u47084 \u45208 \u45716  \u46319 \u54620 \'92 \u44592 \u48516 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u49548 \u46300 3 \u44552 \u51204 \u50868  \u54644 \u49437 \u48176 \u49888 \u51060 \u45208  \u49324 \u44592 \u47484  \u45817 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46024  \u47928 \u51228 \u44032  \u44032 \u44620 \u50868  \u49324 \u46988 \u46308 \u44284  \'91\u50670 \u50668 \u51080 \u45716 \'92 \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51068 \u51060  \u51096 \u47803 \u46112  \u44221 \u50864 , \u50668 \u47084  \u49324 \u46988 \u50640 \u44172  \u51656 \u53440 \u47484  \u48155 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45817 \u48516 \u44036  \u46024  \u44144 \u47000 \u45716  \u44552 \u47932 \u51077 \u45768 \u45796 . \u44049 \u51089 \u49828 \u47084 \u50868  \u49552 \u49892 \u51060  \u51080 \u51012  \u49688 \u46020  \u51079 \u51004 \u45768 , \u48708 \u49345 \u44552 \u51012  \u44845  \u47560 \u47144 \u54644 \u50556  \u54633 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u44284 \u44144 \u51032  \u50500 \u54548 \u50640  \u55064 \u46168 \u47532 \u51648  \u47560 \u49464 \u50836 \u44284 \u44144 \u50640  \u53360  \u49892 \u49688 \u47484  \u54644 \u49436  \u51396 \u52293 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u47732  \u51648 \u44552  \u45817 \u51109  \u48260 \u47140 \u50556  \u54633 \u45