   17. \uc0\u48324 The Star \u48324  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u50668 \u49888 \u51060  \u46405 \u44284  \u54840 \u49688 \u50640  \u47932 \u51012  \u51452 \u45716  \u47784 \u49845 \u55148 \u47581 \u51012  \u44032 \u51648 \u44172  \u46104 \u45716  \u49345 \u54889 \u51104 \u51116 \u47141 \u51060  \u51080 \u45716  \u49324 \u46988 \u44060 \u48169 \u51201 \u51064  \u53468 \u46020 , \u50676 \u47536  \u47560 \u51020 \u45576 \u51060  \u45320 \u47924  \u45458 \u44144 \u45208  \u54788 \u49892 \u51201 \u51064  \u45432 \u47141  \u48512 \u51313 \u44144 \u47532 \u44048  \u45712 \u44788 \u51648 \u45716  \u49345 \u45824  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u55148 \u47581 \u51012  \u44032 \u51648 \u44172  \u46104 \u45716 \u48324  \u52852 \u46300 \u45716  \u55148 \u47581 \u51012  \u44032 \u51648 \u44172  \u46108 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51328  \u45908  \u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u54616 \u47732 , \'91\u51096  \u46112  \u44163  \u44057 \u51008  \u44592 \u48516 \u51060  \u46304 \u45796 \'92 \'91\u45817 \u49888 \u50640 \u44172  \u44592 \u45824 \u47484  \u54408 \u44172  \u46108 \u45796 \'92 \'91\u51060 \u48264 \u50640 \u45716  \u45712 \u45196 \u51060  \u51339 \u45796 \'92 \u51221 \u46020 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u48324 \u51008  \u55148 \u47581 \u51032  \u49345 \u51669 \u51077 \u45768 \u45796 . \u52852 \u46300 \u51032  \u47532 \u46377  \u46608 \u54620  \'91\u55148 \u47581 \u44284  \u44032 \u45733 \u49457 \'92\u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \'91\u49892 \u51228 \u47196  \u55148 \u47581 \u51060  \u51080 \u45716 \u51648 \'92 \u50668 \u48512 \u45716  \u48324 \u44060 \u51077 \u45768 \u45796 . \u51060 \u48120 \u51648 \u50640 \u49436  \'91\u49892 \u51228  \u44208 \u44284 \u47932 \'92\u47196  \u46300 \u47084 \u45212  \u44163 \u51008  \u49352 \u49913 \u46308 \u51060 \u47728 , \u50500 \u51649  \u50672 \u50557 \u54616 \u44256  \u51089 \u50500 \u48372 \u51077 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u55148 \u47581 \u51004 \u47196  \u44032 \u46301  \u52264 \u49436  \u47924 \u50631 \u51060 \u46304  \u51032 \u50837  \u45336 \u52824 \u45716  \u49345 \u54889 \u49345 \u45824 \u48169 \u46020  \u45208 \u50640 \u44172  \u54840 \u44048 \u51060  \u51080 \u45716  \u44163  \u44057 \u51008  \u44592 \u48516 \u51060  \u46304 \u45796 \u49345 \u45824 \u48169 \u51060  \u45236  \'91\u50868 \u47749 \u51032  \u49324 \u46993 \'92\u51060  \u46112  \u44163  \u44057 \u51008  \u45712 \u45196 \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u45817 \u49888 \u51012  \u51060 \u49345 \u54805 \u52376 \u47100  \u49373 \u44033 \u54620 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u50612 \u51804  \u51200 \u47111 \u44172  \u50756 \u48317 \u54624 \u44620 ! \u45796 \u47564 , \u45817 \u49888 \u51060  \'91\u49892 \u51228 \u47196  \u44536 \u47088  \u49324 \u46988 \u51064 \u51648 \'92 \u45716  \u50500 \u51649  \u54869 \u51064 \u54616 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u49892 \u47581 \u51060  \u53364  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u51104 \u51116 \u47141 \u51060  \u51080 \u45716 \u53440 \u47196  \u48324 \u51008  \u51104 \u51116 \u47141  \u51080 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u46405 \u50640 \u49436 \u45716  \u51089 \u51008  \u49352 \u49913 \u46308 \u51060  \u47560 \u44396 \u47560 \u44396  \u51088 \u46972 \u45208 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50668 \u49888 \u51060  \u47932 \u51012  \u44228 \u49549  \u51456 \u45796 \u47732 , \u49352 \u49913 \u51008  \u47924 \u47085 \u47924 \u47085  \u51088 \u46972  \u45208 \u47924 \u44032  \u46112  \u44163 \u51077 \u45768 \u45796 . \u51060 \u45716  \u49352 \u47196 \u50868  \u