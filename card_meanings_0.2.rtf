   \uc0\u50640 \u51060 \u49828  \u52981  \'96 \u44048 \u51221 \u51032  \u49884 \u51089  \u50640 \u51060 \u49828  \u52981  \u44536 \u47548 \u51032  \u51032 \u48120 \u44048 \u51221 \u51060 \u45208  \u49373 \u44033 , \u54840 \u44048 \u51060  \u49352 \u47213 \u44172  \u49373 \u44596  \u49345 \u54889 \u49345 \u45824 \u48169 \u51012  \u51060 \u54644 \u54616 \u44172  \u46104 \u50612  \u49552 \u51012  \u45236 \u48120 \u45716  \u47784 \u49845 \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u44172  \u46120 \u44048 \u51221  \u49548 \u47784 \u44032  \u49900 \u54620  \u49324 \u46988 \u44048 \u51221 \u51012  \u47784 \u46160  \u48708 \u50892 \u45236 \u45716  \u49345 \u54889 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49352 \u47213 \u44172  \u47560 \u51020 \u51060  \u49373 \u44596 \u50640 \u51060 \u49828  \u52981 \u51008  \u51339 \u50500 \u54616 \u45716  \u44048 \u51221 \u51060 \u45208 , \u44536 \u47532 \u50880  \u46321  \u49352 \u47196 \u50868  \u47560 \u51020 \u51060  \u49373 \u44596  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u54840 \u44048  \u44536 \u47532 \u44256  \u49324 \u46993 , \u54868 \u54644 \u51032  \u49884 \u51089 \u51060 \u46972 \u44256  \u48380  \u49688  \u51080 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u47932 \u51008  \u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \u51012  \u45208 \u53440 \u45236 \u45716  \u50836 \u49548 \u51077 \u45768 \u45796 . \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u54728 \u44277  \u49549  \u44396 \u47492 \u50640 \u49436  \u49552 \u51060  \u45208 \u50752  \u52981 \u51012  \u44148 \u45348 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u50896 \u47000  \u50630 \u50632 \u45912  \u44048 \u51221 , \u49373 \u44033 \u51060  \u49352 \u47213 \u44172  \u49373 \u44220 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51649 \u51109  \u46041 \u47308 \u47196 \u47564  \u50668 \u44220 \u45912  \u49324 \u46988 \u50640 \u44172  \u51060 \u49457 \u51201  \u54840 \u44048 \u51060  \u49373 \u44596  \u49345 \u54889 \u51032 \u50808 \u51032  \u47784 \u49845 \u51012  \u48156 \u44204 \u54644 \u49436  \u47691 \u51080 \u45796 \u45716  \u49373 \u44033 \u51060  \u49352 \u47213 \u44172  \u46496 \u50724 \u47480  \u49345 \u54889 \u50864 \u50672 \u55176  \u44536  \u49324 \u46988 \u50640  \u45824 \u54620  \u52628 \u50613 \u51060  \u46496 \u50724 \u47480  \u49345 \u54889 \u50528 \u51064 \u51060  \u55192 \u46308 \u50612 \u54616 \u45716  \u47784 \u49845 \u51012  \u48372 \u47728  \u50672 \u48124 \u51012  \u45712 \u45180 \u45716  \u49345 \u54889 \u49352 \u47213 \u44172  \u51064 \u51221 \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 Ace of Cups\u45716  \u45824 \u54364 \u51201 \u51064  \u51116 \u54924  \u52852 \u46300 \u51077 \u45768 \u45796 . \'91\u44536 \u47532 \u50880 \u51060  \u49352 \u47213 \u44172  \u49373 \u44220 \u45796 \'92\u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51060  \u49345 \u54889 \u50640 \u49440  \u45817 \u49888 \u51012  \u44536 \u47532 \u50892 \u54616 \u47728 , \u46608  \u44536  \u44048 \u51221 \u51060  \u48516 \u49688 \u52376 \u47100  \'91\u49368 \u49567 \u44256 \'92 \u51080 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u50724 \u48652  \u52981 \u51008  \u44288 \u44228 \u51032  \u49884 \u51089 , \u44032 \u45733 \u49457 \u51012  \u51032 \u48120 \u54616 \u45716  \u52852 \u46300 . \u50526 \u51004 \u47196  \u52981 \u51012  \u50612 \u46523 \u44172  \u52292 \u50872 \u51648 \u45716  \u45817 \u49888 \u50640 \u44172  \u45804 \u47160 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u49345 \u45824 \u48169 \u51012  \u51060 \u54644 \u54616 \u45716 \u52981  \u50640 \u51060 \u49828 \u45716  \u49345 \u45824 \u48169 \u51012  \u51060 \u54644 \u54616 \u44172  \u46096 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u44396 \u47492  \u49549 \u50640 \u49436  \u45208 \u50728  \u49552 \u51060  \u49552 \u48148 \u45797 \u51012  \u44256 \u49828 \u46976 \u55176  \u46300 \u47084 \u45236 \u47728  \'91\u44148 \u45348 \u51452 \u45716  \u46319 \u54620 \'92 \u47784 \u49845 \u51012  \u52712 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u49345 \u45824 \u48169 \u50640  \u45824 \u54620  \u48176 \u47140 , \u49345 \u45824 \u48169 \u51012  \u51060 \u54644 \u54616 \u47728  \u48155 \u50500 \u46308 \u51064 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u44221 \u44228 \u49900 \u51012  \u45734 \u52628 \u51648  \u50506 \u45912  \u49324 \u46988 \u51060  \u49436 \u49436 \u55176  \u47560 \u51020 \u51012  \u50668 \u45716  \u49345 \u54889 \u49324 \u51060 \u44032  \u50504  \u51339 \u50520 \u45796 \u47732 , \u47560 \u51020 \u51012  \u50676 \u44256  \u49436 \u47196  \u54868 \u54644 \u54616 \u45716  \u49884 \u44592 \u49345 \u45824 \u48169 \u50640  \u45824 \u54620  \u54840 \u51032 \u47484  \u49345 \u51669 \u47560 \u45285  \u45208 \u49240 \u44172 \u47564  \u48372 \u45912  \u49324 \u46988 \u51060  \u51060 \u54644 \u46104 \u44592  \u49884 \u51089 \u54632 Ace of Cups\u45716  \u48512 \u51221 \u51201 \u51060 \u45912  \u44288 \u44228 \u44032  \u44557 \u51221 \u51201 \u51004 \u47196  \u48320 \u54868 \u46112  \u49688  \u51080 \u45716  \u44592 \u54924 , \u44032 \u45733 \u49457 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \'91\u51116 \u54924 \'92, \'91\u54868 \u54644 \'92, \'91\u48120 \u50504 \u54620  \u44048 \u51221 \'92 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u47564 \u51313 \u44048 \u50640 \u51060 \u49828  \u50724 \u48652  \u52981 \u51008  \u47564 \u51313 \u44048 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52981 \u50640 \u49436  \u55128 \u47084 \u45336 \u52824 \u45716  \u47932 \u51008  \u46412 \u47196  \u44592 \u49256 \u51060 \u45208  \u54665 \u48373 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51593  \u44536 \u47088  \u47560 \u51020 \u51060  \u49116 \u50612 \u51256  \u45208 \u50724 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u50668 \u44592 \u49436  \u54252 \u51064 \u53944 \u45716 , \u51060  \u47564 \u51313 \u44048 \u51060  \'91\u47932 \u51656 \'92\u48372 \u45796  \'91\u44048 \u51221 \'92\u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u45796 \u45716  \u44163 \u51077 \u45768 \u45796 . \u54665 \u48373 , \u49324 \u46993 , \u48176 \u48512 \u47492 , \u54200 \u50504 \u54632 , \u49444 \u47128  \u46321 \u51077 \u45768 \u45796 . \u52981 \u51008  \u47932 \u51032  \u50836 \u49548 \u47484  \u44054 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u44208 \u54844 \u51060 \u45208  \u52636 \u49328 \u52376 \u47100  \u52629 \u54616  \u48155 \u45716  \u51068 \u51060  \u49373 \u44608 \u50896 \u54616 \u45912  \u47785 \u54364 \u47484  \u45804 \u49457 \u54644 \u49436  \u49457 \u52712 \u44048 \u51012  \u45712 \u45180 \u45716  \u49345 \u54889 \u49548 \u48149 \u54620  \u49440 \u47932 \u51060 \u51648 \u47564  \u44048 \u46041  \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 \u49352 \u47196 \u50868  \u50500 \u51060 \u46356 \u50612 \u44032  \u46496 \u50732 \u46972  \u44592 \u49256 \u51012  \u45712 \u45180 \u45716  \u49345 \u54889 \u52981 \u51032  \u50640 \u51060 \u49828 \u45716  \u51088 \u49888  \u49104 \u47564  \u50500 \u45768 \u46972  \u51452 \u50948 \u50640 \u49436 \u46020  \u52629 \u48373  \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u46608  \u49692 \u51652 \u47924 \u44396 \u54620  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u44592  \u46412 \u47928 \u50640 , \u44145 \u51221  \u44540 \u49900  \u46321 \u51008  \u48520 \u54596 \u50836 \u54616 \u45796 \u44256  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u52981 \u51008  \u47932 \u51032  \u49549 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640  \'91\u47932 \u51656 \u51201 \u51064  \u47564 \u51313 \u44048 \'92\u44284 \u45716  \u47924 \u44288 \u54633 \u45768 \u45796 . \u12300 \u52981 \u51032  \u50640 \u51060 \u49828 \u12301 \u51032  \u51032 \u48120 \u50752  \u54644 \u49437  <\u51068 , \u50672 \u50528 > \'96 tarotfun.jp\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44032 \u48316 \u50868  \u54840 \u44048 \u50640  \u44536 \u52824 \u45716 \u50640 \u51060 \u49828  \u52981 \u51008  \u44032 \u48316 \u50868  \u54840 \u44048 , \u54841 \u51008  \u54840 \u51032 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300 \u51032  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u52981 \u50640 \u49436  \u47932 \u51460 \u44592 \u44032  \u45336 \u52824 \u51648 \u47564  \u52981  \u49549 \u51008  \u52292 \u50892 \u51648 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u51060 \u45716  \u51593  \'91\u44048 \u51221 \u51060 \u45208  \u47560 \u51020 \'92\u51060  \u50500 \u51649  \u45916  \u52649 \u51313 \u46096 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u51008  \u51080 \u51004 \u45208  \u44618 \u51648  \u50506 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u51020 \u44536 \u51200  \u51339 \u51008  \u49324 \u46988 \u51004 \u47196  \u45712 \u45180 \u45716  \u44048 \u51221 \u52828 \u54632 \u48372 \u45796 \u45716  \u52828 \u44540 \u44048 \u51060  \u50612 \u50872 \u47532 \u47728 , \'91\u54840 \u51032 \'92\u47484  \u48288 \u54392 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u44536 \u47084 \u45208  \u49552 \u51012  \u45236 \u48128 \u44256  \u51080 \u45796 \u45716  \u51216 \u50640 \u49436  \u44288 \u44228 \u45716  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u50668 \u44592 \u49436  \u47568 \u54616 \u44256  \u49910 \u51008  \u54252 \u51064 \u53944 \u45716  \'91\u44048 \u51221 \'92\u51060  \u52488 \u48152  \u45800 \u44228 \u50640  \u47672 \u47932 \u47084  \u51080 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u52572 \u44540 \u50640  \u49373 \u44596  \u54840 \u44048 , \u52572 \u44540 \u50640  \u49373 \u44596  \u44288 \u49900 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u44048 \u51221  \u49548 \u47784 \u52981  \u50640 \u51060 \u49828 \u45716  \u48516 \u45432 \u45208  \u49836 \u54548  \u46321 \u51004 \u47196  \u44048 \u51221  \u49548 \u47784 \u47484  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u52981 \u50640 \u49436  \u55120 \u47476 \u45716  \u47932 \u51008  \u51339 \u51008  \u44048 \u51221 \u51068  \u49688 \u46020  \u51080 \u51648 \u47564 , \u45208 \u49244  \u44048 \u51221 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u48516 \u45432 , \u49836 \u54548 , \u51396 \u52293 \u44048 , \u51088 \u44340 \u44048 \u51077 \u45768 \u45796 . \u46608 \u54620  \u52981  \u50504 \u51032  \u47932 \u51060  \u50756 \u51204 \u55176  \u52292 \u50892 \u51648 \u51648  \u50506 \u50520 \u45796 \u45716  \u51216 \u50640 \u49436 , \u51648 \u45208 \u52824 \u44172  \u44048 \u51221 \u51012  \'91\u49548 \u47784 \'92\u54616 \u44256  \u51080 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u54756 \u50612 \u51652  \u46244  \u49836 \u54548 \u51012  \u50143 \u44256  \u51080 \u45716  \u49345 \u54889 \u45817 \u49888 \u51032  \u50612 \u46500  \u53468 \u46020 \u47196  \u48516 \u45432  \u54841 \u51008  \u49892 \u47581 \u51032  \u44048 \u51221 \u51012  \u45712 \u45180 \u45716  \u49345 \u54889 \u45572 \u44400 \u44032 \u50752  \u49912 \u50892 \u49436  \u49888 \u44221 \u51060  \u44228 \u49549  \u44144 \u49836 \u47532 \u45716  \u49345 \u54889 \u46608 \u54620  \u52981 \u51008  \u44592 \u48376 \u51201 \u51004 \u47196  \u47932 \u51012  \u48155 \u45716  \u46020 \u44396 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44263  \u45208 \u49244  \u44048 \u51221 \u51012  \u54364 \u52636 \u54616 \u44592 \u48372 \u45796  \u51068 \u48169 \u51201 \u51004 \u47196  \u48155 \u51012  \u49688  \u51080 \u51020 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50756 \u46300 \u45208  \u49548 \u46300 \u45716  \u48150 \u51004 \u47196  \'91\u48156 \u49328 \'92\u54616 \u45716  \u49800 \u53944 \u51060 \u47728 , \u52981 \u51060 \u45208  \u54172 \u53440 \u53364 \u51008  \u50504 \u50640 \u49436  \'91\u52629 \u51201 \'92\u54616 \u45716  \u49800 \u53944 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45224 \u46308 \u51032  \u54616 \u49548 \u50672 \u51012  \u46308 \u50612 \u51452 \u44592 \u47564  \u54616 \u45768 , \u44048 \u51221  \u50416 \u47112 \u44592 \u53685 \u51060  \u46108  \u44163  \u44057 \u51008  \u44592 \u48516 \u51068 \u48169 \u51201 \u51004 \u47196  \u50528 \u51221 \u51012  \u48155 \u44592 \u47564  \u54616 \u45716  \u49345 \u54889  ( = \u48512 \u45812 \u49828 \u47084 \u50880  )\u51649 \u51109 \u50640 \u49436  \u54868 \u54400 \u51060 \u47564  \u45817 \u54616 \u45716  \u49345 \u54889 \u44048 \u51221 \u51060  \u50526 \u51004 \u47196  \u44228 \u49549  \u52292 \u50892 \u51656 \u51648 , \u55128 \u47084 \u45236 \u47540 \u51648 \u45716  \u48120 \u51648 \u49688 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44536  \u49324 \u46988 \u51032  \'91\u44536 \u47495 \'92\u50640  \u45804 \u47536  \u48277 \u51077 \u45768 \u45796 . \u54252 \u50857 \u47141 \u51060  \u52649 \u48516 \u54616 \u47732  \u52981 \u51012  \u45796 \u49884  \u52292 \u50892  \u47560 \u51020 \u51012  \u45796 \u51105 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .Comment\u47932 \u51012  \u50143 \u50500 \u49436  \u48708 \u50868 \u45796 \u45716  \u49324 \u49892 \u50640  \u52488 \u51216 \u51012  \u47582 \u52632 \u45796 \u47732 , \u44048 \u51221 \u51012  \u48708 \u50892 \u49436  \u48120 \u47144 \u51060  \u50630 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437  \u44032 \u45733 \u54633 \u45768 \u45796 . \u52981  \u50504 \u50640 \u45716  \u47932 \u51060  \u44144 \u51032  \u45224 \u50500 \u51080 \u51648  \u50506 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u52981  \u50672 \u50528 \u50868  \u54644 \u49437 \u50640 \u51060 \u49828  \u52981 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54616 \u47728  \u48176 \u47140 \u49900 \u46020  \u44618 \u51008  \u49457 \u44201 \u51077 \u45768 \u45796 . \u52981 \u51008  \u53440 \u47196 \u51032  4\u50896 \u49548  \u51473 \u50640 \u49436  \u44032 \u51109  \'91\u50668 \u49457 \u51201 \'92\u51064  \u49800 \u53944 \u51077 \u45768 \u45796 . MBTI\u50640 \u49440  \'91F\'92 \u44228 \u50676 \u51060  \u44032 \u51109  \u50612 \u50872 \u47532 \u47728 , \u48376 \u51064 \u51032  \u44048 \u51221 , \u49373 \u44033 \u51012  \u54364 \u52636 \u54616 \u44592 \u48372 \u45796  \u45224 \u51032  \u44048 \u51221 , \u49373 \u44033 \u51012  \u48155 \u45716 \u45936  \u51061 \u49689 \u54633 \u45768 \u45796 . ~\u54616 \u51088 . ~\u54644 \u46972  \u52376 \u47100  \u44053 \u50517 \u51201 \u51064  \u47568 \u53804 \u47484  \u49324 \u50857 \u54616 \u44592 \u48372 \u45796  ~\u54664 \u51004 \u47732  \u51339 \u44192 \u45796  ~\u54616 \u45716 \u44172  \u50612 \u46523 \u44192 \u45768  \u52376 \u47100  \u50976 \u54620  \u47568 \u53804 \u47484  \u49324 \u50857 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u52981 \u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u44284  \u54620 \u52789  \u45908  \u44032 \u44620 \u50892 \u51648 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u49352 \u47213 \u44172  \u54840 \u44048 \u51060  \u49373 \u44220 \u44144 \u45208 , \u50612 \u51788 \u47732  \u49324 \u46993 \u50640  \u48736 \u51276 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46608  \u51060 \u51204 \u44284 \u45716  \u45796 \u47480  \u44048 \u51221 \u51077 \u45768 \u45796 . \u48708 \u54840 \u44048 \u50640 \u49436  \u54840 \u44048 \u51004 \u47196 , \u47924 \u44288 \u49900 \u50640 \u49436  \u44288 \u49900 \u51004 \u47196 , \u52828 \u44396 \u50640 \u49436  \u51060 \u49457 \u51004 \u47196 . \u49324 \u51060 \u44032  \u49436 \u47673 \u49436 \u47673 \u54664 \u45796 \u47732  \u51648 \u44552 \u52196  \'91\u50857 \u49436 \'92\u54616 \u45716  \u47560 \u51020 \u51060  \u49373 \u44220 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u50500 \u51649  \u45817 \u49888 \u50640 \u44172  \'91\u44032 \u48316 \u50868  \u54840 \u44048 \'92\u47564 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172 \u49436  \u51648 \u45208 \u52824 \u44172  \'91\u44288 \u49900 \'92\u51012  \u48155 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54620 \u44732 \u48264 \u50640  \u47566 \u51008  \u44048 \u51221 \u51060  \u46308 \u51060 \u45797 \u52824 \u45768  \u51648 \u52828  \u47784 \u49845 \u51077 \u45768 \u45796 . \u50500 \u51649  \u44536 \u51032  \u47560 \u51020 \u51008  \u44032 \u46301  \u52264 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u47560 \u51020 \u51060  \u54620 \u52789  \u45908  \u44053 \u47148 \u54644 \u51648 \u47728  \u49324 \u46993 \u51060  \u55128 \u47084 \u45336 \u52824 \u44172  \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u53945 \u55176  \u49436 \u47196 \u47484  \u44618 \u44172  \u51060 \u54644 \u54616 \u47728 , \u44277 \u44048 \u45824 \u47484  \u47564 \u46308  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u50584 \u44032  \u51060 \u47088  \u47732 \u46020  \u51080 \u50632 \u44396 \u45208 . \u44536 \u47092  \u49688 \u46020  \u51080 \u44192 \u45796  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u44048 \u51221  \u49548 \u47784 \u44032  \u49900 \u54616 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u46168  \u51473  \u45572 \u44400 \u44032 \u45716  \u47564 \u45216  \u46412 \u47560 \u45796  \u49892 \u47581 \u54616 \u44256 , \u54868 \u45208 \u47728 , \u49836 \u54140 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u49373 \u44033 \u51060 \u45208  \u44048 \u51221 \u51012  \'91\u44277 \u50976 \'92\u54616 \u45716  \u49884 \u44036 \u51060  \u51473 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u50640 \u51060 \u49828  \u52981 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u54644 \u49437 \u51032  \u54252 \u51064 \u53944 \u45716  \u53356 \u44172  2\u44032 \u51648 . \u52395  \u48264 \u51704 \u45716  \u45817 \u49888 \u51012  \u51060 \u54644 \u54616 \u47728  \u51060 \u51228 \u45716  \u48155 \u50500 \u46308 \u51064 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44396 \u47492  \u49549  \u49552 \u51060  \u52981 \u51012  \u44148 \u45348 \u51452 \u46319 , \u51116 \u54924 \u47484  \u51228 \u50504 \u54616 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u46160  \u48264 \u51704 \u45716  \u44536 \u47532 \u50880 \u51077 \u45768 \u45796 . \u52981  \u49549 \u51032  \u47932 \u51060  \u45336 \u52824 \u45716  \u44163 \u52376 \u47100 , \u49345 \u45824 \u48169 \u51032  \u49373 \u44033 \u44284  \u44048 \u51221 \u46020  \u55128 \u47084 \u45336 \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50612 \u51788 \u47732  \u51060 \u44172  \u45817 \u49888 \u44284 \u51032  \u52628 \u50613 \u51068  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51116 \u54924 \u50868 \u51012  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54620 \u45796 \u47732 , \u45800 \u49692 \u54620  \'91\u48516 \u45432 \'92 \u54841 \u51008  \'91\u48120 \u47144 \u51012  \u48260 \u47160 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 . \u45796 \u47564  \u51116 \u54924 \u50640  1\u44032 \u51648  \u51109 \u50528 \u47932 \u51060  \u51080 \u45796 \u47732 . \u48148 \u47196  \u49688 \u46041 \u51201 \u51064  \u47560 \u51020 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u50500 \u51649  \u51116 \u54924 \u47484  \'91\u51201 \u44537 \u51201 \u51004 \u47196  \u52628 \u51652 \'92\u54616 \u47140 \u45716  \u47560 \u51020 \u51008  \u50630 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u52981  \u51649 \u50629 \u50868  \u54644 \u49437 \u50668 \u47084 \u47784 \u47196  \u49457 \u52712 \u44048 \u44284  \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u47932 \u51656 \u51201 \u51004 \u47196  \u53360  \u49457 \u44284 \u47484  \u44144 \u46160 \u44592 \u48372 \u45796 \u45716 , \u45236 \u47732 \u51201 \u51004 \u47196  \u44592 \u48516 \u51060  \u45208 \u50500 \u51648 \u44256  \u54665 \u48373 \u54632 \u51012  \u45712 \u45180 \u44172  \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49849 \u51652 , \u49324 \u46988 \u46308 \u51032  \u51064 \u51221 , \u54868 \u44592 \u50528 \u50528 \u54620  \u48516 \u50948 \u44592  \u46321 \u51077 \u45768 \u45796 . \u49352 \u47213 \u44172  \u47924 \u50616 \u44032 \u47484  \u44648 \u45804 \u50500  \'91\u45817 \u49888 \u51032  \u47560 \u51020 \u44032 \u51664 , \u53468 \u46020 \'92\u44032  \u44557 \u51221 \u51201 \u51004 \u47196  \u48320 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u52981  \u44552 \u51204 \u50868  \u54644 \u49437 \u46024  \u47784 \u51060 \u45716  \u50868 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47196  \u51064 \u54644  \u51116 \u51221 \u51201 \u51064  \'91\u47564 \u51313 \u44048 \'92\u51012  \u45712 \u45180 \u45716  \u49884 \u44592 \u44032  \u46104 \u44192 \u49845 \u45768 \u45796 . \u45236 \u44032  \u46024 \u51012  \u51060 \u51221 \u46020 \u45208  \u47784 \u50520 \u45796 \u44256 ? \u51060  \u51221 \u46020 \u47196  \u47566 \u51060  \u48268 \u50632 \u45796 \u44256 ? \u44536 \u47084 \u45208  \u46024 \u51012  \u44284 \u46020 \u54616 \u44172  \'91\u47784 \u51004 \u44592 \u47564  \u54624  \u49688  \u51080 \u45796 \'92\u45716  \u44221 \u44256 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 . \u48120 \u47000 \u47484  \u50948 \u54644  \u46024 \u51012  \u50416 \u44256  \u53804 \u51088 \u54644 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .    \
