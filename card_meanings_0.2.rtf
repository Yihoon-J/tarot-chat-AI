   6. \uc0\u50672 \u51064 The Lovers \u50672 \u51064  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u49440 \u50501 \u44284 \u47484  \u47673 \u44592  \u51204  \u47784 \u49845 \u46160  \u45224 \u45376 \u45716  \u49436 \u47196  \u49457 \u51201 \u51064  \u54840 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u51020 \u44032 \u48316 \u50868  \u54840 \u44048 , \u50672 \u50528  \u52488 \u48152 \u51032  \u49444 \u47128 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u46412 \u50640  \u46384 \u46972  \u51652 \u49900  \u50630 \u45716  \u47560 \u51020 \u51060 \u45208  \u49340 \u44033  \u44288 \u44228 \u47484  \u49345 \u51669 \u50948 \u54744 \u54620  \u50976 \u54841 \u51012  \u44221 \u44256 \u54616 \u45716  \u52852 \u46300  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49457 \u51201 \u51064  \u54840 \u44048 \u51012  \u45712 \u45180 \u45716 \u50672 \u51064  \u52852 \u46300 \u45716  \u50977 \u52404 \u51201  \u44417 \u54633 \u51060  \u51339 \u51008  \u44288 \u44228 , \u54841 \u51008  \u51060 \u49457 \u51004 \u47196 \u49436 \u51032  \u54840 \u44048 \u51060 \u45208  \u44288 \u49900 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u46160  \u45224 \u45376 \u44032  \u48268 \u44144 \u48279 \u51008  \u44148 , \u49436 \u47196 \u47484  \'91\u51060 \u49457 \'92\u51004 \u47196  \u51064 \u49885 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \'91\u45224 \u49324 \u52828 , \u50668 \u49324 \u52828 \'92\u51004 \u47196  \u49373 \u44033 \u54664 \u45912  \u49324 \u46988 , \'91\u44277 \u51201 \u51064  \u44288 \u44228 \u47196 \u47564  \u50668 \u44220 \u45912  \u49324 \u46988 \'92\u51060  \u51060 \u51228 \u45716  \u51060 \u49457 \u51004 \u47196  \u45712 \u44788 \u51652 \u45796 \u45716  \u51032 \u48120 \u51424 .\u50501 \u47560  \u52852 \u46300 \u47484  \u44057 \u51060  \u48372 \u49884 \u47732  \u51060 \u54644 \u44032  \u48736 \u47493 \u45768 \u45796 . \u51060  \u52852 \u46300  \u46608 \u54620  \u46160  \u45224 \u45376 \u44032  \u47784 \u46160  \u48268 \u44144 \u48279 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44396 \u46020 \u46020  \u48708 \u49847 \u54616 \u51424 . \u52852 \u46300 \u51032  \u51032 \u48120 \u46020  \'91\u49457 \u51201 \u51064  \u54840 \u44048 , \u47588 \u47141 \u51201 \u51064  \u44288 \u44228 \'92\u51077 \u45768 \u45796 . \u52264 \u51060 \u44032  \u51080 \u45796 \u47732 , \u50501 \u47560  \u52852 \u46300 \u45716  \u49436 \u47196 \u47484  \'91\u44396 \u49549 \'92\u54616 \u44256  \u51080 \u45796 \u45716  \u51216 , \u50672 \u51064  \u52852 \u46300 \u45716  \u49436 \u47196 \u44032  \'91\u51088 \u50976 \u48516 \u48169 \'92\u54616 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 .\u50501 \u47560  \u52852 \u46300 \u54028 \u50892 \u44032  \u45908  \u49480 \u49457 \u51201 \u51064  \'91\u44396 \u49549 \'92\u51012  \u49345 \u51669 \u50672 \u51064  \u52852 \u46300 \u54028 \u50892 \u44032  \u45796 \u49548  \u50557 \u54632 \u49457 \u51201 \u51064  \'91\u54840 \u44048 \'92\u51012  \u49345 \u51669  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u54644 \u49437 \u54624  \u44221 \u50864 , \u45817 \u49888 \u50640 \u44172  \u49457 \u51201 \u51064  \u54840 \u44048 \u51012  \u45712 \u45180 \u44144 \u45208 , \u47588 \u47141 \u51012  \u45712 \u45184 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50040 \u51060 \u45208  \u50672 \u50528  \u52488 \u48152 \u50640 \u45716  \u47588 \u50864  \u44557 \u51221 \u51201 \u51064  \u52852 \u46300 \u51077 \u45768 \u45796 . \'91\u49436 \u47196 \u47484  \u51032 \u49885 \u54616 \u44592  \u49884 \u51089 \u54664 \u45796 \'92\u45716  \u51032 \u48120 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u45796 \u47564 , \u51060 \u44163 \u51060  \'91\u51652 \u51221 \u54620  \u49324 \u46993 \'92\u51012  \u45208 \u53440 \u45240 \u45796 \u45716  \u46907 \u51008  \u50500 \u45785 \u45768 \u45796 . \u48372 \u45796 \u49884 \u54588  \u45224 \u45376 \u45716  \u45796 \u47480  \u44275 \u51012  \u48148 \u46972 \u48372 \u44256  \u51080 \u44256 , \u45224 \u51088 \u45716  \u50668 \u51088 \u47484  \'91\u52293 \u47581 \u54616 \u46319 \'92 \u52432 \u45796 \u48372 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44032 \u48316 \u50868  \u45132 \u47548 \u51060 \u45208  \u54840 \u44048 , \u49444 \u47128 \u50640  \u44032 \u44620 \u50868  \u52852 \u46300 \u51077 \u45768 \u45796 . \u8251  \u54644 \u49437  \u49884  \u51452 \u51032 \u51216 \u52852 \u46300  \u49549  \u52380 \u49324 \u45716  \u46160  \u45224 \u45376 \u47484  \'91\u52629 \u48373 \'92\u54616 \u44256  \u51080 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u53468 \u50577 \u51032  \u48731 \u51012  \u45216 \u44060 \u50752  \u49552 \u51004 \u47196  \u44032 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536  \u44208 \u44284 , \u52852 \u46300 \u51032  \u54616 \u45800 \u48512 \u45716  \u50864 \u51473 \u52649 \u54620  \u54028 \u46976 \u49353 \u51077 \u45768 \u45796 . \u49457 \u44221 \u50640 \u46020  \u48520 \u51032  \u52860 \u51012  \u46304  \u52380 \u49324 \u44032  \u50500 \u45812 \u44284  \u51060 \u48652 \u47484  \u50640 \u45940  \u46041 \u49328 \u50640 \u49436  \u45236 \u51923 \u50520 \u45796 \u44256  \u46104 \u50612 \u51080 \u45716 \u45936 , \u48512 \u48512 \u47484  \u45236 \u51923 \u51008  \u44536  \u52380 \u49324 \u44032  \u48148 \u47196  \u46972 \u54028 \u50648 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51652 \u49900  \u50630 \u45716  \u54665 \u46041 \u50672 \u51064  \u52852 \u46300 \u45716  \u51652 \u49900  \u50630 \u45716  \u44288 \u44228 \u45208  \u54665 \u46041 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u46160  \u45224 \u45376 \u51032  \u54364 \u51221 \u44284  \u49884 \u49440 \u51012  \u51088 \u49464 \u55176  \u48372 \u47732 , \u45224 \u51088 \u45716  \u50620 \u44404 \u51012  \u52236 \u54392 \u47532 \u47728  \u50668 \u51088 \u47484  \u48148 \u46972 \u48372 \u44256  \u51080 \u44256 , \u50668 \u51088 \u45716  \u45824 \u52380 \u49324  \u46972 \u54028 \u50648 \u51012  \u48148 \u46972 \u48372 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51593  \u49436 \u47196 \u50640 \u44172  \'91\u49457 \u51201 \u51064  \u54840 \u44048 \'92\u51008  \u45712 \u45180 \u44256  \u51080 \u51004 \u45208 , \u50500 \u51649  \u51652 \u51221 \u54620  \u49324 \u46993 \u51004 \u47196  \u48156 \u51204 \u54616 \u51648 \u45716  \u47803 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :Q. \u52572 \u44540  \u46308 \u50612  \u50672 \u46973 \u51060  \u46904 \u54644 \u51652  \u49324 \u46988 \u51060  \u51080 \u45716 \u45936 , \u51200 \u54620 \u53580  \u51652 \u51676 \u47196  \u47560 \u51020 \u51060  \u51080 \u45716  \u44152 \u44620 \u50836 ?A. \u50500 \u47560 \u46020  \u53360  \u54840 \u44048 \u51008  \u50630 \u51012  \u44163  \u44057 \u50500 \u50836 .Q. \u51339 \u50500 \u49436  \u49884 \u51089 \u54620  \u51068 \u51064 \u45936 , \u50836 \u51608  \u46308 \u50612  \u51088 \u44984 \u47564  \u48120 \u47336 \u44172  \u46076 \u50836 . \u51228 \u44032  \u51652 \u49900 \u51004 \u47196  \u51060  \u51068 \u51012  \u51339 \u50500 \u54616 \u45716  \u44152 \u44620 \u50836 ?A. \u50500 \u47560 \u46020  \u44536  \u51068 \u51012  \u51652 \u49900 \u51004 \u47196  \u51339 \u50500 \u54616 \u45716  \u44148  \u50500 \u45772  \u46319  \u54633 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u46321 \u51109 \u54624  \u44221 \u50864 , \u54840 \u44048 \u51060  \u44536 \u47111 \u44172  \u53360  \u49345 \u53468 \u45716  \u50500 \u45768 \u46972 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u46888 \u44144 \u50868  \u49324 \u46993 \u48372 \u45796 \u45716  \'91\u50557 \u44036 \u51032  \u49444 \u47128 , \u47805 \u44544 \u47805 \u44544 \u54632 \'92\u51060  \u50612 \u50872 \u47532 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u49340 \u44033 \u44288 \u44228 \u50672 \u51064  \u52852 \u46300 \u45716  \u49340 \u44033 \u44288 \u44228 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52852 \u46300  \u49549  \u51064 \u47932 \u46308 \u51008  \u47784 \u46160  \u45796 \u47480  \u48169 \u54693 \u51012  \u48148 \u46972 \u48372 \u44256  \u51080 \u51004 \u47728 , \u50620 \u44404  \u46608 \u54620  \u52236 \u54392 \u47532 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u50696 \u52968 \u45824  \u47532 \u46377 \u50640 \u49436 \u45716  :\u50672 \u51064 \u51032  \u44288 \u49900 \u51060  \u49884 \u46308 \u54644 \u51276 \u50612 \u50836 . \u50780 \u51068 \u44620 \u50836 ?\u49352 \u47196  \u54840 \u44048  \u44032 \u45716  \u49345 \u45824 \u44032  \u49373 \u44220 \u44144 \u45208 , \u45817 \u49888 \u50640  \u45824 \u54620  \u55141 \u48120 \u44032  \u46504 \u50612 \u51652  \u46319  \u48372 \u51077 \u45768 \u45796 .\u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51068  \u44221 \u50864 , \u51068 \u48169 \u51201 \u51064  \u51677 \u49324 \u46993 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u54616 \u45720 \u51032  \u52380 \u49324 \u45716  \u44536 \u51200  \u45576 \u51012  \u44048 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51060  \'91\u45576 \u50640  \u50504  \u52272 \'92 \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u50948 \u54744 \u54620  \u50976 \u54841 \u50672 \u51064  \u52852 \u46300 \u45716  \u50948 \u54744 \u54620  \u50976 \u54841 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u51109 \u47732 \u51008 , \u50640 \u45940  \u51221 \u50896 \u50640 \u49436  \u48192 \u51032  \u50976 \u54841 \u50640  \u45336 \u50612 \u44032 \u45716  \u46160  \u45224 \u45376 \u51032  \u51060 \u50556 \u44592 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45800 \u49692 \u54620  \u49324 \u46993  \u49828 \u53664 \u47532 \u44032  \u50500 \u45785 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45208 \u47924 \u50640 \u45716  \u44536  \u50976 \u54841 \u51012  \u49345 \u51669 \u54616 \u45716  \u48192 \u51060  \u47588 \u45804 \u47140  \u51080 \u49845 \u45768 \u45796 . \u54616 \u45712 \u45784 \u44760 \u49436 \u45716  \u48372 \u44592  \u51339 \u44256  \u47579 \u51080 \u45716  \u50676 \u47588 \u44032  \u50676 \u47532 \u45716  \u45208 \u47924 \u47484  \u46405 \u50640 \u49436  \u46027 \u50500 \u45208 \u44172  \u54616 \u49512 \u45796 . \u46608  \u44536  \u46041 \u49328  \u54620 \u44032 \u50868 \u45936 \u50640  \u49440 \u44284  \u50501 \u51012  \u50508 \u44172  \u54616 \u45716  \u45208 \u47924 \u46020  \u46027 \u50500 \u45208 \u44172  \u54616 \u49512 \u45796 . \u54616 \u45712 \u45784 \u44760 \u49436  \u50500 \u45812 \u51012  \u45936 \u47140 \u45796 \u44032 , \u50640 \u45940  \u46041 \u49328 \u51012  \u46028 \u48372 \u44172  \u54616 \u49884 \u47728  \u51060 \u47111 \u44172  \u51060 \u47476 \u49512 \u45796 . \'93\u50640 \u45940  \u46041 \u49328 \u50640  \u51080 \u45716  \u45208 \u47924  \u50676 \u47588 \u47484  \u47924 \u50631 \u51060 \u46304 \u51648  \u47560 \u51020 \u45824 \u47196  \u46384 \u47673 \u50612 \u46972 . \u44536 \u47084 \u45208  \u49440 \u44284  \u50501 \u51012  \u50508 \u44172  \u54616 \u45716  \u45208 \u47924  \u50676 \u47588 \u47564 \u51008  \u46384 \u47673 \u51648  \u47560 \u46972 .\u44536 \u44163 \u51012  \u46384 \u47673 \u45716  \u45216 , \u45320 \u45716  \u48152 \u46300 \u49884  \u51453 \u45716 \u45796 . \u52285 \u49464 \u44592  2\u51109  9, 15~17\u51208 \u50808 \u46020 \u45208  \u46020 \u48149 , \u53804 \u51088  \u44428 \u50976  \u46321  \u50948 \u54744 \u54620  \u50976 \u54841 \u51012  \u44221 \u44256 \u54616 \u45716  \u52852 \u46300 \u47196 \u46020  \u50416 \u51077 \u45768 \u45796 . \u44049 \u51088 \u44592  \u51217 \u44540 \u54616 \u45716  \u49324 \u46988 \u51012  \u51312 \u49900 \u54616 \u44256 , \u47588 \u47141 \u51201 \u51064  \u51228 \u50504 \u51060  \u50724 \u45908 \u46972 \u46020  \u51060 \u49457 \u51201 \u51004 \u47196  \u54032 \u45800 \u54644 \u50556  \u54633 \u45768 \u45796 .Comment\u50812 \u51901  \u45208 \u47924 \u45716  \u49440 \u50501 \u44284 \u47484  \u54408 \u51008  \u51648 \u54812 \u51032  \u45208 \u47924 \u51077 \u45768 \u45796 . \u51060  \u45208 \u47924 \u51032  \u50676 \u47588 \u47484  \u47673 \u51004 \u47732  \u49440 \u50501 \u51012  \u48516 \u48324 \u54616 \u45716  \u51648 \u54812 \u47484  \u50619 \u44172  \u46121 \u45768 \u45796 . \u45224 \u45376 \u45716  \u49440 \u50501 \u44284 \u47484  \u47673 \u51648  \u47568 \u46972 \u45716  \u54616 \u45712 \u45784 \u51032  \u44221 \u44256 \u47484  \u47924 \u49884 \u54620  \u46244 , \u51064 \u47448 \u50640 \u44172  \'91\u52636 \u49328 , \u45432 \u46041 , \u51453 \u51020 \'92\u51060 \u46972 \u45716  \u44256 \u53685 \u51012  \u50504 \u44200 \u51452 \u50632 \u49845 \u45768 \u45796 .\u50672 \u51064  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u50672 \u51064  \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \'91\u51060 \u49457 \u50640 \u44172  \u51064 \u44592 \u44032  \u47566 \u51008  \u50808 \u47784 \'92\u47484  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50668 \u51088 \u46972 \u47732 , \u50668 \u47532 \u50668 \u47532 \u54616 \u44256  U\u51316 \u51060  \u46748 \u47159 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u47728  \u45224 \u51088 \u46972 \u47732 , \u47800 \u47588 \u44032  \u53444 \u53444 \u54616 \u44256  T\u51316 \u51060  \u46748 \u47159 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51060 \u49457 \u51004 \u47196  \u48372 \u51060 \u44592 \u50644  \u52649 \u48516 \u54620  \u50808 \u47784 \u51077 \u45768 \u45796 .\u50672 \u51064  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \'91\u49457 \u51201 \u51064  \u54840 \u44048 \'92\u51012  \u45712 \u45180 \u44256  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u52828 \u54620  \u52828 \u44396 \u45208  \u46041 \u47308 \u47484  \u45336 \u50612  \'91\u51060 \u49457 \'92\u51004 \u47196  \u45712 \u45184 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u50500 \u51649  \u51652 \u51648 \u54620  \u47560 \u51020 \u51012  \u44054 \u44256  \u51080 \u51652  \u50506 \u49845 \u45768 \u45796 . \'91\u44032 \u48316 \u50868  \u54840 \u44048 \'92, \'91\u50557 \u44036 \u51032  \u49444 \u47128 \'92\u50640 \u49436  \u44536 \u52832  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u53945 \u55176  \u44208 \u54844  \u50584 \u44592 \u47484  \u44732 \u45256 \u51012  \u46412  \u47928 \u51228 \u44032  \u49373 \u44600  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54217 \u49373 \u51012  \u54632 \u44760 \u54624  \'91\u54869 \u49888 \'92\u51060  \u49436 \u51648  \u50506 \u50520 \u51004 \u47728 , \u45796 \u47480  \u51060 \u49457 \u46020  \u45576 \u50640  \u46308 \u50612 \u50724 \u45716  \u49345 \u54889 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u50672 \u50528  \u52488 \u48152 \u51060 \u45208  \u50040 \u51012  \u53440 \u44256  \u51080 \u51012  \u46412 \u45716  \u47588 \u50864  \u50976 \u47532 \u54620  \u52852 \u46300 \u51077 \u45768 \u45796 . \u44032 \u51109  \u51473 \u50836 \u54620  \'93\u51060 \u49457 \u51201  \u45132 \u47548 \'94\u51060  \u52649 \u48516 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50500 \u51649 \u44620 \u51648 \u46020  \u49436 \u47196 \u44032  \u47588 \u47141 \u51201 \u51004 \u47196  \u48372 \u51060 \u47728 , \u46160 \u44540 \u45824 \u45716  \u44288 \u44228 \u47484  \u47564 \u46308 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u53945 \u55176  \u50977 \u52404 \u51201  \u44417 \u54633 \u51060  \u51339 \u51008  \u52964 \u54540 \u50640 \u44172 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54624  \u44221 \u50864 , \'91\u47800 \u47564  \u50896 \u54616 \u45716  \u44288 \u44228 \'92 \'91\u51652 \u49900 \u51060  \u49324 \u46972 \u51652  \u44288 \u44228 \'92\u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \u49345 \u45824 \u48169 \u51060  \'91\u51060 \u49457 \'92\u51060 \u47728 , \'91\u49457 \u51201 \u51064  \u45132 \u47548 \u51008  \u51080 \u45796 \'92\u45716  \u51060 \u50976 \u47564 \u51004 \u47196  \u44288 \u44228 \u47484  \u51060 \u50612 \u45208 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u50672 \u51064  \u52852 \u46300 \u45716 , \u45208 \u49240 \u51648  \u50506 \u49845 \u45768 \u45796 . \u44288 \u44228 \u44032  \u45796 \u49884  \u51060 \u50612 \u51656  \u49688  \u51080 \u45716  \u54872 \u44221 \u51077 \u45768 \u45796 . \u51060 \u49457 \u51004 \u47196 \u49436 \u51032  \u54840 \u44048 \u51008  \u45224 \u50500 \u51080 \u45716  \u49345 \u53468 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u54616 \u51648 \u47564  \u51060 \u51204 \u52376 \u47100  \u44618 \u44172  \u49324 \u46993 \u54616 \u44592 \u45716  \u55192 \u46308 \u50612  \u48372 \u51077 \u45768 \u45796 . \'91\u50977 \u52404 \u51201 \u51064  \u47564 \u51313 \'92\u51004 \u47196  \u44536 \u52832  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50668 \u49457 \u51008  \u45796 \u47480  \u44275 \u51012  \u48148 \u46972 \u48372 \u44256  \u51080 \u51004 \u47728 , \u46160  \u49324 \u46988 \u51008  \u44263  \'91\u49373 \u47749 \u51012  \u51075 \u50612 \u48260 \u47540  \u52376 \u51648 \'92\u50640  \u45459 \u50688 \u49845 \u45768 \u45796 . \'91\u45817 \u49888 \u51060  \u50620 \u47560 \u45208  \u44618 \u51008  \u44288 \u44228 \u47484  \u50896 \u54616 \u45716 \u51648 \'92\u50640  \u46384 \u46972 \u49436 , \u51060  \u52852 \u46300 \u45716  \u52572 \u44256 \u51032  \u52852 \u46300 \u44032  \u46112  \u49688 \u46020  \u51080 \u44256 , \u52572 \u50501 \u51032  \u52852 \u46300 \u44032  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50672 \u51064  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u50696 \u49345 \u52824  \u47803 \u54620  \u44275 \u50640 \u49436  \u46024 \u51060  \u46308 \u50612 \u50732  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u51060 \u45716  \u51068 \u54924 \u49457 \u51060  \u51673 \u51008  \u46024 \u51060 \u48064 \u47196  \'91\u51200 \u52629 \'92\u54616 \u45716  \u44163 \u51060  \u44032 \u51109  \u51339 \u49845 \u45768 \u45796 . \u51109 \u44592 \u51201 \u51064  \u44552 \u51204 \u50868 \u51008  \u45208 \u49244  \u54200 \u51064 \u45936 , \'91\u46024 \u51060  \u51096  \u48268 \u47532 \u45716  \u49884 \u44592 \'92\u50640  \u51200 \u52629 \u54616 \u51648  \u50506 \u51004 \u47732  \u53360  \u45229 \u54056 \u47484  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 . \u53945 \u55176  \u53804 \u51088 \u45208  \u49324 \u50629 \u51008  \u49888 \u51473 \u54616 \u44172  \u44208 \u51221 \u54644 \u50556  \u54633 \u45768 \u45796 . \u45804 \u53092 \u54616 \u44172  \u48372 \u50668 \u46020 , \u44536  \u49549 \u50640  \u50948 \u54744 \u51060  \u49704 \u50612 \u51080 \u45796 \u45716  \u44221 \u44256 \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u50672 \u51064  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u51649 \u50629 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u46041 \u47308 \u50752  \u51312 \u54868 \u47213 \u44172  \u54801 \u50629 \u54616 \u47728 , \u53360  \u49457 \u44277 \u51012  \u45576 \u50526 \u50640  \u46160 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50696 \u49345 \u48372 \u45796  \u48736 \u47480  \u49884 \u51068  \u45236 \u50640  \u50896 \u54616 \u45716  \u44208 \u44284 \u47484  \u50619 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564 , \u51648 \u44552 \u51008  \u44361 \u51109 \u55176  \u49888 \u51473 \u54644 \u50556  \u54624  \u49884 \u44592 \u51077 \u45768 \u45796 . \'91\u51060 \u51221 \u46020 \u45716  \u54644 \u46020  \u46104 \u44192 \u51648 \'92\u46972 \u45716  \u47560 \u51020 \u51008  \u51217 \u50612 \u46160 \u50612 \u50556  \u54633 \u45768 \u45796 . \u51088 \u52843  \u51648 \u44552 \u44620 \u51648  \u51068 \u44424 \u50772 \u45912  \u44163 \u46308 \u51012  \u47784 \u46160  \u51075 \u50612 \u48260 \u47532 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50500 \u45812 \u44284  \u51060 \u48652 \u45716  \u50780  \u50640 \u45940  \u46041 \u49328 \u50640 \u49436  \u51923 \u44200 \u45228 \u51012 \u44620 \u50836 ? \u44536 \u46308 \u51008  \u54616 \u45208 \u45784 \u51032  \u50557 \u49549 \u51012  \u50612 \u44592 \u44256 , \u49440 \u50501 \u44284 \u47484  \u46384 \u47673 \u50632 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u50672 \u51064  \u52852 \u46300 \u45716  \'91\u45804 \u53092 \u54644  \u48372 \u51060 \u45716  \u50976 \u54841 \'92\u51012  \u51312 \u49900 \u54644 \u50556  \u54620 \u45796 \u45716  \u47700 \u49884 \u51648 \u47484  \u51204 \u54644 \u51469 \u45768 \u45796 .Comment\u50500 \u45812 \u44284  \u51060 \u48652  \u51060 \u50556 \u44592 \u45716  \u45817 \u49888 \u50640 \u44172  3\u44032 \u51648  \u47700 \u49884 \u51648 \u47484  \u45912 \u51256 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u49440 \u53469 \u50640 \u45716  \u52293 \u51076 \u51060  \u46384 \u47493 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u50976 \u54841 \u50640  \u45336 \u50612 \u44032 \u45716  \u49692 \u44036  \u45817 \u49888 \u51008  \u44256 \u53685 \u51032  \u44600 \u47196  \u48736 \u51256 \u46317 \u45768 \u45796 . \u49464  \u48264 \u51704 , \u44536 \u47100 \u50640 \u46020  \u47564 \u54924 \u54624  \u44592 \u54924 \u45716  \u51452 \u50612 \u51665 \u45768 \u45796 . \u51396 \u47484  \u45656 \u50864 \u52824 \u47728  \u49352 \u47196 \u50868  \u52636 \u48156 \u51012  \u44228 \u54925 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u51652 \u49900 \u51004 \u47196  \u45824 \u54616 \u49464 \u50836 \u52572 \u44540 \u50640  \u47564 \u45212  \u49324 \u46988 , \u52572 \u44540 \u50640  \u49884 \u51089 \u54620  \u51068 . \u51221 \u47568  \u51652 \u49900 \u51004 \u47196  \u45824 \u54616 \u44256  \u51080 \u45208 \u50836 ? \u44536 \u51200  \u44032 \u48316 \u50868  \u53468 \u46020 \u47196  \u45824 \u52649 \u45824 \u52649  \u45824 \u54616 \u44256  \u51080 \u45716  \u44148  \u50500 \u45776 \u51648 \u50836 . \u44536  \u49692 \u44036 \u47564 \u53372 \u51008  \u50728 \u51204 \u55176  \u51665 \u51473 \u51012  \u45796 \u54616 \u49464 \u50836 . \u49345 \u45824 \u48169 \u50640 \u44172  \u51652 \u49900 \u51004 \u47196  \u48736 \u51256 \u46300 \u49464 \u50836 .    \
