   \uc0\u52852 \u46300  \u51060 \u47492  : \u49548 \u46300 5, Five of Swords\u50836 \u50557  \u49444 \u47749  : \u54620  \u45224 \u51088 \u44032  \u52860 \u51012  \u47784 \u46160  \u48764 \u50519 \u44256 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u51312 \u47217 \u54616 \u46319  \u50883 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u51077 \u44144 \u45208 , \u51452 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54616 \u47728 , \u44032 \u52824 \u44288 \u51032  \u52649 \u46028 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u53412 \u50892 \u46300  \u50836 \u50557 \u50672 \u50528 \u50868  : \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u51077 \u45716 , \u44032 \u52824 \u44288 \u51032  \u52264 \u51060 , \u49345 \u45824 \u48169 \u51032  \u50500 \u54548 \u50640  \u54980 \u47144 \u54644  \u54616 \u45716 , \u45336 \u48380  \u49688  \u50630 \u45716  \u49324 \u46993 \u44552 \u51204 \u50868  : \u45236  \u44163 \u51012  \u52313 \u44200 \u50556  \u54616 \u45716 , \u46244 \u53685 \u49688 , \u49324 \u44592 \u51649 \u50629 \u50868  : \u49324 \u45236  \u51221 \u52824 , \u51060 \u44036 \u51656 , \u48176 \u49888 , \u51060 \u44592 \u51201 \u51064 , \u49324 \u44592 , \u44221 \u51137 \u50640 \u49436  \u49849 \u47532 \u54616 \u45716 , \u45336 \u48380  \u49688  \u50630 \u45716  \u47785 \u54364   \u54644 \u49437  1. \u51060 \u44592 \u51201 \u51064 \u49548 \u46300 5\u45716  \u51060 \u44592 \u51201 \u51064  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44144 \u45208 , \u45224 \u51032  \u50500 \u54548 \u51012  \u47924 \u49884 \u54616 \u44256  \u51080 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300 \u51032  \u51452 \u51064 \u44277 \u51008  \u49483 \u51077 \u45768 \u45796 . \u52860 \u51012  \u48764 \u50519 \u44592 \u44256  \u48712 \u53560 \u53560 \u51060 \u47196  \u46104 \u46028 \u50500 \u44032 \u45716  \u49324 \u46988 \u44284 , \u52860 \u51012  \u47784 \u46160  \u48764 \u50519 \u44256  \u51020 \u55113 \u54616 \u44172  \u50883 \u44256  \u51080 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u51060 \u48264 \u50640 \u45716  \u51452 \u51064 \u44277 \u51012  \u51020 \u55113 \u54620  \u49324 \u46988 \u51004 \u47196  \u44032 \u51221 \u54616 \u44192 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u52488 \u47197 \u49353  \u50743 \u51012  \u51077 \u51008  \u45224 \u51088 \u44032  \u44160 \u51012  3\u44060 \u45208  \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48148 \u45797 \u50640 \u46020  \u44160  2\u44060 \u44032  \u51080 \u49845 \u45768 \u45796 . \u50526  \u49324 \u46988 \u46308 \u50640 \u44172  \u44160 \u51012  \u45208 \u45600 \u51460  \u49688  \u51080 \u51020 \u50640 \u46020 , \u51228  \u50837 \u49900 \u50640  \u47784 \u46160  \u52264 \u51648 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47480  \u49324 \u46988 \u51032  \u50500 \u54548 \u51012  \u51228 \u52432 \u46160 \u44256 , \u51088 \u49888 \u51032  \u51060 \u46301 \u47564 \u51012  \u49373 \u44033 \u54616 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u48520 \u54868 \u47484  \u44202 \u45716 \u49548 \u46300 5\u45716  \u51201 \u45824 \u51201 \u51064  \u49345 \u54889 , \u54841 \u51008  \u48520 \u54868 \u47484  \u44202 \u44172  \u46112  \u49345 \u54889 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51452 \u48320 \u50640 \u49436  \u49884 \u44592 \u54616 \u45716  \u49324 \u46988 \u51060  \u49373 \u44592 \u44144 \u45208 , \u51221 \u52824 \u51656 , \u54841 \u51008  \u50672 \u51064  \u45236  \u44040 \u46321 \u51012  \u44202 \u51012  \u49688  \u51080 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49324 \u46988 \u46308 \u51008  \u49436 \u47196  \u46321 \u51012  \u46028 \u47536  \u52292  \u51201 \u45824 \u44048 \u51012  \u46300 \u47084 \u45236 \u44256  \u51080 \u49845