    \uc0\u50756 \u46300  \u54168 \u51060 \u51648  \u44536 \u47548 \u51032  \u51032 \u48120 \u49352 \u47196 \u50868  \u47924 \u50616 \u44032 \u50640  \u54840 \u44592 \u49900 \u51012  \u45712 \u45180 \u45716  \u49324 \u46988 \u49352 \u47196 \u50868  \u44032 \u45733 \u49457 \u51012  \u52286 \u44172  \u46104 \u45716  \u49345 \u54889 \u47924 \u50616 \u44032 \u47484  \u50864 \u47084 \u47084 \u48372 \u47728 , \u44845  \u51648 \u53412 \u44256  \u49910 \u51008  \u47560 \u51020 \u44592 \u49244  \u49548 \u49885 \u51060 \u45208  \u50672 \u46973 \u51012  \u48155 \u44172  \u46120 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u55141 \u48120 \u47484  \u45712 \u45180 \u45716 \u50756 \u46300  \u54168 \u51060 \u51648 \u45716  \u55141 \u48120 \u47484  \u45712 \u45180 \u45716  \u49345 \u53468 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49884 \u51333 \u51008  \u50756 \u46300 \u47484  \u50732 \u47140 \u45796 \u48372 \u47728 , \'91\u55141 \u48120 \u47196 \u50868  \u47924 \u50616 \u44032 \u47484  \u48156 \u44204 \u54664 \u45796 \u45716  \u46319 \'92 \u45576 \u51012  \u48152 \u51677 \u51060 \u44256  \u51080 \u49845 \u45768 \u45796 . \'93\u50752  \u51060 \u44172  \u46104 \u45348 ?\'94 \u51068  \u49688 \u46020  \u51080 \u44256 ,\'93\u51060 \u47088  \u49324 \u46988 \u51060  \u51080 \u50632 \u45796 \u44256 ?\'94 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300  \u54168 \u51060 \u51648 \u44032  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u45716  \u44221 \u50864 , \u44536  \u47560 \u51020 \u51008  \'91\u55141 \u48120 \u50752  \u54840 \u44592 \u49900 \'92\u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u50756 \u46300 \u45716  \u48520 \u51032  \u49549 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640 , \u44536  \u47560 \u51020 \u51008  \u51068 \u49884 \u51201 \u51060 \u44144 \u45208  \u52488 \u44592  \u45800 \u44228 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50672 \u50528 \u50868 \u51012  \u54869 \u51109 \u54644 \u49436  \u54644 \u49437 \u54620 \u45796 \u47732  :\u50040 \u51060 \u45208  \u50508 \u50500 \u44032 \u45716  \u45800 \u44228 \u51068  \u44221 \u50864 , \u52828 \u54644 \u51648 \u44256  \u49910 \u51008  \u47560 \u51020 \u51060 \u45208  \u49692 \u49688 \u54620  \u54840 \u44048 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u49352 \u47196 \u50868  \u47784 \u51076 \u51060 \u45208  \u46041 \u50500 \u47532 \u50640  \u45208 \u44032 \u49464 \u50836 . \u47560 \u51020 \u50640  \u46300 \u45716  \u49324 \u46988 \u51012  \u52286 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732 , \u55141 \u48120 \u47196 \u50868  \u47924 \u50616 \u44032 \u47484  \u49352 \u47213 \u44172  \u49884 \u46020 \u54616 \u44172  \u46121 \u45768 \u45796 . (\u49457 \u51201 \u51004 \u47196 \u46020 )\u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u50630 \u50632 \u45796 \u47732 , \u44288 \u49900  \u50630 \u45912  \u49324 \u46988 \u50640 \u44172 \u49436  \u44049 \u51088 \u44592  \u47588 \u47141 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u49352 \u47196 \u50868  \u51068 \u50640  \u55141 \u48120 \u47484  \u44032 \u51648 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u49548 \u49548 \u54620  \u49457 \u44284 \u47484  \u48372 \u44172  \u46104 \u50612 , \u55141 \u48120 \u47484  \u45712 \u45180 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u45817 \u49888 \u54620 \u53580  \u44288 \u49900  \u50630 \u44256  \u46608  \u45796 \u47480  \u49324 \u46988 \u50640 \u44172  \u55141 \u48120 \u44032  \u49373 \u44596  \u49345 \u54889 , \u44618 \u51008  \u44048 \u51221 \u51060  \u50500 \u45772  \'91\u54840 \u44592 \u49900 \'92\u50640  \u44536 \u52828 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u49352 \u47196 \u50868  \u44032 \u45733 \u49457 \u51012  \u48156 \u44204 \u54616 \u45716 \u54168 \u51060 \u51648  \u50756 \u46300 \u45716  \u49352 \u47196 \u50868  \u44032 \u45733 \u49457 \u51012  \u48156 \u44204 \u54616 \u44172  \u46108 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \'93\u51060 \u47111 \u44172  \u54616 \u47732  \u46104 \u45348 ?\'94 \'93\u51060 \u47088  \u47732 \u46020  \u51080 \u50632 \u44396 \u45208 ?\'94 \'93\u45208 \u54620 \u53580 \u46020  \u44032 \u45733 \u49457 \u51060  \u51080 \u44396 \u45208 !\'94\u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u49324 \u47561  \u54620 \u44032 \u50868 \u45936  \u44866 \u54784 \u51080 \u45716  \u50756 \u46300 \u50640 \u49436  \u49352 \u49913 \u51060  \u51088 \u46972 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u49884 \u51333 \u51008  \u44536  \u49352 \u49913 \u51012  \u48372 \u47728 , \u49352 \u47196 \u50868  \u44032 \u45733 \u49457 \u51012  \u44592 \u45824 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54172 \u53440 \u53364  \u49884 \u51333 \u44284  \u45656 \u50521 \u49828 \u44032  \u48708 \u49847 \u54633 \u45768 \u45796 . \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u51032 \u50808 \u51032  \u47588 \u47141 \u51012  \u50508 \u44172  \u46104 \u44144 \u45208 , \u47792 \u46992 \u45912  \u49324 \u49892 \u51012  \u50508 \u44172  \u46121 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u49352 \u47196 \u50868  \u50500 \u51060 \u46356 \u50612 \u47484  \u46496 \u50732 \u47532 \u44172  \u46121 \u45768 \u45796 . \u49352 \u47196 \u50868  \u54644 \u44208 \u52293 \u51012  \u52286 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51677 \u49324 \u46993  \u51473 \u51060 \u46972 \u47732 , \u49345 \u45824 \u48169 \u46020  \u45208 \u50640 \u44172  \u44288 \u49900  \u51080 \u45796 \u45716  \u49324 \u49892 \u51012  \u50508 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u50672 \u46973 \u51012  \u48155 \u44256  \u44592 \u48848 \u54616 \u45716 \u50756 \u46300  \u49884 \u51333 \u51008  \u50672 \u46973 \u51012  \u48155 \u44256  \u44592 \u48848 \u54620 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300  \u54168 \u51060 \u51648 \u45716  \u50756 \u46300 8\u44284  \u45908 \u48520 \u50612  \'91\u51339 \u51008  \u49548 \u49885 \'92\u51012  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300  \u51473  \u54616 \u45208 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u49884 \u51333 \u51008  \u50756 \u46300 \u47484  \u50732 \u47140 \u45796 \u48372 \u47728 , \u44592 \u49256 \u50640  \u44032 \u46301  \u52268  \u54364 \u51221 \u51012  \u51667 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51656 \u47928 \u50640  \u46384 \u47480  \u45813 \u48320 \u51032  \u50696 \u49884 \u51077 \u45768 \u45796  :\u45236 \u44032  \u50672 \u46973 \u54664 \u51012  \u46412  \u49345 \u45824 \u48169 \u51032  \u44592 \u48516 \u51008 ? \u8594  \u50500 \u47560  \u46104 \u44172  \u44592 \u48848 \u54664 \u51012  \u46319 \u54633 \u44201  \u49548 \u49885 \u51012  \u48155 \u44172  \u46112 \u44620 ? \u8594  \u54633 \u44201  \u47928 \u51088 , \u51204 \u54868 \u47484  \u48155 \u44256  \u44592 \u48848 \u54616 \u45716  \u47784 \u49845 \u51109 \u54617 \u44552 \u51012  \u48155 \u44172  \u46112 \u44620 ? \u8594  \u51109 \u54617 \u44552  \u48155 \u44256  \u44592 \u48848 \u54616 \u45716  \u47784 \u49845 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u47561 \u50672 \u54620  \u49373 \u44033 \u54168 \u51060 \u51648  \u50756 \u46300 \u45716  \u47561 \u50672 \u54620  \u49373 \u44033 , \u54841 \u51008  \u44228 \u54925  \u50630 \u45716  \u49345 \u53468 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u50756 \u46300 \u45716  \u51086 \u49324 \u44480 \u47564  \u45228 \u51012  \u49104 , \u50500 \u51649  \u50676 \u47588 \u47484  \u47610 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u49884 \u51333  \u46608 \u54620  \u50756 \u46300 \u47484  \u48148 \u46972 \u47564  \u48372 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50612 \u46496 \u54620  \u50529 \u49496 \u46020  \u50630 \u51424 . \u51060 \u45716  \u51593  \u50557 \u44036 \u51032  \u44032 \u45733 \u49457 \u47564  \u51316 \u51116 \u54616 \u45716  \u49345 \u53468 , \u54840 \u44592 \u49900 \u47564  \u44032 \u51652  \u49345 \u53468 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50500 \u51649  \u49457 \u44284 \u47484  \u44144 \u46160 \u44144 \u45208 , \u44396 \u52404 \u51201 \u51064  \u49892 \u54665  \u44228 \u54925 \u51012  \u49464 \u50864 \u51648 \u45716  \u50506 \u50520 \u49845 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u45817 \u49888 \u50640 \u44172  \u54840 \u44592 \u49900 (\u54840 \u44048 )\u51008  \u49373 \u44220 \u51004 \u45208 , \u44396 \u52404 \u51201 \u51004 \u47196  \u50612 \u46523 \u44172  \u45796 \u44032 \u44040 \u51648  \u44228 \u54925 \u51008  \u50630 \u45716  \u49345 \u53468 \u51060  \u49324 \u46988  \u44316 \u52270 \u51008 \u45936 ? \u54840 \u44048 \u51060  \u44032 \u45348 .\u49352 \u47196 \u50868  \u51068 \u50640  \u55141 \u48120 \u47484  \u45712 \u44808 \u51004 \u45208 , \u51228 \u45824 \u47196  \u54624  \u49373 \u44033 \u51008  \u51201 \u51020 \u50724  \u51116 \u48140 \u45716 \u45936 ? \u45208 \u51473 \u50640  \u49884 \u44036 \u45208 \u47732  \u54644 \u48400 \u50556 \u51648 \u54644 \u44208 \u52293 \u51008  \u52286 \u50520 \u51004 \u45208 , \u44396 \u54788 \u54624  \u48169 \u48277 \u51008  \u52286 \u51648  \u47803 \u54632 .\u50724  \u51060 \u47088  \u48169 \u48277 \u51060  \u51080 \u50632 \u45348 ! \u44540 \u45936  \u50612 \u46523 \u44172  \u54624  \u49688  \u51080 \u45716 \u45936 ?* \u50756 \u46300  \u54168 \u51060 \u51648 \u47484  \u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864  (1)\u51328  \u45908  \u54788 \u49892 \u51201 \u51060 \u44256  \u44396 \u52404 \u51201 \u51064  \u44228 \u54925 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 .\u49373 \u44033 \u47564  \u54616 \u51648  \u47568 \u44256 , \u50857 \u44592 \u47484  \u45236 \u50612  \u50672 \u46973 \u54644 \u48372 \u49464 \u50836 .\u54644 \u49437  2. \u44221 \u54744 \u51060  \u48512 \u51313 \u54620 , \u48120 \u49689 \u54620 \u54168 \u51060 \u51648  \u50756 \u46300 \u45716  \u45208 \u51060 \u44032  \u51201 \u51008  \u51060 \u49457 , \u54841 \u51008  \u49373 \u44033 \u51060  \u50612 \u47536  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51060 \u44148  \'91\u54168 \u51060 \u51648  \u52852 \u46300 \'92\u51032  \u44277 \u53685 \u51216 \u51077 \u45768 \u45796 . \u50676 \u51221 \u51008  \u45336 \u52824 \u51648 \u47564 , \u44221 \u54744 \u51060  \u48512 \u51313 \u54633 \u45768 \u45796 .\u50504 \u51221 \u51201 \u51064  \u50672 \u50528 \u48372 \u45796  \'91\u48520 \u44867  \u53888 \u47728  \u44592 \u48373 \u51060  \u49900 \u54620  \u50672 \u50528 \'92\u47484  \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u45824 \u54868 \u44032  \u50504  \u53685 \u54616 \u44256 , \u49468 \u49828 \u44032  \u48512 \u51313 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44032 \u45140  \u49345 \u45824 \u50640 \u44172  \u44284 \u46020 \u54616 \u44172  \u46308 \u51060 \u45824  \u48512 \u45812 \u49828 \u47084 \u50880 \u51012  \u51452 \u44592 \u46020  \u54616 \u51424 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51068 \u51012  \u50612 \u49444 \u54532 \u44172  \u52376 \u47532 \u54616 \u44144 \u45208  \u52293 \u51076 \u44048 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u45800 \u51216 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49324 \u46988 \u51012  \u44048 \u51221 \u51201 \u51004 \u47196  \u45824 \u51025 \u54644 \u49436  \u53944 \u47084 \u48660 \u51012  \u51068 \u51004 \u53412 \u44592 \u46020  \u54616 \u51424 . \u50668 \u47084 \u47784 \u47196  \u50612 \u47536 \u50528  \u44057 \u51008  \u47732 \u51060  \u51080 \u49845 \u45768 \u45796 .* \u50756 \u46300  \u54168 \u51060 \u51648 \u47484  \u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864  (2)\u49457 \u44284 \u48372 \u45796  \u44221 \u54744 \u51012  \u51473 \u50836 \u49884 \u54616 \u49464 \u50836 . \u50500 \u51649  \u45817 \u49888 \u50640 \u44176  \u49884 \u44036 \u51060  \u47566 \u49845 \u45768 \u45796 .\u44221 \u49556 \u54620  \u54665 \u46041 \u51012  \u51312 \u49900 \u54616 \u49464 \u50836 . \u44048 \u51221 \u51201 \u51004 \u47196  \u45824 \u51025 \u54616 \u51648  \u47568 \u44256 , \u52264 \u48516 \u55176  \u49373 \u44033 \u54616 \u49464 \u50836 .\u52293 \u51076 \u44048  \u51080 \u44172  \u54665 \u46041 \u54616 \u49464 \u50836 .\u50756 \u46300  \u54168 \u51060 \u51648  \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300  \u54168 \u51060 \u51648 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u45796 \u49548  \u50612 \u49444 \u54532 \u51648 \u47564  \u47784 \u46304  \u51068 \u50640  \u52572 \u49440 \u51012  \u45796 \u54633 \u45768 \u45796 . \u54840 \u44592 \u49900 \u46020  \u47566 \u50500 \u49436  \u51068 \u51060 \u46304  \u49324 \u46988 \u51060 \u46304  \u49789 \u44172  \u48736 \u51256 \u46300 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536 \u47564 \u53372  \'91\u44288 \u49900 \u51060  \u49789 \u44172  \u49885 \u45716 \u45796 \'92\u45716  \u44163 \u46020  \u53360  \u45800 \u51216 \u51077 \u45768 \u45796 . \u51648 \u47336 \u54632 \u51012  \u47803 \u44204 \u46356 \u45716  \u49457 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u50752  \u50504  \u47582 \u45716  \u48512 \u48516 \u51060  \u51080 \u51004 \u47732  \u44552 \u49352  \u47560 \u51020 \u51060  \u49885 \u50612 \u48260 \u47532 \u47728 , \u49892 \u47581 \u46020  \u51088 \u51452  \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u46412 \u50640  \u46384 \u46972  \u47566 \u51008  \u51060 \u49457 \u44284  \u50896 \u45208 \u51079 \u51012  \u51600 \u44592 \u45716  \u49324 \u46988 \u51012  \u49345 \u51669 \u54624  \u49688 \u46020  \u51080 \u51004 \u45768 , \u49345 \u45824 \u48169 \u51012  \u44600 \u44172  \u48372 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 .\u50756 \u46300  \u54168 \u51060 \u51648 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \u49692 \u49688 \u54620  \u54840 \u44592 \u49900 , \u54840 \u44048 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51060  \u44288 \u44228 \u47484  \u47532 \u46300 \u54616 \u44256  \u49910 \u51008  \u49900 \u47532 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536  \u47560 \u51020 \u51008  \u50724 \u47000 \u46104 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u48708 \u50976 \u47484  \u46308 \u51088 \u47732 , \u49884 \u49885  \u53076 \u45320 \u50640 \u49436  \u47579 \u51080 \u45716  \u51020 \u49885 \u51012  \u47673 \u51008  \u49345 \u53468 \u46980 \u44620 \u50836 . \u47751  \u48264  \u47673 \u45796 \u44032  \u51656 \u47532 \u44172  \u46112  \u49688 \u46020 , \u50724 \u47019 \u46041 \u50504  \u54632 \u44760 \u54624  \u49548 \u50872 \u54392 \u46300 \u44032  \u46112  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54788 \u51116 \u45716  \u49345 \u45824 \u48169 \u51032  \u44592 \u45824 \u52824 \u44032  \u45458 \u50500 \u51652  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \u44284 \u45824 \u54217 \u44032 \u54616 \u44144 \u45208 , \u51088 \u49888 \u51032  \u44048 \u51221 \u51012  \u54869 \u45824  \u54644 \u49437  \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51060 \u47088  \u49324 \u46988 \u51060  \u51080 \u45796 \u44256 ? \u45320 \u47924  \u44316 \u52270 \u51008 \u45936 ? \u46412 \u47196 \u45716  \u45817 \u49888 \u50640 \u44172  \u50672 \u46973 \u51012  \u48155 \u50500  \u44592 \u48848 \u54616 \u45716  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u45800 \u49692 \u54620  \u54840 \u44592 \u49900 \u51068  \u49104 , \u45817 \u49888 \u50640  \u45824 \u54620  \u47560 \u51020 \u51060  \'91\u44618 \u51008  \u54840 \u44048 \'92\u51008  \u50500 \u45768 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45796 \u47480  \u49324 \u46988 \u50640 \u44172  \'91\u44288 \u49900 \'92\u51060  \u49373 \u44220 \u45796 \u45716  \u51032 \u48120 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50668 \u51204 \u55176  \u45817 \u49888 \u50640 \u44172  \u52649 \u48516 \u54620  \u49324 \u46993 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50508 \u47732  \u50508 \u49688 \u47197  \u49352 \u47213 \u44256 , \u51676 \u47551 \u54620  \u44288 \u44228 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u50864 \u47084 \u47084  \u48372 \u47728 , \u49552 \u50640  \u44845  \u51536 \u50612  \u51648 \u53412 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u45796 \u47564  \u46168  \u49324 \u51060 \u51032  \u44288 \u44228 \u44032  \'91\u50612 \u47536 \u50500 \u51060 \'92 \u44057 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u48120 \u47000 \u50640  \u45824 \u54620  \'91\u47561 \u50672 \u54620  \u49373 \u44033 \'92\u47564  \u51080 \u51012  \u49104 , \u50612 \u46523 \u44172  \u44288 \u44228 \u47484  \u51060 \u50612 \u45208 \u44040 \u51648  \u54788 \u49892 \u51201  \u44228 \u54925 \u51060  \u50630 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u50640  \u45824 \u54620  \u44592 \u45824 \u47484  \u54408 \u44144 \u45208 , \u44032 \u45733 \u49457 \u51012  \u48376  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45796 \u49884  \u47564 \u45216  \u49688  \u51080 \u51648  \u50506 \u51012 \u44620 ? \u50584 \u46020  \u45208 \u46993  \u51116 \u54924 \u54616 \u44256  \u49910 \u50612 \u54616 \u45716  \u44144  \u44057 \u51008 \u45936 ? \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51060  \u44557 \u51221 \u51201 \u51068  \u44221 \u50864 , \u44263  \u50672 \u46973 \u51012  \u49884 \u46020 \u54616 \u50668  \u47564 \u45208 \u44172  \u46108 \u45796 \u45716  \u46907 \u51004 \u47196  \u48389 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u49345 \u45824 \u48169 \u51008  \u52572 \u44540 \u50640  \u47564 \u45212  \u49345 \u45824 \u50640 \u44172  \u44288 \u49900 \u51060  \u49373 \u44596  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51060  \u49324 \u46988  \u45320 \u47924  \u44316 \u52270 \u51008 \u45936 ? \u51060  \u49324 \u46988  \u51200  \u49324 \u46988  \u52244 \u47084 \u48372 \u44256  \u50500 \u49772 \u50872  \u46412  \u50672 \u46973 \u54624  \u49688 \u46020  \u51080 \u51004 \u45768 , \u51312 \u49900 \u54616 \u49492 \u50556 \u44192 \u49845 \u45768 \u45796 .\u50756 \u46300  \u54168 \u51060 \u51648  \u51649 \u50629 \u50868  \u54644 \u49437 \u49352 \u47196 \u50868  \u51068 \u51060 \u45208  \u54532 \u47196 \u51229 \u53944 \u47484  \u49884 \u51089 \u54616 \u44172  \u46121 \u45768 \u45796 . \u50612 \u51724 \u46304  \u51060 \u51204 \u44284  \u45796 \u47480  \'91\u55141 \u48120 \u47196 \u50868  \u51068 \'92\u46308 \u51060  \u54204 \u52432 \u51656  \u50696 \u51221 \u51077 \u45768 \u45796 . \u51339 \u51008  \u49548 \u49885 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824  \'91\u51339 \u51008  \u51312 \u44148 \u50640  \u51060 \u51649 \u51012  \u51228 \u50504 \u54616 \u45716  \u47700 \u49884 \u51648 \'92, \'91\u54633 \u44201  \u47928 \u51088 \'92 \u46321 \u46321 \u51077 \u45768 \u45796 . \u49352 \u47213 \u44172  \u49884 \u51089 \u54616 \u47140 \u45716  \u44221 \u50864 \u50644  \u45908 \u50630 \u51060  \u51339 \u51008  \u52852 \u46300 \u51060 \u48064 \u47196 , \u50676 \u51221 \u44284  \u52488 \u49900 \u51012  \u51075 \u51648  \u50506 \u45716  \u44163 \u51060  \u51473 \u50836 \u54616 \u44192 \u49845 \u45768 \u45796 .\u50756 \u46300  \u54168 \u51060 \u51648  \u44552 \u51204 \u50868  \u54644 \u49437 \u50696 \u49345 \u52824  \u47803 \u54620  \u46024 \u51012  \u48155 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u52828 \u52377 \u50640 \u44172  \u46024 \u51012  \u44049 \u51089 \u49828 \u47112  \u48155 \u44144 \u45208 , \u54924 \u49324 \u50640 \u49436  \u48372 \u45320 \u49828 \u47484  \u48155 \u44172  \u46104 \u45716  \u51068 \u51077 \u45768 \u45796 . \u49440 \u47932 \u51032  \u54805 \u53468 \u47196  \u50732  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u50756 \u46300  \u54168 \u51060 \u51648 \u45716  \'91\u44540 \u49884 \u50504 \u51201 \u51064  \u49457 \u54693 \'92\u51060  \u45800 \u51216 \u51060 \u48064 \u47196 , \u44536  \u46024 \u51012  \u50416 \u44592 \u48372 \u45796 \u45716  \u51200 \u52629 \u54616 \u44592 \u47484  \u44428 \u51109 \u54633 \u45768 \u45796 .    \
\
    \uc0\u50756 \u46300  \u44592 \u49324  \u44536 \u47548 \u51032  \u51032 \u48120 \u50672 \u50528 \u50868 \u50640 \u49436  \u45824 \u54364 \u51201 \u51064  \u54840 \u44048  \u49888 \u54840  \u51473  \u54616 \u45208 \u47784 \u54744 \u49900 \u44284  \u50676 \u51221 \u51060  \u44032 \u46301 \u54620  \u49345 \u53468 \u47588 \u47141 \u51060  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u49345 \u51669 \u54632 \u48376 \u51064 \u51032  \u50640 \u45320 \u51648 \u47484  \u51452 \u52404 \u54616 \u51648  \u47803 \u54616 \u45716  \u49345 \u54889 \u49457 \u44201 \u51060  \u44553 \u54616 \u44256  \u48320 \u45909 \u49828 \u47084 \u50868  \u49324 \u46988 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50676 \u51032 \u50752  \u50676 \u51221 \u51012  \u44032 \u51652 \u50756 \u46300  \u44592 \u49324 \u45716  \u50672 \u50528 \u50868 \u50640 \u49436  \u45824 \u54364 \u51201 \u51064  \u54840 \u44048  \u49888 \u54840 \u51077 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \'91\u49549 \u47560 \u51020 \'92\u51012  \u51656 \u47928 \u54664 \u45796 \u47732 , \u51060 \u45716  \u51200 \u46028 \u51201 \u51004 \u47196  \u46308 \u51060 \u45824 \u44256  \u49910 \u51008  \u47560 \u51020 \u51076 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u52852 \u46300 \u45716  \u48520 \u51032  \u50836 \u49548 \u47196  \u44032 \u46301 \u54633 \u45768 \u45796 . \u45224 \u51088 \u51032  \u50743 \u50640 \u45716  \u48520  \u47784 \u50577  \u51109 \u49885 \u51060  \u45804 \u47140 \u51080 \u44256 , \u47672 \u47532 \u52852 \u46973  \u46608 \u54620  \u48521 \u51008 \u49353 \u51077 \u45768 \u45796 . \u47568 \u51008  \u47588 \u50864  \u50669 \u46041 \u51201 \u51004 \u47196  \u50880 \u51649 \u51060 \u47728 , \u48744 \u44053 \u50640  \u44032 \u44620 \u50868  \u51452 \u54889 \u49353 \u51077 \u45768 \u45796 . \u51060 \u45716  \u50612 \u46500  \'91\u51068 , \u49324 \u46988 , \u54665 \u46041 \'92\u50640  \u45824 \u54644  \u50676 \u51032 \u50752  \u50676 \u51221 \u51012  \u44032 \u51648 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u50756 \u46300  \u44592 \u49324 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \u44288 \u49900  \u51080 \u45716  \u49345 \u45824 \u44032  \u49373 \u44592 \u47732  \u47581 \u49444 \u51076  \u50630 \u51060  \u46308 \u51060 \u45824 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u50672 \u46973 \u46020  \u49789 \u44172  \u49789 \u44172  \u54616 \u47728 , \u54840 \u44048 \u46020  \u51201 \u44537 \u51201 \u51004 \u47196  \u54364 \u54788 \u54633 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u47564 \u45208 \u45716  \u51060 \u49457 \u51060  \u51088 \u51452  \u48148 \u45068 \u44592 \u46020  \u54616 \u47728 , \u46412 \u50640  \u46384 \u46972  \u48148 \u46988 \u45180 \u44032  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u52572 \u44540  \u49345 \u45824 \u48169 \u50640 \u44172  \u50672 \u46973 \u51060  \u50772 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \'91\u54869 \u49892 \u55176 \'92 \u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u51012  \u44032 \u51648 \u44256  \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u44536  \u47560 \u51020 \u51008  \u50724 \u47000 \u46104 \u51648  \u50506 \u50520 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u46608  \u49692 \u49688 \u54620  \u47785 \u51201 \u48372 \u45796 \u45716  \'91\u50808 \u47196 \u50880  \u45804 \u47000 \u44592 , \u50672 \u50528 \u45208  \u50896 \u45208 \u51079 \'92 \u47785 \u51201 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u47564 \u50557  \u49345 \u45824 \u48169 \u51032  \u54665 \u46041 \u51060  \u49548 \u44537 \u51201 \u51060 \u50632 \u44144 \u45208 , \u45817 \u49888 \u51060  \u54775 \u44040 \u47540 \u47564 \u54620  \u49688 \u51456 \u51060 \u50632 \u45796 \u47732 , \u53356 \u44172  \u47560 \u51020 \u51060  \u50630 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u54840 \u44048 \u51012  \u45824 \u45459 \u44256  \u54000  \u45236 \u45716  \u49457 \u54693 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u50756 \u46300  \u45208 \u51060 \u53944 \u45716  \u51068 \u50640 \u49436 \u46020  \u51339 \u51008  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51068 \u50640  \u50676 \u51221 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49849 \u48512 \u50837 \u44284  \u49457 \u52712 \u50837 \u46020  \u44053 \u54644  \u49457 \u44284 \u46020  \u51339 \u51008  \u54200 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51088 \u52843  \u44228 \u54925  \u50630 \u51060  \u54665 \u46041 \u47564  \u50526 \u49440 \u45796 \u45716  \u44221 \u44256 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u46300  \u53076 \u53944 \u45716  \u44277 \u53685 \u46108  \u53945 \u51669 \u51060  \u51080 \u49845 \u45768 \u45796 . \u48148 \u47196  \u51032 \u50837 \u51060  \u45336 \u52824 \u44256  \u50808 \u44368 \u51201 \u51060 \u47728 , \u45576 \u50640  \u46916 \u44256  \u49910 \u50612 \u54620 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u51088 \u49888 \u51060  \u51473 \u49900 \u50640  \u51080 \u50612 \u50556  \u51649 \u49457 \u51060  \u54400 \u47532 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 .\u12302 \u53440 \u47196  \u52852 \u46300  \u50756 \u46300  \u45208 \u51060 \u53944 \u51032  \u51032 \u48120 \u12303 - \u53440 \u47196  \u54616 \u52824 \u48288 \u54644 \u49437  2. \u47588 \u47141  \u45336 \u52824 \u45716  \u49324 \u46988 \u50756 \u46300  \u44592 \u49324 \u44032  \'91\u49345 \u45824 \u48169 \u51060  \u48372 \u45716  \u45208 , \u44263  \u45236 \u44032  \u47564 \u45208 \u44172  \u46112  \u49324 \u46988 \'92\u52376 \u47100  \u50612 \u46500  \u51064 \u47932 \u49345 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u47588 \u47141 \u51201 \u51060 \u47728  \u50808 \u47784 \u44032  \u46832 \u50612 \u45212  \u49324 \u46988 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 .\u44592 \u49324 \u51032  \u51060 \u47785 \u44396 \u48708 \u45716  \u46748 \u47159 \u54616 \u44256 , \u47672 \u47532 \u46020  \u48744 \u44036 \u49353 \u51004 \u47196  \u47691 \u51080 \u44172  \u50684 \u49353 \u54664 \u49845 \u45768 \u45796 . \u46608  \u44592 \u49324 \u51032  \u54364 \u51221 \u51008  \u50676 \u51032 \u47196  \u44032 \u46301  \u52268  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44536 \u45716  \u51536 \u44256  \u51080 \u45716  \u50756 \u46300 \u47484  \u49688 \u51649 \u51004 \u47196  \u46308 \u50612 , \u51088 \u49888 \u44048  \u51080 \u44172  \u48372 \u50668 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \'91\u51088 \u49888 \u51032  \u54665 \u46041 , \u49345 \u54889 \'92\u51012  \u44144 \u47532 \u45196  \u50630 \u51060  \u48372 \u50668 \u51456 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u51328  \u45908  \u44396 \u52404 \u51201 \u51004 \u47196  \u50584 \u44592 \u54616 \u47732  :\u47588 \u49324 \u50640  \u51088 \u49888 \u44048 \u51060  \u45336 \u52841 \u45768 \u45796 . \u51064 \u49912  \u44592 \u51656 \u51060  \u45796 \u48516 \u54633 \u45768 \u45796 .\u50556 \u47581 \u44284  \u54252 \u48512 \u44032  \u51080 \u50612  \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u51316 \u44221 \u51032  \u45824 \u49345 \u51060  \u46121 \u45768 \u45796 .\u50808 \u47784 \u44032  \u46832 \u50612 \u45208 \u49436  \u51060 \u49457 \u46308 \u50640 \u44172  \u51064 \u44592 \u44032  \u47566 \u49845 \u45768 \u45796 .\u50640 \u45320 \u51648 \u44032  \u48157 \u44256  \u54876 \u44592 \u52264 \u49436  \u54637 \u49345  \u44557 \u51221 \u51201 \u51064  \u44592 \u50868 \u51012  \u54413 \u44609 \u45768 \u45796 .\u45796 \u47564 , \u54728 \u49464 \u44032  \u45336 \u52824 \u44144 \u45208  \u51648 \u45208 \u52824 \u44172  \u50724 \u47564 \u54620  \u49324 \u46988 , \u45320 \u47924  \u45208 \u45824 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51088 \u44592 \u50528 \u44032  \u45336 \u52432  \'91\u51116 \u49688  \u50630 \u45796 \'92\u44256  \u45712 \u44788 \u51656  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u47784 \u54744 \u51012  \u52628 \u44396 \u54616 \u45716 \u50756 \u46300  \u44592 \u49324 \u45716  \u47784 \u54744 \u51012  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u49324 \u47561 \u51008  \u44060 \u52377  \u46104 \u51648  \u50506 \u51008  \u50689 \u50669 \u51077 \u45768 \u45796 . \u47932 \u46020  \u48512 \u51313 \u54616 \u44256 , \u53468 \u50577 \u48731 \u46020  \u47588 \u50864  \u46888 \u44161 \u49845 \u45768 \u45796 . \u44536 \u47088  \u49324 \u47561  \u54620  \u44032 \u50868 \u45936 \u50640 \u49436 , \u44592 \u49324 \u45716  \'91\u47568 \u51012  \u53440 \u47728 \'92 \u50676 \u51032 \u47484  \u48520 \u53468 \u50864 \u44256  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300 \u51032  \u48176 \u44221 \u51008  \u49324 \u47561 \u51076  \u51060 \u47484  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u52376 \u51020  \u48372 \u45716  \u49324 \u46988 \u50640 \u44172 \u46020  \u44144 \u47532 \u45196  \u50630 \u51060  \u45796 \u44032 \u44032 \u45716  \u49828 \u53440 \u51068 \u48264 \u54840 \u47484  \u46384 \u45716  \u51068 \u50640  \u44144 \u48512 \u44048 \u51060  \u50630 \u51020 \u49345 \u45824 \u48169 \u51012  \u48380  \u46412  \u44228 \u49328 \u51201 \u51060 \u44144 \u45208  \u49888 \u51473 \u54620  \u47560 \u51020 \u51008  \u50630 \u51020 \u51068 \u50640 \u49436 \u45716 , \u49324 \u50629 \u51060 \u45208  \u48292 \u52432  \u44592 \u50629 \u51060  \u50612 \u50872 \u47549 \u45768 \u45796 . \u49552 \u54644  \u48372 \u45716  \u54665 \u46041 \u51060 \u45208  \u48120 \u47000 \u47484  \u46160 \u47140 \u50892 \u54616 \u51648  \u50506 \u44256 , \u44053 \u45800  \u51080 \u44172  \u48128 \u50612 \u48537 \u51060 \u45716  \u49457 \u54693 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u45320 \u47924  \u47924 \u47784 \u54644 \u49436  \u51908 \u46385  \u47581 \u54616 \u44592 \u46020  \u49772 \u50868  \u49457 \u44201 \u51077 \u45768 \u45796 . \u49888 \u51473 \u54632 \u44284  \u44228 \u54925 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u44221 \u44256 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49457 \u44201 \u51060  \u44553 \u54616 \u44256  \u48320 \u45909 \u49828 \u47084 \u50868 \u50756 \u46300  \u44592 \u49324 \u45716  \u49457 \u44201 \u51060  \u44553 \u54616 \u44256  \u54665 \u46041 \u51060 \u45208  \u44048 \u51221  \u48320 \u54868 \u44032  \u49900 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u46300 \u45716  \u48520 \u51032  \u49457 \u51656 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48520 \u51008  \u48744 \u47532  \u54140 \u51648 \u44256 , \u49789 \u44172  \u48169 \u54693 \u51012  \u48148 \u45001 \u45768 \u45796 . \u51060 \u52376 \u47100  \u51060 \u49457 \u50640  \u45824 \u54620  \u47560 \u51020 \u46020  \u44553 \u54616 \u44172  \u53440 \u50732 \u46992 \u45796 \u44032  \u44553 \u54616 \u44172  \u49885 \u51004 \u47728 , \u44536  \u45824 \u49345 \u46020  \u51088 \u51452  \u48148 \u45085 \u45768 \u45796 . \u50756 \u46300  \u44592 \u49324 \u45716  \u44552 \u49324 \u48736  + \u44552 \u49324 \u49885  + \u48148 \u46988 \u45180  \u49457 \u54693 \u51012  \u47784 \u46160  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u46608  \u48320 \u45909 \u49828 \u47084 \u50868  \u50672 \u50528  \u54056 \u53556 \u51012  \u44032 \u51648 \u44256  \u51080 \u50612 , \u51109 \u44592 \u51201 \u51064  \u44288 \u44228 \u47484  \u50976 \u51648 \u54616 \u45716  \u44163 \u46020  \u55192 \u46317 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732  :\u48520 \u44057 \u51060  \u49912 \u50868  \u46244  \u8594  \u50500 \u47924 \u47111 \u51648  \u50506 \u44172  \u50672 \u46973  OR \u44049 \u51088 \u44592  \u50528 \u44368 \u47484  \u48512 \u47548 \u47568 \u51012  \u51088 \u51452  \u48148 \u45012 \u49436  \u49345 \u45824 \u48169 \u51012  \u54844 \u46976 \u49828 \u47101 \u44172  \u54632 \u50672 \u50528  \u52488 \u48152 \u50640 \u45716  \u50528 \u51221  \u54364 \u54788 \u46020  \u51088 \u51452  \u54616 \u45908 \u45768 , \u47751  \u51452 \u47484  \u47803  \u44032 \u44256  \u44552 \u48169  \u49885 \u50612 \u48260 \u47548 \u51088 \u51452  \u49296 \u51648 \u44592 \u46020  \u54616 \u44256 , \u44048 \u51221  \u44592 \u48373 \u46020  \u49900 \u54632 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u48320 \u45909 \u49828 \u47084 \u50868  \u49457 \u54693  \u46412 \u47928 \u50640  \u51649 \u51109 \u51060  \u51088 \u51452  \u48148 \u45076  \u49688 \u46020  \u51080 \u51004 \u47728 , \u51060 \u44275 \u51200 \u44275  \u46028 \u50500 \u45796 \u45768 \u45716  \u51649 \u50629 \u44284  \u50612 \u50872 \u47536 \u45796 \u44256  \u54644 \u49437 \u54633 \u45768 \u45796 . \u50724 \u47000  \u50505 \u50500 \u51080 \u45716  \u51649 \u50629 \u44284 \u46020  \u50612 \u50872 \u47532 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51312 \u50616 \u51216 \u51004 \u47196 \u45716 , \'91\u45128 \u44592 , \u52280 \u51012 \u49457 \'92\u51012  \u44032 \u51648 \u44256  \u50724 \u47019 \u46041 \u50504  \u49884 \u46020 \u54644  \u48380  \u44163 .\u54644 \u49437  2. \u50640 \u45320 \u51648 \u47484  \u51452 \u52404  \u47803 \u54616 \u45716 \u50756 \u46300  \u44592 \u49324 \u45716  \u50640 \u45320 \u51648 \u47484  \u51452 \u52404 \u54616 \u51648  \u47803 \u54616 \u45716  \u49900 \u47532 , \u47785 \u51201  \u50630 \u51060  \u50640 \u45320 \u51648 \u47484  \u45229 \u48708 \u54616 \u45716  \u49345 \u53468 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u47568 \u51008  \'91\u54260 \u51677  \u54260 \u51677  \u46832 \u46319 \u51060 \'92 \u45804 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45440 \u51060 \u53552 \u50640 \u49436  \u50612 \u47536  \u50500 \u51060 \u46308 \u51060  \u51060 \u47532 \u51200 \u47532  \u46832 \u50612 \u45796 \u45768 \u45716  \u47784 \u49845 \u51012  \u49345 \u49345 \u54616 \u47732  \u46121 \u45768 \u45796 . \u50500 \u51060 \u46308 \u51008  \u50612 \u46500  \'91\u47785 \u51201 \u51012  \u44032 \u51648 \u44256 \'92 \u46832 \u45716  \u44163 \u51060  \u50500 \u45785 \u45768 \u45796 . \u44536 \u51200  \u50640 \u45320 \u51648 \u44032  \u45224 \u50500 \u46028 \u50500 \u49436 , \u49900 \u49900 \u54644 \u49436  \u46832 \u45716  \u44163 \u51077 \u45768 \u45796 .\u47785 \u51201 \u49457 \u51060  \u48516 \u47749 \u54620  \u49548 \u46300  \u44592 \u49324 \u50752  \u48708 \u44368 \u46121 \u45768 \u45796 .\u49548 \u46300  \u44592 \u49324 \u50526 \u51004 \u47196  \u46028 \u51652 \u50756 \u46300  \u44592 \u49324 \u54260 \u51677  \u54260 \u51677  \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45208 \u54620 \u53580  \u50780  \u50672 \u46973 \u54664 \u51012 \u44620 ? \u8594  \u45320 \u47924  \u49900 \u49900 \u54644 \u49436 , \u54620  \u48264  \u51088 \u48372 \u44256  \u49910 \u50612 \u49436 , \u45320 \u47924  \u50808 \u47196 \u50892 \u49436 \u44536  \u49324 \u46988  \u45208 \u46993  \u51088 \u44256  \u49910 \u50612 \u54624 \u44620 ? \u8594  \u47588 \u50864 . \u49457 \u50837 \u51012  \u51452 \u52404 \u54616 \u51648  \u47803 \u54616 \u45716  \u49345 \u53468 \u50836 \u51608  \u50672 \u46973 \u51060  \u51110 \u50500 \u51276 \u45716 \u45936 , \u54840 \u44048 \u51060  \u52964 \u51652  \u44148 \u44032 ? \u8594  YES\u45208 \u46993  \u51652 \u51648 \u54620  \u47560 \u51020 \u51004 \u47196  \u50724 \u47000 \u44032 \u44256  \u49910 \u50612 \u54624 \u44620 ? \u8594  \u50500 \u47560 \u46020  \u50500 \u45776  \u44163 . \u50724 \u47019 \u46041 \u50504  \u44256 \u48124 \u54616 \u51648 \u45716  \u50506 \u51008  \u46319 .\u54840 \u53461 \u54616 \u44256  \u49884 \u50896 \u49884 \u50896 \u54616 \u51648 \u47564 , \u45224 \u46308 \u51060  \u48372 \u44592 \u50640  \u45796 \u49548  \'91\u47924 \u47168 \u54616 \u44144 \u45208 , \u49884 \u45124 \u47101 \u44172 \'92 \u48372 \u51068  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u54868 \u47484  \u45244  \u46412 \u46020 , \u48376 \u51064  \u44048 \u51221 \u51012  \u51452 \u52404 \u54616 \u51648  \u47803 \u54616 \u44256  \u45216 \u46832 \u45716  \u49457 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u45796 \u54792 \u51656 \u51060 \u45768  \u49457 \u44201 \u51012  \u51453 \u51068  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51060 \u44144  \u54624 \u47000 , \u51200 \u44144  \u54624 \u47000 , \u45236 \u44032  \u54616 \u44256  \u49910 \u51008 \u45824 \u47196  \u49332 \u47000  \u54616 \u44256  \u51080 \u51648 \u47564 , \u51221 \u51089  \u46748 \u47159 \u54620  \u47785 \u54364 \u45208  \u48169 \u54693 \u49457 \u51008  \u50630 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u44536 \u51200  \u47785 \u51201  \u50630 \u51060  \u48376 \u51064  \u50640 \u45320 \u51648 \u47564  \u45132 \u50612 \u45796  \u50416 \u44256  \u51080 \u45716  \u49480 \u51077 \u45768 \u45796 .\u50756 \u46300  \u44592 \u49324  \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300  \u44592 \u49324 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u51200 \u46028 \u51201 \u51060 \u47728  \u50977 \u52404 \u51201 \u51064  \u44288 \u44228 \u47484  \u51339 \u50500 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u49884 \u50896 \u49884 \u50896 \u54620  \u49457 \u44201 \u44284  \u54632 \u44760  \u46832 \u50612 \u45212  \u50808 \u47784 \u47484  \u44032 \u51648 \u44256  \u51080 \u50612  \u51452 \u48320 \u50640  \u51060 \u49457 \u51060  \u47566 \u51008  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u48157 \u44256  \u49884 \u50896 \u49884 \u50896 \u54620  \u48516 \u50948 \u44592 \u44032  \u51080 \u50612  \'91\u44057 \u51060  \u51080 \u51004 \u47732  \u54876 \u47141  \u45336 \u52824 \u45716  \u49324 \u46988 \'92\u51004 \u47196  \u45712 \u44788 \u51648 \u44592 \u46020  \u54633 \u45768 \u45796 . \u48520  \u44057 \u51060  \u50672 \u50528  \u54616 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u44048 \u51221 \u46020  \u48736 \u47476 \u44172  \u53440 \u50724 \u47476 \u44256 , \u46608  \u48736 \u47476 \u44172  \u49885 \u49845 \u45768 \u45796 . \u51652 \u51473 \u54616 \u47728  \u54788 \u49892 \u51201 \u51064  \u50672 \u50528 \u48372 \u45796 \u45716  \u51676 \u47551 \u54616 \u47728  \u55141 \u48516 \u46104 \u45716  \u44288 \u44228 \u47484  \u52628 \u44396 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46412 \u50640  \u46384 \u46972  \'91\u48148 \u46988 \u45180 \'92\u44032  \u51080 \u51012  \u49688 \u46020  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u50756 \u46300  \u44592 \u49324 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u47588 \u47141 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u45716  \u46319  \u54633 \u45768 \u45796 . \u53945 \u55176  \'91\u49457 \u51201 \u51064  \u45132 \u47548 \u51060 \u45208  \u55141 \u48516 \'92\u51012  \u45712 \u45180 \u44256  \u51080 \u49845 \u45768 \u45796 . \u53132 \u46973 \u51012  \u50896 \u54616 \u45716  \u44288 \u44228 \u50640 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u44536  \u47560 \u51020 \u51008  \u52649 \u46041 \u51201 \u51060 \u44256  \u46608  \u48320 \u45909 \u49828 \u47084 \u50892 \u49436 , \u49892 \u51228  \u54665 \u46041 \u51004 \u47196  \u51060 \u50612 \u51648 \u51648  \u50506 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732 , \u50584 \u54620 \u53580  \u50672 \u46973 \u54644 \u48380 \u44620 ? \u50500 \u45768 \u50556 . \u44116 \u46020  \u44316 \u52270 \u50500  \u48372 \u51060 \u45912 \u45936 . \u50500  \u50612 \u46497 \u54616 \u51648  \u45208 \u46020  \u45236  \u47560 \u51020 \u51012  \u51096  \u47784 \u47476 \u44192 \u45348 . \u50756 \u46300  \u45208 \u51060 \u53944 \u44032  \u47924 \u51312 \u44148  \'91\u48148 \u46988 \u45180 \'92\u47484  \u51032 \u48120 \u54616 \u45716  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u44536 \u51200  \u48736 \u47476 \u44172  \u53440 \u50724 \u47476 \u44256  \u48736 \u47476 \u44172  \u49885 \u45716  \u44221 \u54693 \u51060  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50500 \u51649  \'91\u51221 \u49436 \u51201 \u51004 \u47196  \u50672 \u44208 \u46104 \u44592 \'92\u44620 \u51648 \u45716  \u49884 \u44036 \u51060  \u44152 \u47540  \u44163 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50668 \u51204 \u55176  \u46168  \u49324 \u51060 \u44032  \u46888 \u44161 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44048 \u51221 \u51012  \u51452 \u52404 \u54616 \u51648  \u47803 \u54616 \u44256  \u47560 \u51020 \u44751  \u45216 \u46832 \u45716  \u45712 \u45196 \u46020  \u46317 \u45768 \u45796 . \u50528 \u51221  \u54364 \u54788 \u46020  \u44284 \u44201 \u54616 \u47728 , \u49912 \u50872  \u46412 \u46020  \u48520 \u44057 \u51060  \u49912 \u50881 \u45768 \u45796 . \u44208 \u54844 \u51008  \u49888 \u51473 \u55176  \u44256 \u48124 \u54644 \u50556 \u44192 \u49845 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u44288 \u44228 \u50640  \u51648 \u47336 \u54632 \u51012  \u45712 \u45184  \u45208 \u47672 \u51648  \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \'91\u45796 \u47480  \u51060 \u49457 \'92\u44284  \u47564 \u45208 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \'91\u48148 \u46988 \'92\u51068  \u49688 \u46020  \u51080 \u51648 \u47564 , \u44536 \u51200  \'91\u51648 \u47336 \u54632 \u51012  \u45804 \u47000 \u44592  \u50948 \u54620  \u44032 \u48316 \u50868  \u47564 \u45224 \'92\u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u44288 \u44228 \u50640  \'91\u54869 \u49888 \'92\u51060  \u54596 \u50836 \u54644  \u48372 \u51060 \u47728 , \u45824 \u54868  \u46020 \u51473  \u53360  \u49912 \u50880 \u51060  \u49373 \u44600  \u49688  \u51080 \u51004 \u45768  \u49345 \u45824 \u48169 \u51012  \u48512 \u46300 \u47101 \u44172  \u49444 \u46301 \u54644 \u50556 \u44192 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u50756 \u46300  \u45208 \u51060 \u53944 \u45716 , \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u50896 \u54616 \u47728 , \u51648 \u44552  \u49345 \u54889 \u50640 \u49436  \u48279 \u50612 \u45208 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u45796 \u47564  \u44536  \u44592 \u51200 \u50640 \u45716  \'91\u49457 \u50837 , \u52649 \u46041 \u51201 \u51064  \u47560 \u51020 \'92\u51060  \u44628 \u47140 \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u50724 \u47004  \u49900 \u49324 \u49689 \u44256 \u48372 \u45796 \u45716  \u54887 \u44608 \u50640  \u50672 \u46973 \u54624  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u50557 \u44036 \u51008  \u45796 \u51060 \u45208 \u48121 \u54620  \u51204 \u44060 \u44032  \u50696 \u49345 \u46121 \u45768 \u45796 . \u51312 \u49900 \u54616 \u49464 \u50836 . \u50696 \u51204 \u44284  \u44057 \u51008  \u44288 \u44228 \u47196  \u46028 \u50500 \u44032 \u44592 \u45716  \u55192 \u46308  \u44163 \u51077 \u45768 \u45796 . \u49436 \u47196  \u53132 \u46973 \u47564  \u52628 \u44396 \u54616 \u45796  \u45149 \u45216  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300  \u44592 \u49324  \u51649 \u50629 \u50868  \u54644 \u49437 \u49324 \u50629 \u51060 \u45208  \u47800 \u51012  \u50416 \u45716  \u51068 , \u50668 \u44592 \u51200 \u44592  \u50880 \u51649 \u51060 \u45716  \u51649 \u50629 \u50640  \u50612 \u50872 \u47532 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51204 \u48152 \u51201 \u51004 \u47196  \u50640 \u45320 \u51648 \u46020  \u45336 \u52824 \u44256 , \u51032 \u50837 \u46020  \u49373 \u44592 \u45716  \u49884 \u44592 \u46972  \u51649 \u50629 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u45320 \u47924  \u52649 \u46041 \u51201 \u51004 \u47196  \u53748 \u49324 \u47484  \u44208 \u51221 \u54616 \u44144 \u45208 , \u49324 \u50629 \u50640  \u46832 \u50612 \u46300 \u45716  \u51068 \u51008  \u51088 \u51228 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50756 \u46300  \u44592 \u49324 \u51032  \u44032 \u51109  \u53360  \u45800 \u51216 \u51008  \'91\u52649 \u46041 \u51201 \u51064  \u47560 \u51020 \'92\u51077 \u45768 \u45796 . \u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u54633 \u44201 \u51012  \u54616 \u50668  \u51060 \u47532 \u51200 \u47532  \u46832 \u50612 \u45796 \u45768 \u45716  \u47784 \u49845 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u50756 \u46300  \u44592 \u49324  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u51008  \u48372 \u53685 \u51077 \u45768 \u45796 . \u46024 \u51060  \u51096  \u47784 \u51060 \u51648  \u50506 \u44256 , \u52649 \u46041 \u51201 \u51064  \u49548 \u48708 \u46020  \u54616 \u44592  \u49772 \u50868  \u46412 \u51077 \u45768 \u45796 . \u52488 \u49900 \u51012  \u51075 \u51648  \u50506 \u44256 , \'91\u51068 \u44288 \u49457 \'92 \u51080 \u44172  \u45432 \u47141 \u54644 \u50556  \u49688 \u51061 \u51012  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u53804 \u51088  \u51221 \u48372 \u47484  \u47566 \u51060  \u50619 \u44172  \u46104 \u45716  \u49884 \u44592 \u45768 , \u51060 \u44275 \u51200 \u44275  \u46028 \u50500 \u45796 \u45768 \u47728  \u48156 \u54408 \u51012  \u54036 \u50500 \u46020  \u51339 \u44192 \u49845 \u45768 \u45796 .    \
