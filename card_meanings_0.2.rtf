u47196 \u51229 \u53944 \u44032  \u44208 \u44397  \u47581 \u54616 \u44172  \u46120 . \u44536 \u47084 \u45208  \u49324 \u46988 \u46308 \u51008  \u50696 \u51204 \u48512 \u53552  \u51060  \u54532 \u47196 \u51229 \u53944 \u44032  \u47581 \u54624  \u44163 \u51060 \u46972 \u45716  \u49324 \u49892 \u51012  \u50508 \u44256  \u51080 \u50632 \u51020 .\u47924 \u50616 \u44032  \u51096 \u47803 \u46096 \u45796 \u45716  \u49324 \u49892 \u51012  \u44648 \u45803 \u44256 , \u52376 \u51020 \u48512 \u53552  \u45796 \u49884  \u54616 \u45716  \u49345 \u54889 \u51453 \u51020  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u50672 \u50528 \u50868 \u50640 \u49436  \u51453 \u51020  \u52852 \u46300 \u45716  \u44536  \u51088 \u52404 \u47196  \u51032 \u48120 \u47484  \u44054 \u44592 \u48372 \u45796 , \'91\u51204 \u54872 \u51216 , \u48516 \u44592 \u51216 \'92\u51060  \u46104 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u52852 \u46300 \u50752  \u52852 \u46300 \u47484  \u50672 \u44208  \u49884 \u53020 \u51452 \u45716  \u45796 \u47532 \u46972 \u44256  \u49373 \u44033 \u54616 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 .\u51453 \u51020  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45244  \u44221 \u50864 , \u49345 \u45824 \u48169 \u51008  \u51648 \u44552  \u44288 \u44228 \u47484  \'91\u50976 \u51648 \'92\u54616 \u47140 \u45716  \u49373 \u44033 \u51060  \u50630 \u49845 \u45768 \u45796 . \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u50756 \u51204 \u55176  \u51921 \u45236 \u44144 \u45208 , \u52376 \u51020 \u48512 \u53552  \u45796 \u49884  \u49884 \u51089 \u54616 \u44256  \u49910 \u50612 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732  :\u51649 \u51109  \u46041 \u47308 \u50640 \u49436  \u51060 \u49457 \u51004 \u47196 , \u44536 \u51200  \u52828 \u54620  \u52828 \u44396 \u50640 \u49436  \u50672 \u51064 \u51004 \u47196  \u48148 \u45068 \u44256  \u49910 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u50500 \u50696  \u52376 \u51020 \u48512 \u53552  \u44288 \u44228 \u47484  \u51116 \u51221 \u47549 \u54616 \u44256  \u49910 \u50612  \u54633 \u45768 \u45796 .\u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732  :\u51060 \u48324 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54841 \u51008  \u45796 \u47480  \u49324 \u46988 \u44284  \u50756 \u51204 \u55176  \u49352  \u52636 \u48156 \u51012  \u51456 \u48708 \u54616 \u47140 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u53360  \u48320 \u54868 \u50640  \u51649 \u47732 \u54644  \u51080 \u49845 \u45768 \u45796 . \u51068 \u48152 \u51201 \u51004 \u47196  \u51060 \u48324 \u51012  \u51032 \u48120 \u54616 \u51648 \u47564 , \'91\u44208 \u54844 \'92\u52376 \u47100  \u50500 \u50696  \u49352 \u47213 \u44172  \u52636 \u48156 \u54616 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51060 \u48324 \u54616 \u44256  \u49910 \u51648  \u50506 \u45796 \u47732 , \u44592 \u51316  \u49324 \u44256  \u48169 \u49885 \u51060 \u45208  \u49373 \u54876  \u54056 \u53556 \u51012  \u50756 \u51204 \u55176  \u46244 \u48148 \u44984 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u48152 \u48152 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u51116 \u54924 \u50640  \u49457 \u44277 \u54620  \u46244  \'91\u51060 \u51204 \u44284  180\u46020  \u45796 \u47480  \u44288 \u44228 \'92\u47484  \u47564 \u46308 \u50612 \u44040  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u50500 \u50696  \u52376 \u51020  \u47564 \u45212  \u52964 \u54540 \u52376 \u47100  \u47532 \u49483 \u54616 \u44256  \u49352 \u47213 \u44172  \u52636 \u48156 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47564 \u50557  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732 , \u50756 \u51204 \u54620  \u51060 \u48324 \u51012  \u46907 \u54633 \u45768 \u45796 .\u51453 \u51020  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u49892 \u51649 \u51060 \u45208  \u49324 \u50629 \u51012  \u51217 \u44172  \u46104 \u45716  \u49345 \u54889 \u51060  \u48156 \u49373 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51060 \u44163 \u51060  \u44557 \u51221 \u51201 \u51064  \u48320 \u54868 \u47484  \u44032 \u51256 \u50732 \u51648 \u45716  \u48120 \u51648 \u49688 \u51077 \u45768 \u45796 . \u50724 \u55176 \u47140  \u49352 \u47196 \u50868  \u44592 \u54924 \u47196  \u49340 \u51012  \u49688  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51453 \u51020  \u52852 \u46300 \u50752  \u50612 \u50872 \u47532 \u45716  \u47568 \u51012  1\u44032 \u51648  \u51064 \u50857 \u54616 \u44256 \u51088  \u54633 \u45768 \u45796 .\u44397 \u48124 \u51008 \u54665  \u52572 \u51333  \u47732 \u51217 \u50640 \u49436  \u46504 \u50612 \u51276 \u45716 \u45936 , \u44536 \u46412  \u48537 \u50632 \u51004 \u47732  \u50976 \u53916 \u48260  \u50504  \u54664 \u51012  \u44163  \u44057 \u44144 \u46304 \u50836 . \u50612 \u46523 \u44172  \u51060 \u44163 \u48372 \u45796  \u48520 \u54665 \u54624  \u49688  \u51080 \u51012 \u51648  \u54616 \u45716  \u44172  \u45208 \u51473 \u50640  \u50508 \u44256  \u48372 \u47732  \u52572 \u44256 \u51032  \u54665 \u50868 \u51060 \u50632 \u45912  \u44152 \u47196 .. 300\u47564  \u50976 \u53916 \u48260  \u51648 \u47924 \u48708 \u51032  \u55092 \u47676 \u49828 \u53664 \u47532  \u51064 \u53552 \u48624  \u20013 \u54633 \u44201 \u50868 \u51008  \u48152 \u48152 \u51077 \u45768 \u45796 . \u51060 \u51204 \u44620 \u51648 \u51032  \u49688 \u54744  \u49373 \u54876 , \u52712 \u51456  \u49373 \u54876 \u51032  \u44208 \u47568 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51060 \u45716  \u54633 \u44201 \u51012  \u53685 \u54644  \u49352 \u47196 \u50868  \u49884 \u51089 \u51012  \u47560 \u51452 \u54616 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u51004 \u47728 , \u46041 \u44592 \u47484  \u51075 \u50612  \u54252 \u44592 \u54616 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u51473 \u50836 \u54633 \u45768 \u45796 .\u51453 \u51020  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u44049 \u51089 \u49828 \u47084 \u50868  \u51648 \u52636 \u51060 \u45208  \u49552 \u49892 \u51060  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u53804 \u51088 \u45208  \u49324 \u50629 \u51008  \u51312 \u49900 \u55176  \u49884 \u46020 \u54644 \u50556  \u54616 \u47728 , \u49324 \u44592 \u47484  \u45817 \u54624  \u50864 \u47140 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564  \u51648 \u44552 \u44620 \u51648  \u44552 \u51204 \u51201 \u51004 \u47196  \u55192 \u46308 \u50632 \u44256 , \'93\u50526 \u51004 \u47196  \u49345 \u54889 \u51060  \u45208 \u50500 \u51656  \u44163 \u51064 \u51648 \'94 \u51656 \u47928 \u54664 \u45796 \u47732 , \u44263  \u50612 \u47140 \u50868  \u49345 \u54889 \u51060  \u45149 \u45212 \u45796 \u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44284 \u44144 \u47484  \u52397 \u49328 \u54624  \u49688  \u51080 \u44172  \u46121 \u45768 \u45796 .    \
