    \uc0\u50756 \u46300  \u53433  \u44536 \u47548 \u51032  \u51032 \u48120 \u51068 \u44284  \u50672 \u50528  \u47784 \u46160  \u51032 \u50837 \u51060  \u45336 \u52824 \u45716  \u49345 \u53468 \u44288 \u49900 \u51012  \u45132 \u44256  \u49910 \u51008  \u49900 \u47532 \u45224 \u50640 \u44172  \u44288 \u49900 \u51060  \u47566 \u51008  \u49324 \u46988 \u50676 \u51221 \u51201 \u51004 \u47196  \u45796 \u44032 \u44032 \u44256  \u49910 \u51008  \u47560 \u51020 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51032 \u50837 \u51060  \u45336 \u52824 \u45716 \u50756 \u46300  \u53433 \u51008  \u51032 \u50837 \u51060  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50756 \u46300 \u51032  \u50896 \u49548 \u45716  \'91\u48520 \'92\u51077 \u45768 \u45796 . \u48520 \u51008  \u51032 \u50837 \u44284  \u50676 \u51221 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u52852 \u46300 \u47484  \u48372 \u47732 , \u50743 \u51060  \u47784 \u46160  \u48744 \u44036 \u49353 \u51068  \u49104 \u45908 \u47084 , \u50773 \u51340 \u45716  \u48520 \u51032  \u49345 \u51669 \u51064  \u49324 \u51088 \u50752 , \u46020 \u47217 \u45565 \u51004 \u47196  \u44032 \u46301  \u52292 \u50892 \u51256  \u51080 \u49845 \u45768 \u45796 . \u44844 \u47532 \u47484  \u47932 \u44256  \u51080 \u45716  \u46020 \u47217 \u45565 \u51008  \u47924 \u54620 \u54620  \u52628 \u51652 \u47141 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54616 \u45716 \u45936 , \u47560 \u52824  \u47560 \u48277 \u49324  \u52852 \u46300 \u50640 \u49436  \u46321 \u51109 \u54664 \u50632 \u45912  \'91\u50864 \u47196 \u48372 \u47196 \u49828 \'92\u47484  \u48372 \u45716  \u45712 \u45196 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u45817 \u51109 \u51060 \u46972 \u46020  \u46832 \u52432 \u45208 \u44040  \u44592 \u49464 \u51077 \u45768 \u45796 . \u50633 \u45929 \u51060 \u44032  \u46308 \u50025 \u46308 \u50025 \u54620  \u51088 \u49464 \u47196  \u50505 \u50500 \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u54644 \u49437 \u54633 \u45768 \u45796  :\u51068 \u51012  \u52628 \u51652 \u54624  \u46412  \u51201 \u44537 \u51201 \u51004 \u47196  \u51032 \u44204 \u51012  \u51228 \u49884 \u54616 \u45716  \u49324 \u46988 . \u51032 \u50837 \u51060  \u45336 \u52824 \u45716  \u49888 \u51077 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50672 \u50528 \u47484  \u54624  \u46412 \u46020  \u50676 \u51032 \u44032  \u45336 \u52824 \u45716  \u49324 \u46988 . \u51060 \u44144  \u54616 \u51088  \u51200 \u44144  \u54616 \u51088 . \u50724 \u45720 \u46020  \u47564 \u45208 \u44256  \u45236 \u51068 \u46020  \u47564 \u45208 \u51088 . \u48165 \u46020  \u47673 \u44256 ,  \u52852 \u54168 \u46020  \u44032 \u44256 , \u50689 \u54868 \u47484  \u48376  \u46244 \u50640  \u48164 \u49352 \u46020 \u47197  \u53685 \u54868 \u54616 \u51088 .\u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732 , \u51200  \u49324 \u46988  \u44316 \u52270 \u51008 \u45936 !! \u45817 \u51109  \u44032 \u49436  \u47568 \u51012  \u44152 \u50612 \u50556 \u44192 \u50612 !!\u50672 \u50528 \u46020  \u48744 \u47532 \u48744 \u47532 , \u51652 \u46020 \u46020  \u48744 \u47532 \u48744 \u47532 , \u44536 \u45824 \u47196  \u44208 \u54844 \u44620 \u51648  \u44264 \u51064  \u54616 \u45716  \u49457 \u54693 .\u50756 \u46300  \u53433 \u51008  \u52628 \u51652 \u47141 \u51060  \u46832 \u50612 \u45208 \u47728 , \u47581 \u49444 \u51076 \u46020  \u50630 \u44256 , \u54876 \u46041 \u51201 \u51064  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u49334 \u51032  \u54876 \u47141 \u49548 \u44032  \u54596 \u50836 \u54620  \u49324 \u46988 \u50640 \u44172 \u45716  \u47588 \u50864  \u44557 \u51221 \u51201 \u51064  \u52852 \u46300 \u51060 \u51648 \u47564 , \u44032 \u45140  \u54588 \u44260 \u54624  \u46412 \u46020  \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u45576 \u50640  \u46916 \u44256  \u49910 \u51008 \u50756 \u46300  \u53433 \u51008  \u44288 \u51333 \u45180 \u44032  \u45336 \u52824 \u45716  \u49324 \u46988 , \u54841 \u51008  \u45817 \u49888  \u45576 \u50640  \u46916 \u44256  \u49910 \u51008  \u49900 \u47532 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u50773 \u51032  \u50743 \u51008  \u44361 \u51109 \u55176  \u54868 \u47140 \u54633 \u45768 \u45796 . \u47581 \u53664 \u51032  \u47924 \u45740 \u46020  \u45796 \u50577 \u54616 \u44256 , \u49353 \u44628 \u46020  \u50508 \u47197 \u45804 \u47197 \u54633 \u45768 \u45796 . \u51060 \u45716  \u44288 \u44228 \u51032  \u51473 \u49900 \u50640  \u51080 \u44256  \u49910 \u51008  \u49900 \u47532 \u47484  \u49345 \u51669 \u54616 \u47728 , \u49464 \u49345  \u49324 \u46988 \u46308 \u51060  \u45208 \u47484  \u48400 \u51452 \u50632 \u51004 \u47732  \u54616 \u45716  \u48148 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u44172 \u45796 \u44032  \u51032 \u50837  \u45336 \u52824 \u45716  \u49457 \u44201 \u44620 \u51648  \u54633 \u54644 \u51648 \u47732 , \'91\u45208 \u45824 \u45716  \u49457 \u54693 \'92\u51060  \u44537 \u46020 \u47196  \u54364 \u52636 \u46104 \u51424 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u47784 \u51076 \u51060 \u45208  \u46041 \u50500 \u47532 , \u49828 \u53552 \u46356  \u44536 \u47353  \u54876 \u46041 \u51060  \u51110 \u51008  \u49324 \u46988 \u54637 \u49345  \u54876 \u48156 \u54616 \u44256  \u50676 \u51032 \u44032  \u45336 \u52432 \u49436  \u51064 \u49912  \u44592 \u51656 \u51060  \u45796 \u48516 \u54620  \u49324 \u46988 \u45817 \u49888 \u51060  \u45796 \u47480  \u49324 \u46988 \u44284  \u52828 \u54616 \u44172  \u51648 \u45236 \u47732 , \u51656 \u53804  \u54253 \u48156 \u52852 \u53665  \u54532 \u47196 \u54596 \u51012  \u48148 \u45032 \u45796 \u47732 , \u50500 \u47560  \u45817 \u49888 \u50640 \u44172  \u45576 \u50640  \u46916 \u44256  \u49910 \u50612 \u49436 \u51068  \u44032 \u45733 \u49457  \u45458 \u51020 \u51452 \u50948 \u47484  \u47604 \u46028 \u47732 \u49436  \u44284 \u51109 \u46104 \u44172  \u54665 \u46041 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u51020 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48520 \u44057 \u51008  \u49457 \u44201 \u50756 \u46300  \u53433 \u51008  \u48520  \u44057 \u51008  \u49457 \u44201 \u51032  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50756 \u46300 \u45716  \u48520 \u51032  \u49345 \u51669 \u51077 \u45768 \u45796 . \u52852 \u46300 \u46020  \u50728 \u53685  \u48520 \u44284  \u55192 \u51032  \u49345 \u51669 \u51064  \u49324 \u51088 , \u44536 \u47532 \u44256  \u46020 \u47217 \u45565 \u51004 \u47196  \u51109 \u49885 \u46104 \u50612  \u51080 \u49845 \u45768 \u45796 .\u50668 \u44592 \u49436  \u48520 \u44057 \u51008  \u49457 \u44201 \u51060 \u46976  :\u52395  \u48264 \u51704 , \u49457 \u44553 \u54633 \u45768 \u45796 . \u45320 \u47924  \u50526 \u49436 \u44032 \u47140 \u45716  \u49457 \u54693 \u51060  \u51080 \u51004 \u47728 , \u49464 \u49900 \u54620  \u44228 \u54925  \u50630 \u51060  \u47924 \u53553 \u45824 \u44256  \u51068 \u51012  \u52628 \u51652 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45208 \u49240 \u44172  \u54364 \u54788 \u54616 \u47732  \'91\u45800 \u49692  \u47924 \u49885 \'92 \u51077 \u45768 \u45796 .\u46160  \u48264 \u51704 , \u54868 \u47484  \u51096  \u45253 \u45768 \u45796 . \u48376 \u51064 \u51060  \u49345 \u52376 \u47484  \u48155 \u44144 \u45208 , \u51068 \u51060  \u51096 \u47803 \u46112  \u44221 \u50864  \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u54868 \u47484  \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536 \u47564 \u53372  \u44592 \u48516 \u46020  \u48744 \u47532  \u54400 \u47532 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u51088 \u51316 \u49900 \u51012  \u45320 \u47924  \u45236 \u49464 \u50892 , \u51452 \u48320  \u49324 \u46988 \u46308 \u47196 \u48512 \u53552  \'91\u44256 \u51665 \u51060  \u49464 \u45796 \'92\u45716  \u51064 \u49885 \u46020  \u51080 \u51004 \u45768 , \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u51032 \u50837  \u44284 \u45796 \u53433  \u50756 \u46300 \u45716  \u51032 \u50837 \u51060  \u44284 \u45796 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50672 \u50528  \u51473 \u50640 \u45716  \u51060  \u54876 \u46041 , \u51200  \u54876 \u46041 \u51012  \u47784 \u46160  \u54644 \u50556  \u54616 \u47728 , \u50668 \u54665 \u51012  \u44040  \u46412 \u46020  \u47784 \u46304  \u47579 \u51665 \u51012  \u49453 \u47157 \u54644 \u50556  \u51649 \u49457 \u51060  \u54400 \u47549 \u45768 \u45796 . \u54016 \u48324  \u54532 \u47196 \u51229 \u53944 \u47484  \u54624  \u44221 \u50864 \u50640 \u46020  \u45824 \u52649  \u54616 \u45716  \u48277 \u51060  \u50630 \u49845 \u45768 \u45796 . \u51032 \u50837 \u51060  \u45336 \u52432 \u49436  \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u54588 \u44260 \u54616 \u44172  \u47564 \u46308  \u49688  \u51080 \u49845 \u45768 \u45796 .A : \u50724 \u45720  \u51312 \u44552  \u49772 \u47732  \u50504 \u46076 ? \u50612 \u51228 \u46020  \u47564 \u45228 \u51094 \u50500 .B : \u50504 \u46076 . \u45320  \u51200 \u48264 \u50640  \u44144 \u44592  \u44032 \u44256  \u49910 \u45796 \u44256  \u54664 \u51094 \u50500 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51032 \u50837 \u51060  \u44284 \u45796 \u54616 \u50668  \u48512 \u54616  \u51649 \u50896 \u51060 \u45208  \u49345 \u49324 \u47484  \u44480 \u52270 \u44172  \u47564 \u46308  \u49688  \u51080 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \'91\u50504 \u46104 \u45716  \u44172  \u50612 \u46376 \u50612 ! \u44536 \u45285  \u54644 \u50556 \u51648 !\'92 \u44400 \u45824 \u49885  \u49324 \u44256 \u47484  \u44053 \u50836 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u49464 \u49900 \u54632 \u51060  \u48512 \u51313 \u54620 \u50756 \u46300  \u53433 \u51008  \u49464 \u49900 \u54632 \u51060  \u48512 \u51313 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u46300  \u53433 \u51032  \u49884 \u49440 \u51008  \u50808 \u48512 \u47484  \u54693 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620 , \u50773 \u51060  \u46308 \u44256  \u51080 \u45716  \u50756 \u46300 \u46020  \u45824 \u47532 \u49437  \u50808 \u48512 \u50640  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300  \u50773 \u51008  \u50808 \u48512 \u51201 \u51064  \u44163 , \u45576 \u50640  \u48372 \u51060 \u45716  \u44163 \u50640 \u47564  \u51665 \u51473 \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u47588 \u45320 \u44032  \u50630 \u44144 \u45208 , \u46021 \u45800 \u51201 \u51064  \u49457 \u54693 \u46020  \u46300 \u47084 \u45216  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45800 \u49692 \u54620  \u49457 \u44201 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 . \'91\u47924 \u50631 \u51012 , \u50612 \u46523 \u44172 , \u50616 \u51228  \u54624 \u51648 \'92\u44032  \u44032 \u51109  \u51473 \u50836 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796 .A : \u48176 \u44256 \u54532 \u45796 . \u44256 \u44592  \u47673 \u44256  \u49910 \u50612 .B : \u44536 \u47000 ? \u44536 \u47084 \u47732  \u44256 \u44592  \u47673 \u51004 \u47084  \u44032 \u51088 . \u45236 \u44032  \u49324 \u51460 \u44172 .A : \u45208  \u50743 \u50640  \u45252 \u49352  \u48176 \u44592 \u51094 \u50500 . \u51312 \u44552  \u44536 \u47088 \u45936 \'85B : \u49452 \u50976  \u53448 \u52712 \u51228  \u49100 \u47532 \u47732  \u46104 \u51648 !! \u45817 \u51109  \u44032 \u51088 !!A : \u45208  \u44396 \u46160  \u49888 \u50612 \u49436  \u44151 \u44592 \u46020  \u55192 \u46308 \u50612 . \u44536 \u45285  \u50668 \u44592  \u50526 \u50640  \u51080 \u45716  \u49885 \u45817  \u44032 \u51088 .B : \u50620 \u47560  \u50504  \u44152 \u47140 . \u44032 \u51088 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u49464 \u49900 \u54632 \u51060  \u48512 \u51313 \u54616 \u50668  \u51068 \u51012  \u44536 \u47476 \u52832  \u49688  \u51080 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \'91\u46112  \u44163  \u44057 \u45796 \u45716  \u50696 \u44048 \'92 \u54616 \u45208 \u47196  \u51068 \u51012  \u44553 \u51089 \u49828 \u47101 \u44172  \u52628 \u51652 \u54616 \u44144 \u45208 , \u49324 \u50629 \u49457  \u44160 \u53664  \u50630 \u51060  \u45800 \u49692  \u47924 \u49885 \u54616 \u44172  \u51068 \u51012  \u52376 \u47532 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  4. \u50724 \u51648 \u46998 \u51060  \u44284 \u54620 \u45236 \u48512 \u44032  \u50500 \u45772  \u50808 \u48512 \u50640  \u44288 \u49900 \u51012  \u46160 \u45796 \u48372 \u45768 , \u50724 \u51648 \u46998 \u51060  \u44284 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u44032 \u51109  \u45824 \u54364 \u51201 \u51064  \u50696 \u49884 \u45716  \u50684 \u53456 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u47924 \u50631 \u51012  \u54664 \u45716 \u51648 , \u45572 \u44396 \u46993  \u50612 \u46356 \u50640  \u44052 \u45716 \u51648  \u49324 \u49324 \u44148 \u44148  \u52280 \u44204 \u54616 \u44144 \u45208 , \u51060 \u47484  \u50500 \u46301 \u48148 \u46301  \u44040 \u47728  \u48516 \u45432 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u46608 \u54620 , \u50724 \u51648 \u46998 \u51060  \u44284 \u54644 \u49436  \u48376 \u51032  \u50500 \u45768 \u44172  \u51452 \u48320 \u50640 \u44172  \u54588 \u54644 \u47484  \u51452 \u44592 \u46020  \u54616 \u47728 , \u51649 \u51109  \u45236 \u50640 \u49436 \u45716  \'91\u45224 \u51032  \u51068 \u50640  \u44288 \u49900 \u51060  \u47566 \u51008  \u49324 \u46988 \'92 \'91\u52280 \u44204 \u51060  \u49900 \u54644  \u54588 \u44260 \u54620  \u49324 \u46988 \'92\u51004 \u47196  \u48372 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300  \u53433  \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300  \u53433 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u50676 \u51221 \u51201 \u51064  \u52629 \u44396  \u44048 \u46021 \u44057 \u51008  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u51648 \u47336 \u54632 \u51012  \u49899 \u50612 \u54616 \u47728 , \u54876 \u46041 \u51201 \u51077 \u45768 \u45796 . \u51221 \u51008  \u47566 \u51648 \u47564  \u44032 \u45140  \u44848 \u45824  \u44592 \u51656 \u51060  \u51080 \u49845 \u45768 \u45796 . \'91\u50500  \u44536 \u44144  \u44536 \u47111 \u44172  \u54616 \u45716  \u44144  \u50500 \u45772 \u45936 , \u45236 \u44032  \u51649 \u51217  \u46832 \u44256  \u49910 \u45348 \'92 \'91\u48744 \u47532  \u48744 \u47532  \u50880 \u51649 \u50668 !!\'92 \u49324 \u46988 \u46308 \u51012  \u50676 \u51221 \u51201 \u51004 \u47196  \u51060 \u45132 \u47728 , \u46021 \u47140 \u54616 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u50865 \u54616 \u45716  \u49457 \u51656 \u46020  \u51080 \u50612 \u49436  \u49912 \u50872  \u46412  \u49548 \u47532 \u47484  \u51648 \u47476 \u44144 \u45208 , \u47561 \u47568 \u51012  \u45236 \u48201 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u44536 \u47564 \u53372  \u54868 \u46020  \u48744 \u47532  \u54400 \u47532 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 .\u47564 \u50557  \u50756 \u46300  \u53433 \u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u48744 \u47532  \u48156 \u51204 \u49884 \u53412 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u50040 \u51060 \u46972 \u47732  \u48744 \u47532 \u48744 \u47532  \u50672 \u50528 \u54616 \u44256  \u49910 \u50612 \u54616 \u47728 , \u50672 \u50528  \u51473 \u51060 \u46972 \u47732  \u51652 \u46020 \u47484  \u48736 \u47476 \u44172  \u45208 \u44032 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u46412 \u46412 \u47196  \u50756 \u46300 \u45716  \u49457 \u50837 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49457 \u50837  \u46412 \u47928 \u50640  \u51104 \u49884  \u47560 \u51020 \u51060  \u48520 \u53440 \u50724 \u47476 \u45716  \u44163 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620 , \u45817 \u49888 \u51032  \u51068 \u49345 , \u52852 \u52852 \u50724 \u53665  \u54532 \u47196 \u54596 \u44620 \u51648  \u47784 \u46160  \u50684 \u53456  \u51473 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u45817 \u49888 \u50640  \u45824 \u54620  \u44288 \u49900 \u51060  \u47588 \u50864  \u47566 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u46020  \'91\u48376 \u51064 \u51060  \u50612 \u46523 \u44172  \u48372 \u51068 \u51648 \u47484  \u49373 \u44033 \u54616 \u47728 \'92 \u44169 \u47784 \u49845 \u51012  \u49888 \u44221  \u50416 \u45716  \u46319  \u48372 \u51077 \u45768 \u45796 . \u52852 \u52852 \u50724 \u53665  \u54532 \u47196 \u54596 , SNS \u44172 \u49884 \u47932  \u47784 \u46160  \'91\u45817 \u49888 \u50640 \u44172  \u44288 \u49900 \u51012  \u45132 \u44592  \u50948 \u54620  \u51204 \u47029 \'92\u51068  \u44163 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53433  \u50672 \u50528 \u50868  \u54644 \u49437  \u52628 \u44032  \u51088 \u47308  (\u51068 \u48376  \u49324 \u51060 \u53944 )\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u54876 \u44592 \u52264 \u44256  \u51600 \u44144 \u50868  \u50672 \u50528 \u47484  \u54616 \u44256  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u50556 \u50808  \u54876 \u46041 \u51012  \u51600 \u44592 \u44256 , \u49436 \u47196 \u50640 \u44172  \u44048 \u51221  \u54364 \u54788 \u46020  \u54413 \u48512 \u54616 \u44172  \u54616 \u45716  \u54200 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u46168  \u51473  \u45572 \u44400 \u44032 \u45716  \u50508 \u44172  \u47784 \u47476 \u44172  \u49345 \u52376 \u47484  \u48155 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51088 \u49888 \u51060  \u49345 \u52376 \u51452 \u44256  \u51080 \u45796 \u45716  \u49324 \u49892 \u51012  \u51064 \u51648 \u54616 \u51648  \u47803 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47924 \u49832  \u47568 \u51012  \u54664 \u45716 \u51648 , \u50612 \u46500  \u54665 \u46041 \u51012  \u54664 \u45716 \u51648 , \u54364 \u51221 \u51060  \u50612 \u46432 \u45716 \u51648  \u46321  \u45800 \u49692 \u55176  \u45576 \u50640  \u48372 \u51060 \u45716  \u48512 \u48516 \u50640 \u47564  \u51665 \u51473 \u54616 \u45796  \u48372 \u45768 , \u48120 \u47896 \u54620  \u44048 \u51221 \u49440 \u51012  \u45459 \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46021 \u45800 \u51201 \u51004 \u47196  \u47924 \u50616 \u44032 \u47484  \u44208 \u51221 \u54616 \u45716  \u44221 \u54693 \u46020  \u48372 \u51077 \u45768 \u45796 . \u49436 \u47196  \u49464 \u49900 \u54620  \u48176 \u47140 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 .\u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u50784 \u51648  \u47784 \u47476 \u44172  \u51088 \u47564 \u54616 \u44256  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u47676 \u51200  \u50672 \u46973 \u54624  \u44144 \u46972  \u49373 \u44033 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49888 \u44221  \u50504  \u50416 \u45716  \u52377  \u54616 \u51648 \u47564 , \u47792 \u47000 \u47792 \u47000  \u50684 \u53456  \u54616 \u44256 , \u51648 \u51064 \u50640 \u44172  \u49548 \u49885 \u51012  \u47932 \u50612 \u48372 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44288 \u49900 \u51060  \u50500 \u51452  \u47566 \u49845 \u45768 \u45796 . \u49457 \u44201 \u46020  \u44553 \u54644 \u49436  \u50672 \u46973 \u51060  \u50630 \u45716  \u45817 \u49888 \u50640 \u44172  \u45813 \u45813 \u54632 \u51012  \u45712 \u45180 \u44592 \u46020  \u54633 \u45768 \u45796 . \u47560 \u51020 \u51008  \u51080 \u51004 \u45208 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \u50672 \u46973 \u54616 \u51648  \u50506 \u45716  \u49345 \u53468 \u47196  \u48372 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53433  \u51649 \u50629 \u50868  \u54644 \u49437 \u45824 \u54364 \u51201 \u51004 \u47196  \u49324 \u50629 \u51012  \u54616 \u45716  \u49324 \u46988 \u50640 \u44172  \u50612 \u50872 \u47532 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51008  \u51032 \u50837 \u51060  \u45336 \u52824 \u47728 , \u47532 \u45908 \u49901 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49892 \u54665 \u47141 \u51060  \u46832 \u50612 \u45208  \u44208 \u44284 \u47932 \u46020  \u51339 \u51008  \u54200 \u51077 \u45768 \u45796 . \u45796 \u47564  \u48376 \u51032  \u50500 \u45768 \u44172  \'91\u48512 \u54616  \u51649 \u50896 \u51060 \u45208  \u46041 \u47308 \u46308 \u51012  \u54588 \u44260 \u54616 \u44172  \u47564 \u46308  \u49688  \u51080 \u45796 \'92\u45716  \u44221 \u44256 \u51077 \u45768 \u45796 . \u46021 \u45800 \u51201 \u51004 \u47196  \u44208 \u51221 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u50612  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54633 \u44201 \u50868 \u50640 \u49436 \u45716 , \u53945 \u50976 \u51032  \u44256 \u51665  \u46412 \u47928 \u50640  \u48520 \u54633 \u44201 \u54624  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824  \u50504  \u54400 \u47532 \u45716  \u47928 \u51228 \u47484  \u45149 \u44620 \u51648  \u46308 \u50668 \u45796 \u48372 \u45716  \u54665 \u46041 \u51077 \u45768 \u45796 . \u51060 \u44172  \u50780  \u50504  \u54400 \u47532 \u51648 ? \u51676 \u51613 \u45208 \u45348  \u47924 \u51312 \u44148  \u54400  \u44144 \u50556  \u47732 \u51217 \u50640 \u49436 \u45716  \u44060 \u51064 \u51201 \u51064  \u48520 \u54200 \u54632 \u51012  \u45824 \u45459 \u44256  \u46300 \u47084 \u45236 , \u47732 \u51217 \u44288 \u51012  \u45817 \u54889 \u49884 \u53420  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51228 \u44032  \u47784 \u51088 \u46972 \u45796 \u45716  \u50584 \u44592 \u51064 \u44032 \u50836 ? \u44048 \u51221 \u51012  \u45236 \u47140 \u45459 \u44256  \u52264 \u48516 \u54632 \u51012  \u50976 \u51648 \u54616 \u49464 \u50836 . \u47924 \u45212 \u55176  \u54633 \u44201 \u54616 \u45716  \u50868 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53433  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u51088 \u49888 \u44048 \u44284  \u49892 \u54665 \u47141 \u51060  \u54633 \u52432 \u51256 \u49436 , \u50612 \u46356 \u49436 \u46304  \u46024 \u51012  \u45132 \u50612 \u47784 \u51004 \u45716  \u50868 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u45936 \u51060 \u53552 \u44032  \u50500 \u45772  \u44048 \u51004 \u47196 \u47564  \u53804 \u51088 \u54616 \u47140 \u45716  \u44221 \u54693 \u51060  \u48372 \u51077 \u45768 \u45796 . \u49457 \u44553 \u54632 \u46020  \u51109 \u50528 \u47932 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51452 \u49885 \u50640 \u49436  \'91\u48520 \u45208 \u48169 \'92\u44057 \u51008  \u49457 \u54693 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51312 \u44552 \u47564  \u45908  \'91\u49464 \u49900 \u54616 \u44256  \u52264 \u48516 \u54616 \u44172 \'92 \u53804 \u51088 \u54644 \u50556  \u54620 \u45796 \u45716  \u47568 \u50432 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .    \
