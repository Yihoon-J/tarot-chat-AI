   7. \uc0\u51204 \u52264 The Chariot \u51204 \u52264  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u51204 \u52264 \u47484  \u53444  \u45224 \u51088 \u44032  \u49849 \u47532 \u47484  \u54693 \u54644  \u46028 \u51652 \u54616 \u45716  \u47784 \u49845 \u51068 \u51060 \u45208  \u50672 \u50528 \u44032  \u48736 \u47476 \u44172  \u51652 \u54665 \u46108 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u51020 \u46832 \u50612 \u45212  \u52628 \u51652 \u47141 \u51012  \u49345 \u51669 \u54620  \u48264  \u49884 \u51089 \u54620  \u51068 \u51060  \u45149 \u51012  \u48372 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u51452 \u48320 \u51012  \u46028 \u50500 \u48372 \u51648  \u47803 \u54616 \u44144 \u45208  \u46021 \u49440 \u51201 \u51064  \u49457 \u54693 \u51012  \u44221 \u44256 \u51649 \u51652 \u49457 \u51012  \u44054 \u44256  \u51080 \u51020  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48736 \u47476 \u44172  \u51652 \u54665 \u46104 \u45716 \u51204 \u52264  \u52852 \u46300 \u45716  \u51068 \u44284  \u50672 \u50528  \u47784 \u46160  \u48736 \u47476 \u44172  \u51652 \u54665 \u46108 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u47568 \u51008  \u52509  2\u47560 \u47532 \u51077 \u45768 \u45796 . 1\u47560 \u47532 \u44032  \u50500 \u45772  2\u47560 \u47532 \u51060 \u44592  \u46412 \u47928 \u50640 , \u45908 \u50865  \u48744 \u47532  \u45804 \u47540  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51204 \u52264  \u50526 \u50640  \u49352 \u44200 \u51652  \u54756 \u47476 \u47700 \u49828  \u45216 \u44060 \u46020  \u48736 \u47480  \u49549 \u46020 \u47484  \u51032 \u48120 \u54633 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796  :\u50672 \u50528  \u52488 \u48152 \u51060 \u46972 \u47732 , \u49828 \u53416 \u49901  \u51652 \u46020 \u44032  \u48744 \u46972 \u51648 \u44144 \u45208  \u47560 \u51020 \u51060  \u44553 \u49549 \u46020 \u47196  \u44618 \u50612 \u51665 \u45768 \u45796 . \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45716  \u44221 \u50864 \u46972 \u47732 , \u44208 \u54844  \u51456 \u48708 \u46020  \u48736 \u47476 \u44172  \u47560 \u47924 \u47532  \u46121 \u45768 \u45796 .\u49324 \u50629 \u51012  \u50868 \u50689 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49688 \u51077 \u44284  \u51648 \u52636 \u51060  \u47784 \u46160  \u44553 \u44201 \u55176  \u45720 \u50612 \u45208 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 . \u51221 \u49888 \u47564  \u48148 \u51677  \u52264 \u47532 \u47732  \u49324 \u50629 \u51060  \u44553 \u49549 \u46020 \u47196  \u49457 \u51109 \u54616 \u44172  \u46121 \u45768 \u45796 .\u45796 \u47564  \u50668 \u44592 \u49436  1\u44032 \u51648  \u51452 \u51032 \u54624  \u51216 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50577 \u51901  \u47784 \u46160  \u47785 \u54364 \u44032  \u46041 \u51068 \u54644 \u50556  \u48736 \u47476 \u44172  \u51652 \u54665 \u46108 \u45796 \u45716  \u44163 \u51077 \u45768 \u45796 .\u47564 \u50557  \u47568  2\u47560 \u47532 \u44032  \u49436 \u47196  \u48152 \u45824  \u48169 \u54693 \u51004 \u47196  \u45208 \u50500 \u44032 \u47140  \u54620 \u45796 \u47732 , \u51204 \u52264 \u45716  \u50526 \u51004 \u47196  \u44032 \u51648  \u47803 \u54616 \u44256  \u44536 \u45824 \u47196  \'91\u51221 \u51648 \'92\u54616 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 . Two of Swords \u52852 \u46300 \u44032  \u44536  \u49345 \u53468 \u47484  \u51096  \u45208 \u53440 \u45236 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47560 \u52268 \u44032 \u51648 \u47196  \u45817 \u49888 \u44284  \u49345 \u45824 \u48169 \u51032  \u47785 \u54364 \u44032  \u45796 \u47476 \u45796 \u47732 , \u50672 \u50528 \u45716  \u51652 \u52377 \u46104 \u51648  \u50506 \u44256  \u44536 \u45824 \u47196  \u47688 \u52628 \u44172  \u46121 \u45768 \u45796 . \u47751  \u44032 \u51648  \u50696 \u49884 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u47785 \u51201 \u51060  \'91\u51104 \u51088 \u47532 \u47484  \u44054 \u45716  \u44163 \'92\u51060 \u44256 , \u45817 \u49888 \u51032  \u47785 \u51201 \u51060  \'91\u49692 \u49688 \u54620  \u50672 \u50528 \'92\u46972 \u47732 , \u44208 \u44397  \u51032 \u44204 \u51060  \u52649 \u46028 \u54616 \u50668  \u44288 \u44228 \u45716  \u47688 \u52628 \u44172  \u46121 \u45768 \u45796 . \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \u48736 \u47476 \u44172  \u46496 \u45208 \u44040  \u44163 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51008  \u48744 \u47532  \u52964 \u54540 \u51060  \u46104 \u44256  \u49910 \u51648 \u47564 , \u45817 \u49888 \u51008  \u52380 \u52380 \u55176  \u44536  \u49324 \u46988 \u51012  \u50508 \u50500 \u44032 \u44256  \u49910 \u45796 \u47732 , \u44288 \u44228 \u45716  \u44536 \u45824 \u47196  \u51221 \u51648 \u46121 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u52280 \u51648  \u47803 \u54616 \u44256  \u44536 \u45824 \u47196  \u46496 \u45208 \u44032 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u48152 \u47732  \u45817 \u49888 \u44284  \u49345 \u45824 \u48169  \u47784 \u46160  \'91\u48744 \u47532  \u52964 \u54540 \u51060  \u46104 \u44256  \u49910 \u50612 \'92 \u54620 \u45796 \u47732 , \u51648 \u44552 \u44620 \u51648  \u47688 \u52656 \u51080 \u45912  \u44288 \u44228 \u44032  \u44553 \u49549 \u46020 \u47196  \u51204 \u44060 \u46112  \u44163 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196 , \u46168  \u45796  \u47560 \u51020 \u51008  \u51080 \u50632 \u45716 \u45936  \u54364 \u54788 \u51012  \u47803 \u54664 \u45912  \u49345 \u54889 , \u51104 \u49884  \u46504 \u50612 \u51256  \u51080 \u45912  \u49345 \u54889 \u51060 \u51424 .\u49324 \u50629 \u51012  \u49884 \u46020 \u54616 \u47140  \u54620 \u45796 \u47732  : \u53748 \u49324 \u47484  \u47560 \u51020 \u47673 \u51020  (\u48376 \u45733 ) + \u54788 \u49892 \u51201 \u51064  \u44228 \u54925 \u51012  \u49464 \u50880  (\u51060 \u49457 )\u51060  \u54633 \u52432 \u51256 \u50556  \u49324 \u50629 \u51060  \u48736 \u47476 \u44172  \u49457 \u44277 \u54624  \u49688  \u51080 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u47560 \u51020 \u49549 \u51004 \u47196 \u45716  \u53748 \u49324 \u47484  \u49373 \u44033 \u54616 \u51648 \u47564 , \u50500 \u47924 \u47088  \u44228 \u54925 \u51012  \u49464 \u50864 \u51648  \u50506 \u45716 \u45796 \u47732 , \u45817 \u49888 \u51032  \u49324 \u50629 \u51008  \'91\u51221 \u51648 \'92\u46112  \u44163 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u45149 \u51109 \u51012  \u48372 \u45716 \u51204 \u52264  \u52852 \u46300 \u45716  \u51473 \u44036 \u50640  \u47688 \u52628 \u51648  \u50506 \u44256 , \u45149 \u51109 \u51012  \u48376 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u51204 \u52264 \u50640 \u45716  \u48652 \u47112 \u51060 \u53356 \u44032  \u50630 \u49845 \u45768 \u45796 . \u48169 \u54693 \u51012  \u53952  \u49688  \u51080 \u45716  \u54648 \u46308 \u51312 \u52264  \u50630 \u51424 . \u45224 \u51088 \u45716  \u47785 \u54364 \u47932 \u44620 \u51648  \u50500 \u51452  \u48736 \u47480  \u49549 \u46020 \u47196  \u46028 \u51652 \u54616 \u44144 \u45208 , \u46028 \u50640  \u48512 \u46378 \u54784  \u53356 \u44172  \u45796 \u52824 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51032  \u51068 \u46020  \u50612 \u46500  \'91\u44208 \u47200 \'92\u50640  \u46020 \u45804 \u54616 \u44172  \u46121 \u45768 \u45796 . \u51204 \u52264  \u52852 \u46300 \u45716  \u54056 \u48176 \u48372 \u45796  \u49849 \u47532 \u50640  \u44032 \u44620 \u50864 \u48064 \u47196 , \u45817 \u49888 \u51060  \u50732 \u48148 \u47476 \u44172  \u52968 \u53944 \u47204  \u54620 \u45796 \u47732  \u51312 \u47564 \u44036  \u50896 \u54616 \u45716  \u48148 \u47484  \u49457 \u52712 \u54616 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u47564 \u50557  \u52852 \u46300 \u44032  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45244  \u44221 \u50864 , \u47560 \u51020 \u51012  \u48148 \u44984 \u51648  \u50506 \u44256  \u45149 \u44620 \u51648  \u48376 \u51064 \u51060  \u44208 \u49900 \u54620 \u45824 \u47196  \u54616 \u44192 \u45796 \u45716  \u51032 \u48120 \u44032  \u44053 \u54633 \u45768 \u45796 .  \u45817 \u49888 \u51060  \u47560 \u51020 \u50640  \u46308 \u50632 \u45796 \u47732 , \u49345 \u52376 \u47484  \u51077 \u45716  \u51068 \u51060  \u51080 \u45908 \u46972 \u46020  \'91\u46308 \u51060 \u45832  \u44163 \'92\u51060 \u44256  \u51473 \u44036 \u50640  \u47560 \u51020 \u51060  \u49885 \u50632 \u45796 \u47732 , \u53468 \u46020 \u47484  \u48148 \u44984 \u51648  \u50506 \u44256  \u44536 \u45824 \u47196  \'91\u47680 \u50612 \u51648 \u44172  \u46112  \u44163 \'92\u51077 \u45768 \u45796 .\u46384 \u46972 \u49436  \u52852 \u46300 \u44032  \u44284 \u44144  / \u54788 \u51116  / \u48120 \u47000  \u51473  \u50612 \u45712  \u50948 \u52824 \u50640  \u46321 \u51109 \u54664 \u45716 \u51648 \u46020  \u51473 \u50836 \u54633 \u45768 \u45796 . \u44284 \u44144 \u50640  \u49324 \u51060 \u44032  \u51339 \u50520 \u44256  \u54788 \u51116  \u50948 \u52824 \u50640  \u51204 \u52264 \u44032  \u46321 \u51109 \u54664 \u45796 \u47732 , \u44263  \u47564 \u45224 \u51060  \u51060 \u47364 \u51648 \u45716  \u44536 \u47548 . \u54788 \u51116  \u47680 \u50612 \u51652  \u49345 \u53468 \u44256  \u54788 \u51116  \u50948 \u52824 \u50640  \u51204 \u52264 \u44032  \u46321 \u51109 \u54664 \u45796 \u47732 , \u44536 \u45824 \u47196  \u54756 \u50612 \u51648 \u45716  \u44536 \u47548 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u51204 \u52264 \u45716  \u51649 \u51652 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51473 \u44036 \u50640  \u48169 \u54693 \u51060  \u48148 \u45068 \u45716  \u51068 \u51008  \u46300 \u47941 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51060  \u51339 \u51008  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51473 \u44036 \u50640  \u54252 \u44592 \u54616 \u51648  \u50506 \u44256  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51060 \u45716  \u49457 \u51656 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47924 \u50631 \u51060 \u46304  \'91\u46263 \u49900 \u51060  \u54596 \u50836 \u54620  \u49345 \u54889 \'92\u50640  \u55192 \u51012  \u48156 \u55064 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u52628 \u51652 \u47141 \u51060  \u46832 \u50612 \u45212 \u51204 \u52264  \u52852 \u46300 \u45716  \u48376 \u51064 \u51032  \u51032 \u51648 \u47196  \u52628 \u51652 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u45572 \u44400 \u44032 \u50640 \u44172  \u46496 \u48128 \u47140 \u49436  \u54616 \u44144 \u45208 , \u49688 \u46041 \u51201 \u51064  \u49828 \u53472 \u49828 \u47484  \u52712 \u54616 \u45716  \u49345 \u54889 \u51060  \u50500 \u45785 \u45768 \u45796 .\u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  :\u51088 \u49888 \u44048 \u51012  \u44032 \u51648 \u47728 \u51137 \u52712 \u54616 \u44256  \u49910 \u51008  \u47785 \u54364 \u47484  \u54693 \u54644 \u48376 \u51064 \u51032  \u51032 \u51648 \u47196  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51068  \u44163 \u51077 \u45768 \u45796 .\u51452 \u48320 \u50640  \u50612 \u46500  \u48169 \u54644 \u44032  \u51080 \u45908 \u46972 \u46020 , \u47784 \u46160  \u47924 \u49884 \u54616 \u47728  \u47785 \u54364 \u47932 \u51012  \u54637 \u54644  \u46028 \u51652 \u54616 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u46024 \u51060  \u50630 \u50612 \u46020 , \u47680 \u47532  \u46504 \u50612 \u51256  \u51080 \u50612 \u46020 , \u48152 \u45824 \u50640  \u55064 \u47568 \u47140 \u46020  \u45149 \u44620 \u51648  \u50896 \u54616 \u45716  \u51060 \u49457 \u50640 \u44172  \u45796 \u44032 \u44040  \u44163 \u51077 \u45768 \u45796 . \u51200 \u46028 \u51201 \u51064  \u47588 \u47141 , \u49345 \u45224 \u51088  \u44592 \u51656 \u51060  \u51080 \u49845 \u45768 \u45796 .Comment\u44060 \u51064 \u51201 \u51004 \u47196 \u45716  \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u50752  \u51221 \u48152 \u45824 \u50640  \u51080 \u45796 \u45716  \u45712 \u45196 \u51012  \u48155 \u50520 \u49845 \u45768 \u45796 . \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u45716  \u48376 \u51064 \u51032  \u50868 \u47749 \u51012  \'91\u50808 \u48512  \u49345 \u54889 \'92\u50640  \u47585 \u44592 \u45716  \u49828 \u53440 \u51068 \u51060 \u51648 \u47564 , \u51204 \u52264  \u52852 \u46300 \u45716  \u48376 \u51064 \u51032  \u50868 \u47749 \u51012  \'91\u49828 \u49828 \u47196  \u44208 \u51221 \u54616 \u45716 \'92 \u49828 \u53440 \u51068 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u54869 \u49892 \u55176  \'91\u45733 \u46041 \u51201 \'92\u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48736 \u47480  \u49549 \u46020 \u47196  \u47581 \u44032 \u51648 \u45716 \u51204 \u52264 \u45716  \u48736 \u47476 \u44172  \u51060 \u46041 \u54616 \u45716  \u47564 \u53372 , \u48736 \u47476 \u44172  \u47581 \u44032 \u51648 \u45716  \u53945 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u49345 \u45824 \u51032  \u47785 \u54364 \u44032  \'91\u50724 \u47196 \u51648  \u45817 \u49888 \u44284  \u51088 \u45716  \u44163 \'92 \u51060 \u50632 \u44256 , \u50612 \u45712  \u45216  \u44536 \u44172  \u48157 \u54784 \u51256 \u49436  \u44288 \u44228 \u44032  0\u47196  \u46028 \u50500 \u44032 \u45716  \u49345 \u54889 \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \'91\u44256 \u48177  \u44277 \u44201 \'92\u51012  \u44048 \u54665 \u54644 \u49436  \u44288 \u44228 \u44032  \u50756 \u51204 \u55176  \u47581 \u44032 \u51648 \u45716  \u49345 \u54889 \u44552 \u48169  \u49324 \u46993 \u50640  \u48736 \u51648 \u45716  \u47564 \u53372 , \u48736 \u47480  \u49549 \u46020 \u47196  \u49885 \u45716  \u49457 \u54693 \u46384 \u46972 \u49436  \u49345 \u45824 \u48169 \u44284  \u51096  \u46104 \u44256  \u49910 \u45796 \u47732 , \'91\u51201 \u51221 \u54620  \u49549 \u46020 \'92\u47484  \u50976 \u51648 \u54644 \u50556  \u54620 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u45320 \u47924  \u46308 \u51060 \u45824 \u49436 \u46020  \u50504 \u46104 \u44256 , \u45320 \u47924  \u47924 \u49900 \u54644 \u49436 \u46020  \u50504 \u46121 \u45768 \u45796 . \u50672 \u50528  \u49549 \u46020 \u44032  \u47582 \u51648  \u50506 \u45716 \u45796 \u47732 , \u44288 \u44228 \u47484  \u51109 \u44592 \u51201 \u51004 \u47196  \u44032 \u51256 \u44032 \u44592  \u55192 \u46308  \u44163 \u51077 \u45768 \u45796 . \u49104 \u47564  \u50500 \u45768 \u46972 , \u44288 \u44228 \u44032  \u51068 \u49692 \u44036 \u50640  \u47581 \u44032 \u51648 \u44592  \u49772 \u50864 \u45768  \u51312 \u44552 \u51008  \u47560 \u51020 \u51012  \u44032 \u48333 \u44172  \u47673 \u51012  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u51068 \u50640 \u49436 \u46020  : \u44049 \u51088 \u44592  \u51096  \u51652 \u54665 \u46104 \u45912  \u54532 \u47196 \u51229 \u53944 \u45208  \u49324 \u50629 \u51060  \u54620 \u49692 \u44036 \u50640  \u47581 \u44032 \u51656  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49324 \u50629 \u51060  \u44553 \u49549 \u46020 \u47196  \u51204 \u44060 \u46104 \u45716  \u47564 \u53372  \u47928 \u51228 \u46020  \u48736 \u47476 \u44172  \u49373 \u44609 \u45768 \u45796 .\u54644 \u49437  2. \u51452 \u48320 \u51012  \u46028 \u50500 \u48372 \u51648  \u47803 \u54616 \u45716 \u51204 \u52264  \u52852 \u46300 \u45716  \u51452 \u48320 \u51012  \u46028 \u50500 \u48372 \u51648  \u47803 \u54616 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52264 \u47484  \u48736 \u47476 \u44172  \u50868 \u51204 \u54616 \u47732 , \u47785 \u51201 \u51648 \u50640  \u48736 \u47476 \u44172  \u46020 \u45804 \u54616 \u51648 \u47564  \u44536 \u47564 \u53372  \u51452 \u48320  \u54413 \u44221 \u51012  \u48372 \u44592 \u46020  \u55192 \u46308 \u50612 \u51665 \u45768 \u45796 . \u52852 \u46300 \u51032  \u54644 \u49437 \u46020  \u48708 \u49847 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u51088 \u47732  :\u47564 \u50557  \u51204 \u52264  \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51060  \'91\u51068 \'92\u51012  \u47785 \u54364 \u47196  \u54616 \u44256  \u51080 \u45796 \u47732 , \u50724 \u47196 \u51648  \'91\u51068 \'92\u47564  \u48372 \u44256  \u45804 \u47532 \u44592  \u46412 \u47928 \u50640 , \u45817 \u49888 \u51060  \u45576 \u50640  \u46308 \u50612 \u50724 \u51648  \u50506 \u51012  \u44163 \u51077 \u45768 \u45796 .\u50724 \u47196 \u51648  \u51088 \u49888 \u51032  \u47785 \u54364 \u47564  \u49888 \u44221  \u50416 \u44592  \u46412 \u47928 \u50640 , \u51452 \u48320  \u51032 \u44204 \u51012  \u47924 \u49884 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46021 \u45800 \u51201 \u51064  \u49457 \u54693 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u49324 \u49548 \u54620  \u46356 \u53580 \u51068 \u51012  \u45459 \u52832  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51068 \u51008  \u48736 \u47476 \u44172  \u51652 \u54665 \u46104 \u51648 \u47564 , \u51473 \u44036  \u51473 \u44036  \u45459 \u52824 \u45716  \u48512 \u48516  \u46412 \u47928 \u50640  \u51068 \u51060  \u47581 \u44032 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 .\u48120 \u47896 \u54620  \u44048 \u51221 \u49440 \u51012  \u51060 \u54644 \u54616 \u51648  \u47803 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45800 \u49692  \u47924 \u49885 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u44512 \u54805 \u51060  \u54596 \u50836 \u54620  (\u54841 \u51008  \u50577 \u45796 \u47532 )\u51204 \u52264  \u52852 \u46300 \u45716  \u44512 \u54805 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u47700 \u49884 \u51648 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49828 \u54609 \u53356 \u49828 \u45716  \u46160  \u47560 \u47532 \u51077 \u45768 \u45796 . \u54616 \u50560  \u51901 \u51060  \u51060 \u49457 \u51012  \u49345 \u51669 \u54616 \u44256 , \u44160 \u51008  \u51901 \u51008  \u48376 \u45733 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51593  \u51060 \u49457 \u44284  \u48376 \u45733 \u51012  \u44512 \u54805  \u51080 \u44172  \u50976 \u51648 \u54644 \u50556  \u50526 \u51004 \u47196  \u45208 \u50500 \u44040  \u49688  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \'91\u54077 \u51060 \'92 \u46608 \u54620  \u44512 \u54805 \u51012  \u46907 \u54633 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u48376 \u45733 \u51008  \'91\u45817 \u51109  \u44256 \u48177 \u54616 \u46972 \'92\u47484  \u50808 \u52824 \u44256  \u51060 \u49457 \u51008  \'91\u50500 \u51649  \u46412 \u44032  \u50500 \u45768 \u45796 \'92\u47484  \u50808 \u52824 \u45716  \u49345 \u54889 \u51060 \u46972 \u47732 , \u51221 \u45813 \u51008  \u44536  \u51473 \u44036  \u50612 \u46360 \u44032 \u50640  \u51080 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u54924 \u49324 \u47484  \u45796 \u45768 \u47732 \u49436  \u49324 \u50629 \u51012  \u51456 \u48708 \u54616 \u45716  \u49345 \u54889 \u51060 \u46972 \u47732 , \u45817 \u51109  \u53748 \u49324 \u54616 \u44144 \u45208  \u49324 \u50629 \u51012  \u50756 \u51204 \u55176  \u51217 \u44592 \u48372 \u45796 \u45716  \'91\u53804 \u51105 \'92 \u54805 \u49885 \u51004 \u47196  \u51652 \u54665 \u54644 \u50556  \u54633 \u45768 \u45796 . \u46168 \u51008  \u48152 \u46300 \u49884  \u54632 \u44760  \u44032 \u50556  \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u51204 \u52264  \u52852 \u46300 \u45716  \u44032 \u45140 \u50473  \u50577 \u45796 \u47532 \u47484  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51060 \u49457 (\u54788 \u51116  \u50672 \u51064 )\u44284  \u48376 \u45733 (\u49352 \u47196 \u50868  \u51060 \u49457 ) \u49324 \u51060 \u50640 \u49436  \u44512 \u54805 \u51012  \u52286 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51204 \u52264 \u47484  \u53444  \u45224 \u51088 \u45716  \u46160  \u49828 \u54609 \u53356 \u49828 \u47484  \u44512 \u54805  \u51080 \u44172  \'91\u51312 \u51221 \'92 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u50612 \u45712  \u49692 \u44036  \u47560 \u51020 \u51060  \u54620 \u51901 \u51004 \u47196  \u44592 \u50868 \u45796 \u47732 , \u44288 \u44228 \u47484  \u48148 \u47196  \u51221 \u47532 \u54616 \u44256  \u44536 \u51901 \u51004 \u47196  \u46496 \u45216  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u51204 \u52264  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u51204 \u52264  \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \u48376 \u51064 \u51060  \u54616 \u44256  \u49910 \u51008  \u51068 \u51012  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51060 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u52628 \u51652 \u47141 \u44284  \u51032 \u51648  \u47784 \u46160  \u44053 \u54620  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51068 \u50640  \u51080 \u50612  \u49457 \u44284 \u46020  \u46832 \u50612 \u45212  \u54200 \u51060 \u44256 , \u54028 \u51060 \u54021  \u45336 \u52824 \u44172  \u54644 \u45236 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u47560 \u51020 \u50640  \u46300 \u45716  \u49345 \u45824 \u44032  \u51080 \u45796 \u47732 , \u50672 \u50528 \u46020  \u48744 \u47532  \u44208 \u54844 \u46020  \u48744 \u47532 \u54616 \u44256  \u49910 \u50612  \u54633 \u45768 \u45796 . \u45796 \u47564  \u51473 \u44036 \u50640  \u47560 \u51020 \u51060  \u49885 \u45716 \u45796 \u47732 , \u44536 \u45824 \u47196  \u48736 \u47476 \u44172  \u47680 \u50612 \u51648 \u45716  \'91\u44552 \u49324 \u48736 \'92 \u49457 \u54693 \u46020  \u44053 \u54633 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u48148 \u46988 \u45180  \u51080 \u45716  \u49324 \u46988 , \u48176 \u47140 \u44032  \u48512 \u51313 \u54616 \u47728  \u47952 \u46304 \u51648  \u48376 \u51064  \u46907 \u45824 \u47196  \u54616 \u47140 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51204 \u52264  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u48376 \u51064 \u51032  \u49888 \u45392 \u51012  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51060 \u44592 \u47196  \u54620  \u49345 \u54889 \u51077 \u45768 \u45796 . \u44536 \u45824 \u47196  \u51649 \u51652 \u51077 \u45768 \u45796 . \u50668 \u44592 \u49436  \u51452 \u48320  \u52852 \u46300 \u44032  \u51473 \u50836 \u54633 \u45768 \u45796 . \u47564 \u50557  \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51060  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u49345 \u45824 \u48169 \u51008  \'91\u45817 \u49888 \u50640 \u44172  \u51200 \u46028 \u51201 \u51004 \u47196  \u46308 \u51060 \u45824 \u47140 \u45716  \u49373 \u44033 \'92\u51060 \u44256  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732  \'91\u44256 \u48124 \u46020  \u54616 \u51648  \u50506 \u44256  \u46496 \u45208 \u44040  \u49373 \u44033 \'92 \u51077 \u45768 \u45796 . \u54665 \u46041 \u50640  \u47581 \u49444 \u51076 \u51008  \u50630 \u49845 \u45768 \u45796 . \u44036 \u54841  \u48376 \u51064 \u51060  \u54616 \u47140 \u45716  \u47785 \u54364 \u44032  \u45320 \u47924  \u46748 \u47159 \u54644 \u49436  (\u50696 \u47484  \u46308 \u50612  \u51068 ) \'91\u51452 \u48320 \u51012  \u49373 \u44033 \u46020  \u47803 \u54616 \u45716  \u49324 \u46988 \'92\u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u50896 \u54616 \u45716  \u48148 \u44032  \u44057 \u50500 , \u44288 \u44228 \u44032  \u44553 \u49549 \u46020 \u47196  \u51652 \u51204 \u46108 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u47564 \u50557  \u46168  \u47784 \u46160  \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732  \u44208 \u54844 \u51060  \u52488 \u44256 \u49549 \u51004 \u47196  \u51652 \u54665 \u46104 \u44256 , \u49828 \u53416 \u49901 \u50640 \u49436 \u46020  \u49373 \u44033 \u51060  \u44057 \u45796 \u47732  \u49828 \u53416 \u49901 \u46020  \u52488 \u44256 \u49549 \u51004 \u47196  \u51652 \u54665 \u46121 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u47566 \u51060  \u44032 \u44620 \u50892 \u51648 \u45716  \u49884 \u44592 \u44032  \u46112  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47564  \u51032 \u44204  \u52649 \u46028 \u51060  \u49373 \u44220 \u51012  \u46412 , \u44288 \u44228 \u44032  \'91\u50756 \u51204 \u55176  \u51221 \u51648 \'92\u46104 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50612 \u45712  \u54620  \u51901 \u51060  \u51088 \u51316 \u49900 \u51012  \u44413 \u55184  \u54596 \u50836 \u44032  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u51204 \u52264  \u52852 \u46300 \u45716  \u51473 \u47549 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51060 \u49457 \u44284  \u48376 \u45733 \u51012  \u46041 \u51068 \u54616 \u44172  \u51312 \u51208  \u54616 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u51060 \u46412  \u51473 \u50836 \u54620  \u44163 \u51008  \'91\u49345 \u45824 \u48169 \u51060  \u47924 \u50631 \u51012  \u47785 \u54364 \u47196  \u54616 \u45716 \u44032 \'92\u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732  \'91\u45817 \u49888 \u44284 \u51032  \u51116 \u54924 \'92\u47484  \u52572 \u50864 \u49440  \u47785 \u54364 \u47196  \u46160 \u44256  \u48736 \u47476 \u44172  \u45796 \u44032 \u44040  \u44163 \u51060 \u47728 , \u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732  \'91\u45796 \u47480  \u47785 \u54364 \'92\u47484  \u52572 \u50864 \u49440 \u51004 \u47196  \u46160 \u44256  \u48736 \u47476 \u44172  \u47680 \u50612 \u51656  \u44163 \u51077 \u45768 \u45796 .\u51204 \u52264  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u45817 \u48516 \u44036  \u51068 \u51060  \u44553 \u49549 \u46020 \u47196  \u51204 \u44060 \u46104 \u47728  \u44263  \u44208 \u47200  \u45208 \u44172  \u46121 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u49849 \u47532 \u51032  \u49345 \u51669 \u51064  \'91\u50900 \u44228 \u44288 \'92\u51012  \u50416 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46384 \u46972 \u49436  \u51068 \u51032  \u55120 \u47492 \u46020  \u44557 \u51221 \u51201 \u51060 \u46972  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49324 \u50629 \u51012  \u50868 \u50689 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49324 \u50629 \u52404 \u46020  \u48736 \u47476 \u44172  \u52964 \u51656  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45796 \u47564  \u51473 \u44036 \u50640  \u50612 \u44555 \u45212  \u49440 \u53469 \u51012  \u54616 \u44144 \u45208 , \u49892 \u49688 \u47484  \u54616 \u44172  \u46112  \u44221 \u50864  \u51068 \u51060  \u48736 \u47476 \u44172  \u47581 \u54624  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51473 \u50836 \u54620  \u44163 \u51008  \u44512 \u54805 \u51077 \u45768 \u45796 . \u54620  \u51901 \u50640  \'91\u47792 \u48757 \'92\u51012  \u54616 \u45716  \u49440 \u53469 \u51648 \u45716  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 .\u51204 \u52264  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u45817 \u48516 \u44036  \u49688 \u51077 \u44284  \u51648 \u52636  \u47784 \u46160  \u45720 \u50612 \u45208 \u44172  \u46121 \u45768 \u45796 . \u46024 \u51060  \u51096  \u48268 \u47532 \u45716  \u47564 \u53372  \u50432 \u50432 \u51060 \u46020  \u52964 \u51648 \u44172  \u46108 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u47924 \u51089 \u51221  \u51200 \u52629 \u54616 \u44144 \u45208  \u47924 \u51089 \u51221  \u53804 \u51088 \u54616 \u45716  \u44163 \u46020  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51473 \u50836 \u54620  \u44163 \u51008  \'91\u44512 \u54805 \'92\u51077 \u45768 \u45796 . \u49688 \u51077 \u44284  \u51648 \u52636 \u51012  \u51201 \u51208 \u55176  \u53685 \u51228 \u54616 \u47728 , \u50612 \u45712  \u54620  \u51901 \u50640  \u46024 \u51012  \u47792 \u50500 \u49436  \u53804 \u51088 \u54616 \u51648  \u47568 \u46972 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u50857 \u44592 \u47484  \u44032 \u51648 \u44256  \u54665 \u46041 \u54616 \u49464 \u50836 \u44536 \u46041 \u50504  \u47581 \u49444 \u50668  \u50772 \u45912  \u51068 , \u51648 \u44552 \u51060  \u48148 \u47196  \u51201 \u44592 \u51077 \u45768 \u45796 . \u45796 \u49548  \u49436 \u53812 \u47084 \u46020  \u51339 \u49845 \u45768 \u45796 . \u50500 \u47924 \u44163 \u46020  \u54616 \u51648  \u50506 \u51004 \u47732 , \u50500 \u47924 \u44163 \u46020  \u51068 \u50612 \u45208 \u51648  \u50506 \u49845 \u45768 \u45796 . \u49892 \u54056 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 . \u44536 \u44163  \u46608 \u54620  \u49457 \u51109 \u54616 \u45716  \u44284 \u51221  \u51473  \u54616 \u45208 \u51077 \u45768 \u45796 . \u49884 \u46020 \u54644 \u48372 \u49464 \u50836 .\u51312 \u50616  2. \u51312 \u51208 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 \u49892 \u54665 , \u52628 \u51652 \u51008  \u48516 \u47749  \u51339 \u51008  \u51068 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u45320 \u47924  \u52649 \u46041 \u51201 \u51064  \u44148  \u50500 \u45776 \u51648 \u50836 . \u51312 \u44552  \u45908  \u49888 \u51473 \u54616 \u44256  \u51060 \u49457 \u51201 \u51004 \u47196  \u54032 \u45800 \u54616 \u49464 \u50836 . \u51068 \u51060  \u54620  \u48264  \u50612 \u44555 \u45208 \u44592  \u49884 \u51089 \u54616 \u47732 , \u45149 \u50630 \u51060  \u50612 \u44555 \u45216  \u50948 \u44592 \u50640  \u52376 \u54644 \u51080 \u49845 \u45768 \u45796 .\u49457 \u44201 \u51012  \u51453 \u51068  \u54596 \u50836 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44592 \u44032  \u49468  \u44163 \u44284  \u49912 \u44032 \u51648  \u50630 \u45716  \u44163 \u51008  \u48516 \u47749  \u45796 \u47493 \u45768 \u45796 . \u51088 \u49888 \u51032  \u51032 \u44204 \u51012  \u48516 \u47749 \u55176  \u54364 \u52636 \u54616 \u46104 , \u47924 \u47168 \u54616 \u44172  \u44404 \u51648 \u45716  \u47560 \u49464 \u50836 . \u51312 \u51208 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 .    \
