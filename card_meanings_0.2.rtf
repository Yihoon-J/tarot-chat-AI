   \uc0\u50756 \u46300 8 \'96 \u44553 \u48320  \u50756 \u46300 8 \u44536 \u47548 \u51032  \u51032 \u48120 \u50756 \u46300 \u44032  \u47924 \u50616 \u44032 \u47484  \u54693 \u54644  \u51068 \u51228 \u55176  \u45216 \u50500 \u44032 \u45716  \u49345 \u54889 \u49345 \u54889 \u51060  \u44553 \u48320 \u54616 \u45716  \u49884 \u44592 \u50756 \u46300 \u45716  \u44263  \u46405 \u50640  \u44866 \u55184  \u44163 , \u54665 \u46041 \u51060  \'91\u50756 \u49457 \'92\u46104 \u45716  \u49884 \u44592 \u51473 \u50836 \u54620  \u47700 \u49884 \u51648 \u47484  \u48155 \u45716 \u45796 \u45716  \u51032 \u48120  (\u49548 \u49885 , \u45684 \u49828 )[\u54645 \u49900  \u54644 \u49437 ] \u49345 \u54889 \u51060  \u49692 \u49885 \u44036 \u50640  \u48148 \u45068 \u45716 \u50756 \u46300 8\u51008  \u44536 \u46041 \u50504  \u51221 \u52404 \u46104 \u50612  \u51080 \u45912  \u51068 , \u49324 \u44148 \u51060  \u48736 \u47476 \u44172  \u48320 \u54620 \u45796 \u45716  \u46907 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51593  \'91\u44553 \u51204 \u44060 \'92\u47484  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u44536 \u47548 \u51008  \u50756 \u46300 \u44032  \u48736 \u47476 \u44172  \u51648 \u45208 \u44032 \u45716  \u49345 \u54889 \u51012  \u47896 \u49324 \u54620  \u51109 \u47732 \u51077 \u45768 \u45796 . \u51060 \u45716  \'91\u54665 \u46041 , \u49324 \u44148 \'92\u51060  \u48736 \u47476 \u44172  \u51648 \u45208 \u44040  \u44163 \u51060 \u46972 \u45716  \u48708 \u50976 \u51201  \u54364 \u54788 \u51077 \u45768 \u45796 .\u48708 \u49847 \u54620  \u52852 \u46300 \u47196  The Tower \u52852 \u46300 \u44032  \u51080 \u49845 \u45768 \u45796 . The Tower \u52852 \u46300  \u46608 \u54620  \u49345 \u54889 \u51032  \u44553 \u48320 \u54868 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u44536 \u47084 \u45208  \u45656 \u50521 \u49828 \u44032  \u51312 \u44552  \u45796 \u47493 \u45768 \u45796 . \u53440 \u50892  \u52852 \u46300 \u44032  \u52380 \u51116 \u51648 \u48320 , \u50696 \u52769 \u54616 \u44592  \u55192 \u46304  \u51068 \u51060  \u51068 \u50612 \u45208 \u45716  \u44163 \u51060 \u46972 \u47732 \'85\u50756 \u46300 8\u51008  \u45817 \u49888 \u51060  \u51060 \u48120  \u50696 \u52769 \u54616 \u44256  \u51080 \u45912 , \u50696 \u49345 \u54616 \u44256  \u51080 \u45912  \u51068 \u51060  \u51068 \u50612 \u45208 \u45716  \u44163 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u50756 \u46300  8\u44060 \u45716  \u51068 \u51228 \u55176  \u44057 \u51008  \u48169 \u54693 \u51004 \u47196  \u45216 \u50500 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 .\u53440 \u50892  \u52852 \u46300 ,\u50696 \u52769  \u48520 \u44032 \u45733 \u54620  \u49324 \u44148 \u50756 \u46300  8,\u50696 \u52769  \u44032 \u45733 \u54620  \u51068  \u47784 \u46304  \u47561 \u45824 \u44032  \u44368 \u52264 \u54616 \u51648  \u50506 \u51004 \u47728 , \u51221 \u50672 \u54616 \u44172  \u45216 \u50500 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \u51060  \u44305 \u44221 \u51060  \u51032 \u46020 \u46108  \u44163 \u51076 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644 , \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u54788 \u49892 \u51008  \u45817 \u52488 \u48512 \u53552  \u50696 \u49345 \u54624  \u49688  \u51080 \u50632 \u45912  \u51068 . \u44536 \u47084 \u45208  \u44536  \u48320 \u54868 \u44032  \u50696 \u49345 \u48372 \u45796  \u48736 \u47476 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u12302 \u50756 \u46300  8 \u51032 \u48120 \u50752  \u54644 \u49437 \u12303 <\u51068 \u44284  \u50672 \u50528 > \'96 tarotfun.jp\u51060 \u45716  \u45796 \u49884  \u47568 \u54616 \u47732  :\u54217 \u49548 \u50640  \u50696 \u49345 \u51008  \u54616 \u44256  \u51080 \u50632 \u51004 \u45208 , \u44536  \u49884 \u44592 \u44032  \u50526 \u45817 \u44200 \u51664 \u51652 \u54665 \u46104 \u45716  \u49549 \u46020 \u44032  \u50696 \u49345 \u48372 \u45796  \u48744 \u46972 \u51664 \u50696 \u49345 \u48372 \u45796  \u48736 \u47476 \u44172  \u45149 \u45224 \u47532 \u46377 \u50640 \u49436 \u45716  :\u45208 \u50640 \u44172  \u44288 \u49900  \u51080 \u45912  \u49324 \u46988 \u51060 , \u50696 \u49345 \u48372 \u45796  \u48736 \u47476 \u44172  \u45796 \u44032 \u50740 \u50672 \u46973 \u51012  \u50696 \u49345 \u48372 \u45796  \u48744 \u47532  \u48155 \u51020 \u49324 \u50629 \u51060  \u50696 \u49345 \u48372 \u45796  \u48736 \u47476 \u44172  \u52964 \u51664 , OR \u49457 \u44277  \u50668 \u48512 \u44032  \u48736 \u47476 \u44172  \u44208 \u54032  \u45224 \u54217 \u44032  \u49884 \u44592 \u44032  \u50526 \u45817 \u44200 \u51664 \u49888 \u49549 \u54620  \u45824 \u51025 \u51060  \u54596 \u50836 \u54616 \u47728 , \u54217 \u49548 \u50640  \u45824 \u48708 \u47484  \u51096  \u54644 \u50556  \u54620 \u45796 \u45716  \u51312 \u50616 \u51216 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 . \u44263  \u45817 \u49888 \u50640 \u44172  \u50628 \u52397 \u45212  \u44592 \u54924 \u44032  \u51452 \u50612 \u51656  \u44163 \u51077 \u45768 \u45796 .\u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u47749 \u54869 \u54620  \u47785 \u54364 \u47484  \u44032 \u51652 8\u44060 \u51032  \u50756 \u46300 \u45716  \u47749 \u54869 \u54620  \u47785 \u54364 \u47484  \u44032 \u51652  \u49324 \u46988 , \u54841 \u51008  \u48376 \u51064 \u51032  \u47785 \u54364  \u51060 \u50808 \u50640 \u45716  \u44288 \u49900  \u50630 \u45716  \u49345 \u53468 \u47484  \u44032 \u47532 \u53429 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50756 \u46300 \u45716  \'91\u47196 \u53011 \'92\u51060 \u46972 \u44256  \u51060 \u54644 \u54616 \u47732  \u54200 \u54633 \u45768 \u45796 . \u51068 \u51228 \u55176  \u44057 \u51008  \u47785 \u54364 \u47484  \u54693 \u54644  \u46028 \u51652 \u54616 \u47728 , \u51452 \u48320  \u49324 \u47932 \u50640 \u45716  \u44288 \u49900 \u51060  \u50630 \u49845 \u45768 \u45796 . \u51204 \u52264  \u53440 \u47196  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u47484  \u46916 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47785 \u54364 \u44032  \u46748 \u47159 \u54616 \u47728  \u51649 \u51652 \u49457 \u51060  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377  \u54624  \u46412 \u45716  :\u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u51060 \u48120  \u51221 \u54644 \u51256  \u51080 \u45716  \u49324 \u46988 \u51060 \u48120  \u47749 \u54869 \u54620  \u47785 \u54364 \u44032  \u51080 \u50612 , \u50672 \u50528 \u45208  \u45796 \u47480  \u51068 \u50640 \u45716  \u44288 \u49900  \u50630 \u45716  \u49345 \u53468 \u47785 \u51201 \u44284  \u48169 \u54693 \u49457 \u51060  \u51221 \u54644 \u51648 \u45716  \u49884 \u44592 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u50756 \u49457 \u50640  \u46020 \u45804 \u54616 \u45716 Eight of wands\u45716  \u44536 \u46041 \u50504  \u51456 \u48708 \u54664 \u45912  \u44163 \u46308 \u51060  \u50756 \u49457 \u50640  \u46020 \u45804 \u54632 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51456 \u48708 \u44032  \u45916  \u46096 \u45796 \u47732  \u49892 \u54056 \u54616 \u44256 , \u51456 \u48708 \u44032  \u52649 \u48516 \u54664 \u51004 \u47732  \u49457 \u44277 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u44536 \u46041 \u50504  \u44984 \u51456 \u55176  \u45432 \u47141 \u54664 \u51020 \u50640 \u46020 , \u44208 \u44284 \u47484  \u48372 \u51648  \u47803 \u54664 \u51012  \u46412  \u8594  \u51339 \u51008  \u49457 \u44284 \u47484  \u50619 \u44172  \u46120 \u54217 \u49548 \u50640  \u51068 \u51060 \u45208  \u44277 \u48512 \u47484  \u50676 \u49900 \u55176  \u54616 \u51648  \u50506 \u50520 \u45796 \u47732  \u8594  \u51312 \u47564 \u44036  \u45208 \u49244  \u44208 \u44284 \u47484  \u48372 \u44172  \u46120 \u54217 \u49548 \u50640  \u51068 \u51060 \u45208  \u44277 \u48512 \u47484  \u50676 \u49900 \u55176  \u54664 \u45796 \u47732  \u8594  \u51312 \u47564 \u44036  \u51339 \u51008  \u44208 \u44284 \u47484  \u48372 \u44172  \u46120 \u50756 \u46300 \u45716  \u54616 \u45720 \u50640 \u49436  \u45216 \u50500 \u50752 , \u51312 \u47564 \u44036  \u48736 \u47476 \u44172  \u46405 \u50640  \u44866 \u55193 \u45768 \u45796 . \u44536 \u47532 \u44256 , \u44536 \u46041 \u50504 \u51032  \u48708 \u54665 \u51060  \'91\u47560 \u47924 \u47532 \'92\u46121 \u45768 \u45796 . \u48708 \u54665 \u54616 \u45716  \u46020 \u51473 \u50640  \u44452 \u46020 \u44032  \u48320 \u54616 \u51648  \u50506 \u45716 \u45796 \u47732 , \u53360  \u51060 \u48320  \u50630 \u51060  \u50756 \u46300 \u44032  \u44536 \u45824 \u47196  \u44866 \u55184  \u44163 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u51473 \u50836 \u54620  \u47700 \u49884 \u51648 \u47484  \u48155 \u45716 \u50756 \u46300 8\u51008  \u44536 \u46041 \u50504  \u44592 \u45796 \u47532 \u44256  \u51080 \u45912  \u49548 \u49885 \u51012  \u48155 \u44144 \u45208 , \u45572 \u44400 \u44032 \u47196 \u48512 \u53552  \u51473 \u50836 \u54620  \'91\u47700 \u49884 \u51648 \'92\u47484  \u51204 \u45804  \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u54756 \u50612 \u51652  \u50672 \u51064 \u50640  \u45824 \u54620  \u49548 \u49885 \u51012  \u46307 \u44172  \u46120 \u51677 \u49324 \u46993  \u49345 \u45824 \u50640  \u45824 \u54620  \u51221 \u48372 \u47484  \u46307 \u44172  \u46120 \u45572 \u44400 \u44032 \u50640 \u44172  \'93\u51172 \u44032  \u47952  \u54664 \u45796 \u45908 \u46972 \'94 \u46321 \u46321 \u51032  \u49548 \u47928 \u51012  \u51204 \u54644  \u46307 \u44172  \u46120  (\u51221 \u48372 \u44032  \u51652 \u51676 \u51064 \u51648  \u44032 \u51676 \u51064 \u51648 \u45716  \u50508  \u49688  \u50630 \u49845 \u45768 \u45796 )\u50672 \u46973 \u51012  \u44592 \u45796 \u47532 \u44256  \u51080 \u45912  \u49345 \u45824 \u50640 \u44172 , \u50672 \u46973 \u51012  \u48155 \u44172  \u46120 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49457 \u44553 \u54632 \u50756 \u46300 8\u51008  \u49457 \u44553 \u54620  \u49457 \u44201 \u51012  \u44032 \u51652  \u49324 \u46988 , \u54841 \u51008  \u49457 \u44553 \u54620  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u50756 \u46300 \u45716  \u47785 \u54364 \u47932 \u51012  \u54693 \u54644  \u48736 \u47476 \u44172  \u45216 \u50500 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49324 \u49440 \u51004 \u47196  \u46504 \u50612 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640 , \u50526 \u51004 \u47196  \u45908 \u50865 \u45908  \u44032 \u49549 \u51060  \u48537 \u51012  \u50696 \u51221 \u51077 \u45768 \u45796 . \u48736 \u47476 \u44172  \u45796 \u44032 \u44032 \u45716  \u47564 \u53372 , \u47785 \u54364 \u47932 \u50640  \u51201 \u51473 \u54616 \u51648  \u47803 \u54616 \u47732 , \u48736 \u47476 \u44172  \u47680 \u50612 \u51648 \u45716  \u49457 \u51656 \u46020  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51060 \u47484  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u44552 \u49324 \u48736 , \u44552 \u49324 \u49885  \u49457 \u54693 \u51012  \u44032 \u51652  \u49324 \u46988  : \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u49373 \u44592 \u47732  \u44553 \u54616 \u44172  \u46308 \u51060 \u45824 \u44256 , \u48736 \u47476 \u44172  \u44536  \u44048 \u51221 \u51060  \u49885 \u51020 \u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732  : \u46168  \u51473  \u45572 \u44400 \u44032 \u45716  \u45320 \u47924  \u49457 \u44553 \u54616 \u44172  \u46308 \u51060 \u45824 \u44256  \u51080 \u45796 \u45716  \u44221 \u44256 \u44288 \u44228 \u47484  \u48736 \u47476 \u44172  \u50756 \u44208  \u51667 \u44256  \u49910 \u50612  \u54632  : \u54840 \u44048 \u51060  \u51080 \u51004 \u47732  \u48744 \u47532  \u49324 \u44480 \u44256  \u49910 \u50612 \u54616 \u47728 , \u50500 \u45768 \u45796  \u49910 \u51004 \u47732  \u48736 \u47476 \u44172  \u46496 \u45208 \u47140 \u44256  \u54632 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u48169 \u54693 \u49457 \u46020  \u51339 \u44256  \u51456 \u48708 \u46020  \u52384 \u51200 \u54616 \u45208 , \u45320 \u47924  \u49457 \u44553 \u54644 \u49436  \u51068 \u51012  \u44536 \u47476 \u52832  \u49688  \u51080 \u45796 \u45716  \u47700 \u49884 \u51648 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50756 \u46300 \u45716  \'91\u45208 \u46976 \u55176  \u51221 \u47148 \u46108  \u49345 \u53468 \'92\u51077 \u45768 \u45796 .\u54644 \u49437  2. \u46384 \u46972 \u44032 \u45716  \u45936  \u50612 \u47140 \u50880 \u51012  \u44202 \u45716 \u50756 \u46300 8\u51008  \u46384 \u46972 \u44032 \u45716  \u45936  \u50612 \u47140 \u50880 \u51012  \u44202 \u51012  \u49688  \u51080 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u50756 \u46300 \u45716  \u52509  8\u44060 , \u54616 \u45720 \u50640 \u49436  \u46504 \u50612 \u51256  \u51068 \u51228 \u55176  \u48736 \u47480  \u49549 \u46020 \u47196  \u51060 \u46041 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u48120 \u47532  \u51456 \u48708 \u54616 \u51648  \u50506 \u50520 \u45796 \u47732 , \u44536  \u49549 \u46020 \u47484  \u46384 \u46972 \u51105 \u44592 \u45716  \u55192 \u46308 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u45817 \u49888 \u51060  \u44288 \u47532 \u54616 \u44592 \u50640 \u45716  \u45320 \u47924  \u48736 \u47476 \u44172  \u51652 \u54665 \u46104 \u45716  \u51068 \u46308 \u49345 \u45824 \u48169 \u51032  \u50672 \u50528  \u49549 \u46020 \u47484  \u46384 \u46972 \u44032 \u44592  \u55192 \u46304  \u49345 \u54889 \u50629 \u47924  \u54872 \u44221 \u51060  \u45320 \u47924  \u48736 \u47476 \u44172  \u48320 \u54644 \u49436  \u51201 \u51025 \u54616 \u44592  \u55192 \u46310 \u51068 \u51060  \u45320 \u47924  \u48744 \u47532  \u51652 \u54665 \u46104 \u50612 , \u45817 \u48516 \u44036  \u54588 \u47196 \u54624  \u49688  \u51080 \u45716  \u49345 \u54889 \u54644 \u49437  3. \u44984 \u51456 \u54632 \u51060  \u54596 \u50836 \u54620 \u50756 \u46300 8\u51008  \u47196 \u53011 \u51077 \u45768 \u45796 . \u47196 \u53011 \u51008  \u49549 \u46020 \u44032  \u48736 \u47476 \u44256  \u51221 \u54869 \u49457 \u46020  \u45458 \u51648 \u47564 , \u50672 \u47308 \u44032  \u50630 \u45796 \u47732  \u45208 \u50500 \u44040  \u49688  \u50630 \u49845 \u45768 \u45796 .\u51060 \u45716  \u51593 , \u52628 \u51652 \u47141 \u46020  \u49464 \u44256  \u50676 \u51221 \u46020  \u45336 \u52824 \u45208 , \u51060 \u47484  \u50976 \u51648 \u54616 \u45716  \u44172  \u44288 \u44148 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44263  \u45817 \u49888 \u51060  \u44592 \u45796 \u47532 \u45912  \u44208 \u44284 \u44032  \u50732  \u44163 \u51077 \u45768 \u45796 . \u51473 \u50836 \u54620  \u44163 \u51008  \u44984 \u51456 \u54632 \u51077 \u45768 \u45796 . \u52488 \u49900 \u51012  \u50976 \u51648 \u54616 \u49464 \u50836 . \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51060 \u47732  \u50896 \u54616 \u45716  \u44208 \u44284 \u50640  \u46020 \u45804 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 8 \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300 8\u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u51312 \u44553 \u54616 \u47728  \u45796 \u49548  \u45224 \u49457 \u51201 \u51064  \u49457 \u54693 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51060  \u50896 \u54616 \u45716  \u45824 \u49345 , \u47785 \u54364 , \u49324 \u46988 \u51060  \u51080 \u51004 \u47732  \u51200 \u46028 \u51201 \u51004 \u47196  \u52628 \u51652 \u54620 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u47560 \u52824  \'91\u49324 \u45285 \'92\u54616 \u45716  \u46319 \u54620  \u45712 \u45196 \u51012  \u54413 \u44609 \u45768 \u45796 . \u51032 \u49324 \u49548 \u53685  \u45733 \u47141 \u51060  \u46832 \u50612 \u45208 \u47728 , \u44552 \u49324 \u48736  \u49457 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u48128 \u45817 \u51060  \u49884 \u44036 \u45229 \u48708 \u46972 \u44256  \u49373 \u44033 \u54616 \u45716  \u47564 \u53372 , \u51665 \u52265 \u54616 \u45716  \u49457 \u54693 \u46020  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51064  \u55120 \u47492 \u50640  \u51080 \u51012  \u44221 \u50864 , \u52395  \u45576 \u50640  \u48152 \u54616 \u44144 \u45208 , \u46907 \u48150 \u51032  \u49324 \u46993 \u51060  \u49884 \u51089 \u46112  \u44163 \u51076 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u50756 \u46300 8\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  \u46160  \u44032 \u51648 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45796 \u47480  \u51068 \u50640  \u51665 \u51473 \u54616 \u44256  \u51080 \u50612  \u48169 \u54644 \u48155 \u44592  \u49899 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u44536  \u51068 \u51012  \u50724 \u47004  \u44592 \u44036  \u51456 \u48708 \u54644 \u50728  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u45796 \u47480  \u45572 \u44400 \u44032  \u46412 \u47928 \u50640  \u51068 \u51060  \u53952 \u50612 \u51648 \u45716  \u49345 \u54889 \u51012  \u47561 \u44256 \u51088  \u54633 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u44288 \u44228 \u47484  \u48736 \u47476 \u44172  \u44208 \u47200 \u51667 \u44256  \u49910 \u50612 \u54616 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51339 \u50500 \u54616 \u45716  \u50040  \u49345 \u45824 \u50752 \u45716  \u48744 \u47532  \u50672 \u51064 \u51060  \u46104 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u52395  \u45576 \u50640  \u48152 \u54664 \u45796 \u45716  \u46907 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u54756 \u50612 \u51652  \u49345 \u45824 \u48169 \u44284 \u45716  \u48736 \u47476 \u44172  \u51116 \u54924 \u54616 \u44144 \u45208  \u48736 \u47476 \u44172  \u51082 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u50584 \u44592 \u47484  \u47566 \u51060  \u45208 \u45572 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u44288 \u44228 \u51032  \u51652 \u51204  \u49549 \u46020 \u44032  \u48736 \u47476 \u45768 , \u44553 \u44201 \u55176  \u52828 \u54644 \u51648 \u44144 \u45208  \u49828 \u53416 \u49901  \u51652 \u46020 \u44032  \u44553 \u44201 \u55176  \u48744 \u46972 \u51656  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47196  \u51064 \u54644  \u46168  \u51473  \u45572 \u44400 \u44032 \u45716  \u44288 \u44228 \u50640  \u48512 \u45812 \u44048 \u51012  \u45712 \u45180 \u45716  \u48512 \u51089 \u50857 \u51060  \u51068 \u50612 \u45208 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51665 \u52265 \u51012  \u51312 \u49900 \u54644 \u50556  \u54616 \u47728 , \u50672 \u46973 \u51012  \u44053 \u50836 \u54616 \u51648  \u50506 \u45716  \u44163 \u51060  \u51473 \u50836 \u54633 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u50756 \u46300 8\u51008  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . 8\u44060 \u51032  \u50756 \u46300 \u45716  \u49345 \u54889 \u51032  \u48320 \u54868 , \u45572 \u44400 \u44032 \u50640 \u44172  \u47700 \u49884 \u51648 \u47484  \u48155 \u45716 \u45796 \u45716  \u51032 \u48120 \u44032  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u47484  \u48736 \u47476 \u44172  \u49884 \u46020 \u54616 \u44600  \u50896 \u54616 \u47728 , \u50500 \u45768 \u45796  \u49910 \u51004 \u47732  \u44288 \u44228 \u47484  \u48736 \u47476 \u44172  \u50756 \u44208 \u45244  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51312 \u47564 \u44036  \u50672 \u46973 \u51012  \u48155 \u44172 \u46112  \u50868 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51116 \u54924 \u50640 \u49436  \u48512 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54624  \u44221 \u50864 \u50640 \u45716  \u45817 \u49888 \u51012  \u51204 \u54784  \u49888 \u44221  \u50416 \u51648  \u50506 \u44256  \u45796 \u47480  \u47924 \u50616 \u44032 \u50640  \u47792 \u46160 \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u50756 \u46300 8 \u51649 \u50629 \u50868  \u54644 \u49437 \u51312 \u47564 \u44036  \u51060 \u51649 \u51012  \u54616 \u44172  \u46104 \u44144 \u45208 , \u48512 \u49436 \u47484  \u51060 \u46041 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46412 \u47928 \u50640  \u50629 \u47924  \u54872 \u44221 \u51060  \u44553 \u48320 \u54616 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u50504 \u49900 \u54616 \u49464 \u50836 . \u44536  \u48320 \u54868 \u45716  \u45817 \u49888 \u50640 \u44172  \u44557 \u51221 \u51201 \u51064  \u50640 \u45320 \u51648 \u47484  \u49440 \u47932 \u54644 \u51460  \u44163 \u51077 \u45768 \u45796 . \u46608 \u54620  \u44536 \u46041 \u50504  \u45817 \u49888 \u51060  \u51456 \u48708 \u54644 \u50772 \u45912  \u44163 (\u50696 \u47484  \u46308 \u50612  \u49828 \u54169 , \u54532 \u47196 \u51229 \u53944 , \u49884 \u54744 )\u51060  \u48736 \u47476 \u44172  \u50756 \u49457 \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u50724 \u47004  \u44592 \u44036  \u48731 \u51012  \u48156 \u54616 \u51648  \u47803 \u54664 \u45912  \u51068 \u51060  \u44208 \u44397  \u49324 \u46988 \u46308 \u50640 \u44172  \u51064 \u51221 \u48155 \u44172  \u46121 \u45768 \u45796 . \u50756 \u46300 \u44032  \u46405 \u50640  \u44866 \u55176 \u46319 , \u44611 \u48156 \u51012  \u44866 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u50756 \u46300 8\u51032  \u51649 \u50629  \u51312 \u50616  \'96 SibylTarot\u50756 \u46300 8 \u44552 \u51204 \u50868  \u54644 \u49437 \u51116 \u51221 \u51201 \u51064  \u49345 \u54889 \u51060  \u48736 \u47476 \u44172  \u48320 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49324 \u50629 \u51060 \u45208  \u53804 \u51088 \u47484  \u54616 \u44256  \u51080 \u45796 \u47732 , \u45800 \u44592 \u44036  \u45236 \u50640  \u49457 \u44284 \u47484  \u48380  \u50696 \u51221 \u51077 \u45768 \u45796 . \u51060 \u45716  \u45817 \u49888 \u51032  \u50696 \u49345 \u48372 \u45796  \u45908  \u48736 \u47484  \u44163 \u51077 \u45768 \u45796 . \u54788 \u51116  \u46024 \u51004 \u47196  \u47924 \u50616 \u44032 \u47484  \u54616 \u44256  \u51080 \u51648  \u50506 \u45908 \u46972 \u46020 , \u51312 \u47564 \u44036  \u45817 \u49888 \u50640 \u44172  \'91\u44592 \u54924 \'92\u44032  \u50728 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46384 \u46972 \u49436  \u54217 \u49548 \u50640  \u50612 \u45712  \u51221 \u46020  \u54788 \u44552 \u51012  \u44032 \u51648 \u44256  \u51080 \u45716  \u44172  \u51339 \u51004 \u47728 , \u53804 \u51088  \u44592 \u54924 \u50640  \u48156  \u48736 \u47476 \u44172  \u45824 \u52376 \u54644 \u50556  \u54620 \u45796 \u45716  \u51312 \u50616 \u51216 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .    \
\
   \uc0\u50756 \u46300 9 \'96 \u44221 \u44228  \u50756 \u46300 9 \u44536 \u47548 \u51032  \u51032 \u48120 \u47924 \u50616 \u44032 \u47484  \u44221 \u44228 \u54616 \u45716  \u46319 \u54620  \u47784 \u49845 \u54616 \u44256  \u49910 \u51008  \u51068 \u51060  \u51080 \u51004 \u45208 , \u48169 \u54644 \u47932  \u46412 \u47928 \u50640  \u47803 \u54616 \u45716  \u49345 \u54889 \u47800 \u44284  \u47560 \u51020 \u51060  \u51648 \u52828  \u49345 \u53468 , \u44536 \u47084 \u45208  \u54252 \u44592 \u54616 \u51648 \u45716  \u50506 \u51020 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u47800 \u44284  \u47560 \u51020 \u51060  \u51648 \u52828 \u50756 \u46300 9\u45716  \u47800 \u44284  \u47560 \u51020 \u51060  \u51648 \u52828  \u49324 \u46988 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u51060 \u48120 \u51648  : \u52852 \u46300  \u49549  \u45224 \u51088 \u51032  \u47672 \u47532 \u50752  \u54036 \u50640 \u45716  \u48533 \u45824 \u44032  \u44048 \u44200 \u51080 \u49845 \u45768 \u45796 . \u44284 \u44144 \u50640  \u51080 \u50632 \u45912  \u49912 \u50880 \u51004 \u47196  \u51064 \u54644  \u45796 \u52452 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45224 \u51088 \u45716  \u50756 \u46300  \u54616 \u45208 \u47484  \u47785 \u48156 \u52376 \u47100  \u49324 \u50857 \u54644  \u47800 \u51012  \u44592 \u45824 \u44256  \u51080 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796  :\u49888 \u52404 \u44032  \u45796 \u52452 \u45796 \u45716  \u44163 \u51008  : \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u51077 \u50632 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \'91\u44256 \u53685 \'92\u48372 \u45796 \u45716  \'91\u51648 \u52452 \u45796 \'92\u45716  \u54364 \u54788 \u51060  \u50612 \u50872 \u47549 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u51032  \u51088 \u49464 \u50752  \u54364 \u51221 \u51012  \u48372 \u47732  \u50508  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 \u47196  \u48169 \u50612 \u49440 \u51012  \u44396 \u52629 \u54620  \u49345 \u53468  : \u47560 \u51020 \u51012  \u51648 \u52824 \u44172  \u54620  \u49324 \u44148 \u51008  \u44900  \u44284 \u44144 , \u44536 \u44163 \u46020  \u51648 \u49549 \u51201 \u51004 \u47196  \u51060 \u50612 \u51276 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u51333 \u54633 \u54644 \u48372 \u47732  : \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008  \'91\u44284 \u44144 \u50640  \u51080 \u50632 \u45912  \u51068 \u47144 \u51032  \u49324 \u44148 \'92\u51004 \u47196  \u51064 \u54644 , \u47560 \u51020 \u51060  \u51648 \u52828  \u49345 \u53468 \u51077 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u50696 \u47484  \u46308 \u50612 \u49436  :\u51649 \u51109  \u49345 \u49324 \u44032  \u44536 \u47484  \u44340 \u47213 \u55176 \u45716  \u49345 \u54889 \u51060 \u45796 . \u44536 \u47084 \u45208 , \u44536 \u45716  \u54924 \u49324 \u47484  \u46496 \u45208 \u51648  \u50506 \u45716 \u45796 . \u47924 \u50616 \u44032  \u51648 \u53020 \u50556  \u54624  \u45824 \u49345 \u51060 \u45208  \u49888 \u45392 \u51060  \u51080 \u44592  \u46412 \u47928 \u51060 \u45796 . \u44536 \u45716  \u51648 \u44552  \u47800 \u44284  \u47560 \u51020 \u51060  \u51648 \u52828  \u49345 \u53468 \u45796 .\u44228 \u49549 \u46104 \u45716  \u44048 \u51221  \u49912 \u50880 \u50640  \u51648 \u52828  \u47784 \u49845 \u51060 \u45796 . \u44536 \u45716  \u50672 \u51064 \u44284  \u54756 \u50612 \u51648 \u44256  \u49910 \u51648  \u50506 \u45796 . \u49345 \u45824 \u48169 \u51060  \u47676 \u51200  \u49324 \u44284 \u54644 \u51500 \u51004 \u47732  \u54620 \u45796 . \u51648 \u44552  \u44536 \u45716  \u44228 \u49549 \u46104 \u45716  \u44048 \u51221  \u49912 \u50880 \u50640  \u51648 \u52828  \u47784 \u49845 \u51060 \u45796 .\u54644 \u49437  2. \u51109 \u50528 \u47932 \u51060  \u47566 \u51008 \u50756 \u46300 9\u45716  \u47785 \u54364 \u47484  \u49892 \u54788 \u54616 \u44592 \u50640  \u51109 \u50528 \u47932 \u51060  \u47566 \u51008  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u8251  \u51060  \u54644 \u49437 \u51008  \u49324 \u46988 \u46308 \u47560 \u45796  \u48372 \u45716  \u44288 \u51216 \u51060  \u45796 \u47493 \u45768 \u45796 .\u44208 \u47200 \u51008  \u46041 \u51068 \u54616 \u45768 , \u45712 \u45196 \u47564  \u51060 \u54644 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u51200 \u45716  \u44536  \u44288 \u51216 \u51012  \u47784 \u46160  \u50500 \u50864 \u47484  \u49688  \u51080 \u45716  \u54644 \u49437 \u51012  \u51228 \u44277 \u54616 \u44192 \u49845 \u45768 \u45796 .\u45224 \u51088 \u44032  \u51088 \u49888 \u51012  \u48372 \u54840 \u54616 \u44592  \u50948 \u54644  \u50756 \u46300 \u48317 \u51012  \u49464 \u50880  : \u47924 \u50616 \u44032 \u47196 \u48512 \u53552  \u51088 \u49888 \u51012  \u48372 \u54840 \u54616 \u44592  \u50948 \u54644  \u48169 \u50612 \u49440 \u51012  \u44396 \u52629 \u54620  \u49345 \u53468 \u51077 \u45768 \u45796 .\u45224 \u51088 \u44032  \u49464 \u50868  \u50756 \u46300 \u48317 \u50640  \u45224 \u51088 \u44032  \u49828 \u49828 \u47196 \u44032  \u48169 \u54644  \u48155 \u51020  : \u45224 \u51088 \u44032  \u47924 \u50616 \u44032 \u47196 \u48512 \u53552  \u51088 \u49888 \u51012  \u48169 \u50612 \u54616 \u44592  \u50948 \u54644  \u48317 \u51012  \u49464 \u50912 \u51004 \u45208 , \u44536  \u48317  \u46412 \u47928 \u50640  \u48376 \u51064  \u49828 \u49828 \u47196 \u44032  \u48156 \u47785  \u51105 \u55176 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u45572 \u44400 \u44032 \u44032  \u49464 \u50892  \u45459 \u51008  \u50756 \u46300 \u48317  \u46412 \u47928 \u50640  \u45224 \u51088 \u44032  \u48169 \u54644  \u48155 \u51020  : \u45572 \u44400 \u44032 \u44032  \u45224 \u51088 \u51032  \u51452 \u48320 \u50640  \u50756 \u46300 \u48317 \u51012  \u49464 \u50892 \u49436 , \u45224 \u51088 \u44032  \u50880 \u51649 \u51068  \u49688  \u50630 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u44208 \u47200 \u51008 , \u45817 \u49888 \u44284  \u49345 \u45824 \u48169 (\u54841 \u51008  \u47785 \u54364 \u47196  \u54616 \u45716  \u45824 \u49345 ) \u49324 \u51060 \u50640  \u47924 \u50616 \u44032  \u51109 \u50528 \u47932 \u51060  \u51316 \u51116 \u54620 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u50696 \u49884 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796  :\u51452 \u48320 \u50640  \u51060 \u49457 \u51060  \u47566 \u50500  \u45796 \u44032 \u44032 \u44592  \u55192 \u46304  \u49345 \u54889  : \u48317 \u52376 \u47100  \u49464 \u50892 \u51652  \u49688 \u47566 \u51008  \u50756 \u46300 \u45716 , \u53945 \u51221  \u49324 \u46988  \u51452 \u48320 \u50640  \u51080 \u45716  \u51060 \u49457 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u47560 \u51020 \u51032  \u48317 \u51012  \u49939 \u51008  \u49345 \u54889  : \u44284 \u44144 \u50640  \u51080 \u50632 \u45912  \u49345 \u52376 \u46308 \u47196  \u51064 \u54644 , \u48169 \u50612 \u44592 \u51228 \u44032  \u51080 \u45716  \u49345 \u54889 . \u51593  \u50672 \u50528  \u53944 \u46972 \u50864 \u47560 \u44032  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51088 \u49888 \u51032  \u48148 \u50868 \u45908 \u47532 \u50640  \u46308 \u50612 \u50724 \u45716  \u44163 \u51012  \u51068 \u51333 \u51032  \'91\u44277 \u44201 \'92\u51004 \u47196  \u44036 \u51452 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u48512 \u45812 \u49828 \u47084 \u50892 \u49436  \u52384 \u48317 \u51012  \u52452 \u51004 \u45208  \u54840 \u44048 \u51060  \u45208 \u51473 \u50640  \u49373 \u44596  \u49345 \u54889  : \u54840 \u44048 \u51060  \u46244 \u45734 \u44172  \u49373 \u44220 \u51004 \u45208 , \u52488 \u48152 \u50640  \'91\u52384 \u48317 \'92\u51012  \u52432 \u49436 , \u49345 \u45824 \u48169 \u44284  \u44032 \u44620 \u50892 \u51648 \u44592  \u55192 \u46304  \u49345 \u53468 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51060  \u47564 \u46304  \u48169 \u50612 \u48317 \u50640 , \u48376 \u51064 \u51060  \u48169 \u54644  \u48155 \u44256  \u51080 \u45716  \u49480 \u51060 \u51424 .\u50556 \u49900 \u52264 \u44172  \u50668 \u54665 \u51012  \u44228 \u54925 \u54664 \u51004 \u45208  \u47928 \u51228 \u44032  \u45130 \u51076  \u50630 \u51060  \u49373 \u44608  : \u51664 \u46020  \u47924 \u44161 \u44256 , \u52264 \u46020  \u47561 \u55176 \u44256 , \u52404 \u53356 \u51064 \u46020  \u47928 \u51228 \u44032  \u49373 \u44592 \u47728 , \u49689 \u49548 \u50640  \u46020 \u52265 \u54664 \u51012  \u46416  \u50640 \u50612 \u52968 \u51060  \u44256 \u51109 \u45212  \u49345 \u53468 \'85 \u45130 \u51076  \u50630 \u51060  \u47928 \u51228 \u44032  \u49373 \u44592 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u44221 \u44228 \u49900 9\u50756 \u46300 \u45716  \u44221 \u44228 \u54616 \u45716  \u49900 \u47532 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50756 \u46300 9\u45716  \u45236 \u47732 \u51032  \u53944 \u46972 \u50864 \u47560 \u50752 \u46020  \u44288 \u47144  \u51080 \u49845 \u45768 \u45796 . \u49345 \u52376 \u47196  \u51064 \u54644  \u45796 \u47480  \u49324 \u46988 \u51012  \u48520 \u49888 \u54616 \u44256  \u44221 \u44228 \u54616 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51032 \u49900 \u46020  \u49438 \u50668 \u51080 \u49845 \u45768 \u45796 .\u51060 \u48120 \u51648  : \u45224 \u51088 \u51032  \u53944 \u46972 \u50864 \u47560 , \u44221 \u44228 \u49900 \u51060  \'91\u50756 \u46300 \u48317 \'92\u51004 \u47196  \u54364 \u54788 \u46108  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44172 \u45796 \u44032  \u45224 \u51088 \u51032  \u51088 \u49464 \u47484  \u48372 \u50500 \u54616 \u45768 , \u47800 \u51012  \u50885 \u53356 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47924 \u50616 \u44032 \u47484  \u51648 \u53412 \u47140 \u54616 \u44256 , \u49548 \u44537 \u51201 \u51004 \u47196  \u54665 \u46041 \u54616 \u47140 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 .\u44536 \u47084 \u45208  \u54252 \u44592 \u54616 \u44144 \u45208 , \u50756 \u51204 \u55176  \u46020 \u47581 \u44036  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u54620  \u48156 \u51677  \u47932 \u47084 \u49436 \u49436  \u49345 \u54889 \u51012  \u51648 \u53020 \u48372 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 .\u50672 \u50528 \u47484  \u54616 \u44256  \u51080 \u45796 \u47732  : \u49345 \u52376  \u51077 \u45716  \u51068 \u51060  \u49373 \u44220 \u44144 \u45208 , \u50672 \u51064 \u51012  \u51032 \u49900 \u54616 \u45716  \u47560 \u51020 \u51060  \u46308 \u50632 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50500 \u51649  \u47560 \u51020 \u51032  \u47928 \u51012  \u50756 \u51204 \u55176  \u50676 \u51648  \u47803 \u54664 \u49845 \u45768 \u45796 .\u49345 \u45824 \u48169 \u44284  \u51060 \u50612 \u51648 \u44256  \u49910 \u45796 \u47732  : \u49345 \u45824 \u48169 \u51032  \u51032 \u49900 \u51012  \u54400 \u50612 \u50556  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51339 \u51008  \u49324 \u46988 \u51060 \u46972 \u45716  \u54869 \u49888 \u51012  \u49900 \u50612 \u51452 \u50612 \u50556  \u54616 \u44256 , \u54840 \u44048 \u51060  \u51080 \u45796 \u45716  \u49324 \u49892 \u51012  \u54869 \u49892 \u55176  \u50612 \u54596 \u54644 \u50556  \u54633 \u45768 \u45796 .\u45224 \u51088 \u44032  \u51648 \u53412 \u44256 \u51088  \u54616 \u45716  \u44163 \u51008  \u48376 \u51064 \u51032  \u49888 \u45392 , \u44032 \u52824 \u44288 , \u54665 \u46041 \u44144 \u51648 \u51077 \u45768 \u45796 . \u44536  \u49888 \u45392 \u44284  \u50612 \u44555 \u45208 \u45716  \u49324 \u46988 \u51008  \u47924 \u51312 \u44148  \u44221 \u44228 \u54616 \u44256  \u48372 \u45716  \u49457 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824 \'85\u49696 \u51012  \u51088 \u51452  \u47560 \u49884 \u45716  \u51060 \u49457 \u51008  \u47680 \u47532 \u54644 \u50556  \u54620 \u45796 \u45716  \u49373 \u44033  \u8594  \u49345 \u45824 \u48169 \u51060  \u49696 \u51012  \u47566 \u51060  \u47560 \u49884 \u45716  \u44163  \u44057 \u51004 \u47732 , \u44221 \u44228 \u54616 \u44256  \u48388  \u8594  \u49696 \u51012  \u51339 \u50500 \u54616 \u45716  \u51060 \u49457 \u50640 \u44172  \u45208 \u49244  \u44592 \u50613 \u51060  \u51080 \u44592  \u46412 \u47928 \u51076 \u47928 \u49888  \u51080 \u45716  \u49324 \u46988 \u51008  \u54588 \u54644 \u50556  \u54620 \u45796 \u45716  \u49373 \u44033  \u8594  \u49440 \u51077 \u44204 \u51060  \u44053 \u54644 \u49436  \u49789 \u44172  \u44221 \u44228 \u47484  \u54400 \u51648  \u50506 \u51020 \u51064 \u45236 \u49900 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 . \u50724 \u47004  \u44592 \u44036  \u49345 \u45824 \u48169 \u51012  \u44600 \u46308 \u51060 \u46319  \u45796 \u44032 \u49436 \u50556  \u54633 \u45768 \u45796 . \u47560 \u51020 \u51032  \u48317 \u51012  \u50630 \u50528 \u44592 \u44620 \u51648  \u50724 \u47004  \u49884 \u44036 \u51060  \u44152 \u47540  \u44163 \u51077 \u45768 \u45796 .\u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u45128 \u51656 \u44596  \u49324 \u46988 9\u50756 \u46300 \u45716  \u45128 \u51656 \u44596  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u45224 \u51088 \u45716  \u47751  \u48264 \u51032  \u51204 \u53804 \u47484  \u44202 \u50632 \u51648 \u47564 , \u44536 \u45716  \u45149 \u44620 \u51648  \u44221 \u44228 \u49900 \u51012  \u45734 \u52628 \u51648  \u50506 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45908  \u49912 \u50864 \u47140 \u45716  \u51032 \u51648 \u46020  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45224 \u51088 \u51032  \u54408  \u49549  \u50756 \u46300 \u45716  \'91\u48376 \u51064 \u51032  \u54665 \u46041 , \u49373 \u44033 \'92\u51077 \u45768 \u45796 . \u51060 \u47484  \u44048 \u49912  \u45149 \u44620 \u51648  \u51648 \u53412 \u47140 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44557 \u51221 \u51201 \u51004 \u47196  \u47568 \u54616 \u47732 , \'91\u51032 \u51648 \u44032  \u44053 \u54620 \'92 \u44163 \u51060 \u44256 , \u48512 \u51221 \u51201 \u51004 \u47196  \u47568 \u54616 \u47732  \'91\u44256 \u51665 \u51060  \u49468 \'92 \u44163 \u51077 \u45768 \u45796 .\u45796 \u47564 , \u49345 \u45824 \u48169 \u51012  \u44277 \u44201 \u54616 \u50668  \u51060 \u44592 \u47140 \u45716  \u47560 \u51020 \u48372 \u45796 \u45716 , \u49345 \u45824 \u44032  \u54252 \u44592 \u54616 \u44144 \u45208  \u54620  \u49688  \u51217 \u50612 \u51452 \u44592 \u47484  \u44592 \u45796 \u47532 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u45128 \u51656 \u44592 \u44172  \'91\u45720 \u50612 \u51652 \u45796 \'92\u45716  \u54364 \u54788 \u51060  \u47582 \u49845 \u45768 \u45796 .\u50756 \u46300 7 \u52852 \u46300 \u50752  \u55137 \u49324 \u54633 \u45768 \u45796 . \u45796 \u47564  \u50756 \u46300 7\u51060  \u50976 \u47532 \u54620  \u44256 \u51648 \u47484  \u49440 \u51216 \u54620  \u48152 \u47732 , \u50756 \u46300 9\u45716  \u49345 \u45817 \u55176  \u48520 \u47532 \u54620  \u54252 \u51648 \u49496 \u50640  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 7\u50976 \u47532 \u54620  \u54252 \u51648 \u49496 \u50756 \u46300 9\u48520 \u47532 \u54620  \u54252 \u51648 \u49496  \u47532 \u46377 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u54644 \u49437 \u54633 \u45768 \u45796  :\u51221 \u47568  \u48512 \u45812 \u49828 \u47084 \u50868  \u49345 \u45824 \u44032  \u51080 \u51012  \u46412  \u45128 \u51656 \u44592 \u44172  \u52384 \u48317 \u51012  \u52840 \u51116 \u54924 \u50640 \u49436 \u45716 , \u45128 \u51656 \u44592 \u44172  \u44592 \u45796 \u47532 \u45716  \u49345 \u54889  OR \u45128 \u51656 \u44592 \u44172  \u51116 \u54924 \u47484  \u49884 \u46020 \u50756 \u46300 9\u45716  \'91\u54252 \u44592 \u54616 \u51648  \u50506 \u44256  \u45720 \u50612 \u51652 \u45796 \'92\u45716  \u51032 \u48120 \u47484  \u44054 \u44256  \u51080 \u51004 \u48064 \u47196 , \u51649 \u50629 \u50868 \u50640 \u49436  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u50756 \u46300 9 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u45128 \u51656 \u44592 \u47728 , \u44221 \u44228 \u49900 \u51060  \u47566 \u51008  \u49457 \u54693 \u51077 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \u50672 \u50528 \u50868 \u50640  \u45824 \u54644  \u51656 \u47928 \u54664 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u50672 \u50528 \u50640  \u45824 \u54644  \'91\u48512 \u51221 \u51201 \u51064  \u47560 \u51020 \'92\u51012  \u44032 \u51648 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u51032  \u50672 \u50528  \u49828 \u53440 \u51068 \u51060  \u54217 \u48276 \u54616 \u51648 \u45716  \u50506 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u44221 \u44228 \u49900 \u51060  \u44053 \u54616 \u50668 , \u49352 \u47196 \u50868  \u49324 \u46988 \u51060  \u51452 \u48320 \u50640  \u46308 \u50612 \u50724 \u45716  \u44163 \u51012  \u44732 \u47140 \u54633 \u45768 \u45796 . \u49440 \u51077 \u44204 \u51060  \u51080 \u45716  \u54200 \u51060 \u50612 \u49436  \u50500 \u47924 \u45208  \u47564 \u45208 \u51648 \u46020  \u50506 \u49845 \u45768 \u45796 . \u45796 \u47564  \u48376 \u51064 \u51060  \u47560 \u51020 \u50640  \u46300 \u45716  \u49345 \u45824 \u44032  \u51080 \u45796 \u47732 , \u47924 \u49832  \u51068 \u51060  \u51080 \u45908 \u46972 \u46020  \u45149 \u44620 \u51648  \u51339 \u50500 \u54624  \u44163 \u51077 \u45768 \u45796 . \u51088 \u52843  \'91\u51665 \u52265 \u54624  \u49688 \u46020 \'92 \u51080 \u44192 \u49845 \u45768 \u45796 .\u50756 \u46300 9\u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  \u46160  \u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u51060  \u51080 \u51004 \u45208 , \u51452 \u48320 \u50640  \u51109 \u50528 \u47932 \u51060  \u47566 \u50500  \u51217 \u44540 \u54616 \u44592  \u55192 \u46308 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u45817 \u49888  \u51452 \u48320 \u50640  \u51060 \u49457 \u51060  \u47566 \u50500  \u49789 \u49324 \u47532  \u51217 \u44540 \u54616 \u44592  \u55192 \u46308 \u45796 \u44256  \u45712 \u45180 \u45716 \u44400 \u50836 . \u54841 \u51008  \u45817 \u49888 \u51060  \u51008 \u50672 \u51473 \u50640  \'91\u52384 \u48317 \u51012  \u52824 \u45716  \u44163 \'92 \u44057 \u45796 \u44256  \u45712 \u44808 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u52384 \u48317 \u51012  \u52824 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u48512 \u45812 \u49828 \u47084 \u50892 \u54616 \u44144 \u45208 , \u44221 \u44228 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  100% \u47924 \u44288 \u49900 \u51008  \u50500 \u45768 \u46972 \u44256  \u48372 \u51077 \u45768 \u45796 . \u45224 \u51088 \u51032  \u49884 \u49440 \u51008  \u45130 \u51076 \u50630 \u51060  \u50756 \u46300 \u48317 \u51012  \u54693 \u54616 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u51032  \u44221 \u44228 \u49900 \u51012  \u54400 \u44592 \u44620 \u51648 \u45716  \u50724 \u47004  \u44592 \u44036 \u51060  \u44152 \u47540  \u44163 \u51060 \u47728 , \u51064 \u45236 \u49900 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u47568 \u50432 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49548 \u53685 \u50640  \u47928 \u51228 \u44032  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u44284  \u50672 \u51064  \u49324 \u51060 \u50640  \'91\u53360  \u48317 \'92\u51060  \u51316 \u51116 \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u44228 \u49549 \u46104 \u45716  \u47560 \u52272 \u50640  \u47560 \u51020 \u51060  \u51648 \u52828  \u49345 \u53468 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49828 \u53416 \u49901 \u51012  \u49884 \u46020 \u54616 \u51648 \u47564  \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \u44144 \u48512 \u54616 \u44256  \u51080 \u44144 \u45208 , \u52264 \u44032 \u50868  \u53468 \u46020 \u47196  \u49345 \u52376 \u47484  \u51077 \u55176 \u45716  \u49885 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u49345 \u45824 \u48169 \u51008  \u54252 \u44592 \u54616 \u51648  \u50506 \u44256 , \u44060 \u51064 \u51201 \u51004 \u47196  \u47566 \u51008  \u45432 \u47141 \u51012  \u44592 \u50872 \u51060 \u45716  \u46319  \u48372 \u51077 \u45768 \u45796 . \u51648 \u44552  \u44032 \u51109  \u54596 \u50836 \u54620  \u44163 \u51008  \u50948 \u47196 \u50752  \u51025 \u50896 , \u44536 \u47532 \u44256  \u44596  \u45824 \u54868 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  9\u50756 \u46300 \u45716 , \u48512 \u51221 \u44284  \u44557 \u51221 \u51201  \u54644 \u49437 \u51060  \u47784 \u46160  \u44032 \u45733 \u54620  \u52852 \u46300 \u51077 \u45768 \u45796 . \u47676 \u51200  \u44557 \u51221 \u51201  \u54644 \u49437 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u51077 \u45908 \u46972 \u46020 , \u51116 \u54924 \u47484  \u45130 \u51076 \u50630 \u51060  \u49884 \u46020 \u54624  \u44163 \u51077 \u45768 \u45796 . \u44900  \u50724 \u47004  \u44592 \u44036 \u51077 \u45768 \u45796 . \u48512 \u51221 \u51201  \u54644 \u49437 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u50672 \u46973 \u51012  \u47676 \u51200  \u49884 \u46020 \u54620 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \'91\u48317 \u51012  \u52824 \u47728 \'92 \u51116 \u54924 \u47484  \u45149 \u44620 \u51648  \u44144 \u48512 \u54624  \u44163 \u51077 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644 ,\u45817 \u49888 \u51060  \u50672 \u46973 \u51012  \u49884 \u46020 \u54616 \u45716  \u51077 \u51109  \u8594  \u48512 \u51221 \u51201 \u50780  \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u47484  \u50896 \u52824  \u50506 \u45716 \u44032 ? \u8594  \u51060 \u48120  \u45320 \u47924  \u47566 \u51008  \u49345 \u52376 \u47484  \u51077 \u50632 \u44256 , \u50500 \u51649  \u44221 \u44228 \u49900 \u51012  \u54400 \u51648  \u47803 \u54664 \u44592  \u46412 \u47928 \u49345 \u45824 \u48169 \u51060  \u50672 \u46973 \u51012  \u49884 \u46020 \u54616 \u45716  \u51077 \u51109  \u8594  \u44900  \u50724 \u47004  \u44592 \u44036  \u51116 \u54924 \u47484  \u50684 \u46160 \u50640  \u46176 . \u49345 \u52376 \u47484  \u48155 \u45908 \u46972 \u46020  \u44845  \u51116 \u54924 \u54616 \u44256  \u49910 \u50612  \u54632 .\u51060 \u52376 \u47100  \u48372 \u45716  \u44288 \u51216 \u50640  \u46384 \u46972  \u51221 \u48152 \u45824 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u51004 \u45768 , \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51012  \u54632 \u44760  \u48372 \u49492 \u50556  \u54633 \u45768 \u45796 . \u50756 \u46300 9 \u51649 \u50629 \u50868  \u54644 \u49437 9\u50756 \u46300 \u45716  \u54252 \u44592 \u54616 \u44256  \u49910 \u51012  \u46412  \u45208 \u53440 \u45208 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51008  \u50724 \u47004  \u44592 \u44036  \u45432 \u47141 \u54644 \u50772 \u51648 \u47564 , \u45130 \u51076  \u50630 \u51060  \u47928 \u51228 \u44032  \u49373 \u44592 \u45716  \u53459 \u50640 , \u47800 \u46020  \u47560 \u51020 \u46020  \u51648 \u52828  \u46319  \u48372 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  Nine of Wands\u45716  \u50756 \u49457 \u44620 \u51648  10% \u48150 \u50640  \u45224 \u51648  \u50506 \u51008  \u49345 \u54889 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u44144 \u51032  \u45796  \u50772 \u49845 \u45768 \u45796 . \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u46972 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u50756 \u46300 9\u51032  \u51649 \u50629 \u50868 \u51008  \u45824 \u52404 \u47196  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u50612 \u46500  \u50612 \u47140 \u50880 \u51060  \u51080 \u45908 \u46972 \u46020 , \u51060 \u47484  \u44048 \u49688 \u54616 \u47728  \u45149 \u44620 \u51648  \u54644 \u45240 \u45796 \u45716  \u51032 \u48120 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49352 \u47196 \u50868  \u51649 \u51109 \u51012  \u44396 \u54616 \u46304 , \u49324 \u50629 \u51012  \u49884 \u46020 \u54616 \u46304 , \u49884 \u54744 \u51012  \u51456 \u48708 \u54616 \u44256  \u51080 \u46304  \u47568 \u51060 \u51424 . \u45817 \u49888 \u51008  \u47751  \u44032 \u51648  \u50612 \u47140 \u50880 \u50640  \u51649 \u47732 \u54616 \u44192 \u51648 \u47564 , \u44208 \u44397 \u50640 \u45716  \u44536  \u47784 \u46160 \u47484  \u51060 \u44200 \u45236 \u47728  \u49849 \u47532 \u54624  \u44163 \u51077 \u45768 \u45796 .\u50756 \u46300 9\u51032  \u51649 \u50629  \u51312 \u50616  \'96 Sibyl Tarot\u50756 \u46300 9 \u44552 \u51204 \u50868  \u54644 \u49437 \u45817 \u51109 \u51008  \u50612 \u47140 \u50868  \u49345 \u54889 \u50640  \u51649 \u47732 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54616 \u44256  \u49910 \u51008  \u51068 \u51060  \u51080 \u51648 \u47564  (\u49552 \u50640  \u46308 \u44256  \u51080 \u45716  \u50756 \u46300  1\u44060 ), \u49688 \u47566 \u51008  \u51109 \u50528 \u47932 \u50640  \u44032 \u47196 \u47561 \u55180  \u49345 \u53468  (8\u44060 \u51032  \u50756 \u46300 \u47196  \u44396 \u49457 \u46108  \u48317 ) \u51077 \u45768 \u45796 .\u44536 \u47084 \u45208 , \u51452 \u51064 \u44277 \u51008  \u51088 \u54252 \u51088 \u44592  \u54616 \u44144 \u45208 , \u46244 \u47196  \u46104 \u46028 \u50500 \u44032 \u51648  \u50506 \u51004 \u48064 \u47196 \'85 \u44208 \u44397 \u50640  \u50896 \u54616 \u45716  \u44163 \u51012  \u49457 \u52712 \u54616 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u50668 \u54665 \u51012  \u44032 \u44256  \u49910 \u50632 \u45796 \u47732 , \u50612 \u46523 \u44172 \u46304  \u46024 \u51012  \u47784 \u50500  \u50668 \u54665 \u51012  \u44040  \u44163 \u51060 \u44256 \'85 \u49324 \u44256  \u49910 \u50632 \u45912  \u44163 \u51060  \u50632 \u50632 \u45796 \u47732  \u50612 \u46523 \u44172 \u46304  \u49324 \u44172  \u46121 \u45768 \u45796 . \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u49464 \u50836 . \u45817 \u49888 \u51008  \u50732 \u48148 \u47480  \u48169 \u54693 \u51004 \u47196  \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 .    \
\
   \uc0\u50756 \u46300 10 \'96 \u48512 \u45812  \u50756 \u46300 10 \u44536 \u47548 \u51032  \u51032 \u48120 \u50756 \u46300 \u47484  10\u44060 \u45208  \u52313 \u44200 \u44032 \u50556  \u54644 \u49436  \u48512 \u45812 \u49828 \u47084 \u50868  \u47560 \u51020 \u50837 \u49900  \u46412 \u47928 \u50640  \u48260 \u47532 \u51648  \u47803 \u54616 \u45716  \u47784 \u49845 \u51032 \u47924 \u44048  \u46412 \u47928 \u50640 , \u48376 \u51064 \u51060  \u54616 \u45912  \u51068 \u51012  \u45459 \u51648  \u47803 \u54616 \u45716  \u47784 \u49845 \u47924 \u50616 \u51032  \u50517 \u48149 \u50640  \u49884 \u45804 \u47532 \u45716  \u49345 \u53468 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u52293 \u51076 \u44048 \u51060  \u46832 \u50612 \u45212  \u49324 \u46988 \u50756 \u46300 10\u51008  \u52293 \u51076 \u44048 \u51060  \u46832 \u50612 \u45208 \u47728 , \u51088 \u44592  \u55148 \u49373  \u51221 \u49888 \u51060  \u51080 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u45733 \u47141  \u48150 \u51032  \u47924 \u44144 \u50868  \u50756 \u46300 \u47484  \u50613 \u51648 \u47196  \u45132 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u55192 \u44200 \u50868  \u51068 \u46020  \u47560 \u45796 \u54616 \u51648  \u50506 \u44256 , \u45149 \u44620 \u51648  \u51648 \u49549 \u54620 \u45796 \u45716  \u51032 \u48120 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u52293 \u51076 \u44048 \u51012  \u45712 \u45180 \u45716  \u44288 \u44228 \u50640 \u49436  \u51088 \u51452  \u46321 \u51109 . \u52964 \u54540 \u51068  \u44221 \u50864  \u49345 \u45824 \u48169 \u51060  \u51652 \u51648 \u54616 \u44172  \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u44288 \u44228 \u47484  \u44032 \u48333 \u44172  \u50668 \u44592 \u51648  \u50506 \u45716  \u46319 \u54632 . \u44845  \u44032 \u51256 \u44032 \u50556 \u47564  \u54616 \u45716  \u51064 \u50672 \u51004 \u47196  \u54644 \u49437 \u44288 \u44228 \u51032  \u49457 \u44277  \u50668 \u48512 \u44032  \u48376 \u51064 \u50640 \u44172  \u45804 \u47140  \u51080 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u46319 \u54632 . \u44060 \u51064 \u51201 \u51004 \u47196  \u47566 \u51008  \u45432 \u47141 \u51012  \u54616 \u44256  \u51080 \u51020 10\u50756 \u46300 \u45716  \u45572 \u44400 \u44032 \u47484  \u52293 \u51076 \u51648 \u47140 \u45716  \u49324 \u46988 , \u54620  \u44032 \u51221 \u51032  \u44032 \u51109 , \u54924 \u49324 \u51032  \u52293 \u51076 \u51088 \u50640 \u44172 \u49436  \u47566 \u51060  \u45208 \u50724 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45224 \u46308 \u50640 \u44172  \u54224 \u45180 \u52824 \u51648  \u50506 \u44256 , \u48376 \u51064 \u51060  \u47784 \u46160  \u46496 \u47585 \u45716 \u45796 \u45716  \u51216 \u50640 \u49436  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u44208 \u54844  \u54644 \u46020  \u51339 \u51012  \u49324 \u46988 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 \u47564 , \u49828 \u53944 \u47112 \u49828 \u45208  \u48512 \u45812 \u44048 \u51060  \u49900 \u54624  \u49688  \u51080 \u51004 \u45768  \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u46020 \u50880 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48512 \u45812 \u49828 \u47084 \u50868  \u47560 \u51020 \u50756 \u46300 10\u51008  \u50672 \u46973 \u51060  \u48512 \u45812 \u49828 \u47101 \u44144 \u45208 , \u54596 \u50836  \u51060 \u49345 \u51004 \u47196  \'91\u48372 \u49345 , \u48372 \u45813 , \u54840 \u51032 \'92\u47484  \u48155 \u44172  \u46104 \u50612  \u48512 \u45812 \u49828 \u47084 \u50868  \u47560 \u51020 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u49345 \u45824 \u48169 \u51060  \u51648 \u45208 \u52828  \u54840 \u51032 \u47484  \u48372 \u50668 \u49436  \u48512 \u45812 \u49828 \u47084 \u50880 \u49345 \u45824 \u48169 \u51032  \u50672 \u46973 , \u54840 \u51032 , \u49440 \u47932 \u51012  \u44144 \u51208 \u54616 \u44592 \u46020  \u50612 \u47140 \u50868  \u49345 \u54889 \u54840 \u44048  \u54364 \u54788 \u51012  \u51088 \u51228  \u54644 \u51500 \u51004 \u47732  \u54616 \u45716  \u48148 \u46988 \u50612 \u46500  \u48152 \u51025 \u51012  \u48372 \u50668 \u50556  \u54624 \u51648  \u45212 \u44048 \u54632 \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u51088 \u49888 \u51032  \u47800 \u51665 \u47564 \u54620  \u50756 \u46300  10\u44060 \u47484  \u54616 \u45208 \u46020  \u48260 \u47532 \u51648  \u50506 \u44256 , \u47784 \u46160  \u52313 \u44200 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u47560 \u52824  \u50612 \u47536  \u50500 \u51060 \u44032  \u49440 \u47932 \u51012  10\u48149 \u49828 \u45208  \u48155 \u50500 \u49436 , \u55192 \u44217 \u44172  \u51665 \u50640  \u44032 \u51256 \u44032 \u45716  \u47784 \u49845 \u44284  \u48708 \u49847 \u54633 \u45768 \u45796 .\u50500 \u47560  \u52376 \u51020 \u50640 \u45716  \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 , \u54841 \u51008  \u48372 \u45813 \u51060  \u48512 \u45812 \u49828 \u47101 \u51648  \u50506 \u50520 \u51012  \u44163 \u51077 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732 , \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50756 \u46300 \u47484  \u45800  1\u44060 \u46020  \u48260 \u47532 \u51648  \u50506 \u50520 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u45796 \u47564 , \u50672 \u46973 \u51032  \u48712 \u46020 \u44032  \u52572 \u44540  \u46308 \u50612  \u45320 \u47924  \u51110 \u50500 \u51276 \u44144 \u45208 , \u48372 \u45813 \u51060  \u51648 \u45208 \u52824 \u44172  \u45720 \u50612 \u45208 \u45716  \u46321 \'85 \u50612 \u45712  \u49692 \u44036 \u48512 \u53552  \u48512 \u45812 \u51060  \u46104 \u50612 , \u51648 \u44552 \u51008  \u55192 \u44200 \u50892 \u54616 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u51648 \u44552  \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u51068 \u51333 \u51032  \'91\u51664 \'92\u51060 \u46972 \u44256  \u49373 \u44033 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48260 \u47540  \u49688 \u46020  \u50630 \u44256 , \u52313 \u44200 \u44032 \u44592 \u50640 \u45716  \u45320 \u47924  \u47924 \u44144 \u50868 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020  \u47560 \u52268 \u44032 \u51648 \u47196 , \u45320 \u47924  \u47566 \u51008  \u52293 \u51076 \u51012  \u46496 \u47585 \u44144 \u45208 , \u51649 \u51109  \u45236  \u44284 \u46020 \u54620  \u44592 \u45824 \u47196  \u51064 \u54644  \u48512 \u45812 \u49828 \u47084 \u50868  \u49900 \u47532 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 .\u54644 \u49437  2. \u54624  \u51068 \u51060  \u47566 \u51008 \u50756 \u46300 10\u51008  \u54644 \u50556  \u54624  \u51068 \u51060 \u45208 , \u49888 \u44221  \u50024 \u50556  \u54624  \u51068 \u51060  \u47566 \u51008  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u47568  \u44536 \u45824 \u47196  \u51068 \u51060 \u45208  \u54617 \u50629 \u51060  \u45320 \u47924  \u48148 \u48736 \u49436 , \u50672 \u50528 \u47484  \u49888 \u44221  \u50416 \u44592  \u55192 \u46304  \u49345 \u54889 \u45817 \u49888 \u44284  \u47564 \u45216  \u46412 , \u49888 \u44221  \u50424  \u48512 \u48516 \u51060  \u45320 \u47924  \u47566 \u50500 \u49436  \u55192 \u44200 \u50892 \u54616 \u45716  \u49345 \u54889 \u53440 \u47196 \u50640 \u49436  \u50756 \u46300 \u45716  \'91\u54665 \u46041 \'92\u51012  \u45208 \u53440 \u45236 \u45716  \u49800 \u53944 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u51032  \u50756 \u46300 \u45716  10\u44060 , \u51060 \u45716  \u54644 \u50556  \u54624  \u54665 \u46041 \u51060  10\u44032 \u51648 \u45208  \u51080 \u51020 \u51012  \u47568 \u54644 \u51469 \u45768 \u45796 . (\u54665 \u46041 \u51060  \u47566 \u45796 \u45716  \u48708 \u50976 \u51201  \u54364 \u54788 )\u50696 \u52968 \u45824  \u50672 \u50528 \u44032  2\u44060 , \u51649 \u50629 \u51060  3\u44060 , \u44148 \u44053 \u51060  1\u44060 , \u52712 \u48120 \u44032  4\u44060 \'85 \u49334 \u51012  \u46160 \u47336 \u46160 \u47336  \u49888 \u44221  \u50024 \u50556  \u54616 \u45796 \u48372 \u45768 , \u47560 \u51020 \u51032  \u50668 \u50976 \u46020  \u50630 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u51339 \u50500 \u54644 \u49436 , \u54841 \u51008  \u54644 \u50556 \u47564  \u54644 \u49436  \u49884 \u51089 \u54620  \u54665 \u46041 \u46308 \u51060  \u52264 \u44257 \u52264 \u44257  \u49939 \u50668 , \u51648 \u44552 \u51008  \u48512 \u45812 \u51060  \u46104 \u50632 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50756 \u46300 \u47484  \u54844 \u51088 \u49436  \u50504 \u44256  \u44152 \u50612 \u44049 \u45768 \u45796 . \u51060 \u45716  \u45572 \u44400 \u44032 \u44032  \u54644 \u44208 \u54644 \u51460  \u49688  \u51080 \u45716  \u47928 \u51228 \u44032  \u50500 \u45772 , \u44060 \u51064 \u51201 \u51064  \u47928 \u51228 \u51076 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u46608 \u45716  \u50808 \u47196 \u50868  \u49900 \u47532 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u50837 \u49900  \u46412 \u47928 \u50640  \u47924 \u47532 \u54616 \u45716 \u50756 \u46300 10\u51008  \u50837 \u49900  \u46412 \u47928 \u50640  \u47924 \u47532 \u47484  \u54616 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u45224 \u51088 \u45716  \u50620 \u47560 \u46304 \u51648  \u50756 \u46300 \u47484  \u48260 \u47540  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47926 \u50668 \u51080 \u45716  \u49345 \u53468 \u44032  \u50500 \u45768 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49552 \u50640  \u51080 \u45716  \u50756 \u46300 \u47484  2\u44060 \u47564  \u48260 \u47140 \u46020  \u55016 \u50476  \u49688 \u50900 \u54616 \u44172  \u50880 \u51649 \u51068  \u49688  \u51080 \u51648 \u47564 , \u44536 \u45716  \u50756 \u46300 \u47484  \u45800  \u54620  \u44060 \u46020  \u45459 \u51648  \u50506 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50837 \u49900  \u46412 \u47928 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u50837 \u49900  \u46412 \u47928 \u50640  \u49324 \u50629 \u51012  \u44284 \u46020 \u54616 \u44172  \u54869 \u51109 \u54616 \u45716  \u49345 \u54889 \u45224 \u51032  \u51068 \u44620 \u51648  \u45824 \u49888  \u54644 \u51452 \u50612 , \u44208 \u44397 \u50640 \u45716  \u50629 \u47924 \u47049 \u51060  \u44284 \u46020 \u54644 \u51652  \u49345 \u53468 \u44592 \u45824 \u52824 \u44032  \u45458 \u50500 \u49436 , \u49324 \u49548 \u54620  \u46356 \u53580 \u51068 \u44620 \u51648  \u45796  \u46384 \u51648 \u45716  \u49457 \u54693 \u48376 \u51064 \u51008  \u50756 \u48317 \u54644 \u50556  \u46108 \u45796 \u44256  \u49373 \u44033 \u54644 \u49436 , \u51088 \u49888 \u51012  \u45130 \u51076 \u50630 \u51060  \u52292 \u52237 \u51656 \u54616 \u45716  \u49324 \u46988 \u46384 \u46972 \u49436  Ten of Wands\u45716  \'91\u50612 \u45712  \u51221 \u46020  \u48260 \u47140 \u50556  \u54624  \u51460  \u50508 \u50500 \u50556  \u54620 \u45796 \'92 \'91\u51088 \u49888  \u49104 \u47564  \u50500 \u45768 \u46972 , \u45224 \u46308 \u50640 \u44172 \u46020  \u44288 \u45824 \u54644 \u51656  \u54596 \u50836 \u44032  \u51080 \u45796 \'92\u44256  \u51312 \u50616 \u54633 \u45768 \u45796 .\u54644 \u49437  4. \u50517 \u48149 \u51012  \u48155 \u45716 10\u50756 \u46300 \u45716  \u44536 \u47564 \u46160 \u44256  \u49910 \u51648 \u47564 , \u50517 \u48149  \u46412 \u47928 \u50640  \u52280 \u51004 \u47728  \u54616 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50668 \u44592 \u49436  \u50517 \u48149 \u51008 , \u45796 \u47480  \u49324 \u46988 \u50640  \u51032 \u54644 \u49436  \u44053 \u50836  \u45817 \u54620 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u51648 \u47564 , \u51088 \u48156 \u51201 \u51004 \u47196  \u50517 \u48149 \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u50696 \u49884 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796 .\u54756 \u50612 \u51648 \u51088  \u47568 \u54616 \u44256  \u49910 \u51648 \u47564 , \u49345 \u45824 \u48169 \u51008  \u54756 \u50612 \u51648 \u44592  \u49899 \u50612 \u54616 \u45716  \u44163  \u44057 \u50500 \u49436  \u47568 \u51012  \u47803  \u44732 \u45236 \u45716  \u49345 \u54889 \u44592 \u45824 \u50640  \u48512 \u51025 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u51060 \u50612 \u49436 , \u50612 \u51796  \u49688  \u50630 \u51060  \u51652 \u54665 \u54616 \u45716  \u49345 \u54889 \u51649 \u51109 \u50640 \u49436  \u49457 \u44284 \u47484  \u45236 \u50556  \u54620 \u45796 \u45716  \u50517 \u48149 \u44048 \u50640  \u49884 \u45804 \u47532 \u45716  \u49345 \u53468 \u48512 \u47784 \u44032  \u44208 \u54844  \u54664 \u51004 \u47732  \u51339 \u44192 \u45796 \u44256 , \u47924 \u50616 \u51032  \u50517 \u48149 \u51012  \u45347 \u45716  \u49345 \u53468 \u48376 \u51064 \u51060  \u45572 \u44400 \u44032 \u47484  \u52293 \u51076 \u51256 \u50556  \u54620 \u45796 \u44256  \u49373 \u44033 \u54616 \u50668 , \u50613 \u51648 \u47196  \u51068 \u51012  \u54616 \u44256  \u51080 \u45716  \u49345 \u53468 \u50756 \u46300 10 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44592 \u45824 \u52824 \u44032  \u45458 \u44256  \u52293 \u51076 \u44048 \u51060  \u44053 \u54620  \u49457 \u44201 \u51077 \u45768 \u45796 . \u54217 \u49548 \u50640  \u51068 \u51060 \u45208  \u51088 \u44592  \u44228 \u48156 \u51012  \u50676 \u49900 \u55176  \u54616 \u45716  \u49828 \u53440 \u51068 \u51060 \u47728 , \u45224 \u46308 \u50640 \u44172  \u51068 \u51012  \u47585 \u44592 \u45716  \u44163 \u46020  \u49899 \u50612 \u54616 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u51593  \u54616 \u46300  \u50892 \u52964 (Hard Worker)\u47484  \u47896 \u49324 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51088 \u49888 \u51012  \u52292 \u52237 \u51656  \u54616 \u45716  \u49457 \u54693 \u46020  \u51080 \u50612 , \u54217 \u49548  \u49900 \u54620  \u49828 \u53944 \u47112 \u49828 \u47484  \u50515 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45224 \u46308 \u51060  \u48372 \u44592 \u50640  \u48708 \u54633 \u47532 \u51201 \u51004 \u47196  \u48372 \u51060 \u51648 \u47564 , \u51060 \u45716  \u44536  \u49324 \u46988  \u48376 \u50672 \u51032  \u49457 \u44201 \u51077 \u45768 \u45796 . \u54788 \u51116  \u51068 \u51012  \u54844 \u51088  \u51652 \u54665 \u54616 \u44256  \u51080 \u44144 \u45208 , \u45320 \u47924  \u47566 \u51008  \u51032 \u47924 \u47484  \u51648 \u44256  \u51080 \u50612  \u50808 \u47196 \u50880 \u51012  \u47566 \u51060  \u45712 \u45180 \u44256  \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 . \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u46020 \u50880 \u44284  \u50948 \u47196 \u44032  \u54596 \u50836 \u54620  \u49884 \u51216 \u51077 \u45768 \u45796 .\u50756 \u46300 10\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  \u46160  \u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888  \u47568 \u44256 \u46020  \u49888 \u44221  \u50424  \u44172  \u47566 \u50500  \u55192 \u46308 \u45796 \u45716  \u49900 \u51221 \u51077 \u45768 \u45796 . \u54788 \u51116  \u49345 \u45824 \u48169 \u51008  \'91\u54644 \u50556  \u54624  \u44163 \'92\u51012  \u51221 \u54644  \u45459 \u44256  \u52423 \u48148 \u53300  \u46028 \u47532 \u46319  \u49332 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51649 \u51109 \u51064 \u51060 \u45208  \u52712 \u51456 \u49373 \u50640 \u44172  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u50612 \u51788 \u47732  \u45817 \u49888 \u51012  \u51068 \u51333 \u51032  \'91\u51664 \'92\u51060 \u46972 \u44256  \u50668 \u44600  \u49688  \u51080 \u51648 \u47564 , \u44536 \u47100 \u50640 \u46020  \u48260 \u47532 \u51648  \u47803 \u54620 \u45796 \u45716  \u51216 \u50640 \u49436 , \u44288 \u44228 \u45716  \u44557 \u51221 \u51201 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u51032 \u47924 \u44048 \u50640  \u51648 \u49549 \u54616 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51593 , \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51060  \u51339 \u50500 \u49436 \u46972 \u44592 \u48372 \u45796  \'91\u47564 \u45208 \u50556  \u54616 \u44592  \u46412 \u47928 \u50640 , \u52293 \u51076 \u51256 \u50556  \u54616 \u44592  \u46412 \u47928 \u50640 \'92 \u47564 \u45224 \u51012  \u51060 \u50612 \u44032 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u50672 \u50528 \u47484  \u51068 \u51333 \u51032  \'91\u45432 \u46041 \'92\u51004 \u47196  \u49373 \u44033 \u54624  \u49688  \u51080 \u51004 \u47728 , \'91\u47560 \u51020 \u51008  \u50630 \u51004 \u45208  \u54756 \u50612 \u51648 \u44596  \u49899 \u51008 \'92 \u44288 \u44228 \u50640 \u49436  \u46321 \u51109 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49345 \u45824 \u48169 \u51008  \u54788 \u51116  \'91\u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \'92\u47484  \u51032 \u47924 \u46972 \u44256  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u44208 \u54844 \u51012  \u54616 \u44592  \u50948 \u54644  \u48152 \u46300 \u49884  \u52293 \u51076 \u51256 \u50556  \u54624  \u49324 \u46988 , \u54841 \u51008  \u54637 \u49345  \u50612 \u47476 \u44256  \u45804 \u47000 \u51452 \u50612 \u50556  \u54624  \u51316 \u51116 \u46972 \u44256  \u49373 \u44033 \u54616 \u45716  \u46319 \u54633 \u45768 \u45796 . \u48512 \u45812 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51032  \u51068 \u46020  \u48148 \u49240 \u44256 , \u49888 \u44221  \u50424  \u44148  \u47566 \u44256 , \u45817 \u49888 \u46020  \u52293 \u51076 \u51256 \u50556  \u54616 \u45768  \u49900 \u51201 \u51004 \u47196  \u47566 \u51060  \u55192 \u46304  \u49345 \u54889 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u50756 \u46300  10\u51008  \u46160  \u44032 \u51648  \u54644 \u49437 \u51060  \u44032 \u45733 \u54633 \u45768 \u45796 . \u48372 \u51312  \u52852 \u46300 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 . \u52395  \u48264 \u51704 \u45716 , \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u44032  \u48512 \u45812 \u51060 \u50632 \u44256 , \u51648 \u44552 \u51008  \u51116 \u54924 \u47484  \u49888 \u44221  \u50424  \u50668 \u50976 \u44032  \u50630 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50756 \u46300 \u47484  10\u44060  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u49345 \u45824 \u48169 \u51032  \u49334 \u50640 \u49436  \u48279 \u50612 \u45228 \u51020 \u50640 \u46020  \u48520 \u44396 \u54616 \u44256 , \u50756 \u46300  \u54616 \u45208 \u44032  \u49352 \u47213 \u44172  \u52292 \u50892 \u51652  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44536  \u49324 \u46988 \u50640 \u44172 \u45716  \u46608  \u45796 \u47480  \u51032 \u47924 \u44032  \u49373 \u44220 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 \u45716 , \u44536 \u47100 \u50640 \u46020  \u48260 \u47532 \u51648  \u47803 \u54624  \u49324 \u46988 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44592 \u50613 \u51012  \u46496 \u50732 \u47532 \u47732  \u55192 \u46308 \u44256  \u51648 \u52824 \u51648 \u47564 , \u44536 \u47100 \u50640 \u46020  \u45817 \u49888 \u51012  \u45936 \u47140 \u44032 \u44256  \u49910 \u51008  \u49900 \u51221 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51060  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732  \u51116 \u54924 \u47484  \u44592 \u45824 \u54644 \u48380 \u47564  \u54633 \u45768 \u45796 .\u50756 \u46300 10 \u51649 \u50629 \u50868  \u54644 \u49437 \u45320 \u47924  \u47566 \u51008  \u50629 \u47924 \u47049 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u44592 \u45824 , \u51032 \u47924 \u44048 \u50640  \u49884 \u45804 \u47532 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u47560 \u51020 \u50640  \u50630 \u45716  \u51068 \u51012  \u44228 \u49549 \u54616 \u44256  \u51080 \u45796 \u44144 \u45208 , \u47784 \u46304  \u48512 \u48516 \u51012  \u45796  \u51096 \u54616 \u47140 \u44256  \u54616 \u45768 , \u49900 \u51201 \u51004 \u47196  \u55192 \u46304  \u49345 \u54889 \u51077 \u45768 \u45796 . \u45817 \u48516 \u44036  \u55092 \u49885 \u51012  \u52712 \u54616 \u45716  \u44172  \u51339 \u44192 \u49845 \u45768 \u45796 . \u44536 \u44172  \u50500 \u45768 \u46972 \u47732 , \u50864 \u49440 \u49692 \u50948 \u47484  \u51116 \u44160 \u53664 \u54624  \u54596 \u50836 \u44032  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . 10\u44060 \u51032  \u50756 \u46300 \u47484  \u45208 \u45600 \u49436  \u52376 \u47532 \u54644 \u50556  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51060  \u44032 \u51109  \u44732 \u47140 \u54616 \u45716  \u51068 , \u49884 \u44036 \u51060  \u50724 \u47000  \u44152 \u47532 \u45716  \u50629 \u47924 \u47484  \u50948 \u51076 \u54644 \u46020  \u51339 \u44192 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u50640 \u49436 \u46020 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u44592 \u45824 \u50640  \u48512 \u51025 \u54616 \u47140 \u45796 \u48372 \u45768  \u49900 \u51201 \u51004 \u47196  \u55192 \u46304  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u48376 \u51064 \u51012  \u44284 \u46020 \u54616 \u44172  \u52292 \u52237 \u51656  \u51473 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u44172  \u50500 \u45768 \u46972 \u47732 , \'91\u44277 \u48512 \u50640  \u50864 \u49440 \u49692 \u50948 \u44032  \u50630 \u45796 \'92\u45716  \u46907 \u51060 \u48064 \u47196 , \u44228 \u54925 \u51012  \u51116 \u51221 \u48708 \u54624  \u54596 \u50836 \u44032  \u51080 \u44192 \u49845 \u45768 \u45796 .\u50756 \u46300 10 \u44552 \u51204 \u50868  \u54644 \u49437 \u51116 \u51221 \u51201 \u51064  \u48512 \u45812 \u50640  \u49884 \u45804 \u47532 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u45824 \u52636 \u51060 \u45208  \u48730  \u54841 \u51008  \u47588 \u45804  \u45208 \u44032 \u45716  \u50900 \u49464 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u47196  \u51064 \u54644  \u49345 \u45817 \u54620  \u49828 \u53944 \u47112 \u49828 \u47484  \u45712 \u45180 \u44256  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u50756 \u46300 10\u51008  \'91\u52313 \u44592 \u51648  \u50506 \u50500 \u49436 \'92\u44032  \u50500 \u45768 \u46972 , \'91\u45320 \u47924  \u47566 \u51060  \u52313 \u44592 \u47140 \u45796 \u48372 \u45768 \'92 \u47928 \u51228 \u44032  \u49373 \u44592 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51593 , \u54596 \u50836  \u50630 \u45716  \u44275 \u50640  \u45320 \u47924  \u47566 \u51008  \u46024 \u51012  \u50143 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51204 \u48152 \u51201 \u51004 \u47196  \u48708 \u50857 \u51012  \'91\u44036 \u52628 \u47540 \'92 \u54596 \u50836 \u44032  \u51080 \u51004 \u47728 , \u44032 \u44228 \u48512 \u47484  \u53685 \u54644  \'91\u50612 \u46356 \u49436  \u46024 \u51060  \u48736 \u51256 \u45208 \u44032 \u45716 \u51648 \'92 \u47749 \u54869 \u55176  \u50508  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 10\u51032  \u51116 \u51221 \u51201  \u51312 \u50616  \'96 Sibyl Tarot    \
