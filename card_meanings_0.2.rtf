   \uc0\u50756 \u46300 2\u50756 \u46300 2 \u53412 \u50892 \u46300 \u44032 \u44620 \u50868  \u53945 \u49457 \u44536 \u47532 \u50880 \u49548 \u44537 \u51201  \u44228 \u54925 \u54620  \u45576 \u51012  \u54028 \u45716 \u44256 \u48124 \u54616 \u45716 \u44144 \u47532 \u44032  \u47676  \u53945 \u49457 \u48120 \u47144 \u51060  \u50630 \u45716 \u44053 \u54620  \u54665 \u46041 \u47141 \u51088 \u49888 \u44048  \u51080 \u45716 \u54869 \u49892 \u54620 \u50756 \u46300 2 \u51060 \u48120 \u51648  \u54644 \u49437  \u47924 \u50616 \u44032 \u47484  \u44536 \u47532 \u50892 \u54616 \u44256  \u51080 \u45716  \u47784 \u49845 \u44592 \u51316 \u51032  \u49457 \u44284 \u47932 \u51012  \u51228 \u52432 \u46160 \u44256  \u49352 \u47196 \u50868  \u47785 \u54364 \u47484  \u49464 \u50864 \u45716  \u47784 \u49845 \u47784 \u54744 \u44284  \u50504 \u51221  \u49324 \u51060 \u50640 \u49436  \u44040 \u46321 \u54616 \u44256  \u51080 \u45716  \u47784 \u49845 \u49457 \u48317  \u50504 \u50640  \u51080 \u45716  \u44163 \u51004 \u47196  \u48372 \u50500  \u50500 \u51649  \u44228 \u54925 \u51012  \u49892 \u54665 \u54616 \u51648  \u50506 \u51020 \u50672 \u50528 \u45716  \u46263 \u51204 \u51060 \u44256  \u45796 \u47480  \u44256 \u48124 \u51012  \u54616 \u44256  \u51080 \u45716  \u49324 \u46988 \u49352 \u47196 \u50868  \u49324 \u46988 \u51060  \u45576 \u50640  \u46308 \u50612 \u50724 \u45716  \u47784 \u49845  (\u54840 \u44592 \u49900 \u50640  \u44032 \u44620 \u50880 )\u44557 \u51221 \u51201  \u51032 \u48120 \u54644 \u49437  1. \u44536 \u47532 \u50880 \u50756 \u46300 2\u45716  \u47924 \u50616 \u44032 \u47484  \u44536 \u47532 \u50892 \u54616 \u45716  \u49324 \u46988 \u50640 \u44172 \u49436  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50504 \u51204 \u54620  \u49457  \u50504 \u50640  \u51080 \u51648 \u47564 , \u48148 \u44645  \u49464 \u49345 \u50640  \u45576 \u44600 \u51012  \u46028 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 .\u45824 \u54364 \u51201 \u51004 \u47196  \u51116 \u54924 \u50868 \u50640 \u49436 \u45716 , \u50715  \u50672 \u51064 \u50640  \u45824 \u54620  \u44536 \u47532 \u50880 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u49548 \u44060 \u54021 \u51012  \u45208 \u44032 \u49436  \u49352 \u47196  \u50672 \u46973 \u54616 \u45716   \u49345 \u45824 \u44032  \u49373 \u44220 \u51004 \u45208 , \u45572 \u44400 \u44032 \u47484  \u44536 \u47532 \u50892 \u54616 \u47728  \'91\u50696 \u51204 \u51060  \u51339 \u50520 \u51648 \'92 \u54924 \u49345 \u54616 \u45716  \u51109 \u47732 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u47676  \u44275 \u51012  \u48148 \u46972 \u48372 \u45716  \u54665 \u50948 \u47484 \'85\u44536 \u47532 \u50880 \u51004 \u47196  \u48372 \u44592 \u46020 \u47924 \u50616 \u44032 \u50640  \u45824 \u54620  \u54924 \u49345 \u51004 \u47196  \u48372 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51648 \u44552  \u45817 \u49888 \u44284  \u44536  \u49324 \u46988 \u51060  \u46504 \u50612 \u51652  \u49345 \u53468 \u46972 \u47732 , \'91\u45817 \u49888 \u50640  \u45824 \u54620  \u44536 \u47532 \u50880 \'92\u51004 \u47196  \u54644 \u49437 \u54616 \u47728 , \u51109 \u44144 \u47532  \u50672 \u50528 \u47484  \u54624  \u44221 \u50864 \u50640 \u46020 , \u47680 \u47532 \u49436  \u45817 \u49888 \u51012  \u44536 \u47532 \u50892 \u54616 \u45716  \u47784 \u49845 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u47924 \u50616 \u44032  \u54616 \u44592 \u47196  \u44228 \u54925 \u51012  \u49464 \u50864 \u45716 \u45824 \u54364 \u51201 \u51064  \u53412 \u50892 \u46300 \u45716  \'91\u44228 \u54925 \'92\u51077 \u45768 \u45796 . \u45224 \u51088 \u45716  \u51648 \u44396 \u48376 \u51012  \u46308 \u44256  \u49352 \u47196 \u50868  \u49464 \u44228 \u47484  \u44040 \u47581  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \'91\u47924 \u50616 \u44032  \u54616 \u44256  \u49910 \u51008  \u50837 \u47581 \u44284  \u45908 \u48520 \u50612 , \u49892 \u51228 \u47196  \u44228 \u54925 \u51012  \u49464 \u50864 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51060  \u46121 \u45768 \u45796 . \u51648 \u44396 \u48376 \u51008  \'91\u49892 \u51656 \u51201 \u51064  \u44228 \u54925 \'92\u51032  \u49345 \u51669 \u51077 \u45768 \u45796 .\u50696 \u52968 \u45824  \u45817 \u49888 \u51060  \u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732 , \u47561 \u50672 \u54620  \u54840 \u44048 \u50640 \u49436  \u8594  \u49548 \u44537 \u51201 \u51064  \u44228 \u54925 \u51004 \u47196 \u54620  \u45800 \u44228  \u48156 \u51204 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u50756 \u46300 2\u44032  \u45208 \u50772 \u45796 \u47732 , \u50612 \u45712  \u51221 \u46020  \u45817 \u49888 \u50640  \u45824 \u54620  \u49373 \u44033 \u51060  \u51088 \u46972 \u45212  \u44163 \u51004 \u47196  \u48372 \u51060 \u47728 , \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492  \u46608 \u54620  \u44557 \u51221 \u51201 \u51060 \u47732  \'91\u47924 \u50616 \u44032  \u48320 \u54868 \u54624  \u51669 \u51312 \'92\u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u44536 \u47084 \u45208 , \u50500 \u51649  \u49892 \u54665 \u50640  \u50734 \u44592 \u51648 \u45716  \u50506 \u51008  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \u50756 \u46300 3\u44284  \u48708 \u44368 \u54644 \u48372 \u47732  \u45908 \u50865  \u49789 \u44172  \u50508  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300 3 \u49549  \u51064 \u47932 \u51008  \'91\u49457 \u48317  \u50808 \u48512 \'92\u50640  \u51080 \u50612  \u49892 \u51228 \u47196  \u49892 \u54665 \u50640  \u50734 \u44596  \u47784 \u49845 \u51012  \u49345 \u51669 \u54620 \u45796 \u47732 ,\u50756 \u46300 2 \u49549  \u51064 \u47932 \u51008  \u50500 \u51649  \'91\u49457 \u48317  \u45236 \u48512 \'92\u50640  \u51080 \u44592  \u46412 \u47928 \u50640 , \u49892 \u54665 \u50640  \u50734 \u44592 \u51648 \u45716  \u47803 \u54620  \u47784 \u49845 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u49457 \u48317  \u50504 \u50640  \u51080 \u51020  (\u49892 \u54665  X)\u49457 \u48317  \u48150 \u50640  \u51080 \u51020  (\u49892 \u54665  O) \u50756 \u46300 2\u45716  \u50756 \u46300  \u50640 \u51060 \u49828 \u51032  \u51312 \u44552  \u45908  \u49457 \u49689 \u54620  \u48260 \u51204 \u51077 \u45768 \u45796 .\u50756 \u46300  \u50640 \u51060 \u49828 \u44032  \u47924 \u50616 \u44032 \u47484  \u54693 \u54620  \'91\u54253 \u48156 \u51201 \u51064  \u50676 \u51221 \'92, \'91\u47561 \u50672 \u54620  \u54840 \u44048 \'92\u51012  \u49345 \u51669 \u54664 \u45796 \u47732 , \u50756 \u46300 2\u45716  \u44536  \u44048 \u51221 \u51012  \u51312 \u44552  \u52264 \u48516 \u55176  \u54616 \u47732 \u49436  \u49892 \u51228 \u47196  \u44256 \u48124 \u54616 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  :\u50672 \u50528 \u50640 \u49436  \u8594  \u44536  \u49324 \u46988 \u54620 \u53580  \u51060 \u51228  \u45796 \u44032 \u44032 \u50556  \u54616 \u45208 ?\u45796 \u44032 \u44036 \u45796 \u47732  \u50612 \u46523 \u44172  \u45796 \u44032 \u44032 \u51648 ?\u50616 \u51228  \u45796 \u44032 \u44032 \u50556  \u54616 \u51648 ?\u51649 \u50629 \u50640 \u49436  \u8594  \u51060 \u51649 \u51012  \u54624 \u44620  \u47568 \u44620 ?\u51060 \u51649 \u51012  \u54620 \u45796 \u47732  \u50616 \u51228  \u54644 \u50556  \u46104 \u51648 ?\u49352 \u47196 \u50868  \u54924 \u49324 \u45716  \u48127 \u51012 \u47564 \u54620  \u44275 \u51064 \u44032 ? \u54788 \u49892 \u51201 \u51004 \u47196  \u46384 \u51256 \u48400 \u50556 \u51648 \u54644 \u49437  3. \u47784 \u54744 \u50640  \u45824 \u54620  \u44040 \u47581 \u45224 \u51088 \u45716  \u47566 \u51008  \u44163 \u51012  \u51060 \u47384 \u49845 \u45768 \u45796 . \u50504 \u51204 \u54620  \u49457 \u48317  \u50504 \u50640  \u51080 \u44592  \u46412 \u47928 \u50640 , \u48150 \u51004 \u47196  \u45208 \u44032 \u51648  \u50506 \u50500 \u46020  \u54217 \u49373 \u51012  \u54200 \u54616 \u44172  \u49332  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536 \u45716  \u51648 \u44552  \u50504 \u51221 \u44284  \u47784 \u54744  \u51473 \u50640 \u49436  \u44040 \u46321 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .Two of Wands\u45716  \u44592 \u51316 \u51032  \u49440 \u53469 \u51012  \u44256 \u49688 \u54616 \u44144 \u45208 , \u50948 \u54744 \u51012  \u44048 \u49688 \u54616 \u45716  \u44163  \u51473 \u50640 \u49436  \u44256 \u48124 \u54616 \u44256  \u51080 \u51020 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .Biddy Tarot \'96 Two of Wands Meaning\u45824 \u54364 \u51201 \u51004 \u47196  \u51649 \u50629 \u50868 \u50640 \u49436 \u45716  : \u51648 \u47336 \u54620  \u49334 \u51012  \u51665 \u50612 \u45912 \u51648 \u44256 , \u47784 \u54744 \u51012  \u54616 \u44256  \u49910 \u45796 \u45716  \u50837 \u47581 \u51060  \u54364 \u52636 \u46108  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45000 \u51012  \u51923 \u44172 \u46104 \u45716  \u49884 \u44592 \u44032  \u46112  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u46384 \u46972 \u49436  \u50756 \u46300 2\u45716  \u44060 \u51064  \u49324 \u50629 \u51060 \u45208  \u47924 \u50669 \u50629 , \u50668 \u54665  \u44288 \u47144  \u51649 \u51333 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u51032 \u48120 \u54644 \u49437  1. \u45796 \u47480  \u44163 \u51060  \u45576 \u50640  \u46308 \u50612 \u50724 \u45716  \u50756 \u46300 2\u45716  \u49352 \u47196 \u50868  \u47924 \u50616 \u44032 \u44032  \u45576 \u50640  \u46308 \u50612 \u50724 \u45716  \u49345 \u54889 \u51012  \u46907 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u51088 \u49888 \u51060  \u50896 \u47000  \u44054 \u44256  \u51080 \u45912  \u50756 \u46300 \u47484  \u50694 \u50640  \u47926 \u50612 \u46164  \u52292 , \u49352 \u47196 \u50868  \u50756 \u46300 \u50640  \u49884 \u49440 \u51012  \u51665 \u51473 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51060 \u45716  \u50896 \u47000  \u44032 \u51648 \u44256  \u51080 \u45912  [\u45000 , \u49324 \u46988 , \u49324 \u47932 ]\u51012  \u51228 \u52432 \u46160 \u44256  \u49352 \u47196 \u50868  [\u45000 , \u49324 \u46988 , \u49324 \u47932 ]\u50640  \u44288 \u49900 \u51060  \u49373 \u44596  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u50696 \u52968 \u45824 , \u45817 \u49888 \u51060  \u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u51060 \u48120  \u50672 \u51064 (\u50896 \u47000  \u44032 \u51648 \u44256  \u51080 \u50632 \u45912  \u50756 \u46300 )\u44032  \u51080 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50668 \u44592 \u49436  \u54644 \u49437 \u51008  2\u44032 \u51648 \u47196  \u45208 \u45656 \u45716 \u45936  :\u50672 \u51064 \u51012  \u51228 \u52432 \u46160 \u44256  \u45817 \u49888 \u51012  \u48148 \u46972 \u48372 \u45716  \u44536 \u47548 \u51060  \u46112  \u49688 \u46020 \u45817 \u49888 \u51012  \u51228 \u52432 \u46160 \u44256  \u50672 \u51064 \u51012  \u48148 \u46972 \u48372 \u45716  \u44536 \u47548 \u51060  \u46112  \u49688 \u46020 \u44536 \u47084 \u45208  \u48152 \u46300 \u49884  \'91\u50577 \u45796 \u47532 \u45208  \u54872 \u49849 \'92\u51012  \u51032 \u48120 \u54616 \u45716  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u44032  \u47564 \u51313 \u49828 \u47101 \u51648 \u47564 , \u45800 \u49692 \u54620  \u51652 \u47196  \u44256 \u48124 , \u49334 \u50640  \u45824 \u54620  \u44256 \u48124 \u51012  \u46907 \u54624  \u49688 \u46020  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u45817 \u48516 \u44036  \u45236 \u51201  \u44040 \u46321 \u50640  \u48736 \u51656  \u50696 \u51221 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u50612 \u45712  \u51901 \u51032  \u50756 \u46300 \u51060 \u44148  \u49457 \u48317  \u50504 \u50640  \u46308 \u50612 \u50772 \u45796 \u45716  \u51216 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 .\u52964 \u54540 \u51032  \u49345 \u54889 \u51008  \u45908  \u45796 \u50577 \u54616 \u44172  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796  :\u49352 \u47196 \u50868  \u45572 \u44400 \u44032 \u50640 \u44172  \u44288 \u49900 \u51060  \u49373 \u44220 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u49352 \u47196 \u50868  \u45000 \u51060 \u45208  \u47785 \u54364 \u44032  \u49373 \u44220 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u51649 \u51012  \u44256 \u48124 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u44032  \u51648 \u52828  \u45208 \u47672 \u51648 , \u51060 \u51204 \u51032  \u49556 \u47196  \u49373 \u54876 \u50640  \u44536 \u47532 \u50880 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50715  \u50672 \u51064 \u51012  \u52628 \u50613 \u54616 \u45716  \u44536 \u47548 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u46384 \u46972 \u49436  \u50896 \u52852 \u46300 \u47196  \u49345 \u54889 \u51012  \u45800 \u51221 \u51667 \u44592 \u45716  \u47588 \u50864  \u50612 \u47157 \u49845 \u45768 \u45796 .\u51060 \u47484  \u51649 \u50629 \u50868 \u50640  \u51201 \u50857 \u49884 \u53412 \u47732  : \u50612 \u45712 \u51221 \u46020  \u51649 \u50629 \u51201 \u51004 \u47196  \u44592 \u48152 \u51012  \u44054 \u52676 \u51004 \u45208 , \u51068 \u50640  \u45824 \u54620  \u54924 \u51032 \u44048 \u51060  \u46304  \u45208 \u47672 \u51648 , \u51060 \u51649 \u51012  \u44256 \u48124 \u54616 \u44144 \u45208  \u50696 \u51204 \u51032  \u51068 \u51012  \u44536 \u47532 \u50892 \u54616 \u45716  \u49345 \u54889  \u51221 \u46020 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51200 \u45716  \u50612 \u47160 \u51012  \u51201  \u45000 \u51012  \u54924 \u49345 \u54616 \u45716  \u51109 \u47732 \u51004 \u47196 \u46020  \u45712 \u44788 \u51665 \u45768 \u45796 . \u49352 \u47196 \u50868  \u47924 \u50616 \u44032 \u44032  \u45576 \u50640  \u46308 \u50612 \u50724 \u45716  \u49345 \u54889 \u51060 \u44592  \u46412 \u47928 \u50640 , \u51032 \u49324  \u44208 \u51221 \u50640  \u51080 \u50612 \u49436  \u49828 \u53944 \u47112 \u49828 \u47484  \u48155 \u51012  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45800 \u49692 \u54616 \u44172  \'91\u53804 \u51105 \'92\u51012  \u51032 \u48120 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300 2\u44032  \u51109 \u50528 \u47932 \u47196  \u51089 \u50857 \u54624  \u44221 \u50864  \u51032 \u48120  \'96 Sibyl Tarot\u54644 \u49437  2. \u50694 \u51012  \u47803  \u48372 \u44256  \u51080 \u45716  \u49345 \u54889 \u44060 \u51064 \u51201 \u51064  \u50837 \u47581  \u46412 \u47928 \u50640  \u45572 \u44400 \u44032 \u47484  \u49548 \u54848 \u55176  \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u44592 \u51316 \u50640  \u44032 \u51648 \u44256  \u51080 \u45912  \u50756 \u46300 \u47484  \'91\u47926 \u50612 \u46160 \u45716  \u48169 \u49885 \'92\u51004 \u47196  \u53685 \u51228 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u45817 \u49888 \u51060  \u44032 \u51221 \u51012  \u44984 \u47532 \u44144 \u45208  \u45572 \u44400 \u44032 \u47484  \u52293 \u51076 \u51256 \u50556  \u54624  \u49345 \u54889 \u50640  \u51080 \u45796 \u47732 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u49548 \u54848 \u55176  \u50668 \u44600  \u49688  \u51080 \u45796 \u45716  \u44221 \u44256 \u47700 \u49884 \u51648 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732  \u44060 \u51064 \u51201 \u51064  \u51068 \u50640  \u48736 \u51256 \u49436  \u50672 \u51064 \u44284 \u51032  \u44048 \u51221  \u49548 \u53685 \u46020  \u48512 \u51313 \u54644 \u51656  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u51068 \u44284  \u50672 \u50528 \u51032  \u48184 \u47088 \u49828 \u47484  \u47582 \u52656 \u50556  \u54616 \u44592  \u46412 \u47928 \u50640 , \u49828 \u53944 \u47112 \u49828 \u44032  \u49900 \u54620  \u49884 \u44592 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u46300 2 \u50672 \u50528 \u50868  \u54644 \u49437 \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640  \u45824 \u54644  \u51656 \u47928 \u54620  \u44221 \u50864 , \u49345 \u45824 \u48169 \u51008  \u54788 \u51116  \u48320 \u54868 \u51032  \u44592 \u47196 \u50640  \u49436 \u51080 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u44263  \u44228 \u54925 \u51012  \u49464 \u50872  \u50696 \u51221 \u51077 \u45768 \u45796 .\u52395  \u48264 \u51704 \u45716  \u50672 \u50528 \u50640  \u45824 \u54620  \u48320 \u54868 \u51077 \u45768 \u45796 . \'91\u45817 \u49888 \u51012  \u46244 \u47196  \u54620  \u52292 \'92 \u46496 \u45208 \u45716  \u44228 \u54925 \u51012  \u49464 \u50872  \u49688 \u46020  \u51080 \u44256 , \'91\u45796 \u47480  \u47924 \u50616 \u44032 \u47484  \u46244 \u47196  \u54620  \u52292 \'92 \u45817 \u49888 \u50640 \u44172  \u45796 \u44032 \u44032 \u45716  \u44228 \u54925 \u51012  \u49464 \u50872  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51473 \u50836 \u54620  \u51216 \u51008  \u44536 (\u44536 \u45376 )\u44032  \u44040 \u46321 \u54616 \u44256  \u51080 \u45796 \u45716  \u44163 \u51077 \u45768 \u45796 . \'91\u44284 \u44144 \u50640  \u47564 \u45228 \u45912  \u49324 \u46988 \u44284  \u54788 \u51116  \u47564 \u45208 \u44256  \u51080 \u45716  \u49324 \u46988 \'92 \u54841 \u51008  \'91\u54788 \u51116  \u47564 \u45208 \u44256  \u51080 \u45716  \u49324 \u46988 \u44284  \u48120 \u47000 \u50640  \u47564 \u45216  \u49324 \u46988 \'92\u51077 \u45768 \u45796 . \u47564 \u50557  \u49345 \u45824 \u48169 \u51012  \u50508 \u44256  \u51648 \u45240 \u51648  \u50620 \u47560  \u50504  \u46096 \u45796 \u47732 , \'91\u45817 \u49888 \u50640  \u45824 \u54620  \u54840 \u44048 \'92\u51060  \'91\u44396 \u52404 \u51201 \u51004 \u47196  \u45796 \u44032 \u44040  \u44228 \u54925 \'92\u51004 \u47196  \u48148 \u45068 \u50632 \u51020 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u46160  \u48264 \u51704 \u45716  \u51068 \u50640  \u45824 \u54620  \u48320 \u54868 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51652 \u47196  \u44256 \u48124 \u51060 \u47728 , \u51060 \u51649 \u51060 \u45208  \u49352 \u47196 \u50868  \u49324 \u50629 \u50640  \u45824 \u54620  \u46020 \u51204 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u45817 \u49888 \u51012  \u49888 \u44221  \u50424  \u50668 \u50976 \u44032  \u50630 \u50612  \u48372 \u51060 \u47728 , \'91\u44288 \u49900 \u51032  \u52488 \u51216 \u51060  \u51068 \u50640  \u51080 \u51020 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50756 \u46300 2\u45716  \u51452 \u47196  \'91\u48120 \u47000 \u51648 \u54693 \u51201 \u51064  \u44288 \u44228 \'92\u50640 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844 \u51060 \u45208  \u51088 \u45376  \u44228 \u54925 \u51077 \u45768 \u45796 . \u51068 \u45800  \u45817 \u49888 \u51012  \u48376 \u51064 \u51032  \u49457 \u48317  \u45236 \u50640  \u46164  \u46244 , \u50526 \u51004 \u47196 \u45716  \u50612 \u46523 \u44172  \u54644 \u50556 \u54624 \u51648  \u44256 \u48124 \u54616 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u50756 \u46300 \u50640 \u49436  \u48512 \u51313 \u54624  \u49688  \u51080 \u45716  \'91\u54788 \u49892 \u49457 \'92\u51060  \u54252 \u54632 \u46108  \u47784 \u49845 \u51077 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \'91\u44288 \u44228 \u50640  \u54924 \u51032 \u44048 \u51012  \u45712 \u45184  \u45208 \u47672 \u51648 , \u54756 \u50612 \u51656 \u44620  \u44256 \u48124 \u54616 \u45716  \u49345 \u54889 \'92 \u54841 \u51008  \'91\u45796 \u47480  \u45572 \u44400 \u44032 \u44032  \u45576 \u50640  \u46308 \u50612 \u50728  \u49345 \u54889 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u54756 \u50612 \u51652  \u44288 \u44228 \u50640  \u45824 \u54644  \u51656 \u47928 \u54664 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u54788 \u51116  \u45817 \u49888 \u51012  \u44536 \u47532 \u50892 \u54616 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51648 \u44552  \u44536 \u45716  \u45817 \u49888 \u51012  \u51082 \u44256  \u49332 \u50500 \u44040 \u51648 , \u51116 \u54924 \u47484  \u49884 \u46020 \u54624 \u51648  \u44256 \u48124  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50500 \u51649 \u51008  \u44536  \u44256 \u48124 \u51060  \u52488 \u48152  \u45800 \u44228 \u50640  \u45459 \u50668 \u51080 \u49845 \u45768 \u45796 . \u54665 \u46041 \u47141 \u51060  \u48512 \u51313 \u54616 \u44592 \u50640  \u45817 \u48516 \u44036  \u50672 \u46973 \u51008  \u44592 \u45824 \u54616 \u44592  \u55192 \u46308 \u51648 \u47564 , \'91\u44536 \u47532 \u50892 \u54616 \u44256  \u51080 \u45796 \'92\u45716  \u51216 \u51008  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 .\u50756 \u46300 2 \u51649 \u50629 \u50868  \u54644 \u49437 \u45824 \u54364 \u51201 \u51064  \u51109 \u50528 \u47932 \u51008 , \'91\u48320 \u54868 \u47484  \u46160 \u47140 \u50892 \u54616 \u45716  \u47560 \u51020 \'92\u44284  \'91\u48169 \u54693 \u49457  \u48512 \u51313 \'92\u51077 \u45768 \u45796 . \u50500 \u51649  \u45817 \u49888 \u51008  \u51652 \u47196 \u50640  \u45824 \u54620  \u54869 \u49888 \u51012  \u45236 \u47532 \u51648  \u47803 \u54664 \u49845 \u45768 \u45796 . \u50526 \u51004 \u47196  \u45208 \u50500 \u44032 \u45716  \u45936  \u50612 \u47140 \u50880 \u51012  \u44202 \u45716 \u46319  \u48372 \u51077 \u45768 \u45796 . \u50500 \u45768 \u47732 , \u50896 \u52824  \u50506 \u45716  \u51068 \u51012  \u50613 \u51648 \u47196  \u54616 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300 2\u45716  \u51452 \u47196  \u47784 \u54744 \u51012  \u44040 \u47581 \u54616 \u45716  20~30\u45824  \u51649 \u51109 \u51064 \u50640 \u44172 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u51649 \u50629 \u50640  \u45824 \u54620  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u51080 \u45796 \u47732 , \u51221 \u47568  \u44396 \u52404 \u51201 \u51064  \u44228 \u54925 \u51012  \u49464 \u50864 \u44256 , \u51312 \u44552 \u50473  \u49892 \u52380 \u54644 \u50556  \u54620 \u45796 \u45716  \u47568 \u50432 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 . \u50864 \u49440 \u49692 \u50948 \u47484  \u54869 \u49892 \u55176  \u51221 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51060 \u44163  \u51200 \u44163  \u47784 \u46160  \u52313 \u44592 \u47140 \u45796 \u48372 \u47732 , \u44537 \u46020 \u51032  \u49828 \u53944 \u47112 \u49828 \u50640  \u45459 \u51068  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50668 \u47084 \u44032 \u51648  \u50741 \u49496 \u51012  \u52041  \u45208 \u50676 \u54620  \u46244  \u49888 \u51473 \u54616 \u44172  \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54616 \u49464 \u50836 . \u51312 \u44553 \u54644 \u54616 \u51648  \u47568 \u44256  \u51312 \u44552 \u50473  \u51312 \u44552 \u50473  \u49892 \u52380 \u54644 \u48372 \u49464 \u50836 . \u44592 \u54924 \u45716  \u50676 \u47140 \u51080 \u49845 \u45768 \u45796 .\u50629 \u51333 \u51012  \u48320 \u44221 \u54616 \u44144 \u45208  \u49352 \u47196 \u50868  \u51068 \u51012  \u49884 \u51089 \u54616 \u47140  \u54620 \u45796 \u47732 , \u50756 \u46300 2\u45716  \u44557 \u51221 \u51201 \u51064  \u49888 \u54840 \u51077 \u45768 \u45796 . \u54217 \u49548 \u50640  \u45000 \u45012 \u50772 \u45912  \u51068 \u51012  \u46020 \u51204 \u54616 \u44592 \u50640  \u51339 \u51008  \u49884 \u44592 \u51077 \u45768 \u45796 . \u45796 \u47564 , \'91\u47561 \u50672 \u54620  \u50676 \u51221 \'92\u51060  \u50500 \u45772  \'91\u44396 \u52404 \u51201 \u51064  \u49892 \u54665  \u44228 \u54925 \'92\u51060  \u54596 \u50836 \u54633 \u45768 \u45796 . \u50612 \u46500  \u51068 \u51012  \u54616 \u47140 \u44256  \u54616 \u45716 \u51648 , \u46024 \u51008  \u50612 \u46523 \u44172  \u48268  \u44163 \u51064 \u51648 , \u44221 \u47141 \u51008  \u50612 \u46523 \u44172  \u49939 \u51012  \u44163 \u51060 \u47728 , \u50612 \u46523 \u44172  \u44221 \u51137 \u47141 \u51012  \u44054 \u52636  \u44163 \u51064 \u51648  \u51088 \u47928 \u54644 \u48400 \u50556  \u54633 \u45768 \u45796 .\u50756 \u46300 2 \u44552 \u51204 \u50868  \u54644 \u49437 \u50756 \u46300 2\u45716  \u44552 \u51204 \u50868 \u50640 \u49436  \u51116 \u51221 \u51201  \u50504 \u51221 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45336 \u52824 \u45716  \u48512 \u47484  \u49345 \u51669 \u54616 \u45716  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u44536 \u51200  \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49345 \u54889 \u51060  \u54200 \u50504 \u54616 \u44256  \u50504 \u51221 \u51201 \u51076 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \'91\u47784 \u54744 \'92\u51012  \u44256 \u47140 \u54644 \u48380  \u49688  \u51080 \u45716  \u44163 \u51060 \u51424 . \u45796 \u47564 , \u45817 \u49888 \u51060  \u47924 \u50616 \u44032 \u47484  \u49324 \u44144 \u45208  \u53360  \u53804 \u51088 \u47484  \u54616 \u47140 \u45716  \u49345 \u53468 \u46972 \u47732 , \u44396 \u52404 \u51201 \u51064  \u44228 \u54925 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u47700 \u49884 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u51452 \u49885 \u51012  \u50616 \u51228  \u49332  \u44148 \u51648 , \u50620 \u47560 \u45208  \u49332  \u44148 \u51648 , \u50616 \u51228 \u52196  \u54036 \u44256  \u45208 \u50732  \u44163 \u51064 \u51648  \u46321 \u46321 \u51077 \u45768 \u45796 .    \
