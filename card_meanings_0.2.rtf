   \uc0\u50756 \u46300 7 \'96 \u48169 \u50612  \u50756 \u46300 7 \u44536 \u47548 \u51032  \u51032 \u48120 \u48376 \u51064 \u51032  \u51452 \u51109 , \u49888 \u45392 \u51012  \u51648 \u53412 \u44592  \u50948 \u54644  \u44256 \u44400 \u48516 \u53804 \u54616 \u45716  \u49345 \u54889 \u45796 \u47480  \u50756 \u46300 \u48372 \u45796  \u54620 \u52280  \u50948 \u50640  \u51080 \u44592  \u46412 \u47928 \u50640 , \u50976 \u47532 \u54620  \u50948 \u52824 \u52376 \u47532 \u54644 \u50556  \u54624  \u47928 \u51228 , \u49373 \u44033 \u46308 \u51060  \u47566 \u51008  \u49345 \u54889 \u50808 \u48512 \u50640 \u49436  \u44036 \u49453 \u51060  \u46308 \u50612 \u50752  \u49828 \u53944 \u47112 \u49828  \u48155 \u51012  \u49688  \u51080 \u51020 \u44048 \u51221 \u51060 \u45208  \u44592 \u50613 \u51012  \u50528 \u50024  \u50613 \u45572 \u47476 \u45716  \u49345 \u53468 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50976 \u47532 \u54620  \u50948 \u52824 \u47484  \u49440 \u51216 \u54620 \u50756 \u46300 7\u51008  \u50976 \u47532 \u54620  \u50948 \u52824 \u47484  \u49440 \u51216 \u54620  \u49324 \u46988 , \u54841 \u51008  \u50976 \u47532 \u54620  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u52852 \u46300  \u49549  \u44536 \u47548 \u51012  \u48372 \u47732 , \u45224 \u51088 \u45716  \u44256 \u51648 \u47484  \u51216 \u47161 \u54620  \u46244 , \u48169 \u50612 \u50640  \u50976 \u47532 \u54620  \u51088 \u49464 \u47484  \u44054 \u52628 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54981 \u54981  \u46308 \u50612 \u50724 \u45716  \u50756 \u46300 \u47484  \u48169 \u50612 \u54616 \u44592  \u49772 \u50868  \u49345 \u53468 \u51077 \u45768 \u45796 . \u49688 \u48708 \u54617 \u50640 \u49436  \u49707 \u51088  7\u51008  \'91\u51452 \u44592 \'92\u47484  \u46907 \u54633 \u45768 \u45796 . \u51068 \u51452 \u51068 \u51060  7\u51068 \u47196  \u44396 \u49457 \u46108  \u44163 \u52376 \u47100 , \u54616 \u45208 \u51032  \u47785 \u54364 (\u51452 \u44592 )\u44032  \u45804 \u49457 \u46108  \u46244 , \u45908  \u45458 \u51008  \u47785 \u54364 \u50640  \u45208 \u50500 \u44048 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u49324 \u50629 \u50640 \u49436 \u45716  \u51060 \u48120  \u50976 \u47532 \u54620  \u50948 \u52824 \u47484  \u49440 \u51216 \u54620  \u49345 \u54889 \u51077 \u45768 \u45796 . \u45800 \u44592  \u47785 \u54364 \u47484  \u45804 \u49457 \u54616 \u44256 , \u49324 \u50629 \u51012  \u49457 \u51109 \u49884 \u53412 \u45716  \u44284 \u51221 \u50640  \u51080 \u49845 \u45768 \u45796 . \u51060 \u51228  \u45796 \u44032 \u50724 \u45716  \u47928 \u51228 \u46308 \u47564  \u51096  \u52376 \u47532 \u54616 \u47732 , \u49324 \u50629 \u51060  \u50896 \u54876 \u54616 \u44172  \u55128 \u47084 \u44040  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u54633 \u44201 \u50868 \u50640 \u49436 \u46020 , \u44277 \u48512 \u47484  \u48169 \u54644 \u54616 \u45716  \u49345 \u54889 , \u48520 \u50504 \u54620  \u49900 \u47532  \u49345 \u53468  \u46321  \u50668 \u47084  \u47928 \u51228 \u44032  \u49373 \u44592 \u51648 \u47564 , \u54633 \u44201  \u44032 \u45733 \u49457 \u51008  \u44900  \u45458 \u51008  \u49345 \u53468 \u51077 \u45768 \u45796 . \u54596 \u50836 \u54620  \u44277 \u48512 \u47484  \u50612 \u45712 \u51221 \u46020  \u47560 \u52828  \u49345 \u53468 \u51077 \u45768 \u45796 .\u50672 \u50528 \u50868 \u50640 \u49436 \u45716 , \u44049 \u51032  \u50948 \u52824 \u47484  \u49440 \u51216 \u54664 \u44592  \u46412 \u47928 \u50640 , \u46308 \u51060 \u45824 \u45716  \u51077 \u51109 \u51060  \u50500 \u45772  \u45824 \u49884 \u47484  \u48155 \u45716  \u51077 \u51109 \u51076 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u52376 \u47532 \u54644 \u50556  \u54616 \u45716  \u51068 \u46308 \u51060  \u47566 \u51008 \u50756 \u46300 7\u51008  \u54624  \u44172  \u45320 \u47924  \u47566 \u51008  \u49324 \u46988 , \u45236 \u48512 \u51201 \u51004 \u47196  \u44040 \u46321 \u51012  \u44202 \u44256  \u51080 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u54844 \u51088 \u49436  \u52509  6\u44060 \u51032  \u50756 \u46300 \u47484  \u49345 \u45824 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51060  \u50756 \u46300 \u46020  \u52432 \u45236 \u44256 , \u51200  \u50756 \u46300 \u46020  \u52432 \u45236 \u50556  \u54616 \u51424 . \u54624  \u44172  \u47566 \u50500 \u48372 \u51077 \u45768 \u45796 .\u52628 \u44032 \u47196 , \u50612 \u45712  \u50756 \u46300 \u47484  \u47676 \u51200  \u52432 \u45236 \u50556  \u54624 \u51648 \u46020  \u44256 \u48124  \u46121 \u45768 \u45796 . \u51068 \u51032  \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54616 \u44592 \u46020  \u55192 \u46304  \u49345 \u54889 \u51077 \u45768 \u45796 .\u51452 \u48320 \u50640  \u49324 \u46988 \u46308 \u51060  \u50630 \u45716  \u44163 \u51004 \u47196  \u48372 \u50500  \u45224 \u51088 \u45716  \u44060 \u51064 \u51201 \u51064  \u47928 \u51228 \u47196  \u44040 \u46321 \u51012  \u44202 \u44256  \u51080 \u51020 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u50756 \u46300 7\u51008  \u44277 \u51201 \u51064  \u47928 \u51228 \u48372 \u45796  \u49324 \u51201 \u51064  \u47928 \u51228 \u47196  \u44040 \u46321 \u51012  \u44202 \u44256  \u51080 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50756 \u46300 \u45716  \u47784 \u46160  \u45224 \u51088 \u51032  \u44163 \u51077 \u45768 \u45796 . \u50756 \u46300  7\u48264  \u52852 \u46300 \u50640 \u45716  \u45796 \u47480  \u49324 \u46988 \u51060  \u46321 \u51109 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51593 , \u45236 \u44032  \u54665 \u54616 \u45716  \u51068 , \u45236 \u44032  \u45712 \u45180 \u45716  \u44048 \u51221  \u46412 \u47928 \u50640  \u44256 \u44400 \u48516 \u53804 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u51060 \u44163 \u51200 \u44163  \u54644 \u50556  \u54624  \u44172  \u47566 \u50500  \u49373 \u44033 \u51060  \u48373 \u51105 \u54620  \u49324 \u46988 . \u51068 \u46020  \u54644 \u50556  \u46104 \u44256 , \u50672 \u50528 \u46020  \u54644 \u50556  \u46104 \u44256 , \u50868 \u46041 \u46020  \u54644 \u50556  \u46104 \u44256 , \u51060 \u44152  \u54624 \u51648  \u51200 \u44152  \u54624 \u51648  \u47581 \u49444 \u51060 \u44256  \u51080 \u45716  \u49345 \u53468 \u48373 \u51105 \u54620  \u44048 \u51221 \u51060  \u46300 \u45716  \u49324 \u46988 . \u44536  \u49324 \u46988 \u51012  \u49373 \u44033 \u54616 \u47732  \u54868 \u46020  \u45208 \u44256 , \u49836 \u54532 \u44592 \u46020  \u54616 \u44256 , \u44536 \u47532 \u50880 \u46020  \u45712 \u45180 \u44256 \'85 \u54644 \u50556  \u54624  \u51068 \u51060  \u51080 \u50612 , \u50732 \u46972 \u50724 \u45716  \u44048 \u51221 \u51012  \u50613 \u51228 \u54616 \u44592  \u50948 \u54644  \u44256 \u44400 \u48516 \u53804  \u54616 \u45716  \u49324 \u46988 \u44592 \u50613 \u51060  \u46496 \u50724 \u47476 \u45716  \u49324 \u46988 . \u50756 \u46300 \u47484  \u54665 \u46041 , \u44048 \u51221 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u51648 \u47564 , \'91\u44592 \u50613 \'92\u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51088 \u44984  \u44592 \u50613 \u46308 \u51060  \u46496 \u50732 \u46972 \u49436  \u51068 \u50640  \u51665 \u51473 \u54624  \u49688  \u50630 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \'91\u51060 \u48520 \u53413 \'92\u51068  \u49688 \u46020  \u51080 \u44256 , \u54756 \u50612 \u51652  \u50672 \u51064 \u50640  \u45824 \u54620  \u52628 \u50613 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u48169 \u50612 \u54616 \u45716 \u50756 \u46300 7\u51008  \u54788  \u49345 \u53468 \u47484  \u50976 \u51648 \u54616 \u44256  \u49910 \u51008  \u49324 \u46988 \u50640 \u44172 \u49436  \u45208 \u53440 \u45208 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u46160  \u49552 \u51004 \u47196  \u50756 \u46300 \u51032  \u50577  \u45149 \u51012  \u51105 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u45572 \u44400 \u44032 \u47484  \u44277 \u44201 \u54616 \u44592  \u50948 \u54620  \u51088 \u49464 \u44032  \u50500 \u45768 \u46972 , \u48376 \u51064 \u51012  \u48169 \u50612 \u54616 \u44592  \u50948 \u54620  \u51088 \u49464 \u51077 \u45768 \u45796 . \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732 , \u48376 \u51064 \u51032  \u51077 \u51109 \u51060 \u45208  \u51452 \u51109 \u51012  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u51060 \u45716  \u49345 \u54889 , \u44048 \u51221  \u46041 \u50836 \u47484  \u50896 \u52824  \u50506 \u45716  \u49324 \u46988 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  :\u44277 \u48512 \u47484  \u54644 \u50556  \u54616 \u51648 \u47564 , \u51088 \u44984  \u45796 \u47480  \u49373 \u44033 \u51060  \u46308 \u50612  \u51665 \u51473 \u51012  \u47803 \u54616 \u44256  \u51080 \u51020 . \u51665 \u51473 \u54616 \u44592  \u50948 \u54644  \u44256 \u44400 \u48516 \u53804 \u54616 \u45716  \u54617 \u49373 \u51088 \u44984  \u51204  \u50672 \u51064 \u50640  \u44288 \u54620  \u44592 \u50613 \u46308 \u51060  \u46496 \u50724 \u47476 \u44256  \u51080 \u51020 . \u51204  \u50672 \u51064 \u51012  \u51082 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u45716  \u49324 \u46988 \u51452 \u48320 \u50640 \u49436  \u48152 \u45824 \u54616 \u51648 \u47564 , \u45149 \u44620 \u51648  \u49345 \u45824 \u48169 \u50640 \u44172  \u54840 \u44048 \u51012  \u54364 \u54788 \u54616 \u45716  \u49324 \u46988 \u49345 \u45824 \u48169 \u51060  \u48512 \u45812 \u49828 \u47084 \u50868  \u45208 \u47672 \u51648 , \u52384 \u48317 \u51012  \u52824 \u45716  \u49345 \u54889 \u49345 \u45824 \u48169 \u51060  \u45320 \u47924  \u51339 \u51004 \u45208 , \u54000 \u47484  \u45236 \u44256  \u49910 \u51648  \u50506 \u51008  \u47560 \u51020 \u54644 \u49437  3. \u44036 \u49453 \u51060  \u46308 \u50612 \u50724 \u45716 \u50756 \u46300 7\u51008  \u50808 \u48512 \u50640 \u49436  \u44036 \u49453 \u51060  \u46308 \u50612 \u50752  \u49828 \u53944 \u47112 \u49828 \u47484  \u48155 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50756 \u46300  \u54616 \u45208 \u47484  \u45003 \u45003 \u51060  \u44256 \u49688 \u54616 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u50668 \u44592 \u49436  \u45224 \u51088 \u44032  \u46304  \u50756 \u46300 \u45716  \u54665 \u46041 \u51060 \u45208  \u51452 \u51109 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51593 , \u51088 \u49888 \u51032  \u54665 \u46041 \u51060 \u45208  \u51452 \u51109 \u51012  \u44288 \u52384 \u54616 \u45796 \u48372 \u45768  \u51452 \u48320 \u50640 \u49436  \u45130 \u51076 \u50630 \u45716  \u48708 \u54032 , \u48152 \u45824  \u51032 \u44204 \u50640  \u55064 \u47568 \u47536  \u49345 \u54889 \u51077 \u45768 \u45796 .\u50696 \u52968 \u45824  :\u49324 \u50629 \u51060  \u51096  \u51652 \u54665 \u46104 \u44256  \u51080 \u51648 \u47564  \u8594  \u51648 \u51064  \u51473  \u45572 \u44400 \u44032 \u44032  \u50612 \u46523 \u44172  \u54644 \u46972  \u50612 \u46523 \u44172  \u54644 \u46972  \u44036 \u49453 \u54616 \u45716  \u49345 \u54889 \u50672 \u50528 \u46020  \u51096  \u54616 \u44256  \u51080 \u51648 \u47564  \u8594  \u51452 \u48320 \u50640 \u49436  \u49884 \u46020  \u46412 \u46020  \u50630 \u51060  \u50672 \u50528 \u50640  \u52280 \u44204 \u54616 \u45716  \u49345 \u54889 , \u50724 \u45720  \u47952 \u54664 \u45264  \u44536 \u47111 \u44172  \u50672 \u50528 \u54616 \u47732  \u50504 \u46108 \u45796  \u46321 \u46321 \u51452 \u47785  \u48155 \u45716  \u50672 \u50528 \u47484  \u54644 \u49436  \u8594  \u51312 \u50857 \u55176  \u50672 \u50528 \u54624  \u49688  \u50630 \u45716  \u49345 \u54889 \u50756 \u46300 7 \u50672 \u50528 \u50868  \u54644 \u49437 \u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u51339 \u50500 \u54616 \u45716  \u49345 \u45824 \u47484  \u54693 \u54644 \u49436  \u45796 \u44032 \u44032 \u45716  \u49345 \u54889 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51060 \u46412  \u51452 \u48320 \u50640 \u49436  \u44036 \u49453 \u51060  \u46308 \u50612 \u50724 \u44144 \u45208 , \u51656 \u53804 \u49900 \u51012  \u45712 \u45180 \u45716  \'91\u44221 \u51137 \u51088 \'92\u44032  \u49373 \u44600  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564  \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u50976 \u47532 \u54620  \u50948 \u52824 \u50640  \u51080 \u49845 \u45768 \u45796 . \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u44284 \u51032  \u44288 \u44228 \u46020  \u44557 \u51221 \u51201 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51452 \u48320 \u50640 \u49436  \u48152 \u45824  \u51032 \u44204 \u51060  \u46308 \u50612 \u50752 \u46020 , \u45817 \u49888 \u51032  \u51077 \u51109 \u51012  \u52384 \u51200 \u55176  \u44256 \u49688 \u54644 \u50556  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51008  \u44536 \u46308 \u51012  \u52432 \u45236 \u47732 \u49436 \u46020  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51012  \u54693 \u54644  \u45149 \u44620 \u51648  \u45796 \u44032 \u44040  \u44163 \u51077 \u45768 \u45796 .\u49556 \u47196  \u50672 \u50528 \u50868 \u49345 \u54889  : \u51339 \u50500 \u54616 \u45716  \u49345 \u45824 \u47484  \u54693 \u54644 \u49436  \u51649 \u51652 \u54616 \u45716  \u49345 \u53468 \u51109 \u50528 \u47932  : \u44221 \u51137 \u51088 \u44032  \u49373 \u44592 \u44144 \u45208 , \u51452 \u48320 \u50640 \u49436  \u44036 \u49453 \u51060  \u46308 \u50612 \u50732  \u49688 \u46020 \u44208 \u47200  : \u44536 \u47084 \u45208  \u44288 \u44228 \u51032  \u55120 \u47492 \u51008  \u44557 \u51221 \u51201 \u51076  (\u50976 \u47532 \u54620  \u50948 \u52824 \u47484  \u51216 \u47161 )\u50756 \u46300 7\u51060  \u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50728 \u45796 \u47732 , \u53356 \u44172  \u46160  \u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u51060  \u48512 \u45812 \u49828 \u47084 \u50868  \u47560 \u51020 \u51077 \u45768 \u45796 . \'91\u45236  \u50689 \u50669 \u50640  \u44618 \u49689 \u51060  \u46308 \u50612 \u50724 \u51648  \u47568 \u46972 \'92\u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u45817 \u49888 \u51060  \u44536 \u51200  \u47560 \u51020 \u50640  \u50504  \u46308 \u50612 \u49436  \u51068 \u49688 \u46020  \u51080 \u44256 , \'91\u45320 \u47924  \u47566 \u51008  \u51656 \u47928  \u44277 \u49464 \'92\u47484  \u54204 \u52452 \u44592  \u46412 \u47928 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49324 \u49548 \u54620  \u51656 \u47928 \u51060  \u44228 \u49549 \u46104 \u45796  \u48372 \u45768 , \u48376 \u51064 \u51032  \u50689 \u50669 \u51012  \u52840 \u48276 \u45817 \u54664 \u45796 \u45716  \u45712 \u45196 \u51012  \u48155 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u51339 \u50500 \u54616 \u45716  \u44048 \u51221 \u51012  \u50528 \u50024  \u45572 \u47476 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u45817 \u49888 \u44284  \u51096  \u51060 \u50612 \u51648 \u44592  \u50948 \u54644 \u49436  \u44256 \u44400 \u48516 \u53804 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320 \u51032  \u44036 \u49453 , \u45236 \u51201  \u44040 \u46321 \u50640  \u49884 \u45804 \u47532 \u45716  \u49345 \u54889 \u51060 \u51648 \u47564 , \u44536 \u47100 \u50640 \u46020  \u45817 \u49888 \u51012  \u44054 \u44256  \u49910 \u50612  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51012  \u52968 \u53944 \u47204 \u54616 \u44256  \u49910 \u50612 \u54616 \u45716  \u47560 \u51020 \u46020  \u51080 \u50612 \u48372 \u51060 \u47728 , \u51452 \u46020 \u51201 \u51064  \u50672 \u50528 \u47484  \u51339 \u50500 \u54616 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u44032 \u45733 \u49457  1. \u48512 \u45812 \u49828 \u47084 \u50892  \u54616 \u45716  \u47560 \u51020  \u8594  \u45908  \u51060 \u49345  \u44032 \u44620 \u51060  \u50724 \u51648 \u47560  OR \u51656 \u47928 \u51060  \u45320 \u47924  \u47566 \u50500 \u44032 \u45733 \u49457  2. \u51339 \u50500 \u54616 \u45716  \u44048 \u51221 \u51012  \u50528 \u50024  \u45572 \u47476 \u45716  \u47560 \u51020  \u8594  \u51328  \u45908  \u51096  \u54644 \u48372 \u44256  \u49910 \u50612  OR \u45320 \u47484  \u52968 \u53944 \u47204 \u54616 \u44256  \u49910 \u50612 \u52964 \u54540 \u51060 \u46972 \u47732 , \u44288 \u44228 \u51032  \u55120 \u47492 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u51648 \u53412 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u46412 \u47928 \u50640  \'91\u47924 \u50616 \u44032 \u50752  \u49912 \u50864 \u45716 \'92 \u44163 \u52376 \u47100  \u48372 \u51077 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u45817 \u49888 \u44284  \u47564 \u45208 \u44592  \u50948 \u54644  \u50676 \u49900 \u55176  \u46024 \u51012  \u48268 \u44256 , \u44277 \u48512 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u44060 \u51064 \u51201 \u51004 \u47196  \u47566 \u51008  \u45432 \u47141 \u51012  \u44592 \u50872 \u51060 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u51452 \u48320 \u51032  \u44036 \u49453 \u51004 \u47196  \u51064 \u54644  \u49828 \u53944 \u47112 \u49828 \u47484  \u48155 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u51204  \u50528 \u51064 , \u52828 \u44396 \u46308 \u51032  \u51656 \u53804 \u45208  \u48169 \u54644 \u47484  \u48155 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 (\u44536 \u45376 )\u51032  \u51452 \u46108  \u44288 \u49900 \u49324 \u45716  \'91\u51060  \u48169 \u54644 \u44988 \u46308 \u51012  \u50612 \u46523 \u44172  \u52376 \u47532 \u54624 \u44620 \'92\u51077 \u45768 \u45796 .\u52964 \u54540  \u50672 \u50528 \u50868 \u45236  \u50528 \u51064 \u51008  \u51648 \u44552  : \u45817 \u49888 \u51012  \u50948 \u54644  \u44256 \u44400 \u48516 \u53804  \u54616 \u45716  \u51473 \u54788 \u51116  \u49345 \u53468 \u45716  : \u49828 \u53944 \u47112 \u49828 \u47484  \u48155 \u44256  \u51080 \u51004 \u45768  \u55184 \u47553 \u51060  \u54596 \u50836 \u54644  \u48372 \u51076 \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u50756 \u46300 7\u51008  2\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u49345 \u45824 \u48169 \u51008  \u49567 \u50500  \u50724 \u47476 \u45716  \u47560 \u51020 \u51012  \u50613 \u45572 \u47476 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51593  \u45817 \u49888 \u50640 \u44172  \u50672 \u46973 \u51012  \u54616 \u44256  \u49910 \u51648 \u47564 , \u51060 \u47484  \u52280 \u44256  \u45149 \u44620 \u51648  \u48376 \u50629 \u50640  \u51665 \u51473 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u44536  \u51032 \u51648 \u46020  \u44053 \u54616 \u50668 , \u45817 \u48516 \u44036  \u51116 \u54924 \u45716  \u44592 \u45824 \u54616 \u44592  \u55192 \u46317 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u49345 \u45824 \u48169 \u51008  \u50612 \u46500  \u48152 \u45824 \u44032  \u51080 \u50612 \u46020  \u51116 \u54924 \u47484  \u49884 \u46020 \u54616 \u44192 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51452 \u48320 \u50640 \u49436  \u45796 \u49884  \u47564 \u45208 \u51648  \u47560 \u46972 , \u51060 \u48120  \u45149 \u45212  \u44288 \u44228 \u45796  \u46972 \u44256  \u47568 \u54644 \u46020 , \u45149 \u44620 \u51648  \u51116 \u54924 \u47484  \u49884 \u46020 \u54624  \u44163 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51032  \u51032 \u51648 \u47484  \u52384 \u51200 \u55176  \u45796 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50868 \u44032 \u45733 \u49457  1. \u47560 \u51020 \u51012  \u50528 \u50024  \u50613 \u45572 \u47476 \u47728 , \u45817 \u49888 \u51012  \u51082 \u51004 \u47140 \u44256  \u45432 \u47141 \u54616 \u45716  \u49345 \u53468 \u44032 \u45733 \u49457  2. \u50612 \u46500  \u48152 \u45824 \u50640  \u49884 \u45804 \u47140 \u46020  \u51116 \u54924 \u47484  \u45003 \u45003 \u51060  \u49884 \u46020 \u54616 \u47140 \u45716  \u47560 \u51020 \u50756 \u46300 7 \u51649 \u50629 \u50868  \u54644 \u49437 \u49849 \u51652 \u50868 \u51060  \u51080 \u49845 \u45768 \u45796 . \u48512 \u54616  \u51649 \u50896 \u46308 \u51032  \u54588 \u46300 \u48177 , \u48372 \u44256 \u47484  \u48155 \u44256  \u51312 \u50984 \u54616 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \'91\u47924 \u50616 \u44032 \u47484  \u52509 \u44292 \u54616 \u45716  \u50948 \u52824 \'92\u50640  \u50724 \u47476 \u44172  \u46121 \u45768 \u45796 . \u45796 \u47564 , \u49828 \u53944 \u47112 \u49828 \u44032  \u45720 \u50612 \u45216  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49849 \u51652 \u44284  \u45908 \u48520 \u50612  \u50629 \u47924 \u47049 \u51060  \u45720 \u50612 \u45208 \u44256 , \u49888 \u44221  \u50024 \u50556  \u54624  \u50689 \u50669 \u51060  \u45331 \u50612 \u51648 \u44172  \u46121 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u51060 \u51649 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u52964 \u47532 \u50612  \u48156 \u51204 \u51012  \u45432 \u47540  \u49688  \u51080 \u45716  \u44592 \u54924 \u51077 \u45768 \u45796 . \u51060 \u51649 \u51012  \u44428 \u51109 \u54633 \u45768 \u45796 .\u51068 \u51032  \u51201 \u49457 \u50640  \u45824 \u54644  \u51656 \u47928 \u54620  \u44221 \u50864 , \u49324 \u46988 \u51012  \u44288 \u47532 \u54616 \u44144 \u45208  \u47924 \u50616 \u44032 \u47484  \u52509 \u44292 \u54616 \u45716  \u50629 \u47924 \u50640  \u51201 \u54633 \u54633 \u45768 \u45796 . \u50672 \u44396 \u51649 \u50640 \u45716  \u51201 \u51208 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 . \u54617 \u49373 \u51012  \u44032 \u47476 \u52824 \u45716  \u44368 \u49324 \u46020  \u51339 \u49845 \u45768 \u45796 .\u49324 \u50629 \u51032  \u55120 \u47492 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u51060 \u48120  \u50976 \u47532 \u54620  \u54252 \u51648 \u49496 \u50640  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u49324 \u49548 \u54620  \u47928 \u51228 \u45208  \u52980 \u54540 \u47112 \u51064 \u50640  \u49884 \u45804 \u47540  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u49828 \u53944 \u47112 \u49828  \u44288 \u47532 \u50640  \u51665 \u51473 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51648 \u44552  \u49884 \u44592 \u47564  \u51096  \u45336 \u44592 \u47732  \u49324 \u50629 \u51060  \u49849 \u49849 \u51109 \u44396  \u54624  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u48376 \u51064 \u51032  \u49888 \u45392 \u51060 \u45208  \u51452 \u51109 \u51060  \u45320 \u47924  \u50756 \u44256 \u54644 \u49436 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u47803 \u49332 \u44172  \u44404  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47196  \u51064 \u54644  \u49345 \u45824 \u48169 \u46020  \u49345 \u52376  \u48155 \u44256 , \u48376 \u51064 \u46020  \u49828 \u53944 \u47112 \u49828  \u48155 \u45716  \u49345 \u54889 \u51060  \u51068 \u50612 \u45216  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51648 \u44552  \u45817 \u49888 \u50640 \u44172  \u54596 \u50836 \u54620  \u44163 \u51008  \'91\u50997 \u53685 \u49457 \'92\u51077 \u45768 \u45796 . \u50500 \u45768 \u46972 \u44256  \u49373 \u44033 \u54616 \u45716  \u51032 \u44204 \u46020  \u48155 \u50500 \u46308 \u51068  \u51460  \u50508 \u50500 \u50556  \u54633 \u45768 \u45796 .\u50756 \u46300 7\u51012  \u51649 \u50629 \u51201  \u51312 \u50616 \u51004 \u47196  \u54644 \u49437 \u54620 \u45796 \u47732  \'96 Sibyl Tarot\u50756 \u46300 7 \u44552 \u51204 \u50868  \u54644 \u49437 \u45817 \u48516 \u44036  \u46024  \u47928 \u51228 \u47196  \u49828 \u53944 \u47112 \u49828  \u48155 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 . \'91\u51116 \u51221 \u51201 \u51064  \u50948 \u54801 (\u45824 \u52636  \u46321 \u46321 )\'92\u51004 \u47196 \u48512 \u53552  \u48376 \u51064 \u51012  \u51648 \u53412 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u45716  \u44536 \u47548 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51648 \u44552 \u51008  \u46024 \u51012  \u50612 \u46523 \u44172  \u51648 \u53420 \u51648  \u44145 \u51221 \u54644 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u46608 \u54620  \u51312 \u44552 \u51008  \u51060 \u44592 \u51201 \u51004 \u47196  \u45208 \u44040  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51060  \u52313 \u44200 \u50556  \u54624  \u47787 \u51060  \u51080 \u45796 \u47732 , \u51452 \u48320 \u50640 \u49436  \u48708 \u54032 \u51060  \u46308 \u50612 \u50752 \u46020  \u45149 \u44620 \u51648  \u48128 \u50612 \u48537 \u50668 \u50556  \u54633 \u45768 \u45796 . \u44536 \u44172  \u45817 \u49888 \u51060  \u49373 \u44033 \u54616 \u44592 \u50640  \u51221 \u45817 \u54616 \u45796 \u47732  \u47568 \u51060 \u51424 .    \
