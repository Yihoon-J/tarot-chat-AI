   14. \uc0\u51208 \u51228 Temperance \u51208 \u51228  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u50668 \u49888 \u51060  \u52981 \u50640  \u45812 \u44596  \u47932 \u51012  \u51312 \u51208 \u54616 \u45716  \u47784 \u49845 \u51312 \u51208  \u54841 \u51008  \u52572 \u51201 \u54868 \u45208 \u49244  \u49345 \u53468 \u51032  \u54924 \u48373 \u49549 \u46020 \u44032  \u48708 \u49847 \u54620  \u50672 \u50528 , \u51312 \u54868 \u47196 \u50868  \u50672 \u50528 \u50612 \u51109  \u44288 \u47532  \u54841 \u51008  \u49828 \u47540  \u50630 \u45716  \u50672 \u50528 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54632  \u51208 \u51228  \u52852 \u46300 \u51032  \u45656 \u50521 \u49828  \u51060 \u54644 \u54616 \u44592 \u51208 \u51228  \u52852 \u46300 \u45716  \'91\u51208 \u51228 \'92\u48372 \u45796  \u51312 \u51208 \u54616 \u45796 , \u52572 \u51201 \u54868  \u54616 \u45796 \u46972 \u45716  \u46907 \u51060  \u50612 \u50872 \u47549 \u45768 \u45796 .\u51208 \u51228 \u51032  \u50612 \u50896  \u46608 \u54620  \u46972 \u54004 \u50612  temparare\u50640 \u49436  \u50976 \u47000 \u54620  \u44163 \u51004 \u47196 , \u54844 \u54633 \u44284  \u51312 \u51208 \u51060 \u46972 \u45716  \u46907 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47560 \u52824  \u54693 \u49688 \u47484  \u51228 \u51312 \u54616 \u45716  \u44284 \u51221 \u44284  \u48708 \u49847 \u54616 \u45796 \u44256  \u54624 \u44620 \u50836 . \u49345 \u53372  \u45804 \u53092 \u51012  \u51201 \u51208 \u55176  \u49438 \u50612 \u49436  \u47588 \u47141 \u51201 \u51064  \u54693 \u49688 \u47484  \u47564 \u46300 \u45716  \u45712 \u45196 \u51077 \u45768 \u45796 . \u50948 \u53412 \u48177 \u44284 \u50640 \u49436 \u45716  \u53596 \u54168 \u46972 \u47484  \u51060 \u47111 \u44172  \u49444 \u47749 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51208 \u51228  \u52852 \u46300 \u51032  \u51228 \u45824 \u47196  \u46108  \u46907 \u51008  \u45336 \u52824 \u47732  \u45916 \u44256  \u48512 \u51313 \u54616 \u47732  \u52292 \u50868 \u45796 \u51077 \u45768 \u45796 . \u9312  \u45908 \u50892 \u49436  \u50640 \u50612 \u52968 \u51012  \u53412 \u45716  \u51068  \u9313  \u52628 \u50892 \u49436  \u45212 \u48169 \u51012  \u53412 \u45716  \u51068  \u9314  \u44148 \u51312 \u54644 \u49436  \u44032 \u49845 \u44592 \u47484  \u53412 \u45716  \u51068 \u51060  \u51208 \u51228  \u52852 \u46300 \u51032  \u45656 \u50521 \u49828 \u50752  \u46385  \u47582 \u49845 \u45768 \u45796 .\u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44048 \u51221  \u51312 \u51208 \u51012  \u51096  \u54616 \u45716 \u51208 \u51228  \u52852 \u46300 \u45716  \u44048 \u51221  \u51312 \u51208 \u51012  \u51096  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u47932 \u51008  \u49324 \u46988 \u51032  \u44048 \u51221 \u51060 \u45208  \u49373 \u44033  \u46321  \u45576 \u51004 \u47196  \u48372 \u51060 \u51648  \u50506 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50668 \u49888 \u51008  \u52981 \u50640  \u45812 \u44596  \u47932 \u51012  \u51060 \u47532 \u51200 \u47532  \u50734 \u44592 \u47728  \u44512 \u54805 \u51012  \u50976 \u51648 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \u54788 \u49892 \u50640 \u49436  \u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \u51012  \'91\u45336 \u52824 \u44144 \u45208  \u48512 \u51313 \u54616 \u51648  \u50506 \u44172 \'92 \u51312 \u51208 \u54620 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49892 \u51228 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :Q. \u49688 \u54744 \u49373 \u51064 \u45936  \u49688 \u45733 \u50640 \u49436  \u44596 \u51109 \u54624 \u44620 \u48400  \u44145 \u51221 \u51060 \u50640 \u50836 . \u46504 \u51648  \u50506 \u44256  \u49884 \u54744  \u52832  \u49688  \u51080 \u51012 \u44620 \u50836 ?A. \u45348 . \u51201 \u45817 \u54620  \u44596 \u51109 \u44048 \u51004 \u47196  \u49884 \u54744 \u51012  \u52824 \u44172  \u46121 \u45768 \u45796 . (\u54633 \u44201  \u50668 \u48512 \u45716  \u50508  \u49688  \u50630 \u51020 )Q. \u54756 \u50612 \u51652 \u51648  \u50620 \u47560  \u50504 \u46104 \u50632 \u45716 \u45936 , \u44536  \u49324 \u46988 \u51008  \u51648 \u44552  \u50612 \u46500  \u49345 \u54889 \u51068 \u44620 \u50836 ?A. \u49836 \u54532 \u51648 \u47564  \u44536 \u47000 \u46020  \u47925 \u47925 \u55176  \u51060 \u44200 \u45236 \u45716  \u51473 \u51077 \u45768 \u45796 . \u44048 \u51221 \u51012  \u52264 \u48516 \u55176  \u51312 \u51208 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .Q. \u45208 \u54620 \u53580  \u48516 \u47749  \u54840 \u44048  \u51080 \u45716  \u44163  \u44057 \u51008 \u45936 , \u50780  \u54364 \u54788 \u51012  \u50504  \u54624 \u44620 \u50836 ?A. \u45817 \u49888 \u51060  \u48512 \u45812 \u49828 \u47084 \u50892  \u54624 \u44620 \u48400  \u54364 \u54788 \u51012  \u51088 \u51228 \u54616 \u45716  \u51473 \u51077 \u45768 \u45796 .\u47564 \u50557  \u49549 \u47560 \u51020 \u51012  \u51656 \u47928 \u54664 \u45796 \u47732 , \'91\u51201 \u45817 \u54620  \u54840 \u44048 , \u51201 \u45817 \u54620  \u54840 \u51032 \'92 \u51221 \u46020 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u45208 \u49244  \u49345 \u53468 \u44032  \u54924 \u48373 \u46104 \u45716 \u51208 \u51228  \u52852 \u46300 \u45716  \u44288 \u44228 \u51032  \u51221 \u49345 \u54868 , \u45208 \u49244  \u49345 \u53468 \u44032  \u54924 \u48373 \u46108 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51068 \u49884 \u51201 \u51004 \u47196  \u45208 \u48756 \u45912  \u44163 \u46308 \u51060  \u50896 \u47000  \u51088 \u47532 \u47196  \u46104 \u46028 \u50500 \u44036 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u51088 \u47732  :\u51104 \u49884  \u49436 \u47673 \u54664 \u45912  \u44288 \u44228 \u44032  \u54924 \u48373 \u46108 \u45796  (\u50724 \u47019 \u46041 \u50504  \u49436 \u47673 \u54664 \u45796 \u47732  \u54644 \u45817 \u54616 \u51648  \u50506 \u51020 )\u50500 \u54536  \u47800 \u51060  \u45227 \u44172  \u46108 \u45796  (\u50724 \u47019 \u46041 \u50504  \u50500 \u54048 \u45796 \u47732  \u54924 \u48373 \u46104 \u51648  \u50506 \u51020 )\u54887 \u44608 \u50640  \u49912 \u50892 \u49436  \u54756 \u50612 \u51276 \u45796 \u47732 , \u44263  \u45796 \u49884  \u47564 \u45208 \u44172  \u46108 \u45796 \u50668 \u44592 \u49436  \u51473 \u50836 \u54620  \u44148 , \u45208 \u49244  \u49345 \u53468 \u44032  \'91\u51068 \u49884 \u51201 \'92\u51060 \u50612 \u50556  \u54620 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u50896 \u47000 \u48512 \u53552  \u49324 \u51060 \u44032  \u50504  \u51339 \u50520 \u44144 \u45208 , \u50896 \u47000 \u48512 \u53552  \u44148 \u44053 \u51060  \u45208 \u48756 \u45796 \u47732  \u54924 \u48373 \u46104 \u51648  \u50506 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u49549 \u46020 \u44032  \u48708 \u49847 \u54620  \u50672 \u50528 \u51208 \u51228  \u52852 \u46300 \u45716  \u49549 \u46020 \u44032  \u48708 \u49847 \u54620  \u50672 \u50528 , \u48708 \u49847 \u54620  \u49549 \u46020 \u47196  \u49373 \u44592 \u45716  \u54840 \u44048 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u50724 \u47560 \u51060 \u44152 \u51032  \'91\u54620  \u48156 \u51677  \u46160  \u48156 \u51677 \'92\u51060 \u46972 \u45716  \u45432 \u47000 \u44032  \u51060  \u49345 \u54889 \u51012  \u51096  \u54364 \u54788 \u54644 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 .\u45348 \u44032  \u54620  \u48156 \u51677 , \u46160  \u48156 \u51677  \u47680 \u50612 \u51648 \u47732 \u45212  \u49464  \u48156 \u51677  \u45796 \u44032 \u44040 \u44172 \u50864 \u47532 \u51032  \u44144 \u47532 \u44032  \u45908  \u51060 \u49345  \u47680 \u50612 \u51648 \u51648  \u50506 \u44172 \u45348 \u44032  \u54620  \u48156 \u51677 , \u46160  \u48156 \u51677  \u45796 \u44032 \u50724 \u47732 \u45212  \u44536 \u45824 \u47196  \u49436  \u51080 \u51012 \u44172 \u50864 \u47532 \u51032  \u49324 \u46993 \u51060  \u48736 \u47476 \u44172  \u45712 \u44788 \u51648 \u51648  \u50506 \u44172 \u54620  \u48156 \u51677  \u46160  \u48156 \u51677  \'96 \u50724 \u47560 \u51060 \u44152 \u44396 \u52404 \u51201 \u51004 \u47196 , \u54620 \u51901 \u51060  \u49828 \u53416 \u49901  \u51652 \u46020 \u47484  \u45320 \u47924  \u50526 \u49436 \u45208 \u44032 \u44144 \u45208 , \u48512 \u45812 \u49828 \u47101 \u44172  \u54616 \u51648  \u50506 \u45716 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u53945 \u55176  \u52964 \u54540 \u50640 \u44176  \u54788 \u51116  \u54028 \u53944 \u45320 \u44032  \u52572 \u44256 \u51076 \u51012  \u47568 \u54644 \u51452 \u47728 , \u44208 \u54844 \u54644 \u46020  \u51339 \u51008  \u44288 \u44228 \u51077 \u45768 \u45796 . \u49436 \u47196  \u47582 \u52656 \u44032 \u47728  \u44288 \u44228 \u47484  \u50504 \u51221 \u51201 \u51004 \u47196  \u47564 \u46308  \u49688  \u51080 \u44172  \u46121 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50612 \u51109 \u44288 \u47532  \u51473 \u51064 \u46412 \u50640  \u46384 \u46972  \u51208 \u51228  \u52852 \u46300 \u45716  \u50612 \u51109 \u44288 \u47532 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u50668 \u47084  \u49324 \u46988 \u44284  \u51201 \u45817 \u54620  \u44144 \u47532 \u47564 \u51012  \u50976 \u51648 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u46308 \u51060 \u45824 \u47732  \u47680 \u50612 \u51648 \u44256 , \u47680 \u50612 \u51648 \u47732  \u45796 \u44032 \u44032 \u44256 . \u48708 \u49847 \u54620  \u50896 \u47532 \u47196 , \u48128 \u45817 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50584 \u46020  \u44316 \u52270 \u44256  \u51172 \u46020  \u44316 \u52270 \u45348 . \u51201 \u45817 \u55176  \u44036 \u47564  \u48400 \u50556 \u44192 \u45796 .\u54644 \u49437  2. \u44277 \u44284 \u49324  \u44396 \u48516 \u51060  \u52384 \u51200 \u54620 \u44277 \u44284 \u49324  \u44396 \u48516 \u51060  \u52384 \u51200 \u54644 \u49436 , \u54840 \u44048  \u50619 \u44592  \u55192 \u46304  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52964 \u54540 \u51060  \u47564 \u46308 \u50612 \u51648 \u44592  \u50948 \u54644 \u49440 , \u44277 \u51201 \u51064  \u44288 \u44228 \u51032  \u49440 \u51012  \u45336 \u50612 \u50556  \u54633 \u45768 \u45796 . \u45800 \u49692 \u55176  \u44512 \u54805 \u51012  \u50976 \u51648 \u54616 \u47728  \u44036 \u47564  \u48372 \u45716  \u49345 \u54889 \u51012  \u45336 \u50612 \u49436 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u54616 \u51648 \u47564  \u51208 \u51228 \u45716  \u49440  \u45336 \u45716  \u49692 \u44036 \u51012  \u47561 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u49696 \u51012  \u47560 \u49884 \u44256  \u49892 \u49688 \u54616 \u44144 \u45208 , \u54887 \u44608 \u50640  \u44256 \u48177 \u51012  \u54644 \u48260 \u47532 \u45716  \u54665 \u46041 \u51012  \u51088 \u51228 \u54616 \u44172  \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50732  \u44221 \u50864 \u50644 , \u50500 \u51649  \u49440 \u51012  \u45336 \u50612 \u49436  \u45796 \u44032 \u44040  \u49373 \u44033 \u51060  \u50630 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u49440 \u51012  \u45336 \u50612 \u50556  \u52964 \u54540 \u51060  \u47564 \u46308 \u50612 \u51665 \u45768 \u45796 .\u54644 \u49437  3. \u49828 \u47540 , \u48156 \u51204 \u51060  \u50630 \u45716 \u49828 \u47540 \u51060 \u45208  \u48156 \u51204  \u50630 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u51676 \u47551 \u54616 \u44144 \u45208  \u55141 \u48516 \u46104 \u45716  \u49444 \u47128 \u51012  \u45712 \u45180 \u44592  \u55192 \u46308 \u45796 \u45716  \u46907 \u51060 \u44256 , \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49444 \u47128 \u51012  \u45712 \u45180 \u44592  \u55192 \u46308 \u44144 \u45208  \u45817 \u49888 \u51060  \u51116 \u48120  \u50630 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u48156 \u51204 \u51060  \u50630 \u45796 \u45716  \u51032 \u48120 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u49457 \u44277 \u51008  \u47785 \u54364 \u50640  \u45824 \u54620  \u51665 \u52265 , \u47792 \u51077 \u51012  \u54596 \u50836 \u47196  \u54616 \u51648 \u47564 , \u51208 \u51228 \u45716  \u47924 \u50616 \u44032 \u50640  \u45824 \u54620  \'91\u51665 \u52265 , \u47792 \u51077 \'92\u48372 \u45796  \'91\u50892 \u46972 \u48184 \'92\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u51208 \u51228  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44048 \u51221  \u51312 \u51208  \u45733 \u47141 \u51060  \u46832 \u50612 \u45208 \u47728 , \u54200 \u50504 \u54620  \u44288 \u44228 \u47484  \u51339 \u50500 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u45320 \u47924  \u48744 \u47532  \u52828 \u54644 \u51648 \u44144 \u45208 , \u51652 \u46020 \u44032  \u48736 \u47480  \u50672 \u50528 \u45716  \u51339 \u50500 \u54616 \u51648  \u50506 \u51004 \u48064 \u47196 \'85 \u49345 \u45824 \u48169 \u51012  \u45813 \u45813 \u54616 \u44172  \u54616 \u44144 \u45208 , \'91\u50612 \u51109 \u44288 \u47532 \'92\u47196  \u50724 \u54644 \u54616 \u44172 \u45140  \u47564 \u46308  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u53440 \u47196  \u51208 \u51228 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45852 \u47732 , \u53356 \u44172  3\u44032 \u51648 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51012  \u51096  \u48372 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 .\u51312 \u49900 \u49828 \u47084 \u50880  : \u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u51012  \u44032 \u51648 \u44256  \u51080 \u51004 \u45208 , \u48512 \u45812  \u51452 \u44592  \u49899 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u49443 \u48512 \u47476 \u44172  \u54665 \u46041 \u54644 \u49436  \u44288 \u44228 \u44032  \u47581 \u44032 \u51648 \u45716  \u44163 \u51012  \u50896 \u52824  \u50506 \u49845 \u45768 \u45796 . \u52380 \u52380 \u55176  \u45796 \u44032 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 .\u44032 \u48316 \u50868  \u54840 \u44048  : \u50500 \u51649  \u45817 \u49888 \u51012  \u49324 \u46993 \u54616 \u44256  \u51080 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 . \'91\u44032 \u48316 \u50868  \u54840 \u44048 \'92 \u51221 \u46020 \u47564  \u45712 \u45180 \u44256  \u51080 \u51004 \u47728 , \u45817 \u49888 \u51060  \u50724 \u54644 \u54616 \u51648  \u50506 \u46020 \u47197  \u54364 \u54788 \u51012  \u51088 \u51228 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51648 \u44552  \u49324 \u51060 \u44032  \u46385  \u51201 \u45817 \u54616 \u45796  \u45712 \u45180 \u44256  \u51080 \u51004 \u47728 , \u44403 \u51060  \u47680 \u50612 \u51648 \u44144 \u45208 , \u45796 \u44032 \u44040  \u49373 \u44033 \u51060  \u50630 \u49845 \u45768 \u45796 .\u50612 \u51109 \u44288 \u47532  : \u47568  \u44536 \u45824 \u47196  \u50612 \u51109 \u44288 \u47532  \u51473 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060  \u49324 \u46988  \u51200  \u49324 \u46988  \u48708 \u44368 \u54616 \u47728  \u45796 \u44032 \u44040 \u51648  \u47568 \u51648  \u44256 \u48124 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u44208 \u54844 \u51012  \u49373 \u44033 \u54624  \u47564 \u53372  \u50504 \u51221 \u51201 \u51064  \u44288 \u44228 \u51076 \u51012  \u46907 \u54633 \u45768 \u45796 . \u49436 \u47196  \u48176 \u47140 \u54616 \u47728 , \u49457 \u49689 \u54620  \u44288 \u44228 \u47484  \u47564 \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50672 \u50528  \u49549 \u46020 \u46020  \u47582 \u49845 \u45768 \u45796 . \u46608  \u47196 \u47592 \u54001 \u54624  \u46416  \u47196 \u47592 \u54001 \u54616 \u44256 , \u51068 \u50640  \u51665 \u51473 \u54624  \u46416  \u51068 \u50640  \u51665 \u51473 \u54616 \u45716  \u46319  \u48372 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51032  \u49324 \u46993 \u51008  \u54788 \u51116  \u51652 \u54665 \u54805  \u51077 \u45768 \u45796 . \u47688 \u52628 \u44144 \u45208  \u49885 \u51648  \u50506 \u44256  \u44984 \u51456 \u54633 \u45768 \u45796 . \u54788 \u51116  \u54028 \u53944 \u45320 \u44032  \'91\u51060 \u49345 \u54805 \'92\u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51312 \u47564 \u44036  \u44428 \u53468 \u44592 \u44032  \u50732  \u49688 \u46020  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u44288 \u44228 \u50640  \u49828 \u47540 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u49912 \u50880 \u51004 \u47196  \u51064 \u54644  \u51104 \u49884  \u54756 \u50612 \u51652  \u49345 \u53468 \u46972 \u47732  \u45796 \u49884  \u47564 \u45216  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u50526 \u49436  \u47568 \u54664 \u46319  \'91\u51221 \u49345 \u54868 , \u54924 \u48373 \'92\u51060 \u46972 \u45716  \u51032 \u48120 \u47484  \u44054 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u47484  \u52380 \u52380 \u55176  \u49884 \u46020 \u54616 \u47140  \u54633 \u45768 \u45796 . \u44536 \u47084 \u45208  \u50724 \u47000  \u51204 \u50640  \u54756 \u50612 \u51276 \u45796 \u47732  \u51116 \u54924 \u45716  \u50612 \u47157 \u49845 \u45768 \u45796 . \u51060  \u44221 \u50864 \u50644  \u49436 \u47196  \u46504 \u50612 \u51256  \u51080 \u45716  \u49345 \u53468 \u44032  \'91\u51221 \u49345 \'92\u51060 \u44256  \'91\u50896 \u47000  \u49345 \u53468 \'92\u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51060 \u48324  \u54980  \u49345 \u45824 \u48169 \u51008  \'91\u54200 \u50504 \u54632 \'92\u51012  \u45712 \u45180 \u44256  \u51080 \u51004 \u47728  \u44403 \u51060  \u51116 \u54924 \u47484  \u49884 \u46020 \u54624  \u47560 \u51020 \u51060  \u50630 \u49845 \u45768 \u45796 .\u51208 \u51228  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u51648 \u44552 \u51008  \u48320 \u54868 \u48372 \u45796  \u50504 \u51221 \u50640  \u51665 \u51473 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51060 \u51649 \u51060 \u45208  \u49352 \u47196 \u50868  \u49324 \u50629 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732  \u48372 \u47448 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51648 \u44552 \u51008  \u44221 \u47141 \u51012  \u49939 \u44592 \u50640  \u51339 \u51008  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u44256  \u48260 \u54000 \u49464 \u50836 . \u54665 \u50868 \u51060  \u51312 \u50857 \u55176  \u52286 \u50500 \u50741 \u45768 \u45796 . \u51109 \u44592 \u51204 \u51012  \u54204 \u52824 \u47732  \u51339 \u51008  \u49457 \u44284 \u47484  \u44144 \u46168  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u50640 \u49436 \u45716 , \u51216 \u49688 \u44032  \u49892 \u47141 \u45824 \u47196  \u45208 \u50741 \u45768 \u45796 . \u44284 \u46020 \u54620  \u44592 \u45824 \u45208  \u48520 \u50504 \u51008  \u48260 \u47532 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 . \u53945 \u55176  \u47732 \u51217 \u51060 \u45208  \u49884 \u54744 \u51012  \u46504 \u51648  \u50506 \u44256  \u48372 \u44172  \u46121 \u45768 \u45796 . \u54217 \u49548  \u44596 \u51109 \u51012  \u47566 \u51060  \u54664 \u45796 \u47732  \u55148 \u49548 \u49885 \u51060 \u45348 \u50836 .\u51208 \u51228  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u46024 \u51032  \u55120 \u47492 \u51060  \u44984 \u51456 \u54633 \u45768 \u45796 . \u49688 \u51077  \u51648 \u52636  \u47784 \u46160  \u45130 \u51060 \u51648  \u50506 \u51012  \u50696 \u51221 \u51077 \u45768 \u45796 . \u53945 \u55176  \u53804 \u51088 \u45716 , \u50724 \u47000  \u51648 \u49549 \u54624  \u44221 \u50864  \u51339 \u51008  \u49457 \u44284 \u44032  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44049 \u51088 \u44592  \u45824 \u48149 \u51012  \u52824 \u44144 \u45208  \u51901 \u48149 \u51012  \u52264 \u45716  \u51068 \u51008  \u50630 \u49845 \u45768 \u45796 . \u45817 \u51109  \u46024 \u51060  \u54596 \u50836 \u54616 \u44144 \u45208 , \u49457 \u44201 \u51060  \u44553 \u54620  \u49324 \u46988 \u51068  \u44221 \u50864  \u45813 \u45813 \u54616 \u44172  \u45712 \u44788 \u51656  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u44284 \u44048 \u54616 \u44172  \u49884 \u46020 \u54616 \u49464 \u50836 \u47924 \u50616 \u44032  \u51221 \u52404  \u46104 \u50612  \u51080 \u44256 , \u48156 \u51204 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u49373 \u44033 \u51060  \u46300 \u49884 \u45208 \u50836 ? \u44536 \u47111 \u45796 \u47732  \u44284 \u44048 \u54616 \u44172  \u49884 \u46020 \u54644 \u50556  \u54624  \u46412 \u51077 \u45768 \u45796 .\u54217 \u49548 \u50752  \u45796 \u47480  \u48516 \u50948 \u44592 , \u54217 \u49548 \u50752  \u45796 \u47480  \u47568 \u53804 \u47196  \u51217 \u44540 \u54644 \u48372 \u49464 \u50836 . \u49353 \u45796 \u47480  \u45936 \u51060 \u53944  \u53076 \u49828 \u46020  \u51339 \u49845 \u45768 \u45796 . \u44288 \u44228 \u50640  \u51088 \u44537 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51088 \u52843  \u51648 \u47336 \u54620  \u49324 \u51060 \u44032  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50640 \u49436 \u46020 , \u47952 \u46304  \'93\u51201 \u45817 \u55176 \'94 \u46972 \u45716  \u53468 \u46020 \u47484  \u52712 \u54616 \u44256  \u51080 \u51648 \u45716  \u50506 \u51008 \u51648 \u50836 . \u51652 \u51221 \u51004 \u47196  \u48156 \u51204 \u54616 \u44256  \u49910 \u45796 \u47732 , \'93\u51201 \u45817 \u54620  \u49345 \u53468 \'94\u47484  \u46832 \u50612 \u45336 \u50612 \u50556  \u54633 \u45768 \u45796 . \u44284 \u44048 \u54616 \u44172  \u48128 \u50612 \u48537 \u51060 \u49464 \u50836 .\u51312 \u50616  2. \u49888 \u51473 \u54616 \u44172  \u51312 \u51208 \u54616 \u49464 \u50836 \u52572 \u44540  \u45320 \u47924  \u52649 \u46041 \u51201 \u51004 \u47196  \u44208 \u51221 \u54616 \u44256  \u51080 \u45796 \u47732  \'93\u49888 \u51473 \u54616 \u44172  \u51312 \u51208 \u54616 \u46972 \'94\u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u47560 \u51020 \u50640  \u46300 \u45716  \u49345 \u45824 \u47484  \u49324 \u47196 \u51105 \u44256  \u49910 \u45796 \u47732 , \'93\u51201 \u45817 \u54620  \u48128 \u45817 \'94\u51008  \u54596 \u49688 \u51077 \u45768 \u45796 .    \
