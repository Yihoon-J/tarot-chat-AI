   \uc0\u52981  \u44592 \u49324  \u51060 \u48120 \u51648 \u52981  \u44592 \u49324  \'96 \u48512 \u46300 \u47084 \u50868  \u51228 \u50504  \u48177 \u47560 \u47484  \u53444  \u44592 \u49324 \u44032  \u52981 \u51012  \u48512 \u46300 \u47101 \u44172  \u44148 \u45348 \u45716  \u47784 \u49845 \u50672 \u46973 \u50868 \u51060  \u51080 \u45716  \u47700 \u49888 \u51200  \u52852 \u46300  (\u51228 \u50504 , \u49548 \u49885 )\u49345 \u45285 \u54616 \u44256  \u54801 \u51312 \u51201 \u51064  \u49324 \u46988 \u49373 \u44033 \u51060  \u47566 \u51008  \u49324 \u46988 \u48148 \u46988 \u44592 \u44032  \u51080 \u44144 \u45208  \u44032 \u48316 \u50868  \u47564 \u45224 \u51012  \u51600 \u44596 \u45796 \u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u51060 \u48120 \u51648  \u49345 \u51669 \u48177 \u47560  : \u49692 \u49688 \u54632 \u52981  : \u44048 \u51221 , \u47560 \u51020 , \u49324 \u46993 \u45216 \u44060  \u45804 \u47536  \u53804 \u44396  : \u51204 \u47161 \u51032  \u49888  \u54756 \u47476 \u47700 \u49828 \u44053  : \u49324 \u46988 \u51032  \u44048 \u51221 , \u54413 \u48512 \u54620  \u44048 \u49688 \u49457 \u48148 \u50948  \u49328  : \u54744 \u45212 \u54632 , \u50669 \u44221 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50672 \u46973 \u51012  \u48155 \u45716 \u52981  \u44592 \u49324 \u45716  \u50672 \u46973 \u50868 \u51060  \u52572 \u44256 \u51064  \u52852 \u46300  \u51473  \u54616 \u45208 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44256 \u48177  \u54841 \u51008  \u51116 \u54924  \u50672 \u46973 \u51077 \u45768 \u45796 .\u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732  : \u53804 \u44396 \u50640  \u45804 \u47140  \u51080 \u45716  \u51109 \u49885 \u51008  \u54756 \u47476 \u47700 \u49828  \u45216 \u44060 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u54756 \u47476 \u47700 \u49828 \u45716  \u51333 \u51333  \'91\u47700 \u49888 \u51200 \'92\u47484  \u45208 \u53440 \u45236 \u45716 \u45936 \u50836 . \u51060 \u45716  \u54756 \u47476 \u47700 \u49828 \u44032  \'91\u49548 \u49885 \u51060 \u45208  \u46907 \u51012  \u51204 \u45804 \u54616 \u45716  \u51204 \u47161 \u51032  \u49888 \'92 \u51060 \u50632 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u52981 \u51012  \u46304  \u44592 \u49324 \u45716  \u49884 \u49440 \u51012  \u51221 \u47732 \u50640  \u46160 \u44256 , \u49345 \u45824 \u48169 \u50640 \u44172  \u47924 \u50616 \u44032  \u51228 \u50504 \u54616 \u46319  \u48512 \u46300 \u47101 \u44172  \u51217 \u44540 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u46412  \u44592 \u49324 \u44032  \u46304  \'91\u52981 \'92\u51008  \u47560 \u51020 \u51060 \u45208  \u44048 \u51221 \u51012  \u51032 \u48120 \u54616 \u51424 . \u51060 \u47484  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u45236  \u47560 \u51020 \u51008  \u51060 \u47088 \u45936 , \u45348  \u47560 \u51020 \u51008  \u50612 \u46523 \u45768 ?\u45236  \u49373 \u44033 \u51008  \u51060 \u47088 \u45936 , \u45348  \u49373 \u44033 \u51060  \u46307 \u44256  \u49910 \u50612 \u50864 \u47532  \u54632 \u44760  ~~\u47484  \u54644 \u48372 \u45716  \u44172  \u50612 \u46504 \u44620 ?\u47568 \u51060  \u51200 \u46028 \u51201 \u51004 \u47196  \u45804 \u47532 \u51648  \u50506 \u44256 , \u44152 \u50612 \u44032 \u44256  \u51080 \u45796 \u45716  \u51216 \u50640 \u49436  \u45324 \u51648 \u49884  \u51228 \u50504  \u54616 \u44144 \u45208  \u49836 \u51789  \u46496 \u48372 \u45716  \u45656 \u50521 \u49828 \u44032  \u45712 \u44788 \u51665 \u45768 \u45796 . \u44592 \u49324 \u46020  \u48176 \u44861 \u50640  \u49552 \u51012  \u50617 \u44256  \u51221 \u51473 \u54620  \u53468 \u46020 \u47484  \u52712 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u49345 \u45285 \u54616 \u44256  \u54801 \u51312 \u51201 \u51064 \u52981  \u44592 \u49324 \u45716  \u49345 \u45285 \u54616 \u44256  \u54801 \u51312 \u51201 \u51064  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u48512 \u46300 \u47101 \u44256  \u46384 \u46907 \u54620  \u53468 \u46020  \u53459 \u50640 , \u51060 \u49457 \u50640 \u44172 \u46020  \u51064 \u44592 \u44032  \u47566 \u49845 \u45768 \u45796 .\u51060 \u48120 \u51648 \u47484  \u48372 \u47732  : \u52852 \u46300  \u49549 \u51032  \u48177 \u47560 \u45716  \'91\u49692 \u49688 \u54632 \'92\u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u44592 \u49324 \u51032  \u44277 \u49552 \u54620  \u53468 \u46020 \u46020  \u51452 \u47785 \u54624  \u47564  \u54633 \u45768 \u45796 . \u50696 \u51032  \u48148 \u47476 \u51648 \u47564 , \u44413 \u49888 \u44144 \u47532 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 . \u51593  \u51088 \u49888 \u47564 \u51032  \u49888 \u45392 \u51012  \u45236 \u49464 \u50872  \u51460 \u46020  \u50500 \u45716  \u49324 \u46988 \u51060 \u51424 .\u44277 \u49552 \u54616 \u51648 \u47564 , \u47564 \u47564 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796  \u50696 \u52968 \u45824  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u51012  \u46020 \u50752 \u51452 \u44144 \u45208  \u44201 \u47140 \u54644 \u51452 \u44256  \u49910 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u48176 \u47140 \u54616 \u44256  \u49910 \u51008  \u47560 \u51020 \u46020  \u48372 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \'91\u51088 \u49888 \u44284  \u46041 \u46321 \u54620  \u44288 \u44228 \'92\u47196  \u51064 \u49885 \u54616 \u44256  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u51064 \u44036 \u44288 \u44228 \u44032  \u50896 \u54876 \u55176  \u54400 \u47532 \u44172  \u46121 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u51452 \u48320  \u49324 \u46988 \u46308 \u51060  \u54801 \u51312 \u51201 \u51004 \u47196  \u45208 \u50724 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u45796 \u47564  \u49345 \u45285 \u54620  \u49457 \u44201  \u53459 \u50640 , \u51088 \u52843  \u49552 \u54644 \u47484  \u51077 \u44144 \u45208  \u51068 \u51012  \u44536 \u47476 \u52824 \u45716  \u44221 \u50864 \u46020  \u45796 \u48152 \u49324 \u51077 \u45768 \u45796 . \u45224 \u51032  \u44592 \u48516 \u51012  \u49373 \u44033 \u54616 \u45716  \u44163 \u46020  \u51339 \u51648 \u47564 , \u48376 \u51064 \u51032  \u47560 \u51020 \u46020  \u52313 \u44200 \u50556  \u54620 \u45796 \u45716  \u51312 \u50616 \u51216 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u49373 \u44033 \u51060  \u47566 \u51008 \u45208 \u51060 \u53944  \u52981 \u51008  \u54217 \u49548  \u49373 \u44033 \u51060  \u47566 \u51008  \u49324 \u46988 , \u54841 \u51008  \u45572 \u44400 \u44032 \u47484  \u44618 \u44172  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \'91\u44618 \u51008  \u49373 \u44033 \'92\u51012  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u45716  \u47784 \u46160  \u51221 \u51201 \u51077 \u45768 \u45796 . \u50668 \u49324 \u51228 , \u51008 \u46164 \u51088 , \u50756 \u46300 2, \u50756 \u46300 3 \u47784 \u46160  \u47688 \u52656 \u51080 \u49845 \u45768 \u45796 . \'91\u49373 \u44033 \u54620  \u46244 \u50640  \u50880 \u51649 \u51060 \u44192 \u45796 \'92\u45716  \u51032 \u48120 \u51424 . \u52981  \u44592 \u49324 \u46020  \u47560 \u52268 \u44032 \u51648 \u47196  \u45712 \u47551 \u45712 \u47551  \u44152 \u50612 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \'91\u46244 \u50640 \u49436  \u51204 \u47029 \u51012  \u51676 \u47732 \u49436  \u54665 \u46041 \u54616 \u44192 \u45796 , \u44256 \u48124 \u54616 \u44256  \u51080 \u45796 , \u49443 \u48520 \u47532  \u54665 \u46041 \u54616 \u51648  \u50506 \u44192 \u45796 \'92\u45716  \u45656 \u50521 \u49828 \u44032  \u44053 \u54633 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u49373 \u44033 \u51060  \u45320 \u47924  \u47566 \u50500 \u49436  \u51201 \u44537 \u49457 \u51060  \u48512 \u51313 \u54620  \u49324 \u46988 \u45796 \u44032 \u49436 \u44592 \u47484  \u51452 \u51200 \u54624  \u49688 \u46020  \u51080 \u51020 \u54637 \u49345  \u50880 \u51649 \u51060 \u44592  \u51204 \u50640  \u49373 \u44033 \u54616 \u45716  \u49828 \u53440 \u51068 \u51060 \u46972  \u51652 \u51648 \u54616 \u44256  \u49888 \u51473 \u54620  \u49457 \u44201 \u46412 \u47196 \u45716  \u44228 \u49328 \u51201 \u51060 \u44144 \u45208  \u50573 \u49341 \u54616 \u44172  \u48372 \u51068  \u49688  \u51080 \u51020 \u50864 \u50976 \u48512 \u45800 \u54632 \u51012  \u44221 \u44256 \u54616 \u44592 \u46020  \u54632 \u44536 \u47111 \u51648 \u47564 , \u45208 \u51060 \u53944 (Knight)\u45716  \u54665 \u46041 \u47141 \u51060  \u51080 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u47560 \u45285  \u47688 \u52656 \u51080 \u51648 \u47564 \u51008  \u50506 \u51012  \u44163 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48148 \u46988 \u44592 \u44032  \u51080 \u45716 \u8251  \u47784 \u46160 \u44032  \u46041 \u51032 \u54616 \u45716  \u54644 \u49437 \u51008  \u50500 \u45785 \u45768 \u45796 . \u51060 \u45716  \u44060 \u51064 \u51201 \u51004 \u47196  \u46356 \u49884 \u45208  \u54644 \u50808  \u54252 \u47100  \u46321  \u51076 \u49345  \u54980 \u44592  + \u51060 \u48120 \u51648  \u48516 \u49437 \u51012  \u53685 \u54644  \u46020 \u52636 \u54620  \u54644 \u49437 \u51077 \u45768 \u45796 . \u51068 \u48512 \u45716  \u44152 \u47084  \u46308 \u51004 \u49492 \u46020  \u51339 \u49845 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u47932 \u51008  \u49324 \u46988 \u51032  \u44048 \u51221 \u51060 \u45208  \u47560 \u51020 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u52981 \u51008  \u44536  \u47560 \u51020 \u51012  \u45812 \u45716  \u44536 \u47495 \u51060 \u51424 . \u51593  \u52981 \u51060  \u44032 \u46301  \u52264 \u51080 \u45796 \u47732  \u47560 \u51020 \u51060 \u45208  \u44048 \u51221 \u51060  \u54413 \u48512 \u54616 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u44256  \u48708 \u50612  \u51080 \u45796 \u47732  \u44536 \u47564 \u53372  \u47560 \u51020 \u51060 \u45208  \u44048 \u51221 \u51060  \u51201 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u44536 \u47084 \u45208  \u44592 \u49324 \u44032  \u46304  \u52981 \u51008  \u48708 \u50612 \u51080 \u45716 \u51648 , \u52264 \u51080 \u45716 \u51648  \u48520 \u48516 \u47749 \u54633 \u45768 \u45796 . \u51593  \u51060 \u45716  \u44048 \u51221 \u51060  \u54413 \u48512 \u54616 \u51648  \u50506 \u51008  \u49345 \u53468 \u50640 \u49436  \u45796 \u44032 \u44032 \u45716  \u49345 \u54889 , \u46608  \u45796 \u49884  \u49352 \u47196 \u50868  \u49324 \u46988 \u50640 \u44172  \u45796 \u44032 \u44032 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44172 \u45796 \u44032  \u52981  \u44592 \u49324 \u45716  \u48512 \u46300 \u47101 \u44256  \u49345 \u45285 \u54616 \u44592 \u50640  \u51060 \u49457 \u50640 \u44172  \u49789 \u44172  \u49789 \u44172  \u51217 \u44540 \u54624  \u49688  \u51080 \u51424 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u44536 \u47111 \u44172  \u53360  \u44048 \u51221 \u51060  \u50500 \u45776  \u49688 \u46020  \u51080 \u51020 \u49457 \u54693  : \u51060 \u49457 \u50640 \u44172  \u52628 \u54028 \u47484  \u45912 \u51648 \u44144 \u45208 , \u54840 \u44048 \u51012  \u54364 \u54788 \u54616 \u45716  \u45936  \u51061 \u49689 \u54620  \u49324 \u46988 \u52981 \u50640  \u47932 \u51060  \u44032 \u46301  \u52264  \u51080 \u45716 \u51648  \u48708 \u50612  \u51080 \u45716 \u51648 \u45716  \u48520 \u48516 \u47749 \u54633 \u45768 \u45796 . \u44592 \u49324 \u44032  \u45572 \u44400 \u44032 \u50640 \u44172  \u47932 \u51012  \u51452 \u45716  \u44148 \u51648 , \u50500 \u45768 \u47732  \u47932 \u51012  \u52292 \u50892 \u45804 \u46972 \u45716  \u44148 \u51648 \u46020  \u48520 \u48516 \u47749 \u54633 \u45768 \u45796 .\u52280 \u44256  \u51088 \u47308  : Knight of Cups Tarot Card Meanings \'96 Tarot Merchant\u52981  \u44592 \u49324  \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \u49345 \u45285 \u54616 \u44256  \u48512 \u46300 \u47084 \u50864 \u47728  \u49888 \u51473 \u54620  \u49457 \u44201 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54616 \u44256  \u50696 \u48124 \u54620  \u49457 \u54693  \u53459 \u50640  \u50696 \u49696 \u44228 \u50640  \u51333 \u49324 \u54616 \u47732  \u51339 \u51008  \u49457 \u44284 \u44032  \u51080 \u49845 \u45768 \u45796 . \u44032 \u49844  \u49549 \u50640 \u45716  \u54637 \u49345  \u51060 \u49345 \u51012  \u54408 \u44256  \u51080 \u51648 \u47564 , \u49457 \u49892 \u54616 \u44256  \u54788 \u49892 \u51201 \u51077 \u45768 \u45796 . \u44032 \u51109  \u51201 \u51208 \u54620  \u54364 \u54788 \u51004 \u47196 \u45716  \'93\u45229 \u47564  \u51080 \u45716  30\u45824 \'94 \u46980 \u44620 \u50836 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972  \u48148 \u46988 \u44592 \u44032  \u51080 \u44144 \u45208  \u44032 \u48316 \u50868  \u47564 \u45224 \u51012  \u51600 \u44592 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51333 \u51333  \u44048 \u51221 \u51201 \u51004 \u47196  \u45208 \u50732  \u49688 \u46020  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u52981  \u44592 \u49324 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  2\u44032 \u51648 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u50640 \u44172  \u45229 \u47564 \u51201 \u51064  \u49324 \u46993 \u51012  \u54408 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51312 \u47564 \u44036  \u50672 \u46973 \u51012  \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50612 \u51788 \u47732 , \'91\u49324 \u46993 \u50640  \u48736 \u51652  \u48376 \u51064 \u51032  \u47784 \u49845 \'92\u50640  \u52712 \u54616 \u44256  \u51080 \u51012 \u51648 \u46020  \u47784 \u47493 \u45768 \u45796 . \u45572 \u44400 \u44032 \u47484  \u49324 \u46993 \u54620 \u45796 \u45716  \u45712 \u45196  \u51088 \u52404 \u47484  \u51339 \u50500 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47560 \u52824  \u48376 \u51064 \u51012  \u50689 \u54868  \u49549  \u51452 \u51064 \u44277 \u52376 \u47100  \u49373 \u44033 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u45817 \u49888 \u51012  \u44201 \u47140 \u54616 \u44144 \u45208  \u50948 \u47196 \u54644 \u51452 \u44256  \u49910 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u50672 \u50528  \u44048 \u51221 \u48372 \u45796 \u45716  \u44536 \u51200  \'93\u51649 \u51109  \u46041 \u47308 \u47484  \u52313 \u44200 \u51452 \u45716 \'94 \u45712 \u45196 \u51077 \u45768 \u45796 . \u44277 \u53685 \u51201 \u51004 \u47196 , \u45817 \u49888 \u50640 \u44172  \u51312 \u49900 \u49828 \u47084 \u50868  \u47560 \u51020 \u51012  \u54408 \u44256  \u51080 \u45716  \u46319 \u54616 \u47728 , \'91\u44032 \u44637 \u45796 \'92\u44256  \u45712 \u45180 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50500 \u51649 \u46020  \u47196 \u47592 \u54001 \u54620  \u44288 \u44228 \u47484  \u47564 \u46308 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44428 \u53468 \u44592 \u50752 \u45716  \u44144 \u47532 \u44032  \u47689 \u45768 \u45796 . \u49436 \u47196 \u47484  \u48176 \u47140 \u54616 \u44256  \u51316 \u51473 \u54616 \u45716  \u49324 \u51060 \u51077 \u45768 \u45796 . \u46608  \u51312 \u47564 \u44036  \u54532 \u47196 \u54252 \u51592 \u47484  \u48155 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u50668 \u49324 \u52828 \u51060 \u45208  \u45224 \u49324 \u52828 \u51012  \u44032 \u48333 \u44172  \u46496 \u48372 \u44256  \u51080 \u45796 \u44256  \u54644 \u49437 \u54633 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u50612 \u46500  \u54805 \u53468 \u47196 \u46304  \u45817 \u49888 \u50640 \u44172  \u51116 \u54924  \u50672 \u46973 \u51012  \u54616 \u44256  \u49910 \u50612  \u54633 \u45768 \u45796 . \u44536  \u44048 \u51221 \u51008  \u52649 \u46041 \u51201 \u51060 \u44592 \u48372 \u45796  \u51312 \u49900 \u49828 \u47101 \u44256 , \u48512 \u46300 \u47101 \u49845 \u45768 \u45796 . \u50724 \u47004  \u44592 \u44036  \u51116 \u54924 \u47484  \u49373 \u44033  \u54620  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732 , \u45796 \u47480  \u45572 \u44400 \u44032 \u50640 \u44172  \u51217 \u44540 \u54616 \u44256  \u51080 \u45716  \u47784 \u49845 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u54840 \u44048  \u54364 \u54788 \u51012  \u54616 \u44144 \u45208 , \u49345 \u45824 \u48169 \u51032  \u47560 \u51020 \u51012  \u46496 \u48372 \u47140  \u45796 \u44032 \u44032 \u45716  \u51473 \u51077 \u45768 \u45796 .\u52981  \u44592 \u49324  \u51649 \u50629 \u50868  \u54644 \u49437 \u49436 \u47673 \u49436 \u47673 \u54664 \u45912  \u46041 \u47308 \u50752  \u54868 \u54644 \u54616 \u45716  \u51068 \u51060  \u49373 \u44609 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u51452 \u48320  \u46041 \u47308 \u46308 \u44284  \u49324 \u51060 \u44032  \u51339 \u50500 \u51648 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u45320 \u47924  \u45224 \u50640 \u44172  \u47582 \u52656 \u51452 \u44144 \u45208  \u45132 \u47140 \u45796 \u45776  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45224 \u51012  \u48176 \u47140 \u54616 \u45716  \u44163 \u46020  \u51339 \u51648 \u47564  \u45817 \u49888 \u51032  \u44592 \u48516 , \u51032 \u44204 \u51012  \u54364 \u54788 \u54616 \u45716  \u51068 \u46020  \u51473 \u50836 \u54633 \u45768 \u45796 .\u51339 \u51008  \u51228 \u50504 \u51060 \u45208  \u49548 \u49885 \u51060  \u50732  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u51068 \u51088 \u47532 \u47484  \u51228 \u50504  \u48155 \u44144 \u45208 , \u45824 \u54617 \u51012  \u54633 \u44201 \u54664 \u45796 \u45716  \u49548 \u49885  \u46321 \u51077 \u45768 \u45796 . \u52981  \u44592 \u49324 \u45716  \u45824 \u54364 \u51201 \u51064  \u47700 \u49888 \u51200  \u52852 \u46300 \u47196 , \'93\u51339 \u51008  \u49548 \u49885 \'94\u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52981  \u44592 \u49324  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u45572 \u44400 \u44032 \u50640 \u44172  \u51339 \u51008  \u53804 \u51088 \u47484  \u51228 \u50504  \u48155 \u44172  \u46121 \u45768 \u45796 .\u51652 \u49900  \u50612 \u47536  \u49440 \u53469 \u51060  \u44208 \u49892 \u51012  \u47610 \u44172  \u46108 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51593  \'93\u44060 \u51064 \u51201 \u51064  \u50676 \u47581 , \u48148 \u46988 \'94\u44284  \u51068 \u52824 \u54624 \u49688 \u47197  \u51339 \u51008  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51060 \u45716  \u45817 \u49888 \u50640 \u44172  \u51116 \u51221 \u51201 \u51064  \u51060 \u46301  \u49104 \u47564  \u50500 \u45768 \u46972 , \u44060 \u51064 \u51201 \u51064  \u49457 \u52712 \u44048 \u44620 \u51648  \u44032 \u51256 \u50732  \u44163 \u51077 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ]\u51312 \u50616 \u51216 \u51312 \u50616  1. \u45796 \u44032 \u44032 \u45716  \u51088 \u49464 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 \u54217 \u49548 \u50640  \u44288 \u49900  \u51080 \u45912  \u49324 \u46988 , \u49436 \u47673 \u49436 \u47673  \u54664 \u45912  \u46041 \u47308 \u50640 \u44172  \u45796 \u44032 \u44032 \u45716  \u44148  \u50612 \u46504 \u44620 \u50836 ? \u47784 \u46304  \u49324 \u46988 \u44284  \u52828 \u54644 \u51656  \u54596 \u50836 \u45716  \u50630 \u51648 \u47564 , \u50672 \u51060  \u45823 \u45716  \u49324 \u46988 \u44284  \u49789 \u44172  \u47680 \u50612 \u51656  \u54596 \u50836 \u46020  \u50630 \u49845 \u45768 \u45796 . \u50857 \u44592 \u47484  \u45236 \u50612  \u48512 \u46300 \u47101 \u44172  \u45796 \u44032 \u44032 \u49464 \u50836 .\u51312 \u50616  2. \u45236  \u51032 \u44204 \u51012  \u51452 \u51109 \u54624  \u51460 \u46020  \u50508 \u50500 \u50556  \u54633 \u45768 \u45796 \u45224 \u50640 \u44172  \u47582 \u52656 \u51452 \u44256 , \u45236  \u44592 \u48516 \u51012  \u50613 \u45572 \u47476 \u47728  \u49332 \u44256  \u51080 \u45716  \u44148  \u50500 \u45772 \u51648  \u46028 \u51060 \u53020 \u48372 \u49464 \u50836 . \u51088 \u49888 \u51012  \u51453 \u51060 \u47728  \u49332 \u45796  \u48372 \u47732 , \u50612 \u45712 \u49356 \u44032  \u53360  \u49828 \u53944 \u47112 \u49828 \u47484  \u45712 \u45180 \u44592  \u47560 \u47144 \u51077 \u45768 \u45796 . \u45236  \u51452 \u44288 \u46020  \u51312 \u44552 \u50473  \u49324 \u46972 \u51648 \u44172  \u46104 \u51424 . \u51088 \u49888 \u44048 \u51012  \u44032 \u51648 \u44256  \u51201 \u44537 \u51201 \u51004 \u47196  \u48376 \u51064 \u51012  \u45236 \u49464 \u50864 \u49464 \u50836 .    \
