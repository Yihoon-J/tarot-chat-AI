   \uc0\u52981 3 \'96 \u51339 \u51008  \u52828 \u44396  \u52981 3 \u44536 \u47548 \u51032  \u51032 \u48120 3\u47749 \u51032  \u50668 \u49457 \u51060  \u52629 \u48176 \u47484  \u46300 \u45716  \u47784 \u49845 \u52629 \u54616  \u48155 \u51012  \u51068 \u51060  \u49373 \u44596 \u45796 \u45716  \u46907 \u52828 \u54644 \u51648 \u44256  \u49910 \u51008  \u47560 \u51020 \u49548 \u49549 \u44048 \u51060 \u45208  \u52828 \u48128 \u44048 \u51012  \u45712 \u45180 \u45716  \u49345 \u53468 \u46412 \u50640  \u46384 \u46972  \u49340 \u44033 \u44288 \u44228 \u47484  \u51032 \u48120 \u54616 \u44592 \u46020  \u54632 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u52629 \u54616  \u48155 \u51012  \u51068 \u51060  \u49373 \u44592 \u45716 \u52981 3 \u52852 \u46300 \u45716  \u52629 \u54616  \u48155 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49324 \u46988 \u46308 \u51060  \u52629 \u48176 \u47484  \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u48156  \u48145 \u51012  \u48372 \u47732  \u44284 \u51068 \u44284  \u44867 , \u54840 \u48149 \u51060  \u46405 \u50640  \u51088 \u46972 \u44256  \u51080 \u51424 . \u51060  \u44284 \u51068 \u44284  \u44867 \u46308 \u51008  \u47784 \u46160  \u54413 \u50836 \u47196 \u50880 \u44284  \u44208 \u49892 \u51032  \u49345 \u51669 \u51077 \u45768 \u45796 . \u51593 , \'91\u44277 \u46041 \u51032  \u47785 \u54364 \'92\u47484  \u50948 \u54644  \u50676 \u49900 \u55176  \u45432 \u47141 \u54664 \u44256 , \u51060 \u51228  \u44536  \u48372 \u49345 \u51012  \u45572 \u47540  \u52264 \u47168 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u47532 \u46377 \u50640 \u49436 \u45716  :\u54788 \u51116  \u52964 \u54540 \u51068  \u44221 \u50864 , \u44208 \u54844 \u51004 \u47196  \u51060 \u50612 \u51652 \u45796 \u45716  \u54644 \u49437 \u51060  \u44032 \u45733 \u54633 \u45768 \u45796 .\u49556 \u47196 \u51068  \u44221 \u50864 , \u49548 \u44060 \u54021 \u51060  \u49457 \u44277 \u51201 \u51004 \u47196  \u51060 \u47364 \u51665 \u45768 \u45796 . \u49548 \u44060 \u54021  \u51452 \u49440 \u51088 \u50752  \u45208 \u47672 \u51648  \u46160  \u49324 \u46988 \u51060  \u52629 \u54616  \u45208 \u45572 \u45716  \u47784 \u49845 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51652 \u54665 \u54616 \u45716  \u54532 \u47196 \u51229 \u53944 \u44032  \u51096  \u47560 \u47924 \u47532 \u46104 \u50612  \u46041 \u47308 \u50752  \u54632 \u44760  \u52629 \u54616 \u47484  \u45208 \u45572 \u44172  \u46121 \u45768 \u45796 .\u54644 \u49437  2. \u52828 \u54616 \u45796 \u44256  \u45712 \u45180 \u45716 , \u52828 \u54644 \u51648 \u44256  \u49910 \u51008 \u52981 3\u51008  \u54632 \u44760  \u51080 \u51004 \u47732  \u51600 \u44144 \u50868  \u49324 \u46988 , \u52828 \u54616 \u45796 \u44256  \u45712 \u45180 \u45716  \u44048 \u51221 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u49548 \u49549 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u54633 \u45768 \u45796 . \u51060  \u52852 \u46300 \u45716  \u51060 \u49457 \u51004 \u47196 \u49436  \u45712 \u45180 \u45716  \'91\u49444 \u47128 \'92\u48372 \u45796 , \u46041 \u47308 \u47196 \u49436  \u45712 \u45180 \u45716  \'91\u52828 \u48128 \u44048 \'92\u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 . Four of Wands\u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u52981 3\u50756 \u46300 4 \u52852 \u46300  \u49549  \u49324 \u46988 \u46308 \u51008  \u52509  3\u47749 \u51004 \u47196 , \u47784 \u46160  \u50668 \u49457 \u51077 \u45768 \u45796 . \u45224 \u45376 \u44036 \u51032  \'91\u45824 \u47549  \u44396 \u46020 \'92\u45716  \u48372 \u51060 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51060 \u51204  \u52852 \u46300 \u50688 \u45912  \u52981 2\u44032  1:1 \u44288 \u44228 \u47484  \u45208 \u53440 \u45256 \u45796 \u47732 , Three of Cups\u45716  \u54620  \u52964 \u48036 \u45768 \u54000  \u51204 \u52404 \u47484  \u44032 \u47532 \u53412 \u45716  \u44288 \u44228 \u51077 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ]\u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45231 \u49440  \u49324 \u46988 \u44284  \u52828 \u48516 \u51012  \u49939 \u51012  \u44592 \u54924 \u44032  \u49373 \u44609 \u45768 \u45796 .\u45817 \u49888 \u51012  \u51060 \u49457 \u51004 \u47196 \u49436  \u51339 \u50500 \u54616 \u44592 \u48372 \u45796 \u45716  \u51339 \u51008  \u46041 \u47308 , \u51339 \u51008  \u52828 \u44396 \u47196 \u49436  \u48372 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51060  \'91\u51652 \u51648 \u54620  \u48516 \u50948 \u44592 , \u47924 \u44144 \u50868  \u48516 \u50948 \u44592 \'92\u47484  \u49899 \u50612 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51068 \u47732 \u49885 \u51060  \u50630 \u45796 \u47732 , \u52828 \u54644 \u51648 \u44256  \u49910 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51649 \u51109  \u46041 \u47308 \u46308 \u44284  \u52828 \u54644 \u51648 \u44172  \u46121 \u45768 \u45796 .\u44277 \u46041 \u51032  \u54532 \u47196 \u51229 \u53944 \u47484  \u49688 \u54665 \u54616 \u47732 \u49436 , \u49436 \u47196  \u52828 \u48128 \u54644 \u51648 \u45716  \u49884 \u44036 \u51012  \u44054 \u44172  \u46121 \u45768 \u45796 .\u50612 \u49353 \u54632 \u51060  \u51460 \u50612 \u46317 \u45768 \u45796 .\u54644 \u49437  3. \u49436 \u47196  \u54801 \u47141 \u54616 \u45716 Three of Cups\u45716  \u49436 \u47196  \u54801 \u47141 \u54616 \u45716  \u49345 \u54889 , \u49373 \u44033 \u51012  \u45208 \u45572 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u52981 \u51008  \u50864 \u47532 \u51032  \'91\u44048 \u51221 , \u49373 \u44033 \'92 \u46321 \u46321  \u45576 \u50640  \u48372 \u51060 \u51648  \u50506 \u45716  \u44163 \u46308 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51593  \u52852 \u46300  \u49549  \u49464  \u50668 \u49457 \u51008  \u44033 \u51088 \u51032  \u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \u51012  \u45208 \u45572 \u44256  \u51080 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u51424 . \u48708 \u49847 \u54620  \u52852 \u46300 \u47196 , Three of Pentacles\u44032  \'91\u47932 \u51656 , \u51068 \'92\u50640  \u51080 \u50612 \u49436 \u51032  \u54801 \u47141 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , Three of Cups\u45716  \'91\u44048 \u51221 , \u49373 \u44033 \'92\u51012  \u44277 \u50976 \u54620 \u45796 \u45716  \u45656 \u50521 \u49828 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u47564 \u50557  \u52981 3\u51060  \u44396 \u52404 \u51201 \u51064  \u49345 \u54889 \u51012  \u44032 \u47532 \u53416 \u45796 \u47732  :\u49696 \u51665 \u50640 \u49436  \u45572 \u44400 \u44032 \u47484  \u49888 \u45208 \u44172  \u46263 \u45812 \u54868  \u54616 \u45716  \u49345 \u54889 \u52852 \u54168 \u50640 \u49436  \u49436 \u47196 \u47484  \u50948 \u47196 \u54616 \u47728 , \'91\u51004 \u49968 \u51004 \u49968 \'92 \u54616 \u45716  \u49345 \u54889 \u52572 \u44540 \u50640  \u49373 \u44596  \u51068 \u46308 \u51012  \u44057 \u51060  \u51060 \u50556 \u44592 \u54616 \u47728 , \u46608  \u52629 \u54616  \u54616 \u45716  \u49345 \u54889 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49340 \u44033 \u44288 \u44228 \u52981 3\u51008  \u46412 \u50640  \u46384 \u46972  \u49340 \u44033 \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52981 \u51060  \u44032 \u50868 \u45936 \u47196  \u47784 \u51060 \u51648  \u50506 \u44256  \u44033 \u44033  \u45796 \u47480  \u48169 \u54693 \u51012  \u44032 \u47532 \u53412 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44048 \u51221  \u54841 \u51008  \u49373 \u44033 \u51012  1:1:1\u47196  \u45208 \u45572 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u8251  \u51221 \u54869 \u54620  \u51060 \u48120 \u51648  \u48516 \u49437 \u48372 \u45796 \u45716 , \u51076 \u49345  \u44536 \u47532 \u44256  \u45656 \u50521 \u49828 \u50640  \u51032 \u54620  \u54644 \u49437 \u51077 \u45768 \u45796 .\u44845  \u49340 \u44033 \u44288 \u44228 \u44032  \u50500 \u45768 \u45908 \u46972 \u46020 , \u51228 3\u51088 \u44032  \u45180 \u50612 \u46300 \u45716  \u49345 \u54889  \u51204 \u48512 \u47484  \u45208 \u53440 \u45244  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u50696 \u49884 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796 \'85\u54532 \u47196 \u51229 \u53944 \u47484  \'91\u45208 \'92 \u44536 \u47532 \u44256  \'91A\'92\u44032  \u45796 \u54664 \u45716 \u45936 , \u44049 \u51088 \u44592  \'91B\'92\u44032  \u45180 \u50612 \u46308 \u50612  \u44057 \u51060  \u52629 \u54616  \u48155 \u45716  \u49345 \u54889 \u45208 \u46993  \u50040 \u45224 (\u50040 \u45376 )\u44032  \u45824 \u54868  \u51473 \u51060 \u50632 \u45716 \u45936 , \u44049 \u51088 \u44592  \'91C\'92\u44032  \u45180 \u50612 \u46308 \u50612  \u45824 \u54868 \u44032  \u47581 \u44032 \u51648 \u45716  \u49345 \u54889 \u46168 \u51060 \u49436  \u50584 \u44592 \u54616 \u44256  \u49910 \u51008 \u45936 , \u45576 \u52824  \u50630 \u44172  \'91D\'92\u44032  \u54632 \u44760  \u51080 \u45716  \u49345 \u54889 \u52981 3 \u50672 \u50528 \u50868  \u54644 \u49437 \u52981 3\u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u54632 \u44760  \u51080 \u51004 \u47732  \u51600 \u44161 \u44256  \u52828 \u54644 \u51648 \u44592  \u49772 \u50868  \u49324 \u46988 \u51077 \u45768 \u45796 . \u48537 \u51076 \u49457 \u51060  \u51339 \u50500  \u45231 \u49440  \u49324 \u46988 \u44284  \u49548 \u53685 \u46020  \u50896 \u54876 \u54633 \u45768 \u45796 . \u48513 \u51201 \u48513 \u51201 \u54620  \u54872 \u44221 \u51060 \u45208 , \u50612 \u46360 \u44032  \u49548 \u49549 \u46104 \u50612  \u51080 \u45716  \u44163 \u51012  \u51339 \u50500 \u54633 \u45768 \u45796 . \u54844 \u51088 \u49436  \u51648 \u45236 \u44600  \u51339 \u50500 \u54616 \u45716  \u49828 \u53440 \u51068 \u51008  \u50500 \u45785 \u45768 \u45796 . \u46412 \u50640  \u46384 \u46972 , \u44032 \u48316 \u50868  \u49696 \u51088 \u47532 \u45208  \u50976 \u55141 \u51012  \u51339 \u50500 \u54644  \'91\u49340 \u44033 \u44288 \u44228 \'92\u50640  \u51088 \u51452  \u45459 \u51060 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49324 \u46988 \u46308 \u44284  \u54632 \u44760  \u51080 \u51012  \u46412  \u55184 \u47553  \u46104 \u45716  E \u49457 \u54693  MBTI\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u52981 3\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \'91\u52828 \u48128 \u44048 , \u49548 \u49549 \u44048 \'92\u51012  \u45712 \u45180 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51060 \u49457 \u51004 \u47196 \u49436 \u51032  \u44048 \u51221 \u48372 \u45796 \u45716  \'91\u51339 \u51008  \u46041 \u47308 , \u52828 \u54620  \u52828 \u44396 \'92 \u51221 \u46020 \u47196  \u50668 \u44592 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45796 \u47564  \u51060  \u47568 \u51060  \'91\u50672 \u51064 \u51060  \u46104 \u44592 \u45716  \u55192 \u46308 \u45796 \'92\u45716  \u46907 \u51008  \u50500 \u45785 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u52828 \u44396  \u44057 \u51008  \u50672 \u50528 , \u54200 \u50504 \u54620  \u50672 \u50528 \u47484  \u49440 \u54840 \u54620 \u45796 \u47732  \u45908 \u50630 \u51060  \u51339 \u51008  \u52852 \u46300 \u44032  \u46104 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u50612 \u46500  \u50672 \u50528 \u47484  \u52628 \u44396 \u54616 \u45712 \u45264 \u50640  \u46384 \u46972 \u49436  \u44557 \u51221 \u51201 \u51064 \u51648  \u48512 \u51221 \u51201 \u51064 \u51648  \u44208 \u51221 \u46121 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u54632 \u44760  \u51080 \u51004 \u47732  \u51600 \u44161 \u44256 , \u54200 \u50504 \u54616 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49436 \u47196  \u45824 \u54868 \u46020  \u51096  \u53685 \u54616 \u47728 , \'91\u45236  \u49324 \u46988 \'92\u51060 \u46972 \u45716  \u49548 \u49549 \u44048  \u46608 \u54620  \u45712 \u45180 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45817 \u49888 \u44284  \u49345 \u45824 \u48169 , \u46608  \u45572 \u44400 \u44032 \u51032  \u52629 \u48373 \u51012  \u48155 \u45716  \u44536 \u47548 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564  \u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51068  \u44221 \u50864 , \u46608  \u45796 \u47480  \u51228 3\u51088 \u44032  \u46321 \u51109 \u54644 \u49436  \u44288 \u44228 \u47484  \u48169 \u54644 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45572 \u44400 \u44032  \u45817 \u49888 \u51032  \u54028 \u53944 \u45320 \u47484  \u51339 \u50500 \u54616 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u51060  \u52852 \u46300 \u45716  \'91\u54801 \u47141 , \u52629 \u54616 , \u52828 \u48128 \u44048 \'92\u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45572 \u44400 \u44032 \u51032  \u46020 \u50880 \u51012  \u48155 \u44144 \u45208 , \u50528 \u51064 \u44284 \u51032  \u52828 \u48128 \u44048 \u51012  \u51082 \u51648  \u47803 \u54616 \u44256  \u51116 \u54924 \u47484  \u49884 \u46020 \u54616 \u45716  \u44536 \u47548 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u50612 \u52236  \u46104 \u50632 \u46304  \u51204 \u50528 \u51064 \u44284  \u45796 \u49884  \u47564 \u45216  \u44592 \u54924 \u45716  \u49373 \u44600  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u44536  \u44208 \u44284 \u44032  \u51339 \u51648  \u47803 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46608  \u45796 \u47480  \u51228 3\u51088 \u44032  \u45208 \u53440 \u45208  \u44288 \u44228 \u47484  \u48373 \u51105 \u54616 \u44172  \u47564 \u46308  \u49688  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47564 \u50557  \u51116 \u54924 \u50640  \u49457 \u44277 \u54616 \u44172  \u46108 \u45796 \u47732 , \u51064 \u44036 \u44288 \u44228 \u47484  \u49900 \u54540 \u54616 \u44172  \u44032 \u51256 \u44040  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u52981 3 \u51649 \u50629 \u50868  \u54644 \u49437 \u51649 \u50629 \u50868 \u51008  \u47588 \u50864  \u51339 \u49845 \u45768 \u45796 . \u51452 \u48320  \u46041 \u47308 \u46308 \u44284  \u54801 \u47141 \u54616 \u47728 , \u52629 \u54616 \u47484  \u45208 \u45572 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u44536 \u46041 \u50504  \u45432 \u47141 \u54644 \u50772 \u45912  \u51068 \u46308 \u51060  \u53360  \u49457 \u44284 \u47484  \u48372 \u44172  \u46121 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \'91\u52629 \u51228  \u44057 \u51008  \u48516 \u50948 \u44592 \'92\u47484  \u51600 \u44600  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u47924 \u45212 \u55176  \u54633 \u44201 \u54644 \u49436  \u44032 \u51313  \u51648 \u51064 \u46308 \u44284  \u52629 \u54616 \u47484  \u45208 \u45572 \u45716  \u44536 \u47548 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u52981 3 \u44552 \u51204 \u50868  \u54644 \u49437 \u49324 \u50629  \u54841 \u51008  \u53804 \u51088 \u50640 \u49436  \u45796 \u47480  \u49324 \u46988 \u46308 \u44284  \u54801 \u47141 \u54616 \u47732  \u51339 \u51008  \u49457 \u44284 \u44032  \u51080 \u49845 \u45768 \u45796 . \u54844 \u51088 \u49436  \u47924 \u47532 \u54616 \u44172  \u51068 \u51012  \u52376 \u47532 \u54616 \u44592 \u48372 \u45796 \u45716  \u54016 \u50892 \u53356 \u47484  \u48156 \u55064 \u54644 \u50556  \u54616 \u45716  \u49884 \u51216 \u51077 \u45768 \u45796 . \u51068 \u49884 \u51201 \u51004 \u47196  \u46024 \u51012  \u54633 \u52432 \u49436  \'91\u44277 \u46041 \u51032  \u53804 \u51088 \'92\u47484  \u54616 \u45716  \u44163 \u46020  \u51339 \u50500 \u48372 \u51077 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u44032 \u48316 \u50868  \u47564 \u45224 \u51012  \u51312 \u49900 \u54616 \u49464 \u50836 \u51060  \u49324 \u46988  \u51200  \u49324 \u46988  \u44032 \u48333 \u44172  \u47564 \u45208 \u45796  \u48372 \u47732 , \u51221 \u51089  \u51452 \u50948 \u50640  \u51080 \u45716  \u49324 \u46988 \u50640  \u49548 \u54848 \u54644 \u51648 \u44592  \u47560 \u47144 \u51077 \u45768 \u45796 . \u49104 \u47564  \u50500 \u45768 \u46972 , \'91\u44032 \u48333 \u44256  \u54756 \u54536  \u49324 \u46988 \'92\u51060 \u46972 \u45716  \u51064 \u49885 \u51060  \u48149 \u55184  \u49688 \u46020  \u51080 \u51424 . \u51452 \u50948  \u49324 \u46988 \u46308 \u50640 \u44172  \u52828 \u51208 \u54616 \u46104 , \'91\u45236  \u49324 \u46988 \'92\u51012  \u49548 \u51473 \u55176  \u50668 \u44592 \u45716  \u49884 \u44036 \u46020  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u51312 \u50616  2. \u54801 \u47141 \u54616 \u49464 \u50836 \u54844 \u51088 \u49436  \u47784 \u46304  \u51068 \u51012  \u52376 \u47532 \u54616 \u45716  \u44163 \u51060  \u45908  \u45227 \u45796 \u44256  \u49373 \u44033 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u54632 \u44760  \u55192 \u51012  \u54633 \u52432  \u51068 \u51012  \u54644 \u45256 \u51012  \u46412  \u44536  \u44592 \u49256 \u51008  \u48176 \u47196  \u52964 \u51665 \u45768 \u45796 . \u50696 \u49345 \u52824  \u47803 \u54620  \u49884 \u45320 \u51648 \u44032  \u45216  \u49688 \u46020  \u51080 \u51424 . \u44845  \u51068 \u51060  \u50500 \u45768 \u45908 \u46972 \u46020 , \u51452 \u50948  \u49324 \u46988 \u46308 \u44284  \u49324 \u51060 \u51339 \u44172  \u51648 \u45236 \u45716  \u44163 \u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 . \u54637 \u49345  \u48157 \u51008  \u47784 \u49845 \u51004 \u47196  \u49324 \u46988 \u46308 \u51012  \u47560 \u51452 \u54616 \u49464 \u50836 .\u52280 \u44256  \u51088 \u47308  : \u53440 \u47196  \u52852 \u46300  \'93\u52981  3\u8243 \u51032  \u51032 \u48120  \'b7 \u54644 \u49437  \'b7 \u49345 \u51669  \'96 \u53440 \u47196  \u54616 \u52824 \u48288     \
