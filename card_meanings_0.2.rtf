 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u54801 \u47141 \u54616 \u49464 \u50836 \u51068 \u51012  \u54844 \u51088 \u49436  \u54616 \u45716  \u44163 \u48372 \u45796 , \u45224 \u44284  \u44057 \u51060  \u54616 \u45716  \u44172  \u51339 \u51012  \u44163  \u44057 \u49845 \u45768 \u45796 . \u54596 \u50836 \u54624  \u46412 \u45716  \u51452 \u51200 \u54616 \u51648  \u47568 \u44256  \u46020 \u50880 \u51012  \u52397 \u54616 \u49464 \u50836 . \u51064 \u44036  \u44288 \u44228 \u50640 \u49436 \u46020  \u51452 \u48320 \u51032  \u49552 \u44600 \u51012  \u44144 \u48512 \u54616 \u45716  \u51068 \u51008  \u54588 \u54644 \u50556  \u54633 \u45768 \u45796 . \u49324 \u46988 \u46308 \u44284  \u50612 \u50872 \u47140 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u51312 \u50616  2. \u51473 \u44036 \u50640  \u49552 \u51012  \u46524 \u51648  \u47560 \u49464 \u50836 \u44144 \u51032  \u45796  \u50772 \u45796 \u44256  \u54616 \u45908 \u46972 \u46020 , \u51473 \u44036 \u50640  \u49552 \u51012  \u46524 \u45716  \u51068 \u51008  \u50630 \u50612 \u50556  \u54633 \u45768 \u45796 . 95\u47484  \u54664 \u45908 \u46972 \u46020 , 5\u44032  \u45224 \u50500 \u51080 \u51004 \u47732  \u44536  \u51068 \u51008  \u54217 \u49373  \'93\u48120 \u50756 \u49457 \'94\u51004 \u47196  \u45224 \u44172  \u46121 \u45768 \u45796 . \u44144 \u51032  \u45796  \u50772 \u49845 \u45768 \u45796 . \u46263 \u49900 \u51012  \u48156 \u55064 \u54616 \u50668  \u51068 \u51012  \u47560 \u47924 \u47532  \u51648 \u51004 \u49464 \u50836 . \u51339 \u51008  \u49457 \u44284 \u44032  \u51080 \u49845 \u45768 \u45796 .    \
