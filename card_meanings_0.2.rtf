   \uc0\u47785 \u52264 \u49548 \u46300  \u50640 \u51060 \u49828  \u52852 \u46300  \u51060 \u48120 \u51648 \u53412 \u50892 \u46300  \u50836 \u50557 \u50640 \u51060 \u49828  \u52852 \u46300 \u51032  \u44277 \u53685 \u51201  \u53945 \u51669 \u49552 \u50640  \u46308 \u47536  \u44160 \u44160  \u44845 \u45824 \u44592 \u51032  \u50773 \u44288 \u44536 \u47000 \u46020  \u48120 \u47000 \u45716  \u54744 \u45212 \u54624  \u44163 \u51060 \u45796 \u49892 \u51656 \u51201 \u51064  \u45432 \u47141 \u44284  \u44536  \u48169 \u54693 \u49457 \u51060  \u51473 \u50836 \u54616 \u45796 \u49548 \u46300  \u50640 \u51060 \u49828  \u54644 \u49437  \'96 \u50672 \u50528  \
