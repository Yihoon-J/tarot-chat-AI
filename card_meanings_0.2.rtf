   \uc0\u50756 \u46300 5 \'96 \u47560 \u52272  \u50756 \u46300 5 \u44536 \u47548 \u51032  \u51032 \u48120 \u49436 \u47196  \u45796 \u47480  \u51032 \u44204 \u51004 \u47196  \u51064 \u54644  \u49912 \u50864 \u45716  \u51473 \u52376 \u51020 \u48512 \u53552  \u49912 \u50864 \u47140  \u54664 \u45912  \u44163 \u51008  \u50500 \u45768 \u45208 , \u50612 \u51788 \u45796  \u48372 \u45768  \u49912 \u50864 \u44172  \u46120 \u49457 \u51064  5\u47749 \u51060  \u44221 \u51137 \u54616 \u45716  \u49345 \u54889 , \u44221 \u51137 \u51060  \u49900 \u54868 \u46120 \u49912 \u50880 \u51004 \u47196  \u51064 \u54644 , \u49345 \u54889 \u51060  \u51652 \u51204 \u46104 \u44144 \u45208  \u44060 \u51064 \u51201 \u51004 \u47196  \u48156 \u51204 \u54624  \u44592 \u54924 \u44032  \u49373 \u44608 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51652 \u51204 \u51060  \u51080 \u45716 \u50756 \u46300 5\u45716  \u51652 \u51204 \u51060  \u51080 \u44144 \u45208  \u48156 \u51204 \u54624  \u44592 \u54924 \u44032  \u49373 \u44592 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u49688 \u48708 \u54617 \u50640 \u49436  \u49707 \u51088  5\u45716  \u48320 \u54868 \u50752  \u51652 \u48372 \u47484  \u51032 \u48120 \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49324 \u46988 \u46308 \u51008  \u49912 \u50864 \u44592  \u50948 \u54644  \u47784 \u51064  \u44163 \u51060  \u50500 \u45785 \u45768 \u45796 . \u44536 \u48372 \u45796 \u45716  \'91\u51060 \u51061 \u51012  \u50619 \u44144 \u45208 \'92, \'91\u53440 \u54801 \u54616 \u44592  \u50948 \u54644  \u47784 \u51064  \u49345 \u54889 \'92\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \'91\u54028 \u50629 , \u45432 \u51312  \u54801 \u49345 \'92\u44284  \u48708 \u49847 \u54633 \u45768 \u45796 .\u46384 \u46972 \u49436 \'93\u54620  \u48156 \u51677  \u50526 \u51004 \u47196  \u45208 \u50500 \u44032 \u44592  \u50948 \u54620  \u53804 \u51137 \'94 \'93\u48156 \u51204 \u51012  \u50948 \u54620  \u49457 \u51109 \u53685 \'94 \u51221 \u46020 \u47196  \u54644 \u49437 \u54616 \u47732  \u51339 \u49845 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u50696 \u47484  \u46308 \u50612 \u48372 \u44192 \u49845 \u45768 \u45796 .\u50672 \u51064 \u51060  \u54217 \u49548 \u50752  \u45796 \u47480  \u53468 \u46020 \u47484  \u48372 \u51064 \u45796 . \u48516 \u47749  \u47924 \u50616 \u44032  \u47560 \u51020 \u50640  \u50504  \u46300 \u45716  \u44172  \u51080 \u50612  \u48372 \u51064 \u45796 . \u44536 \u47088 \u45936  \u47568 \u51012  \u50504  \u54644 \u51456 \u45796 . \u47728 \u52832  \u49352  \u45813 \u45813 \u54620  \u44592 \u47448 \u44032  \u55120 \u47480 \u45796 .\u50612 \u45712  \u45216  \u44049 \u51088 \u44592  \u54624  \u50584 \u44592 \u44032  \u51080 \u45796 \u44256  \u50672 \u46973 \u51060  \u50772 \u45796 . \u45208 \u45716  \u50724 \u55176 \u47140  \u51096  \u46096 \u45796 \u44256  \u49373 \u44033 \u54664 \u45796 . \u51060 \u45824 \u47196  \u51648 \u45244  \u48148 \u50640 \u50556  \u45824 \u54032  \u49912 \u50864 \u44256  \u54756 \u50612 \u51648 \u51088 .\u47568 \u45796 \u53820 \u51060  \u50724 \u44052 \u45796 . \u44536 \u47084 \u45796  \u48372 \u45768  \u54217 \u49548 \u50640  \u48372 \u51648  \u47803 \u54664 \u45912  \u51032 \u50808 \u51032  \u47784 \u49845 , \u46307 \u51648  \u47803 \u54664 \u45912  \u49549 \u47560 \u51020 \u51012  \u46308 \u51012  \u49688  \u51080 \u50632 \u45796 . \u49912 \u50868  \u46244 \u50640  \u50864 \u47532 \u45716  \u45908  \u50528 \u53963 \u54644 \u51276 \u45796 .\u45224 \u51088 \u46308 \u51008  \u49912 \u50864 \u44256  \u45212  \u46244 \u50640  \u45908  \u52828 \u54644 \u51652 \u45796 \u45716  \u47568 \u51060  \u50612 \u50872 \u47549 \u45768 \u45796 . \u50756 \u46300 5\u44032  \u48708 \u49847 \u54620  \u49345 \u54889 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u44284  \u51032 \u44204 , \u44032 \u52824 \u44288 \u51060  \u54620  \u48264  \u52649 \u46028 \u54620  \u46244 , \u44288 \u44228 \u44032  \u45908 \u50865 \u45908  \u51652 \u51204 \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51060 \u51204 \u44620 \u51648  \u45813 \u45813 \u54616 \u44256  \u51652 \u51204  \u50630 \u45716  \u44288 \u44228 \u47484  \u51060 \u50612 \u50772 \u45796 \u47732 , \u50526 \u51004 \u47196 \u45716  \u48320 \u54868 \u54616 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u49849 \u48512 \u50837 \u51060 \u45208  \u50676 \u51221 \u51012  \u51088 \u44537 \u54624 \u47564 \u54620  \u51316 \u51116 \u44032  \u46321 \u51109 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u52964 \u47532 \u50612  \u48156 \u51204 \u51012  \u45432 \u47540  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51032 \u44204 \u51060  \u50504  \u47582 \u50500 \u49436  \u44040 \u46321 \u51060  \u49373 \u44592 \u45716 \u50756 \u46300 5\u45716  \u51032 \u44204  \u48520 \u51068 \u52824  \u48143  \u44040 \u46321 \u51012  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52852 \u46300  \u44536 \u47548 \u51012  \u48372 \u47732 , \u49457 \u51064  \u50668 \u47084  \u47749 \u51060  \u50756 \u46300 \u47484  \u54616 \u45208 \u50473  \u46308 \u44256  \u49436 \u47196  \u45796 \u47480  \u48169 \u54693 \u51012  \u44032 \u47532 \u53412 \u47728  \u49912 \u50864 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50668 \u44592 \u49436  \u50756 \u46300 \u45716  \u54665 \u46041 \u51012  \u51032 \u48120 \u54616 \u45716  \u50836 \u49548 \u51077 \u45768 \u45796 .\'91\u44033 \u51012  \u51105 \u44256  \u49912 \u50864 \u45716  \u45712 \u45196 \'92\u48372 \u45796 \u45716  \'91\u49324 \u54924  \u44277 \u46041 \u52404 \u50640 \u49436  \u51088 \u50672 \u49828 \u47112  \u51068 \u50612 \u45208 \u45716  \u48516 \u51137 \'92 \'91\u50612 \u51788 \u45796  \u48372 \u45768  \u47560 \u52272 \u51060  \u51068 \u50612 \u45208 \u44172  \u46104 \u45716 \'92 \u45712 \u45196 \u51060  \u44053 \u54633 \u45768 \u45796 . \u52852 \u46300  \u44536 \u47548 \u51012  \u48372 \u47732 , \u49912 \u50864 \u47140 \u44256  \u45208 \u50728  \u48373 \u51109 \u51060  \u50500 \u45785 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  \u50672 \u50528 \u50868 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u51089 \u50857 \u54633 \u45768 \u45796  :\u44208 \u54844 \u50640  \u45824 \u54620  \u49373 \u44033 \u51060  \u45796 \u47484  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45224 \u51088 \u45716  \u47784 \u46304  \u44163 \u51060  \u51456 \u48708 \u46108  \u51060 \u54980 \u50640  \u44208 \u54844 \u54616 \u44256  \u49910 \u50612 \u54616 \u45716  \u48152 \u47732 , \u50668 \u51088 \u45716  \u51068 \u45800  \u44208 \u54844 \u51012  \u47676 \u51200 \u54616 \u44256  \u49910 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 .\u49324 \u49548 \u54620  \u47568 \u45796 \u53820 \u51012  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u44536 \u47084 \u45208  \u51060 \u45716  \u47568 \u45796 \u53820 \u51060 \u45208  \u47800 \u49912 \u50880 \u47564 \u51012  \u51032 \u48120 \u54616 \u45716  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u53440 \u47196 \u51032  \u44536 \u47548 \u51008  \'91\u49345 \u54889 \u50640  \u45824 \u54620  \u48708 \u50976 \u51201  \u54364 \u54788 \'92\u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u50668 \u44592 \u49436  \u50756 \u46300 5\u45716  \'91\u49436 \u47196 \u51032  \u48169 \u54693 \u49457 \u51060  \u45796 \u47480  \u49345 \u54889 \'92 \u51004 \u47196  \u45331 \u44172  \u48148 \u45012  \u51060 \u54644 \u54616 \u47732  \u51339 \u49845 \u45768 \u45796 .\u50696 \u52968 \u45824  \u50040 \u51012  \u53440 \u45716  \u49345 \u54889 \u50640 \u49436 \u45716  \'91\u48128 \u45817 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u54620  \u49324 \u46988 \u51008  \u45817 \u44592 \u47140 \u44256  \u54616 \u45208 , \u54620  \u49324 \u46988 \u51008  \u48120 \u45716  \u49345 \u54889 . \u49436 \u47196 \u51032  \u48169 \u54693 \u49457 \u51060  \u45796 \u47476 \u44592  \u46412 \u47928 \u50640 , \u50025  \u51339 \u51008  \u49345 \u54889 \u51008  \u50500 \u45768 \u46972 \u44256  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51060  \u44284 \u51221 \u50640 \u49436  \u44048 \u51221 \u51060  \u49345 \u54620  \u45208 \u47672 \u51648  \u49436 \u47196 \u44032  \u47680 \u50612 \u51648 \u45716  \u51068 \u46020  \u48156 \u49373 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u51649 \u51109 \u51060 \u45208  \u54617 \u44368  \u45236 \u50640 \u49436  \u49324 \u49548 \u54620  \u53944 \u47084 \u48660 \u51060  \u51068 \u50612 \u45216  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51060 \u45716  \u45817 \u49888  \u53459 \u51060  \u50500 \u45776  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u50756 \u46300 5\u45716  \'91\u44060 \u51064 \u51032  \u51096 \u47803 \'92\u48372 \u45796  \u44277 \u46041 \u52404 \u50640 \u49436  \u51068 \u50612 \u45208 \u45716  \'91\u51032 \u44204  \u52264 \u51060 \'92\u47484  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u44221 \u51137 \u51088 \u44032  \u47566 \u51008 \u50756 \u46300 5\u51032  \u53945 \u51060 \u54620  \u53945 \u51669  \u51473  \u54616 \u45208 \u45716  \'91\u49324 \u46988 \u51060  \u47566 \u45796 \u45716  \u44163 \'92\u51077 \u45768 \u45796 . \u49324 \u46988  \u49688 \u47196  \u46384 \u51648 \u47732 , 78\u51109 \u51032  \u53440 \u47196  \u52852 \u46300  \u51473  \u49464  \u49552 \u44032 \u46973  \u50504 \u50640  \u44861 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u44536  \u47566 \u51008  \u49324 \u46988 \u46308 \u51060  \'91\u47924 \u50616 \u44032 \u47484  \u50948 \u54644  \u45796 \u53804 \u44256 \'92 \u51080 \u49845 \u45768 \u45796 .\u51593  \u54788 \u45824 \u54032 \u51004 \u47196  \u48148 \u45012  \u47568 \u54616 \u47732 , \u44221 \u51137 \u51060  \u49900 \u54616 \u45796 \u45716  \u46907 \u51004 \u47196  \u51060 \u54644 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50696 \u52968 \u45824  \u44221 \u51137 \u51060  \u49900 \u54616 \u45796 \u45716  \u44163 \u51008  :\u44221 \u51137 \u51088 \u44032  \u47566 \u50500  \u54633 \u44201 \u51060  \u55192 \u46308 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51060 \u45716  \u54633 /\u48520 \u51032  \u54645 \u49900 \u51060  \u45817 \u49888 \u51032  \u51032 \u51648 \u44032  \u50500 \u45772 , \u50808 \u48512  \u49345 \u54889 \u50640  \u45804 \u47140 \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50868 \u51060  \u54596 \u50836 \u54624  \u44163  \u44057 \u49845 \u45768 \u45796 .\u50672 \u50528 \u50868 \u50640 \u49436 \u45716 , \u45817 \u49888 \u51060  \u45572 \u44400 \u44032 \u47484  \u51339 \u50500 \u54616 \u44256  \u51080 \u45796 \u47732 , \u44221 \u51137 \u51088 \u44032  \u47566 \u45796 \u45716  \u44163 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u45796 \u47480  \u45572 \u44400 \u44032 \u46020  \u44536  \u49324 \u46988 \u51012  \u51339 \u50500 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u49324 \u45236  \u50668 \u47084  \u49324 \u46988 \u46308 \u44284 \u51032  \u44221 \u51137 \u51060  \u49900 \u54868 \u46112  \u44163 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u50668 \u47084  \u44032 \u51648 \u47196  \u49888 \u44221  \u50024 \u50556  \u54616 \u45716 \u50756 \u46300 5\u45716  \u51060 \u44163 \u51200 \u44163  \u49888 \u44221  \u50424  \u44172  \u47566 \u51008  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732  1\u45824 1 \u53664 \u45320 \u47676 \u53944  \u48169 \u49885 \u51060  \u50500 \u45768 \u46972 , \u54620 \u44732 \u48264 \u50640  \u51652 \u55129 \u53461  \u49912 \u50880 \u51012  \u48268 \u51060 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51060 \u45716  \u44263  \u51068 \u51060 \u45208  \u50672 \u50528 \u50640 \u49436  \u48156 \u49373 \u54616 \u45716  \u47928 \u51228 \u46308 \u46020  \'91\u54616 \u45208 \u50473 \'92 \u52376 \u47532  \u54616 \u51648  \u50506 \u44256 , \u46041 \u49884 \u45796 \u48156 \u51201 \u51004 \u47196  \u52376 \u47532 \u54644 \u50556  \u54632 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u51088 \u47732  :\u46024 \u51012  \u54620  \u44275 \u50640 \u49436  \u51665 \u51473 \u54644 \u49436  \u48260 \u45716  \u44163 \u51060  \u50500 \u45768 \u46972 , \u50668 \u47084  \u44032 \u51648  \u49688 \u51077 \u50896 \u50640 \u49436  \u48268 \u44172  \u46121 \u45768 \u45796 .\u50668 \u47084  \u44032 \u51648  \u49888 \u44221  \u50424  \u44172  \u47566 \u50500 \u49436 , \u50672 \u50528 \u50640  \u51665 \u51473  \u47803 \u54616 \u45716  \u49345 \u54889 \u51060  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u49828 \u53944 \u47112 \u49828 \u44032  \u49900 \u54620  \u49345 \u53468 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51658 \u50612 \u51648 \u44256  \u51080 \u45716  \u47928 \u51228 \u44032  \u45320 \u47924  \u47566 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u50668 \u47084 \u47784 \u47196  \u50526 \u51004 \u47196  \u45208 \u50500 \u44032 \u51648  \u47803 \u54616 \u44256 , \u51221 \u52404 \u46104 \u45716  \u49884 \u44592 \u44032  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51060 \u44163 \u51200 \u44163  \u49324 \u49548 \u54620  \u53944 \u47084 \u48660 \u51012  \u52376 \u47532 \u54644 \u50556  \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49692 \u53444 \u54664 \u45912  \u51068 , \u50672 \u50528 \u50640 \u49436 \u46020  \u50668 \u47084  \u47928 \u51228 \u44032  \u53552 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51004 \u47196 \u45716  \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54616 \u44256 , \u54620  \u44032 \u51648  \u51068 \u50640 \u47564  \u51665 \u51473 \u54616 \u46972 \u45716  \u47568 \u50432 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .\u50756 \u46300 5 \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300 5\u45716  \u44221 \u51137  \u49900 \u47532 \u44032  \u51080 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51593  \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \'91\u46972 \u51060 \u48268 \u51060  \u47566 \u51008  \u49324 \u46988 \'92\u50640 \u44172  \u45132 \u47532 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u44536  \u49324 \u46988 \u50640 \u44172  \u48376 \u51064 \u51060  \u51064 \u44592  \u47566 \u45796 \u45716  \u49324 \u49892 \u51012  \'91\u51032 \u46020 \u51201 \u51004 \u47196  \u50612 \u54596 \'92\u54616 \u47732  \u51339 \u49845 \u45768 \u45796 . \u51656 \u53804 \u49900 \u51012  \u50976 \u48156 \u54644 \u46020  \u51339 \u44192 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u54788 \u51116 \u45716  \u50672 \u50528 \u54616 \u44592  \u55192 \u46304  \u49345 \u54889 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51068 , \u44148 \u44053 , \u52712 \u48120  \u46321 \u46321  \u51060 \u44163 \u51200 \u44163  \u49888 \u44221 \u50424  \u44172  \u47566 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51068 \u45800  \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54644 \u50556  \u54633 \u45768 \u45796 . \'91\u44032 \u51109  \u47676 \u51200  \u47560 \u47924 \u47532  \u54644 \u50556  \u54624  \u44163 \'92\u51060  \u47924 \u50631 \u51064 \u51648  \u49373 \u44033 \u54616 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 .\u46608 \u54620 , \u54788 \u51116  \u45817 \u49888 \u51008  \u51032 \u44204  \u52264 \u51060 \u47484  \u45320 \u47924  \u49900 \u44033 \u54616 \u44172  \u49373 \u44033 \u54616 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51032 \u44204  \u52264 \u51060 \u45716  \u55124 \u54620  \u51068 \u51077 \u45768 \u45796 . \u50672 \u50528 \u47484  \u54616 \u44592  \u51204 , \'91\u45817 \u49888 \u51060  \u50857 \u45225 \u54624  \u49688  \u50630 \u45716  \u54665 \u46041 \'92\u51008  \u47924 \u50631 \u51064 \u51648  \u47676 \u51200  \u50508 \u50500 \u50556  \u54633 \u45768 \u45796 . \u44536  \u49440 \u51012  \u45336 \u51648  \u50506 \u45716  \u54620 , \u49345 \u45824 \u48169 \u44284  \u51201 \u51208 \u55176  \u54868 \u54644 \u54616 \u44256  \u47560 \u47924 \u47532  \u51667 \u45716  \'91\u44288 \u45824 \u54632 \'92\u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u51080 \u45796 \u47732 , \u51060 \u48120  \u44221 \u51137 \u51088 \u44032  \u47566 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \'91\u45796 \u47480  \u51060 \u49457 \u50640 \u44172 \u46020  \u51064 \u44592  \u47566 \u51008  \u49324 \u46988 \'92\u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u44536  \u49324 \u51060 \u50640 \u49436  \u46027 \u48372 \u51060 \u47140 \u47732 , \'91\u45208 \u46020  \u49772 \u50868  \u49324 \u46988 \u51008  \u50500 \u45768 \u45796 \'92 \'91\u45208 \u46020  \u44221 \u51137 \u47141  \u51080 \u45716  \u49324 \u46988 \u51060 \u45796 \'92\u46972 \u45716  \u51064 \u49885 \u51012  \u49900 \u50612 \u51452 \u50612 \u50556  \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u50640 \u44172  \u44413 \u49888 \u44144 \u47140 \u49436 \u45716  \u50504 \u46121 \u45768 \u45796 .\u50756 \u46300 5\u44032  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 ,\u45817 \u49888 \u51060  \u47924 \u49832  \u49373 \u44033 \u51012  \u54616 \u45716 \u51648  \u50508  \u49688 \u44032  \u50630 \u45796 . \u50780  \u44536 \u47111 \u44172  \u54665 \u46041 \u54616 \u45716 \u51648  \u51060 \u54644 \u44032  \u50504  \u44036 \u45796 . \u50724 \u55176 \u47140  \u54620  \u48264  \u49549 \u53552 \u45459 \u44256  \u49912 \u50864 \u44256  \u49910 \u45796 . \u44536 \u47084 \u47732  \u48376 \u49900 \u51060  \u46300 \u47084 \u45216 \u53580 \u45768 .\u45817 \u49888 \u50640 \u44172  \u44221 \u51137  \u49900 \u47532 \u47484  \u50976 \u48156 \u54616 \u44256  \u49910 \u45796 . \u48128 \u45817 \u51012  \u54616 \u44192 \u45796 . \u45208 \u47484  \u46160 \u44256  \u53804 \u45797 \u53804 \u45797 \u54616 \u45716  \u47784 \u49845 \u51060  \u48372 \u44256 \u49910 \u45796 .\u49334 \u50640  \u52824 \u50612  \u50672 \u50528 \u44256  \u47952 \u44256  \u54624  \u50668 \u50976 \u44032  \u50504  \u49373 \u44596 \u45796 . \u45320 \u47924  \u49888 \u44221 \u50424  \u44172  \u47566 \u45796 . \u47672 \u47551 \u49549 \u46020  \u48373 \u51105 \u54616 \u45796 .\u45817 \u49888 \u51060  \u54788 \u51116  \u52964 \u54540 \u51060 \u46972 \u47732 , \u51652 \u51648 \u54620  \u45824 \u54868 \u44032  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 . \u50672 \u51064  \u49324 \u51060 \u50640  \u47924 \u50616 \u44032  \u51032 \u44204  \u52264 \u51060 \u44032  \u51316 \u51116 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51060 \u45716  \u49324 \u49548 \u54620  \'91\u44048 \u51221  \u47928 \u51228 \'92\u44032  \u50500 \u45772 , \u50616 \u51232 \u44032 \u45716  \u44845  \u54801 \u49345 \u54616 \u44256  \u45336 \u50612 \u44032 \u50556  \u54624  \'91\u51473 \u45824 \u49324 \u50504 \'92\u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \'91\u44032 \u52824 \u44288 \'92\u51032  \u52264 \u51060 \u51077 \u45768 \u45796 . \u51060 \u48264  \u44592 \u54924 \u50640  \u49549 \u49884 \u50896 \u54616 \u44172  \u53560 \u50612 \u45459 \u45716  \u44284 \u51221 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51652 \u49900 \u51004 \u47196  \u49548 \u53685 \u54616 \u45716  \u44284 \u51221 \u50640 \u49436  \'91\u50528 \u53963 \u54632 \'92\u51012  \u45712 \u45188  \u44163 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u50640 \u44172  \'91\u48376 \u51064 \u51008  \u50612 \u46523 \u44172  \u49373 \u44033 \u54616 \u45716 \u51648 \'92 \u46608  \'91\u49345 \u45824 \u48169 \u51032  \u51077 \u51109 \u51008  \u50612 \u46500 \u51648 \'92 \u46308 \u50612 \u48372 \u49464 \u50836 .\u45817 \u49888 \u51060  \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u51116 \u54924  \u44032 \u45733 \u49457 \u51008  \u45230 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45796 \u47480  \u47924 \u50616 \u44032 \u50640  \u52824 \u50612  \u49324 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51116 \u54924 \u47484  \u49373 \u44033 \u52824 \u46020  \u47803 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52628 \u44032 \u47196  \u49345 \u45824 \u48169 \u51008  \'91\u45817 \u49888 \u44284  \u46020 \u51200 \u55176  \u53440 \u54801 \u54624  \u49688  \u50630 \u50612 \u49436 \'92 \u54756 \u50612 \u51652  \u46319  \u48372 \u51077 \u45768 \u45796 . \u46384 \u46972 \u49436  \u49345 \u45824 \u48169 \u51008  \u44288 \u44228 \u47484  \u45796 \u49884  \u51060 \u50612 \u45208 \u44040 \u51648  \u54869 \u49888 \u51060  \u50630 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u50756 \u46300 5 \u51649 \u50629 \u50868  \u54644 \u49437 \u50676 \u49900 \u55176  \u54644 \u46020  \u44208 \u44284 \u44032  \u50504  \u45208 \u50732  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47560 \u52824  \u45149 \u50630 \u45716  \u44172 \u51076 \u51012  \u54616 \u45716  \u44163 \u44284  \u44057 \u49845 \u45768 \u45796 . \u50668 \u44592 \u47564  \u53364 \u47532 \u50612  \u54616 \u47732  \u46112  \u51460  \u50508 \u50520 \u45716 \u45936  \u46608  \u45796 \u47480  \u48372 \u49828 \u44032  \u46321 \u51109 \u54616 \u44256 , \u46608 \u46608  \u45796 \u47480  \u48372 \u49828 \u44032  \u46321 \u51109 \u54616 \u44256 \'85 \u49324 \u49548 \u54620  \u47928 \u51228 \u44032  \u44228 \u49549  \u49373 \u44592 \u45796  \u48372 \u45768  \u47800 \u46020  \u47560 \u51020 \u46020  \u51648 \u52828  \u49345 \u53468 \u44032  \u46104 \u44192 \u49845 \u45768 \u45796 . \u50808 \u48512 \u51201 \u51004 \u47196 \u46020  \u50868 \u51060  \u50630 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u49345 \u45817 \u54620  \u44221 \u51137 \u50640  \u51649 \u47732 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u45796 \u47564 , \u51648 \u44552  \u55192 \u46304  \u49345 \u54889 \u47564  \u51060 \u44200 \u45236 \u47732 , \u50724 \u55176 \u47140  \u47928 \u51228 \u44032  \u49373 \u44592 \u44592  \u51204 \u48372 \u45796  \u50900 \u46321 \u55176  \u49457 \u51109 \u54644 \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 . \u49707 \u51088  5\u45716  \u48320 \u54868 \u50752  \u49457 \u51109 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u49324 \u49548 \u54620  \u47928 \u51228 \u47484  \u52376 \u47532 \u54616 \u45716  \u44284 \u51221 \u50640 \u49436  \'91\u51649 \u50629 \u51201  \u50756 \u47141 \'92\u51060  \u49373 \u44600  \u44163 \u51060 \u47728 , \u45908  \u45458 \u51008  \u47785 \u54364 \u47484  \u54693 \u54644  \u46020 \u51204 \u54616 \u45716  \u51088 \u49888 \u51012  \u48156 \u44204 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51649 \u51109 \u50640 \u49436 \u45716  \u46041 \u47308 \u50752  \u44040 \u46321 \u51012  \u44202 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536  \u50896 \u51064 \u51008  \u51452 \u47196  \'91\u49457 \u44284 \'92\u51077 \u45768 \u45796 . \u46041 \u47308  \u49324 \u51060 \u50640 \u49436  \u49457 \u44284 \u47484  \u44032 \u47196 \u52292 \u47140 \u54616 \u44144 \u45208 , \u49457 \u44284 \u47196  \u44221 \u51137  \u54616 \u47140 \u45716  \u49900 \u47532 \u44032  \u49373 \u44600  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49324 \u49548 \u54620  \u49828 \u53944 \u47112 \u49828  \u46412 \u47928 \u50640  \u50629 \u47924 \u50640  \u51665 \u51473 \u54616 \u44592  \u55192 \u46308 \u50612 \u51656  \u49688  \u51080 \u44192 \u51004 \u47728 , \'91\u48376 \u51064 \u51012  \u50612 \u54596 \u54616 \u47140 \u45716 \'92 \u45432 \u47141 \u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u50756 \u46300 5 \u44552 \u51204 \u50868  \u54644 \u49437 \u47112 \u46300 \u50724 \u49496 \u50640  \u46832 \u50612 \u46300 \u45716  \u44163 \u51008  \u50500 \u45772 \u51648  \u46028 \u50500 \u48400 \u50556  \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51452 \u49885 \u51077 \u45768 \u45796 . \u51648 \u44552  \u45817 \u49888 \u51008  \'91\u53960 \u49352  \u49884 \u51109 \'92\u51060  \u50500 \u45768 \u46972  \'91\u49548 \u47928 \u45212  \u51092 \u52824 \'92\u50640  \u46832 \u50612 \u46304  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51060 \u47196  \u51064 \u54644  \u45796 \u47480  \u49324 \u46988 \u44284  \u44040 \u46321 \u51012  \u48730 \u44172  \u46104 \u44144 \u45208 , \u49324 \u49548 \u54620  \u49828 \u53944 \u47112 \u49828 \u50640  \u55064 \u47568 \u47540  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45817 \u48516 \u44036  \u53804 \u51088 \u45716  \u48372 \u47448 \u54644 \u50556  \u54633 \u45768 \u45796 .    \
