   \uc0\u52981 7- \u49345 \u49345  \u52981 7 \u44536 \u47548 \u51032  \u51032 \u48120 \u44396 \u47492  \u49549 \u50640  \u45224 \u51088 \u51032  7\u44032 \u51648  \u49345 \u49345 \u51060  \u51080 \u45716  \u47784 \u49845 \u47581 \u49345 \u51060 \u45208  \u52265 \u44033 \u51012  \u51032 \u48120 \u44040 \u54588 \u47484  \u47803  \u51105 \u44144 \u45208  \u49440 \u53469 \u51648 \u44032  \u47566 \u51008  \u49345 \u54889 \u49464 \u49549 \u51201 \u51064  \u50837 \u47581 \u50640  \u51665 \u52265 \u49892 \u54665 \u47141 \u51060  \u46504 \u50612 \u51648 \u45716  \u49324 \u46988 \u49345 \u45824 \u48169 \u51012  \u51648 \u45208 \u52824 \u44172  \u51060 \u49345 \u54868 \u54624  \u49688 \u46020  \u51080 \u51020 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u54844 \u51088  \u49345 \u49345 \u50640  \u48736 \u51652 \u52981 7\u51008  \u54844 \u51088  \u49345 \u49345 \u50640  \u48736 \u51648 \u44144 \u45208 , \u52265 \u44033 \u54616 \u44256  \u51080 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u44396 \u47492 \u51008  \'91\u49345 \u49345 \u51032  \u50689 \u50669 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u44396 \u47492  \u49549 \u50640  \u51080 \u45716  \u52981 \u46308 \u51008  \u47784 \u46160  \'91\u45224 \u51088 \u51032  \u49345 \u49345 \'92\u51076 \u51012  \u45208 \u53440 \u45236 \u51424 . \u44172 \u45796 \u44032  \u52981  \u49549  \u45236 \u50857 \u47932  \u51473 \u50644  \'91\u50857 \'92\u52376 \u47100  \u49373 \u44596  \u46041 \u47932 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \'91\u49324 \u49892 \u50640  \u45824 \u54620  \u49345 \u49345 \'92\u51060  \u50500 \u45768 \u46972 , \'91\u54728 \u44396 \u50640  \u45824 \u54620  \u49345 \u49345 \'92\u51076 \u51012  \u46907 \u54633 \u45768 \u45796 . \u46608  \u45796 \u47480  \u52981 \u51012  \u48372 \u47732 , \u48372 \u49437 \u51060 \u45208  \u50900 \u44228 \u44288 , \u49457 , \u48192  \u46321  \u45796 \u50577 \u54620  \u47932 \u52404 \u46308 \u51060  \u51080 \u45716 \u45936 \u50836 . \u51060 \u45716  \u45224 \u51088 \u51032  \u47932 \u50837 , \u49457 \u52712 \u50837  \u49104 \u47564  \u50500 \u45768 \u46972 , \'91\u54200 \u50504 \u54616 \u44172  \u49332 \u44256  \u49910 \u51008  \u50837 \u47581 , \u50808 \u47784 \u50640  \u45824 \u54620  \u50837 \u47581 \'92 \u46321  \u47784 \u46304  \u51333 \u47448 \u51032  \u50837 \u47581 \u51012  \u45208 \u53440 \u45236 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51473 \u50836 \u54620  \u49324 \u49892 \u51008  \u51060  \u47784 \u46304  \u44163 \u46308 \u51060  \'91\u54788 \u49892 \u44284 \u45716  \u44144 \u47532 \u44032  \u47680 \u45796 \'92\u46972 \u45716  \u51216 \u51077 \u45768 \u45796 . \u49892 \u51228  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732 , \u47924 \u50616 \u44032  \u51648 \u45208 \u52824 \u44172  \u46160 \u47140 \u50892 \u54616 \u44144 \u45208 , \u55148 \u47581 \u51201 \u51060 \u44144 \u45208 , \u54841 \u51008  \u51228 \u47691 \u45824 \u47196  \u49345 \u49345  \u54924 \u47196 \u47484  \u46028 \u47532 \u44256  \u51080 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45224 \u51088 \u45716  \'91\u48376 \u51064 \u51032  \u49345 \u49345 \u51012  \u44284 \u49884 \u54616 \u45716  \u46319 \'92 \u54036 \u51012  \u48268 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50612 \u46412 ? \u51060 \u44172  \u45236  \u44228 \u54925 , \u47785 \u54364 \u50556 .\u50696 \u47484  \u46308 \u50612 \u48372 \u51088 \u47732  :\u49345 \u45824 \u48169 \u46020  \u45208 \u47484  \u51339 \u50500 \u54616 \u44256  \u51080 \u45796 \u44256  \u52265 \u44033 \u50640  \u48736 \u51256 \u51080 \u45716  \u49345 \u54889 \u53097 \u44621 \u51648 \u44032  \u49900 \u54616 \u44172  \u50444 \u50668  \u49345 \u45824 \u48169 \u51012  \'91\u51060 \u49345 \u54868 \'92 \u54616 \u45716  \u49345 \u54889 \u49345 \u45824 \u48169 \u51012  \u51228 \u47691 \u45824 \u47196  \u54032 \u45800 \u54644 \u49436  \u51060 \u48120 \u51648 \u47700 \u51060 \u53433  \u54616 \u45716  \u49900 \u47532 \u47568 \u47564  \u48264 \u51648 \u47476 \u47476 \u54620  \u49324 \u46988 \u52852 \u46300 \u44032  \u48376 \u51064 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u49345 \u51060  \u50500 \u45772  \u54788 \u49892 \u50640  \u45576 \u51012  \u46028 \u47532 \u46972 \u45716  \u51312 \u50616 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .Comment\u50857 \u51008  \u49436 \u50577 \u50640 \u49436  \u46160 \u47140 \u50880 \u51032  \u49345 \u51669 , \u46041 \u50577 \u50640 \u49436 \u45716  \u54665 \u50868 \u51032  \u49345 \u51669 \u51077 \u45768 \u45796 . \u53440 \u47196 \u51032  \u44592 \u50896 \u51008  \u49436 \u50577 \u51060 \u44592 \u50640 , \u52981 7\u51032  \u50857 \u51008  \u48372 \u53685  \'91\u46160 \u47140 \u50880 \'92\u51004 \u47196  \u54644 \u49437 \u46121 \u45768 \u45796 .\u54644 \u49437  2. \u44040 \u54588 \u47484  \u47803  \u51105 \u45716 \u52981 7\u51008  \u44040 \u54588 \u47484  \u47803  \u51105 \u44256  \u51080 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 , \u54841 \u51008  \u54844 \u46976 \u49828 \u47084 \u50868  \u49900 \u47532 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 .\u52852 \u46300 \u45716  \'91\u50612 \u45712  \u52981 \u51012  \u49440 \u53469 \u54644 \u50556  \u54624 \u51648  \u47581 \u49444 \u51060 \u45716  \u44536 \u47548 \'92\u51004 \u47196 \u46020  \u48372 \u51077 \u45768 \u45796 . \u52981 \u51008  \u52509  7\u44060 , \u49440 \u53469 \u51648 \u44032  \u47588 \u50864  \u45796 \u50577 \u54633 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640 \u49436 \u45716  :\u51060  \u49324 \u46988  \u51200  \u49324 \u46988  \u45796  \u47560 \u51020 \u50640  \u46300 \u45768 , \u45572 \u44396 \u47484  \u49440 \u53469 \u54624 \u51648  \u47581 \u49444 \u51060 \u45716  \u49345 \u54889 \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54616 \u51648  \u47803  \u54632 \u51060  \u51649 \u50629 , \u51200  \u51649 \u50629  \u51473  \u47924 \u49832  \u51068 \u51012  \u49440 \u53469 \u54644 \u50556  \u54624 \u51648  \u44256 \u48124  \u46104 \u45716  \u49345 \u53468 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51068 \u45800  \u54616 \u45208 \u47484  \u51221 \u54620  \u46244  \u52041  \u49892 \u54665 \u54616 \u46972 \u45716  \u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u50500 \u45768 \u45796  \u49910 \u51004 \u47732  \u44536 \u46412  \u48708 \u47196 \u49548  \u48169 \u54693 \u51012  \u53952 \u50612 \u46020  \u51339 \u44192 \u49845 \u45768 \u45796 .Comment\u44557 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \'91\u49440 \u53469 \u51648 \u44032  \u47566 \u45796 \'92\u45716  \u46907 \u51004 \u47196  \u51060 \u54644 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45236 \u50857 \u47932 \u51060  \u45224 \u51088 \u51032  \u47581 \u49345 \u51060  \u50500 \u45772  \'91\u49440 \u47932 \'92\u47196  \u54644 \u49437 \u46104 \u45716  \u44163 \u51060 \u51424 . 7\u44060 \u51032  \u49440 \u47932  \u49345 \u51088 \u44032  \u45576 \u50526 \u50640  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u49464 \u49549 \u51201 \u51064  \u50837 \u47581 \u50640  \u51665 \u52265 \u54616 \u45716 \u52981 7\u51008  \u50808 \u47784 \u45208  \u46024 \u52376 \u47100  \u49464 \u49549 \u51201 \u51064  \u50837 \u47581 \u50640  \u51665 \u52265 \u54616 \u45716  \u49324 \u46988 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52981  \u49549 \u50640  \u45812 \u44596  \u47932 \u52404 \u51032  \u51032 \u48120 \u47484  \u54644 \u49437 \u54616 \u47732  \u45796 \u51020 \u44284  \u44057 \u49845 \u45768 \u45796 [\u52280 \u44256  \u51088 \u47308 ] :\u54028 \u46976  \u50620 \u44404  : \u50500 \u47492 \u45796 \u50880 \u49457  : \u50504 \u51221 \u44048 \u50640  \u45824 \u54620  \u50837 \u44396 , \u51665 \u48372 \u49437  : \u48512 \u51032  \u49345 \u51669 \u50900 \u44228 \u44288  : \u49457 \u44277 \u44284  \u49849 \u47532 \u51032  \u49345 \u51669 \u50857  : \u46160 \u47140 \u50880  \u54841 \u51008  \u54665 \u50868 \u51032  \u49345 \u51669  (\u48372 \u53685  \u46160 \u47140 \u50880 )\u48192  : \u50976 \u54841  \u54841 \u51008  \u49457 \u50837 \u54616 \u50560  \u47581 \u53664 \u47484  \u46160 \u47480  \u49324 \u46988  : \u50689 \u51201 \u51064  \u47785 \u54364 \u48372 \u49884 \u45796 \u49884 \u54588 , \u44144 \u51032  \u47784 \u46304  \u49345 \u51669 \u51060  \'91\u50808 \u47784 , \u48512 , \u49457 \u44277 \'92 \u46321  \u49464 \u49549 \u51201 \u51064  \u50837 \u47581 \u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u49345 \u45824 \u48169 \u51032  \u51652 \u51676  \u47784 \u49845 \u51060 \u45208  \u51109 \u51216 \u51012  \u48372 \u51648  \u47803 \u54620  \u52292 , \'91\u49464 \u49549 \u51201 \u51064  \u51107 \u45824 \'92\u47196 \u47564  \u54217 \u44032 \u54624  \u49688  \u51080 \u51020 \u51012  \u44221 \u44256 \u54633 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45236 \u47732 \u51032  \u47785 \u49548 \u47532 \u47484  \u46384 \u47476 \u44592 \u48372 \u45796 \u45716  \'91\u51080 \u50612  \u48372 \u51060 \u45716 \'92 \u51649 \u50629 \u51012  \u49440 \u53469 \u44536  \u49324 \u46988 \u51032  \u45236 \u49892 \u44284  \u49345 \u44288 \u50630 \u51060  \'91\u51080 \u50612  \u48372 \u51060 \u45716 \u51648 \'92 \u47564 \u51012  \u48372 \u44256  \u51080 \u51020 \u46024 \u51060  \u47566 \u44256  \u50808 \u47784 \u44032  \u46832 \u50612 \u45212  \u49324 \u46988 \u51012  \u51648 \u45208 \u52824 \u44172  \u51060 \u49345 \u54868 \u52981  7\u48264  \u52852 \u46300 \u45716  \u51216 \u49457 \u54617 \u51201 \u51004 \u47196  \'91\u51204 \u44040 \u51088 \u47532 \'92\u47484  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51204 \u44040 \u51088 \u47532 \u45716  \u49457 \u51201 \u51064  \u50837 \u47581 , \u48708 \u48128 , \u44552 \u44592 \u50752  \u50672 \u44288 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u50501 \u47560  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u47484  \u44032 \u51665 \u45768 \u45796 .\u54644 \u49437  4. \u49892 \u54665 \u47141 \u51060  \u46504 \u50612 \u51648 \u45716 Seven of Cups\u45716  \u49892 \u54665 \u47141 \u51060  \u46504 \u50612 \u51648 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45224 \u51088 \u45716  \u49345 \u49345  \u49549  \u50689 \u50669 \u51064  \u44396 \u47492  \u47564 \u51012  \u52432 \u45796 \u48372 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u51593 , \u47561 \u50672 \u54616 \u44172  \u49373 \u44033 \u47564  \u54616 \u44256 , \u44396 \u52404 \u51201 \u51064  \u49892 \u54665 \u47141 \u51060  \u50630 \u51020 \u51012  \u46907 \u54633 \u45768 \u45796 . \u54788 \u49892 \u50640  \u45576 \u51012  \u46028 \u47140 \u50556  \u54620 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640 \u49436 \u45716  :\u49345 \u45824 \u48169 \u51012  \'91\u51060 \u49345 \u54868 \'92\u54616 \u44256  \u46608  \u51339 \u50500 \u54616 \u51648 \u47564 , \u49892 \u51228 \u47196  \u45796 \u44032 \u49436 \u47140 \u45716  \u45432 \u47141 \u51060  \u50630 \u45796 \u49345 \u49345  \u49549  \u50672 \u50528 \u47484  \u51600 \u44600  \u44032 \u45733 \u49457  \u45458 \u51020 \u50500 \u51060 \u46028  \u45909 \u51656 \u54616 \u45716  \u44163 \u44284  \u48708 \u49847 \u54620  \u49345 \u53468 \u49457 \u44277 \u51012  \u45000 \u44984 \u51648 \u47564 , \u44396 \u52404 \u51201 \u51064  \u44228 \u54925 \u51060 \u45208  \u49892 \u54665 \u47141 \u51060  \u50630 \u45716  \u49345 \u53468 \u47564 \u50557  7\u52981 \u51060  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u46321 \u51109 \u54664 \u45796 \u47732 , \u45817 \u49888 \u51032  \'91\u51652 \u51676  \u47784 \u49845 \'92\u51012  \u50508 \u44256  \u49892 \u47581 \u54616 \u44172  \u46112  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51339 \u50500 \u54616 \u45912  \u50500 \u51060 \u46028 \u51012  \u49892 \u51228 \u47196  \u47564 \u45208 \u48372 \u45768  \u48324 \u47196 \u50688 \u45796 \'85 \u46972 \u45716  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u52981 7 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u49345 \u49345 \u47141 \u51060  \u54413 \u48512 \u54616 \u44256  \u44557 \u51221 \u51201 \u51064  \u49457 \u44201 \u51077 \u45768 \u45796 . \'91\u50808 \u51201 \u51064  \u47784 \u49845 \'92\u50640  \u51665 \u52265 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50500 \u51060 \u46028  \u45909 \u54980 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u54637 \u49345  \u51060 \u49345 \u51201 \u51064  \u49345 \u45824 \u47484  \u52628 \u44396 \u54616 \u44592 \u50640  \u45576 \u51060  \u45458 \u51008  \u54200 \u51060 \u47728 , \u47560 \u51020 \u50640  \u46300 \u45716  \u49345 \u45824 \u48169 \u51012  \u51648 \u45208 \u52824 \u44172  \'91\u51060 \u49345 \u54868 \'92 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49324 \u46988  \u48372 \u45716  \u45576 \u51060  \u50630 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u49373 \u44033 \u51060  \u45320 \u47924  \u47566 \u45796  \u48372 \u45768 , \u49440 \u53469 \u51012  \u48120 \u47336 \u45716  \u44221 \u50864 \u46020  \u45796 \u48152 \u49324 . \'91\u51068 \u45800  \u49892 \u54665 \u54616 \u44256 , \u54788 \u49892 \u51012  \u44202 \u50612 \u48400 \u46972 \'92\u46972 \u45716  \u51312 \u50616 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .\u52981 7\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  3\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u50640 \u44172  \u50612 \u46500  \'91\u54872 \u49345 \'92\u51012  \u50504 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51032  \u51060 \u48120 \u51648 \u47484  \u48376 \u51064  \u51077 \u47579 \u45824 \u47196  \'91\u54200 \u51665 \'92 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \'93\u44116 \u45716  \u51060 \u47088  \u49324 \u46988 \u51068  \u44144 \u50556 \'94 \u54872 \u49345 \u51060  \u53360  \u47564 \u53372 , \u49892 \u47581 \u46020  \u53360  \u48277 . \u45817 \u49888 \u51032  \u48376 \u47784 \u49845 \u51012  \u50508 \u44172  \u46112  \u44221 \u50864  \u53356 \u44172  \u49892 \u47581 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u50668 \u47084  \u51060 \u49457 \u51060  \u45576 \u50640  \u46308 \u50612 \u50752  \'91\u47581 \u49444 \u51060 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 . \u50577 \u45796 \u47532 \u45208  \u48148 \u46988 \u51012  \u51032 \u48120 \u54616 \u44592 \u48372 \u45796 \u45716  \'91\u50500 \u51060 \u46028  \u45909 \u51656 \'92\u50640  \u44032 \u44620 \u50868  \u49900 \u47532 \u51077 \u45768 \u45796 . \u51060  \u50500 \u51060 \u46028  \u51200  \u50500 \u51060 \u46028  \u47784 \u46160  \u51339 \u50500 \u54624  \u49688  \u51080 \u51094 \u50500 \u50836 ? \u48708 \u49847 \u54620  \u44161 \u45768 \u45796 . \u54665 \u46041 \u47141 \u51060  \u46504 \u50612 \u51256  \u50672 \u46973  \u50732  \u44032 \u45733 \u49457 \u51008  \u51312 \u44552  \u45230 \u49845 \u45768 \u45796 . \u47560 \u51648 \u47561 , \u51648 \u45208 \u52824 \u44172  \u47924 \u50616 \u44032 \u47484  \u46160 \u47140 \u50892 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51656 \u45208 \u49244  \u49324 \u46988 \u51068 \u44620  \u46160 \u47140 \u50892 \u54616 \u44144 \u45208 , \u46496 \u45208 \u44040 \u44620 \u48400  \u46160 \u47140 \u50892 \u54616 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49440 \u53469 \u51648 \u44032  \u45320 \u47924  \u47566 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49324 \u46993 \u44284  \u51068 , \u44208 \u54844 \u44284  \u48708 \u54844  \u46321 \u46321 \'85 \u50668 \u47084 \u47784 \u47196  \u54844 \u46976 \u49828 \u47084 \u50868  \u49884 \u44592 \u51077 \u45768 \u45796 . \u46608 \u54620 , \u49345 \u45824 \u48169 \u51012  \u51648 \u45208 \u52824 \u44172  \'91\u54872 \u49345 \u51201 \'92\u51004 \u47196  \u50668 \u44600  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54788 \u49892 \u51012  \u50780 \u44257 \u54616 \u45796 \u48372 \u45768  \u50612 \u46500  \'91\u50948 \u54744  \u49888 \u54840 \'92\u47484  \u45459 \u52824 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u54788 \u49892  \u44048 \u44033 \u44284  \u49888 \u51473 \u54632 \u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u45208 \u49257 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \'91\u51060  \u49440 \u53469 , \u51200  \u49440 \u53469 \'92 \u54616 \u47336  \u51333 \u51068  \u44256 \u48124 \u54616 \u47728 , \u44032 \u49345 \u51032  \u49884 \u48044 \u47112 \u51060 \u49496 \u51012  \u46028 \u47532 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51060 \u47088  \u49885 \u51004 \u47196  \u50672 \u46973 \u54616 \u47732 \'85 \u44116 \u45716  \u51060 \u47088  \u49885 \u51004 \u47196  \u48152 \u51025 \u54616 \u44192 \u51648 ? \u47564 \u50557 \u50640  \u44144 \u51208  \u45817 \u54616 \u47732  \u50612 \u46497 \u54616 \u51648 ? \u45208 \u47484  \u47952 \u46972 \u44256  \u49373 \u44033 \u54624 \u44620 ? \u47564 \u50557 \u50640  \u45796 \u49884  \u49884 \u51089 \u54616 \u47732  \u50612 \u46523 \u44172  \u54644 \u50556  \u46104 \u51648 ? \u49464 \u44172  \u45208 \u44032 \u50556  \u46104 \u45208 ? \u50500 \u45768 \u47732  \u51060 \u47088  \u49885 \u51004 \u47196 ? \u49345 \u49345 \u47564  \u47566 \u44256  \u54665 \u46041 \u47141 \u51008  \u46504 \u50612 \u51648 \u45768 , \u45817 \u48516 \u44036  \u50672 \u46973  \u50724 \u51648  \u50506 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u52981 7 \u51649 \u50629 \u50868  \u54644 \u49437 \u45796 \u50577 \u54620  \u49440 \u53469 \u50640  \u51649 \u47732 \u54620  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51060  \u51068 \u51008  \u50612 \u46504 \u51648 , \u51200  \u51068 \u51008  \u50612 \u46504 \u51648  \u47581 \u49444 \u51060 \u45716  \u51473 \u51077 \u45768 \u45796 . \u47932 \u47200  \u50668 \u47084  \u49440 \u53469 \u51648 \u47484  \u44256 \u47140 \u54616 \u45716  \u54665 \u46041 \u51008  \u54788 \u47749 \u54633 \u45768 \u45796 . \u54616 \u51648 \u47564 , \u44536 \u44172  \u51109 \u44592 \u54868 \u46104 \u47732  \'91\u49884 \u44036  \u45229 \u48708 \'92\u44032  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51068 \u45800  \u47924 \u50631 \u51060 \u46304  \u44984 \u51456 \u55176  \u54644 \u48372 \u49464 \u50836 . \u49892 \u54056 \u54620  \u46244 \u50640  \u49440 \u53469 \u51012  \u49688 \u51221 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51064  \u45432 \u47141 \u48372 \u45796 \u45716  \'91\u54633 \u44201 \u54620  \u54980 \u51032  \u47784 \u49845 \'92\u51012  \u49345 \u49345 \u54616 \u44144 \u45208 , \'91\u44256 \u48124 \u44284  \u44145 \u51221 \'92\u50640  \u47566 \u51008  \u49884 \u44036 \u51012  \u50143 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49373 \u44033 \u51008  \u44536 \u47564 . \u51665 \u51473 \u54616 \u49464 \u50836 .\u52981 7 \u44552 \u51204 \u50868  \u54644 \u49437 \u46024 \u51012  \u50612 \u46523 \u44172  \u50424 \u51648  \u44256 \u48124 \u54616 \u49884 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45572 \u44396 \u45716  \u50612 \u46523 \u44172  \u46024 \u51012  \u48268 \u50632 \u45796 \u45908 \u46972 . \u47952 \u44032  \u51339 \u45796 \u45908 \u46972 . \u49440 \u53469 \u51648 \u44032  \u45320 \u47924  \u47566 \u50500  \u54844 \u46976 \u49828 \u47084 \u50892 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u55148 \u47581  \u54924 \u47196 \u45716  \u44552 \u47932 . \u51652 \u51676  \u46020 \u50880 \u51060  \u46104 \u45716  \u51312 \u50616 \u44284  \u45800 \u49692 \u54620  \u54872 \u49345 \u51012  \u44396 \u48324 \u54644 \u50556  \u54616 \u45716  \u49884 \u51216 \u51077 \u45768 \u45796 . \u51312 \u50616 \u51012  \u46300 \u47536 \u45796 \u47732 , \'93\u52572 \u45824 \u54620  \u54788 \u49892 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u49464 \u50836 .\'94\u51312 \u50616 \u51216 \u54644 \u49437  1. \u54788 \u49892 \u51012  \u51228 \u45824 \u47196  \u54028 \u50501 \u54616 \u49464 \u50836 \'91\u48512 \u51221 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u46972 \'92\u45716  \u46907 \u51060  \u50500 \u45785 \u45768 \u45796 . \u47561 \u50672 \u54620  \u44592 \u45824 \u44048 \u44284  \u46160 \u47140 \u50880 \u51012  \u54588 \u54616 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \'93\u44316 \u52270 \u51012  \u44163  \u44057 \u51004 \u45768 \u44620 \'94 \'93\u50612 \u47140 \u50872  \u44163  \u44057 \u51004 \u45768 \u44620 \'94 \u44057 \u51008  \u45712 \u45196 \u50640  \u51032 \u51316 \u54616 \u51648  \u47560 \u49464 \u50836 . \u44061 \u44288 \u51201 \u51064  \u49324 \u49892 , \u45936 \u51060 \u53552 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 . \u52712 \u50629 \u50640 \u49436 \u45716 , \'91\u47561 \u50672 \u54620  \u54840 \u44048 \'92\u51060  \u50500 \u45768 \u46972  \'91\u50672 \u48393 \u51060 \u45208  \u49892 \u51228  \u50629 \u47924  \u48516 \u50948 \u44592 , \u53685 \u44540  \u49884 \u44036 \'92 \u46321  \u54788 \u49892 \u51201 \u51064  \u51312 \u44148 \u51012  \u46384 \u51256 \u48400 \u50556  \u54616 \u47728  \u51064 \u44036  \u44288 \u44228 \u50640 \u49436 \u45716 , \u51228 \u45824 \u47196  \u47564 \u45212  \u51201  \u50630 \u45716  \u49324 \u46988 \u50640  \u45824 \u54644  \u54032 \u45800 \u54616 \u45716  \u44163 \u51012  \u49340 \u44032 \u50556  \u54633 \u45768 \u45796 .    \
