   \uc0\u54172 \u53440 \u53364 8 \u49444 \u47749 \u54788 \u51116  \u54616 \u44256  \u51080 \u45716  \u51068 \u50640  \u50756 \u51204 \u55176  \u51665 \u51473 \u54632 \u47785 \u54364 \u47484  \u54693 \u54644  \u52404 \u44228 \u51201 \u51004 \u47196  \u45432 \u47141 \u54616 \u44256  \u51080 \u51020 \u44048 \u51221 \u51201 \u51064  \u44368 \u47448 \u48372 \u45796 , \u49892 \u50857 \u51201 \u51060 \u47728  \u54788 \u49892 \u51201 \u51064  \u51456 \u48708 \u47484  \u54616 \u44256  \u51080 \u51020 \u52293 \u51076 \u51060 \u45208  \u51032 \u47924 \u47484  \u45796 \u54616 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u44256  \u51080 \u51020 \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u54172 \u53440 \u53364 \u51012  \u47564 \u46300 \u45716  \u45936  \u51665 \u51473 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060  \u52852 \u46300 \u45716  \u50896 \u54616 \u45716  \u47785 \u54364 \u47484  \u45804 \u49457 \u54616 \u44592  \u50948 \u54644  \u47792 \u46160 \u54616 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u48169 \u54644 \u48155 \u51648  \u50506 \u44592  \u50948 \u54644 , \u47560 \u51012 \u50640 \u49436  \u47680 \u47532  \u46504 \u50612 \u51652  \u47784 \u49845 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u54172 \u53440 \u53364 \u51008  \u52509  8\u44060 \u47196 , \u45208 \u47924 \u50640  6\u44060 , \u49552 \u50640  1\u44060 , \u48156  \u48145 \u50640  1\u44060 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44033 \u44033  \u44284 \u44144 , \u54788 \u51116 , \u48120 \u47000 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \'91\u54788 \u51116 \'92\u50640  \u51665 \u51473 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u46020  \u48120 \u47000 \u47484  \u46160 \u47140 \u50892 \u54616 \u44144 \u45208 , \u44284 \u44144 \u50640  \u50672 \u50672 \u54616 \u51648  \u50506 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u51004 \u47728 , \u44536 \u51200  \u47785 \u54364 \u47484  \u51060 \u47336 \u44592  \u50948 \u54620  \u54788 \u49892 \u51201  \u45824 \u48708 \u47484  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u49552 \u50640  \u46308 \u44256  \u51080 \u45716  \u54172 \u53440 \u53364 \u51032 \u48120 \u54788 \u49892 \u50640  \u51665 \u51473 \u54616 \u44256  \u51080 \u51020  (\u51068 \u48152 ) \u45208 \u53440 \u45236 \u45716  \u49345 \u54889  : \u54172 \u53440 \u53364 8\u51008  \u51452 \u50612 \u51652  \u44284 \u51228 \u45208  \u51032 \u47924 \u47484  \u54644 \u44208 \u54616 \u44592  \u50948 \u54644  \u47792 \u46160 \u54616 \u44256  \u51080 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u51068 \u51060 \u45208  \u44277 \u48512 \u51077 \u45768 \u45796 . \u44284 \u44144 \u50640  \u54644 \u50772 \u45912  \u45432 \u47141 \u51012  \u52041  \u51648 \u49549 \u54616 \u45716  \u49345 \u54889 \u51060 \u46972 \u47732 , \u47588 \u45320 \u47532 \u51608 \u50640  \u48736 \u51648 \u44144 \u45208 , \u51064 \u45236 \u49900 \u51060  \u48512 \u51313 \u54616 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .(\u51068 \u48152 ) \u51109 \u50528 \u47932  : \u47924 \u50616 \u44032  \u44592 \u49696 \u51060 \u45208  \u45432 \u47141 \u51060  \u48512 \u51313 \u54616 \u44144 \u45208 , \u46356 \u53580 \u51068 \u54620  \u48512 \u48516 \u51012  \u45459 \u52832  \u49688  \u51080 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u47924 \u50616 \u44032 \u50640  \u49689 \u45804 \u54616 \u47140 \u47732 , \u45128 \u44592 \u50752  \u51064 \u45236 \u51032  \u49884 \u44036 \u51060  \u54596 \u50836 \u54632 \u51012  \u50508 \u47140 \u51469 \u45768 \u45796 .\u44557 \u51221 \u51201  \u51032 \u48120 \u51665 \u51473 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 , \u47924 \u50616 \u44032  \u48176 \u50892 \u50556  \u54616 \u45716  \u49345 \u54889 , \u47564 \u46308 \u50612 \u50556  \u54616 \u45716  \u49345 \u54889 \u50640 \u49436  \u55192 \u51012  \u48156 \u55064 \u54633 \u45768 \u45796 . \u44032 \u51109  \u45824 \u54364 \u51201 \u51064  \u50696 \u49884 \u47196  \u49688 \u45733 , \u51088 \u44201 \u51613  \u49884 \u54744 , \u50629 \u47924  \u54532 \u47196 \u51229 \u53944  \u46321 \u46321 \u51077 \u45768 \u45796 . \u51068 \u51008  \u48372 \u53685  \u49457 \u44277 \u51201 \u51004 \u47196  \u51652 \u54665 \u46104 \u51648 \u47564 , \u51473 \u44036  \u51473 \u44036  \u51064 \u45236 \u49900 \u51060  \u48148 \u45797 \u45208 \u44144 \u45208  \u47588 \u45320 \u47532 \u51608 \u50640  \u48736 \u51648 \u45716  \u51068 \u46020  \u49373 \u44600  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47560 \u47924 \u47532  \u45800 \u44228 \u44032  \u51473 \u50836 \u54633 \u45768 \u45796 .\u46608 \u54620  \u54788 \u49892 \u51201 \u51060 \u47728 , \u49892 \u50857 \u51201 \u51064  \u51456 \u48708 \u47484  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844  \u51456 \u48708 , \u49548 \u50948  \u47568 \u54616 \u45716  \'91\u49828 \u54169 \'92\u51012  \u44054 \u52628 \u44592  \u50948 \u54644  \u45432 \u47141 \u54620 \u45796 \u44144 \u45208 , \u50504 \u51221 \u46108  \u44592 \u48152 (\u51665 , \u51649 \u50629 )\u51012  \u44054 \u44592  \u50948 \u54644  \u45432 \u47141 \u54620 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u51032 \u48120 \u51452 \u48320 \u51012  \u46168 \u47084 \u48372 \u51648  \u50506 \u44256  \u48376 \u51064  \u54624  \u51068 \u50640 \u47564  \u51665 \u51473 \u54616 \u45716  \u47784 \u49845 \u51004 \u47196 , \u48512 \u51221 \u51201  \u51032 \u48120 \u44032  \u46300 \u47084 \u45208 \u44592 \u46020  \u54633 \u45768 \u45796 . \u54028 \u53944 \u45320 \u45716  \u50504 \u51473 \u50640 \u46020  \u50630 \u51004 \u47728 , \u47785 \u54364 \u47484  \u54693 \u54620  \u51665 \u52265  \u46412 \u47928 \u50640  \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u46496 \u45208 \u44172  \u47564 \u46300 \u45716  \u52852 \u46300  \u51473  \u54616 \u45208 \u51077 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u45320 \u47924  \u49464 \u49464 \u54620  \u46356 \u53580 \u51068 \u50640  \u51665 \u52265 \u54620 \u45796 \u45716  \u44163 \u46020  \u54616 \u45208 \u51032  \u45800 \u51216 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u44048 \u51221 \u51201  \u44368 \u47448 \u44032  \u48512 \u51313 \u54616 \u45796 \u45716  \u49888 \u54840 \u47196 \u46020  \u54644 \u49437 \u54616 \u47728 , \u47196 \u47592 \u54001 \u54620  \u45936 \u51060 \u53944 \u45208  \u51060 \u48292 \u53944 \u44032  \u54596 \u50836 \u54632 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u54172 \u53440 \u53364 8 \u50672 \u50528 \u50868 \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u54172 \u53440 \u53364 8\u51060  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u54788 \u51116 \u51032  \u44288 \u44228 \u47484  \u48156 \u51204 \u49884 \u53412 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u54172 \u53440 \u53364 \u51032  \u50640 \u45320 \u51648 \u45716  \u44048 \u51221 \u48372 \u45796  \u49892 \u50857 \u49457 , \u54788 \u49892 \u49457 \u50640  \u52824 \u51473 \u46104 \u50612  \u51080 \u49845 \u45768 \u45796 . \u52964 \u54540 \u51060 \u46972 \u47732 , \u49892 \u50857 \u51201 \u51060 \u44256  \u54788 \u49892 \u51201 \u51064  \u51456 \u48708 \u47484  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54616 \u47728 , \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844  \u51456 \u48708 (\u51665  \u47560 \u47144 )\u51077 \u45768 \u45796 . \u44288 \u44228 \u44032  \u49457 \u49324 \u46104 \u44592  \u51060 \u51204 \u51060 \u46972 \u47732 , \u51339 \u51008  \u54924 \u49324  \u46041 \u47308 , \u51339 \u51008  \u54617 \u44368  \u52828 \u44396  \u51221 \u46020 \u51032  \u54840 \u44048 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972 , \'91\u45908  \u51339 \u51008  \u49345 \u45824 \u44032  \u46104 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u44256  \u51080 \u51020 \'92\u51060 \u46972 \u45716  \u51032 \u48120 \u46020  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888  \u46608 \u54620  \u51312 \u44552  \u45908  \u50808 \u47784 \u47484  \u44032 \u44984 \u44144 \u45208 , \u44221 \u51228 \u51201 \u51004 \u47196  \u50504 \u51221 \u46108  \u49345 \u54889 \u51012  \u47564 \u46308 \u44592  \u50948 \u54644  \u45432 \u47141 \u54644 \u50556  \u54620 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u44288 \u44228 \u44032  \u49457 \u47549 \u54616 \u51648  \u50506 \u51008  \u49345 \u54889  : \u44288 \u44228 \u44032  \u49457 \u47549 \u54616 \u44592  \u51060 \u51204 \u51060 \u46972 \u47732 , \u50672 \u51064 \u51060  \u46104 \u44592  \u50948 \u54620  \u51456 \u48708 \u45208  \u49884 \u44036 \u51060  \u54596 \u50836 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50500 \u51649  \u46168  \u49324 \u51060 \u45716  \'91\u48520 \u44867  \u53888 \u45716  \u47196 \u47592 \u49828 \'92\u46972 \u44592 \u48372 \u45796  \'91\u51060 \u54644 \u44288 \u44228 \u44032  \u50621 \u54784  \u51080 \u45716  \u49324 \u51060 \'92 \u51221 \u46020 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \'91\u51339 \u51008  \u46041 \u47308 \'92, \'91\u50619 \u50612 \u44040  \u44172  \u47566 \u51008  \u49324 \u46988 \'92 \u51221 \u46020 \u47196  \u49373 \u44033 \u54616 \u44256  \u51080 \u51004 \u47728 , \u48376 \u51064 \u46020  \u51060 \u47484  \u50948 \u54644  \u45432 \u47141 \u54616 \u44256  \u51080 \u45716  \u51473 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u44057 \u51060  \u47924 \u50616 \u44032 \u47484  \u54616 \u45716  \u44284 \u51221 \u50640 \u49436  \u44048 \u51221 \u51060  \u49373 \u44200 \u45208 \u45716  \u44221 \u50864 \u46020  \u51080 \u51004 \u45768 , \u51312 \u44552  \u45908  \u44592 \u45796 \u47532 \u47728  \u44288 \u44228 \u47484  \u51652 \u54665 \u54644 \u50556  \u54633 \u45768 \u45796 .\u44288 \u44228 \u44032  \u49457 \u47549 \u54620  \u49345 \u54889  : \u54732 \u49888 \u44284  \u45432 \u47141 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 . \u52964 \u47532 \u50612 \u50640  \u51665 \u51473 \u54644 \u50556  \u54616 \u45716  \u49884 \u44036 \u51060  \u54596 \u50836 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u52852 \u46300  \u49549  \u54172 \u53440 \u53364 \u51008  \u52509  8\u44060 , \u51060 \u48120  \u54172 \u53440 \u53364  6\u44060 \u44032  \u50724 \u47480 \u51901 \u50640  \u44152 \u47536  \u44163 \u51004 \u47196  \u48372 \u50500 , \u44284 \u44144 \u45716  \u44900 \u45208  \u51339 \u51008  \u44288 \u44228 \u50688 \u51020 \u51012  \u48372 \u50668 \u51469 \u45768 \u45796 . \u54788 \u51116 \u50752  \u48120 \u47000  \u44033 \u44033  1\u44060 \u50473  \u45224 \u50520 \u51004 \u45768 , \u44208 \u44284 \u47932 \u51032  \u50756 \u49457 (\u44208 \u54844 )\u51060  \u47672 \u51648 \u50506 \u50520 \u51020 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u45320 \u47924  \u49324 \u49548 \u54620  \u44163 \u50640  \u51665 \u52265 \u54616 \u45796  \u48372 \u47732 , \u44288 \u44228 \u44032  \u53952 \u50612 \u51656  \u49688  \u51080 \u51004 \u45768  \u50612 \u50689 \u48512 \u50689  \u45336 \u50612 \u44032 \u45716  \u53468 \u46020 \u46020  \u54596 \u50836 \u54633 \u45768 \u45796 .\u44288 \u44228 \u44032  \u45130 \u44596  \u49345 \u54889  : \u49345 \u45824 \u48169 \u44284  \u45796 \u49884  \u51060 \u50612 \u51648 \u44592 \u47484  \u50896 \u54620 \u45796 \u47732 , \u44284 \u44144 \u45208  \u48120 \u47000 \u50640  \u51665 \u52265 \u54624  \u44163 \u51060  \u50500 \u45768 \u46972 , \u54788 \u51116 \u50640  \u47792 \u46160 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51648 \u44552 \u51008  \u50868 \u46041 \u51060 \u45208  \u51068 , \u51088 \u44592 \u44228 \u48156 \u50640  \u51665 \u51473 \u54644 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u52628 \u44032 \u47196 , \u54756 \u50612 \u51652  \u51060 \u50976 \u44032  \u47924 \u50631 \u51064 \u51648 , \u45236  \u45800 \u51216 \u51008  \u47924 \u50631 \u51064 \u51648 , \u51648 \u44552  \u45817 \u51109  \u54624  \u49688  \u51080 \u45716  \u51068 \u51008  \u47924 \u50631 \u51064 \u51648  \u44256 \u48124 \u54644 \u48372 \u50500 \u50556  \u54633 \u45768 \u45796 . \u45208 \u47924 \u50640  \u44152 \u47140 \u51080 \u45716  \u54172 \u53440 \u53364 \u51008  \u47784 \u46160  \u46609 \u44057 \u51008  \u54056 \u53556 \u51012  \u51648 \u45768 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u47484  \u51025 \u50857 \u54644  \u54644 \u49437 \u54616 \u47732 , \u44284 \u44144  \u49892 \u49688 \u47484  \u46609 \u44057 \u51060  \u46104 \u54400 \u51060 \u54624  \u49688  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196 , \u51096 \u47803 \u46108  \u48512 \u48516 \u51008  \u44622 \u50500 \u45236 \u50556  \u54632 \u51012  \u50508 \u47140 \u51469 \u45768 \u45796 .\u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988  : \u48376 \u51064  \u54624  \u51068 \u50640  \u52649 \u49892 \u54620  \u49324 \u46988 \u51077 \u45768 \u45796 . \u44540 \u47732 \u54616 \u44256  \u49457 \u49892 \u54616 \u47728 , \u47924 \u50616 \u44032 \u50640  \u47792 \u46160 \u54624  \u46412 , \u48731 \u51012  \u48156 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u54217 \u54032 \u46020  \u51339 \u51008  \u54200 \u51077 \u45768 \u45796 . \u54616 \u51648 \u47564  \u46412 \u47196  \u45320 \u47924  \u49324 \u49548 \u54620  \u46356 \u53580 \u51068 \u50640  \u51665 \u52265 \u54644 \u49436  \u50997 \u53685 \u49457  \u50630 \u45716  \u49324 \u46988 , \u49549  \u51329 \u51008  \u49324 \u46988 \u51004 \u47196 \u46020  \u48708 \u52656 \u51648 \u44592  \u49789 \u49845 \u45768 \u45796 . \u53945 \u51221  \u49324 \u44148 \u51060 \u45208  \u54788 \u49345 \u51012  \u47680 \u47532 \u49436  \u48380  \u54596 \u50836 \u44032  \u51080 \u51004 \u47728 , \u51452 \u48320  \u49324 \u46988 \u46308 \u44284  \u54801 \u47141 \u54644 \u49436  \u51068 \u54624  \u54596 \u50836 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51088 \u52843  \'91\u45440  \u51460  \u47784 \u47476 \u45716  \u45320 \u46300 \u45224 /\u45320 \u46300 \u45376 \'92\u47196  \u48708 \u52656 \u51656  \u49548 \u51648 \u44032  \u51080 \u51004 \u45768 , \u45908 \u50865  \u45908  \u51088 \u44592  \u44288 \u47532 \u50640  \u55192 \u50024 \u50556  \u54633 \u45768 \u45796 .\u54172 \u53440 \u53364 8 \u44552 \u51204 /\u51649 \u50629 \u50868 \u48376 \u51064 \u51060  \u49549 \u54620  \u48516 \u50556 \u50640 \u49436  \u52572 \u44256 \u44032  \u46104 \u44144 \u45208  \u51109 \u51064  \u51221 \u49888 \u51012  \u48156 \u55064 \u54624  \u51068 \u51060  \u49373 \u44609 \u45768 \u45796 . \u51064 \u45236 \u49900 \u51012  \u44032 \u51652 \u45796 \u47732 , \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u54217 \u54032 \u51012  \u45328 \u47532  \u50508 \u47540  \u44592 \u54924 \u44032  \u49373 \u44609 \u45768 \u45796 . \u50500 \u51649  \u54617 \u49373 \u51060 \u46972 \u47732 , \u49345 \u51109 \u51060 \u45208  \u51109 \u54617 \u44552 \u51012  \u48155 \u44172  \u46120 \u51012  \u51032 \u48120 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54172 \u53440 \u53364 8\u51008  \'91\u54732 \u49888 \u44284  \u45432 \u47141 \'92, \u44536 \u47532 \u44256  \u47792 \u51077 \u51060  \u51473 \u50836 \u54620  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51105 \u49373 \u44033 \u51008  \u51217 \u50612 \u46160 \u44256 , \u54644 \u50724 \u45912  \u45824 \u47196  \u52280 \u51012 \u49457  \u51080 \u44172  \u51068 \u51012  \u51652 \u54665 \u54616 \u49464 \u50836 . \u44284 \u44144  6 : \u54788 \u51116  1 : \u48120 \u47000  1\u51077 \u45768 \u45796 . \u49457 \u44277 \u51060  \u47672 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 .    \
