   8. \uc0\u55192 Strength \u55192  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u54620  \u50668 \u51064 \u51060  \u49324 \u51088 \u47484  \u45796 \u49828 \u47532 \u45716  \u47784 \u49845 \u50837 \u47581 \u51060 \u45208  \u48376 \u45733 \u51012  \u45796 \u49828 \u47536 \u45796 \u45716  \u46907 \u52649 \u46041 \u51201 \u51064  \u47560 \u51020 \u51012  \u51088 \u51228 \u54616 \u45716  \u47784 \u49845 \u54620 \u51901 \u51060  \u52280 \u44256  \u51064 \u45236 \u54616 \u45716  \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u45236 \u47732 \u51032  \u55192 \u51004 \u47196  \u53685 \u51228 \u54616 \u45716 \u55192  \u52852 \u46300 \u45716  \u47704 \u53448 \u51060  \u44053 \u54620  \u49324 \u46988 , \u54841 \u51008  \u50837 \u47581 \u51012  \u53685 \u51228 \u54616 \u47140 \u45716  \u49900 \u47532 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50668 \u44592 \u49436  \u47568 \u54616 \u45716  \u50837 \u47581 \u51060 \u46976 , \u45824 \u52404 \u47196  \u49457 \u50837  \u44057 \u51008  \u48376 \u45733 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u52852 \u46300 \u50640  \u44536 \u47140 \u51652  \u49324 \u51088 \u45716  \u50864 \u47532 \u51032  \u50837 \u47581 , \u48376 \u45733 \u51012  \u49345 \u51669 \u54616 \u45716  \u50836 \u49548 \u51077 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u49885 \u50837 , \u49457 \u50837 , \u49688 \u47732 \u50837  \u46321 \u51077 \u45768 \u45796 . \u50668 \u51064 \u51008  \u49324 \u51088 \u44032  \u45216 \u46832 \u51648  \u50506 \u46020 \u47197  \'91\u48372 \u49332 \u54588 \u44256 \'92 \u51080 \u45716 \u45936 , \u51060 \u45716  \u44263  \u50837 \u47581 \u51060 \u45208  \u48376 \u45733 \u51012  \u48512 \u46300 \u47101 \u44172  \u45796 \u49828 \u47536 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54788 \u51116  \u49324 \u51088 \u45716  \u44844 \u47532 \u47484  \u45236 \u47140  \u50668 \u51064 \u50640 \u44172  \u48373 \u51333 \u54616 \u45716  \u49345 \u53468 \u51424 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u49884 \u54744  \u44592 \u44036 \u50640  \u45440 \u44256  \u49910 \u51648 \u47564  \u47925 \u47925 \u55176  \u46020 \u49436 \u44288 \u51012  \u44032 \u45716  \u49345 \u54889 \u49345 \u45824 \u48169 \u51060  \u55141 \u48516 \u54616 \u51648  \u50506 \u46020 \u47197  \u48516 \u50948 \u44592 \u47484  \u52264 \u48516 \u54616 \u44172  \u54616 \u45716  \u49324 \u46988 \u48516 \u45432  \u51312 \u51208  \u45733 \u47141 \u51060  \u46832 \u50612 \u45212  \u49324 \u46988 \u49345 \u45824 \u48169  \u44592 \u48516 \u51060  \u49345 \u54616 \u51648  \u50506 \u46020 \u47197 , \u50896 \u54616 \u45716  \u48148 \u47484  \u46028 \u47140 \u49436  \u47568 \u54616 \u45716  \u49828 \u53440 \u51068 Strength \u52852 \u46300 \u45716  \u49828 \u49828 \u47196 \u51032  \u55192 \u51004 \u47196  \u51068 \u51012  \u45149 \u44620 \u51648  \u54644 \u45240 \u45796 \u45716  \u46907 \u46020  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51064 \u45236 \u49900 \u44284  \u45128 \u44592 \u47484  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51424 . \u46412 \u47928 \u50640  \u47676  \u48120 \u47000 \u51032  \u49324 \u50629 \u50868 \u51060 \u45208  \u54633 \u44201 \u50868 \u46020  \u51339 \u51008  \u54200 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51068 \u51032  \u48120 \u47000 \u47484  \u51656 \u47928 \u54664 \u51012  \u44221 \u50864 , \u45817 \u49888 \u51032  \u45733 \u47141 \u51012  \u49884 \u54744 \u54616 \u44592  \u50948 \u54644  \u53360  \u44592 \u54924 \u44032  \u48169 \u47928 \u54620 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u53440 \u47196  \u52852 \u46300 \u12300 8\u48264  : \u55192 \u12301 \u51032  \u51032 \u48120 \u50752  \u54644 \u49437  <\u51068 , \u50672 \u50528 > \'96 tarotfun.jp\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50732  \u44221 \u50864 , \'91\u54665 \u46041 \u44284  \u47560 \u51020 \u51060  \u51221 \u48152 \u45824 \'92\u51068  \u44032 \u45733 \u49457 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50837 \u47581 \u51060 \u45208  \u48376 \u45733 \u51012  \u50613 \u45572 \u47476 \u44256  \u53685 \u51228 \u54620 \u45796 \u45716  \u51032 \u48120 \u51060 \u44592  \u46412 \u47928 \u51060 \u51424 . \u50696 \u47484  \u46308 \u50612 , \u44169 \u51008  \u46384 \u46907 \u54616 \u51648 \u47564  \u49549 \u51004 \u47196  \u50837 \u51012  \u54616 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u44256 , \u44169 \u51008  \u52264 \u44049 \u51648 \u47564  \'91\u51339 \u50500 \u54616 \u45716  \u47560 \u51020 \'92\u51012  \u49704 \u44592 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u52649 \u46041 \u51201 \u51064  \u47560 \u51020 \u51012  \u50613 \u51228 \u54616 \u45716 8\u48264  \u55192  \u52852 \u46300 \u45716  \u52649 \u46041 \u51201 \u51064  \u47560 \u51020 \u51012  \u50613 \u51228 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u49324 \u51088 \u45716  \u48520 \u51032  \u49549 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48520 \u51008  \u54869  \u53440 \u50732 \u46992 \u45796 \u44032  \u54869  \u44732 \u51648 \u45716  \u49457 \u51656 \u51060  \u51080 \u51424 . \u50612 \u46356 \u47196  \u53896 \u51648  \u50696 \u52769 \u54616 \u44592 \u46020  \u55192 \u46317 \u45768 \u45796 . \u48520 \u51008  \u44263  \'91\u52649 \u46041 \u51201 \u51064  \u48376 \u45733 , \u50837 \u47581 \'92 \u46321 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50668 \u51064 \u51008  \'91\u48520 \u51032  \u49549 \u49457 \'92\u51012  \u52264 \u48516 \u54616 \u44172  \u52968 \u53944 \u47204 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u50668 \u47084  \u51060 \u49457 \u44284  \u51104 \u51088 \u47532 \u47484  \u44032 \u51648 \u44256  \u49910 \u51008  \u47560 \u51020 \u51012  \u50613 \u51228 \u54616 \u45716  \u51473  (\u48148 \u46988  \u54588 \u51648  X)\u49345 \u45824 \u48169 \u51060  \u48512 \u45812  \u44032 \u51656 \u44620  \u51339 \u50500 \u54616 \u45716  \u47560 \u51020 \u51012  \u50528 \u50024  \u50613 \u45572 \u47476 \u44144 \u45208 \u54887 \u44608 \u50640  \u50672 \u46973 \u54616 \u45716  \u54665 \u46041 \u51012  \u52572 \u45824 \u54620  \u51088 \u51228 \u54616 \u45716  \u51473 \u45824 \u52404 \u47196  \u49457 \u50837 \u51060  \u44053 \u54616 \u51648 \u47564 , \u51312 \u51208  \u45733 \u47141 \u46020  \u46832 \u50612 \u45212  \u49324 \u46988 \u50640 \u44172  \u47566 \u51060  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u54620 \u51901 \u51060  \u52280 \u44256  \u51064 \u45236 \u54616 \u45716 \u55192  \u53440 \u47196  \u52852 \u46300 \u45716  \u51068 \u48169 \u51201 \u51004 \u47196  \u54620 \u51901 \u51060  \u52280 \u50500 \u51452 \u45716  \u44288 \u44228 , \u54841 \u51008  \u50724 \u47019 \u46041 \u50504  \u51064 \u45236 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u47560 \u52824  \u51664 \u49849 \u51012  \u44600 \u46308 \u51060 \u46319  \u47568 \u51060 \u51424 . \u55192  \u52852 \u46300 \u45716  \u49324 \u49892  \u48512 \u51221 \u51201 \u51064  \u48512 \u48516 \u51060  70%\u51077 \u45768 \u45796 . \u55192 \u51012  \u50024 \u50556  \u54620 \u45796 \u45716  \u44163 \u51008  \u44263  \'91\u51664 \u51060  \u47924 \u44161 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 .\u47564 \u50557  \u55192  \u52852 \u46300 \u44032  \u54788 \u45824 \u54032 \u51004 \u47196  \u45208 \u50728 \u45796 \u47732  :\u51204 \u48391 \u45824  \u50694 \u50640  \u44284 \u51020 \u54620  \u48512 \u51109 \u45784 \u51060  \u49436 \u51080 \u49845 \u45768 \u45796 . \u44536 \u44032  \u45908  \u51060 \u49345  \u48120 \u52432  \u45216 \u46832 \u51648  \u50506 \u46020 \u47197 , \u48512 \u54616  \u51649 \u50896 \u51060  \u48708 \u50948 \u47484  \u47582 \u52656 \u51452 \u44256  \u51080 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u48512 \u54616  \u51649 \u50896 \u51008  \u51228 \u48156  \u44536 \u44032  \u51312 \u50857 \u55176  \u51665 \u50640  \u44032 \u44592 \u47484  \u48148 \u46972 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 .\u50672 \u50528 \u50868 \u50640 \u49436 \u45716 , \u50612 \u45712  \u54620 \u51901 \u51060  \'91\u50724 \u45264 \u50724 \u45264 \'92 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50672 \u50528 \u47484  \u47560 \u52824  \u50528  \u53412 \u50864 \u46319  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45320 \u47924  \u51088 \u51452  \u49296 \u51652 \u45796 \u44144 \u45208 , \u54868 \u47484  \u45240 \u45796 \u44144 \u45208 , \u50836 \u44396  \u49324 \u54637 \u51060  \u47566 \u51008  \u49324 \u46988 \u51012  \u45804 \u47000 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u45208 \u51060 \u52264 \u44032  \u47566 \u51060  \u45208 \u45716  \u52964 \u54540 \u50640 \u44172  \u51088 \u51452  \u46321 \u51109 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54596 \u50672 \u51201 \u51004 \u47196  \u45572 \u44400 \u44032 \u44032  \u55148 \u49373 \u54616 \u47728  \u51064 \u45236 \u49900 \u51012  \u48156 \u55064 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824 . \u49324 \u50629 \u50868 \u51012  \u47932 \u50632 \u51012  \u46412 \u46020  \u51312 \u44552 \u47564  \u45908  \u52280 \u50500 \u46972 . \u51109 \u44592 \u51204 \u51060  \u46112  \u44163 \u51060 \u45796  \u46972 \u45716  \u45656 \u50521 \u49828 \u44032  \u44053 \u54633 \u45768 \u45796 . \u44592 \u54924  \u46608 \u54620  \'91\u49884 \u47144 \'92\u51032  \u54805 \u53468 \u47196  \u50732  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u55192  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u55192  \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u52380 \u49457 \u51201 \u51004 \u47196  \u54868 \u46020  \u49457 \u50837 \u46020  \u47566 \u51648 \u47564 , \u51064 \u45236 \u49900 \u51060  \u44053 \u54644  \u44169 \u51004 \u47196  \u46300 \u47084 \u45236 \u51648  \u50506 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u50784 \u51648  \u47784 \u47476 \u44172  \u54200 \u50504 \u54620  \u48516 \u50948 \u44592 \u47484  \u45236 \u49116 \u51004 \u47728  \u50612 \u47480 \u49828 \u47101 \u45796 \u45716  \u51064 \u49345 \u51012  \u51469 \u45768 \u45796 . (\u49892 \u51228 \u47196  \u50672 \u49345 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 )\u55192  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u50640  \u45824 \u54620  \u47560 \u51020 \u51012  \u53685 \u51228 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44557 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u44284  \u49324 \u46993 \u51012  \u47784 \u46160  \u45712 \u45180 \u51648 \u47564 , \u45817 \u49888 \u51060  \u48512 \u45812 \u49828 \u47084 \u50892  \u54624 \u44620 \u48400  \u44536  \u47560 \u51020 \u51012  \u50613 \u45572 \u47476 \u45716  \u51473 \u51077 \u45768 \u45796 . \u50696 \u51032 \u47484  \u51648 \u53412 \u44592  \u50948 \u54644  \'91\u48376 \u51064 \u51012  \u50628 \u44201 \u55176  \u45796 \u49828 \u47532 \u44256  \u51080 \u45796 \'92\u45716  \u45712 \u45196 \u46020  \u51080 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45800 \u49692 \u55176  \u51104 \u51088 \u47532 \u47484  \u44032 \u51648 \u44256  \u49910 \u51008  \u50837 \u47581 \u51060  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44536 \u51200  \u46308 \u53412 \u51648  \u50506 \u44592  \u50948 \u54644  \'91\u49704 \u44592 \u44256  \u51080 \u45716 \'92 \u51473 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u48520 \u53440 \u50724 \u47476 \u45716  \u47196 \u47592 \u49828 \u50752  \u54632 \u44760  \u49436 \u47196  \u44053 \u47148 \u54620  \u44048 \u51221 \u51012  \u45712 \u45184 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51060 \u51204 \u44620 \u51648  \u44221 \u54744 \u54644 \u48372 \u51648  \u47803 \u54664 \u45912  \u51676 \u47551 \u54632 \u51012  \u45712 \u45180 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u45800 \u45800 \u54620  \u50976 \u45824 \u44048 \u51012  \u44032 \u51648 \u44256  \u51080 \u50612 , \u51109 \u49688 \u54616 \u45716  \u52964 \u54540 \u50640 \u44172  \u47566 \u51060  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u54620 \u51901 \u51060  \u51068 \u48169 \u51201 \u51004 \u47196  \'91\u50864 \u52040 \u52040 \'92 \u54644 \u51452 \u44144 \u45208 , \u52280 \u50500 \u51452 \u45716  \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u46412 \u47928 \u50640  \u44048 \u51221 \u51201  \u54253 \u48156 \u51012  \u51068 \u51004 \u53412 \u44592 \u46020  \u49789 \u49845 \u45768 \u45796 . \u48512 \u47784 \u45784 \u51032  \u47560 \u51020 \u51012  \u54756 \u50500 \u47532 \u46319 , \u50672 \u51064 \u51032  \u47560 \u51020 \u51012  \u54756 \u50500 \u47532 \u45716  \u44163 \u46020  \u51473 \u50836 \u54616 \u44192 \u49845 \u45768 \u45796 . \u45208  \u46412 \u47928 \u50640  \u55192 \u46308 \u44192 \u44396 \u45208 \'85\u51116 \u54924 \u50640 \u49436  \u55192  \u52852 \u46300 \u45716  \u48512 \u51221 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \u50672 \u46973 \u54616 \u44256  \u49910 \u50612 \u54616 \u45208 , \u51060 \u47484  \u44985  \u52280 \u45716  \u45712 \u45196 \u51060  \u44053 \u54633 \u45768 \u45796 . \u46608 \u54620  \u48120 \u47000 \u50640 , \u49345 \u45824 \u48169 \u51008  \u51060 \u48324 \u51032  \u49836 \u54548 \u50640 \u49436  \u52824 \u50976 \u46104 \u50612 , \u45817 \u49888 \u51012  \u50896 \u52824  \u50506 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u55192  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u55192 \u44200 \u50868  \u49345 \u54889 \u46020  \u44053 \u54620  \u45236 \u47732 \u51004 \u47196  \u51060 \u44200 \u45244  \u49688  \u51080 \u51020 \u51012  \u46907 \u54633 \u45768 \u45796 . \u51060 \u45716  \u44288 \u51216 \u50640  \u46384 \u46972  \u44557 \u51221 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688 \u46020 , \u48512 \u51221 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 \'85\u48372 \u51060 \u49828 \u54588 \u49905  \u51204 \u54868 \u44032  \u50772 \u45796  \u8594  \u52840 \u52265 \u54616 \u44172  \u45824 \u52376 \u54644 \u49436  \u50500 \u47924 \u47088  \u54588 \u54644  \u50630 \u51060  \u51648 \u45208 \u44052 \u45796 \u49324 \u50629 \u51060  \u55192 \u46308 \u50612 \u51276 \u45796  \u8594  \u44053 \u54620  \u47704 \u53448 \u44284  \u45733 \u47141 \u51004 \u47196  \u44208 \u44397 \u50644  \u50948 \u44592 \u47484  \u49688 \u49845 \u54644 \u45256 \u45796 \u44148 \u44053 \u51060  \u45208 \u48736 \u51276 \u45796  \u8594  \u50724 \u55176 \u47140  \u49334 \u51032  \u49548 \u51473 \u54632 \u51012  \u44648 \u45803 \u44256  \u45908 \u50865  \u54665 \u48373 \u54644 \u51276 \u45796 \u47784 \u51032 \u44256 \u49324 \u47484  \u47803  \u48420 \u45796  \u8594  \u52649 \u44201 \u51004 \u47196  \u44277 \u48512 \u47484  \u45908  \u50676 \u49900 \u55176  \u54644  \u49688 \u45733 \u51012  \u51096  \u48372 \u44172  \u46096 \u45796 \u45768 \u52404 \u45716  \u50976 \u47749 \u54620  \u47568 \u51012  \u45224 \u44220 \u49845 \u45768 \u45796 . \u45208 \u47484  \u51453 \u51060 \u51648  \u47803 \u54616 \u45716  \u44163 \u51008  \u45208 \u47484  \u45908  \u44053 \u54616 \u44172  \u47564 \u46304 \u45796 . \u44536 \u47084 \u45208  \u44536 \u44032  \u49892 \u49688 \u54616 \u50668  \u44053 \u51312 \u54616 \u51648  \u50506 \u50520 \u45912  \u51216 \u51008  \u44536  \u49892 \u49688 \u44032  \u50864 \u47532 \u47484  \u44144 \u51032  \u51453 \u51068  \u48852 \u54664 \u45796 \u45716  \u49324 \u49892 \u51077 \u45768 \u45796 . \u53076 \u45212  \u50724 \u48652 \u46972 \u51060 \u50616  \u45796 \u53944 \u47672 \u49828  \u45824 \u54617  \u50672 \u49444  \u20013 \u55192  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u49548 \u48708 \u47484  \u50504 \u51221 \u51201 \u51004 \u47196  \u53685 \u51228 \u54624  \u49688  \u51080 \u44172  \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u46024 \u51060  \u51096  \u47784 \u51060 \u45716  \u49884 \u44592 \u44032  \u46104 \u44192 \u49845 \u45768 \u45796 . \u44536 \u46041 \u50504  \u52649 \u46041 \u51201 \u51064  \u49548 \u48708 \u47196  \u51396 \u52293 \u44048 \u51012  \u45712 \u44788 \u50772 \u45796 \u47732 , \u51648 \u44552 \u51060  \u54644 \u44208 \u54624  \u53440 \u51060 \u48141 \u51077 \u45768 \u45796 . \u50556 \u49885  3\u48264  \u47673 \u51012  \u44144  1\u48264 \u47564  \u47673 \u44172  \u46104 \u44256 , \u53469 \u49884  \u53448  \u44144 \u47532 \u47484  \u44152 \u50612 \u44040  \u49688  \u51080 \u44172  \u46121 \u45768 \u45796 . \'91\u51064 \u45236 \u49900 \'92\u51032  \u51473 \u50836 \u49457 \u51012  \u44053 \u51312 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u53804 \u51088 \u45208  \u49324 \u50629 \u51012  \u54616 \u44256  \u51080 \u45796 \u47732 , \u51312 \u44552 \u47564  \u45908  \u48260 \u54000 \u49464 \u50836 . \u53360  \u49457 \u44284 \u44032  \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u52280 \u51004 \u49464 \u50836 \u44228 \u49549 \u54644 \u49436  \u52649 \u46041 \u51201 \u51064  \u50837 \u47581 \u51060  \u50732 \u46972 \u50728 \u45796 \u47732 , \u51648 \u44552 \u51008  \u52280 \u51012  \u46412 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49900 \u54620  \u47568 \u51012  \u45236 \u48201 \u44144 \u45208 , \u47924 \u53553 \u45824 \u44256  \u44256 \u48177  \u44277 \u44201 \u51012  \u44048 \u54665 \u54644 \u48260 \u47532 \u45716 \'85 \u44984 \u51456 \u55176  \u52280 \u45796  \u48372 \u47732  \u49345 \u54889 \u51008  \u45208 \u50500 \u51656  \u44163 \u51077 \u45768 \u45796 . \u49888 \u51473 \u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u50689 \u47928 \u47749 \u50640 \u49436  The\u44032  \u48736 \u51652  \u51060 \u50976 \u48148 \u48372 \u45716  The Fool, \u50672 \u51064 \u51008  The Lovers, \u44536 \u47084 \u45208  \u55192  \u52852 \u46300 \u45716  Strength\u47196  \u50526 \u50640  The\u44032  \u50630 \u49845 \u45768 \u45796 . \u44536  \u51060 \u50976 \u45716  \u55192  \u52852 \u46300 \u44032  \u53945 \u51221  \u45824 \u49345 \u51012  \u44053 \u51312 \u54616 \u51648  \u50506 \u44256 , \u51204 \u52404 \u51201 \u51064  \u44060 \u45392 \u44284  \u49345 \u54889 \u51012  \u45796 \u47336 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .    \
\
   9. \uc0\u51008 \u46164 \u51088 The Hermit \u51008 \u46164 \u51088  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u45733 \u47141  \u51080 \u45716  \u45432 \u51064 \u51060  \u49464 \u49345 \u51012  \u54588 \u54644  \u49704 \u50612  \u49324 \u45716  \u51473 \u51064 \u49373 \u51032  \u48169 \u54693 \u49457 \u51012  \u44256 \u48124  \u51473 \u51064  \u47560 \u51020 \u53685 \u52272 \u47141 \u51060  \u46832 \u50612 \u45208 \u47728  \u49324 \u47140  \u44618 \u51008  \u49324 \u46988 \u51012  \u44032 \u47532 \u53428 \u48512 \u45812 \u49828 \u47084 \u50892 \u54616 \u45716  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u49548 \u44537 \u51201 \u51060 \u44256  \u48372 \u49688 \u51201 \u51064  \u49457 \u44201 \u51012  \u49345 \u51669 \u49324 \u54924 \u47196 \u48512 \u53552  \u44256 \u47549 \u46104 \u50632 \u45796 \u45716  \u44221 \u44256  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u45733 \u47141 \u51060  \u46832 \u50612 \u45212 \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u45733 \u47141 \u51060  \u46832 \u50612 \u45212  \u49324 \u46988 , \u53945 \u55176  \u53685 \u52272 \u47141 \u51060  \u51339 \u51008  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u51008 \u46164 \u51088 \u51032  \u44284 \u44144  \u47784 \u49845 \u51008  1\u48264  \u47560 \u48277 \u49324  \u52852 \u46300 \u51077 \u45768 \u45796 . \u47560 \u48277 \u49324 \u45716  \u45733 \u47141 , \u50976 \u47672 , \u49468 \u49828 \u47484  \u47784 \u46160  \u44054 \u52632  \u50977 \u44033 \u54805  \u51064 \u51116 \u50688 \u51424 . \u45796 \u47564  \u49884 \u44036 \u51060  \u55128 \u47084  \u45236 \u54693 \u51201 \u51004 \u47196  \u48320 \u54616 \u44256 , \u51328  \u45908  \u44216 \u49552 \u54644 \u51276 \u45796 \u44256  \u48372 \u49884 \u47732  \u46121 \u45768 \u45796 .\u51008 \u46164 \u51088 \u51032  \u44284 \u44144 \u51008 \u46164 \u51088 \u51032  \u54788 \u51116 \u50724 \u47480 \u49552 \u50640  \u46304  \u46321 \u48520 \u51008  \u44536 \u51032  \u45733 \u47141 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u54616 \u45720 \u51032  \u48324 \u51008  \u50500 \u47924 \u45208  \u46392  \u49688  \u50630 \u44256 , \u51060 \u47484  \u46321 \u48520 \u50640  \u44032 \u46160 \u44592 \u46020  \u49789 \u51648  \u50506 \u49845 \u45768 \u45796 . \u46608 \u54620  \u44536 \u45716  \u50724 \u47019 \u46041 \u50504  \u54844 \u51088  \u49332 \u47732 \u49436 , \u49373 \u44033 \u50640  \u49373 \u44033 \u51012  \u44144 \u46317 . \u45236 \u47732 \u51032  \u53685 \u52272 \u47141 \u51012  \u44600 \u47084 \u50772 \u49845 \u45768 \u45796 . \u51648 \u44552 \u51004 \u47196  \u46384 \u51648 \u47732  \u44368 \u49688 \u45208  \u52384 \u54617 \u44032 \u47484  \u46496 \u50732 \u47532 \u47732  \u46121 \u45768 \u45796 .\u50724 \u47480 \u49552 \u50640  \u46304  \u46321 \u48520  \u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u49373 \u44033 \u51060  \u50612 \u47480 \u49828 \u47101 \u44144 \u45208  \u49549  \u44618 \u51008  \u49324 \u46988 \u54217 \u49548 \u50640  \u52293 \u51012  \u47566 \u51060  \u51069 \u44144 \u45208  \u51648 \u54812 \u47196 \u50868  \u49324 \u46988 \u45236 \u47732 \u51060  \u45800 \u45800 \u54616 \u50668  \u49884 \u47144 \u51060  \u50752 \u46020  \u45003 \u45003 \u51060  \u51060 \u44200 \u45244  \u49688  \u51080 \u51020 \u44036 \u54841  \u55176 \u53412 \u53076 \u47784 \u47532 \u45208  \u45432 \u49689 \u51088  \u52196 \u51004 \u47196  \u50724 \u54644 \u54616 \u49884 \u45716  \u48516 \u46308 \u51060  \u51080 \u51648 \u47564 , \u50628 \u50672 \u55176  \u45656 \u50521 \u49828 \u44032  \u45796 \u47493 \u45768 \u45796 . \u47564 \u50557  \u51008 \u46164 \u51088  \u52852 \u46300 \u44032  \u51656 \u47928 \u51088  \u48376 \u51064 \u51012  \u44032 \u47532 \u53420  \u44221 \u50864 , \u48149 \u54617 \u45796 \u49885 \u54616 \u47728  \u54217 \u49548 \u50640  \u49373 \u44033 \u51060  \u47566 \u50500 \u48372 \u51064 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50612 \u45712  \u51221 \u46020  \'91\u51316 \u44221 \'92\u51032  \u51032 \u48120 \u46020  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u44618 \u51060  \u49373 \u44033  \u51473 \u51064 \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u44618 \u51060  \u49373 \u44033 \u54616 \u44256  \u51080 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u46412 \u50640  \u46384 \u46972  \'91\u54596 \u50836  \u51060 \u49345 \u51004 \u47196  \u44145 \u51221 \'92\u54616 \u44256  \u51080 \u45796 \u45716  \u51312 \u50616 \u51004 \u47196  \u54644 \u49437 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51060 \u48120 \u51648  \u54644 \u49437  : \u51008 \u46164 \u51088 \u51032  \u44596  \u49688 \u50684 \u51008  \u49373 \u44033 \u51012  \u53685 \u54644  \u49939 \u50500 \u50728  \u51648 \u54812 , \u50672 \u47452 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u45925 \u49688 \u47337 \u54620  \u49688 \u50684  \u50696 \u47484  \u46308 \u45796 \u47732 , \u47924 \u50631 \u51012  \u50948 \u54644  \u49332 \u50500 \u50556  \u54624 \u44620 , \u50612 \u46523 \u44172  \u49332 \u50500 \u50556  \u54665 \u48373 \u54624 \u44620  \u44057 \u51008  \u49373 \u44033 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u48152 \u46300 \u49884  \u44144 \u52285 \u54620  \u44256 \u48124 \u47564 \u51012  \u51032 \u48120 \u54616 \u45716  \u44148  \u50500 \u45785 \u45768 \u45796 . \u44536  \u50668 \u51088 \u46993  \u51096  \u46096 \u51004 \u47732  \u47924 \u49832  \u51068 \u51060  \u48268 \u50612 \u51276 \u51012 \u44620 \'85 \u46024  \u47784 \u50500 \u49436  \u47952 \u54624 \u44620 \'85 \u49324 \u49548 \u54620  \u44145 \u51221 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54616 \u51424 .\u47564 \u50557  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  : \u54844 \u51088  \u45145 \u45145  \u50515 \u45716  \u51677 \u49324 \u46993 \u51012  \u46907 \u54616 \u44592 \u46020  \u54616 \u44256 , \u54756 \u50612 \u51656 \u51648  \u47568 \u51648  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51008 \u46164 \u51088 \u45716  \'91\u51677 \u49324 \u46993 , \u51060 \u48324 , \u44256 \u48124 \'92 \u46321 \u46321  \u47784 \u46304  \u51333 \u47448 \u51032  \u49373 \u44033 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u45796 \u47564  \u51060  \u49373 \u44033 \u51032  \u51333 \u47448 \u47484  \u44396 \u52404 \u51201 \u51004 \u47196  \u50508 \u44256  \u49910 \u45796 \u47732  \u48372 \u51312  \u52852 \u46300 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 .\u50672 \u50528 \u50868  \u54028 \u53944 \u50640 \u49436  \u51088 \u49464 \u55176  \u54980 \u49696 \u54616 \u44192 \u49845 \u45768 \u45796 .Comment\u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u46321 \u48520 \u51008  \u48156 \u48145 \u51012  \u48708 \u52628 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \u48120 \u47000 \u45208  \u44284 \u44144 \u47484  \u49888 \u44221  \u50416 \u51648  \u47568 \u44256 , \u54788 \u49892 \u50640  \u45576 \u51012  \u46028 \u47532 \u46972 \u45716  \u47700 \u49884 \u51648 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48512 \u45812 \u51012  \u45712 \u45180 \u45716 \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u48512 \u45812 \u51012  \u45712 \u45180 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50526 \u49436  \u47568 \u54664 \u46319 , \u51008 \u46164 \u51088 \u45716  \u45733 \u47141 \u51060  \u46832 \u50612 \u45212  \'91\u47560 \u48277 \u49324 \'92\u50688 \u45796 \u44256  \u54664 \u49845 \u45768 \u45796 . \u44536 \u47111 \u45796 \u47732  \u44536 \u45716  \u50780  \u49704 \u50612  \u49332 \u44172  \u46108  \u44152 \u44620 \u50836 ? \u50780 \u45264 \u54616 \u47732  \u49464 \u49345 \u49332 \u51060 \u50640  \u54924 \u51032 \u44048 \u51012  \u45712 \u44808 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49688 \u47566 \u51008  \u44288 \u49900 , \u48512 \u53441 , \u51656 \u53804  \u46321 \u46321 \'85 \u44536  \u47784 \u46304  \u44163 \u46308 \u50640  \u48512 \u45812 \u51012  \u45712 \u45180 \u44256  \u46608  \u51648 \u44200 \u50892 \u51652  \u44163 \u51077 \u45768 \u45796 .\u54788 \u45824 \u54032 \u51004 \u47196  \u48148 \u44988 \u45796 \u47732  : \u51096 \u45208 \u44032 \u45912  \u50672 \u50696 \u51064 \u51060  \u51008 \u53748  \u54980  \u49884 \u44264 \u50640 \u49436  \u45453 \u49324 \u47484  \u51648 \u51004 \u47728  \u49328 \u45796 \u44144 \u45208 , \u54217 \u48276 \u54620  \u51068 \u51012  \u54616 \u47728  \u51068 \u48152 \u51064 \u52376 \u47100  \u49332 \u50500 \u44032 \u45716  \u49334 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796 .Q. \u50780  \u45236  \u50672 \u46973 \u51012  \u44144 \u48512 \u54616 \u45716  \u44152 \u44620 ?A. \u45817 \u49888 \u51032  \u44288 \u49900 \u51060  \u48512 \u45812 \u49828 \u47101 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .Q. \u49345 \u45824 \u48169 \u51008  \u50780  \u50672 \u50528 \u47484  \u54616 \u51648  \u50506 \u45716  \u44152 \u44620 ?A. \u50672 \u50528 \u50640  \u45824 \u54644  \u54924 \u51032 \u44048 \u51012  \u45712 \u44808 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .Q. \u49345 \u45824 \u48169 \u51008  \u45208 \u47484  \u50612 \u46523 \u44172  \u48372 \u44256  \u51080 \u51012 \u44620 ?A. \u48512 \u45812 \u51012  \u45712 \u45180 \u45716  \u44163 \u52376 \u47100  \u48372 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u48320 \u54868 \u47484  \u50896 \u54616 \u51648  \u50506 \u45716 \u53440 \u47196 \u50640 \u49436  \u51008 \u46164 \u51088 \u45716  \u49548 \u44537 \u51201 \u51060 \u44256  \u48372 \u49688 \u51201 \u51064  \u49373 \u44033 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \'91\u48320 \u54616 \u44592  \u49899 \u45796 \'92\u45716  \u47560 \u51020 \u51012  \u46907 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51008 \u46164 \u51088 \u45716  \u49704 \u50612 \u49328 \u51648  \u50620 \u47560 \u45208  \u46096 \u51012 \u44620 \u50836 ? \u50500 \u47560  \u44900  \u50724 \u47000 \u46096 \u51012  \u44163 \u51077 \u45768 \u45796 . 10\u45380 , 20\u45380  \u44536  \u51060 \u49345 \u51068  \u49688 \u46020  \u51080 \u51424 . \u49688 \u50684 \u51012  \u48372 \u47732  \u50508  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49688 \u50684 \u51060  \u51060 \u47111 \u44172  \u44600 \u46020 \u47197  \u49324 \u54924 \u50640  \u45208 \u44032 \u51648  \u50506 \u51008  \u51060 \u50976 \u45716  \u48376 \u51064 \u51060  \u50896 \u54616 \u51648  \u50506 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51064 \u44592  \u47566 \u45912  \u47560 \u48277 \u49324 \u47484  \u49688 \u49901 \u45380  \u46041 \u50504  \u49324 \u46988 \u46308 \u51060  \u54620  \u48264 \u46020  \u52286 \u51648  \u50506 \u50520 \u51012 \u44620 \u50836 ? \u48376 \u51064 \u51060  \u49899 \u50612 \u49436  \u50504  \u44036  \u44161 \u45768 \u45796 .\u51060 \u47484  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :Q. \u50780  \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u47484  \u49884 \u46020 \u54616 \u51648  \u50506 \u45716  \u44152 \u44620 ?A. \u51648 \u44552  \u49345 \u53468 \u44032  \u44900 \u45208  \u47564 \u51313 \u49828 \u47101 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .Q. \u45208 \u54620 \u53580  \u47560 \u51020 \u51060  \u51080 \u45716  \u44163  \u44057 \u51008 \u45936 , \u50780  \u54364 \u54788 \u51012  \u50504  \u54624 \u44620 ?A. \u49548 \u44537 \u51201 \u51064  \u49457 \u44201  \u53459 \u51077 \u45768 \u45796 .Q. \u51064 \u44592  \u47566 \u50500  \u48372 \u51060 \u45912 \u45936  \u50780  \u50668 \u51088 \u52828 \u44396 \u44032  \u50630 \u51012 \u44620 ?A. \u49345 \u45824 \u48169 \u51008  \u49556 \u47196  \u49373 \u54876 \u51060  \u55016 \u50476  \u54200 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u46020 \u51204 \u51012  \u44732 \u47532 \u45716  \u49457 \u54693 \u51060  \u51109 \u50528 \u47932 \u47196  \u51089 \u50857 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49548 \u44537 \u51201 \u51064  \u49457 \u44201  \u53459 \u50640  \u49324 \u54924 \u47196 \u48512 \u53552  \u44256 \u47549 \u46112  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u47588 \u45320 \u47532 \u51608 \u50640  \u48736 \u51648 \u44144 \u45208  \u48156 \u51204 \u51060  \u51221 \u52404 \u46112  \u49688  \u51080 \u45796 \u45716  \u44221 \u44256 \u51077 \u45768 \u45796 .\u51008 \u46164 \u51088  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u51008 \u46164 \u51088  \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \u49549 \u51060  \u44618 \u51004 \u47728  \u49345 \u45824 \u48169 \u50640  \u45824 \u54620  \u48176 \u47140 \u49900 \u51060  \u44032 \u46301 \u54633 \u45768 \u45796 . \u45796 \u47564  \u45236 \u54693 \u51201 \u51064  \u49457 \u44201  \u53459 \u50640  \u47928 \u51228 \u47484  \u51649 \u51217  \u54644 \u44208 \u54616 \u51648  \u50506 \u44256 , \u46020 \u54588 \u54616 \u47140 \u45716  \u49457 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51088 \u52843 \u54616 \u47732  \u49324 \u54924 \u47196 \u48512 \u53552  \u44256 \u47549 \u46112  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54620 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u51677 \u49324 \u46993  \u51204 \u47928 \u44032 \u51060 \u47728 , \u44536  \u44592 \u44036  \u46608 \u54620  \u44900  \u44609 \u45768 \u45796 . \u50672 \u50528 \u47484  \u44592 \u54588  \u54616 \u47728 , \'91\u51088 \u49888 \u47564 \u51032  \u51068 \'92\u50640  \u51665 \u51473 \u54616 \u45716  \u49324 \u46988 \u50640 \u49436  \u45208 \u53440 \u45208 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51008 \u46164 \u51088  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  3\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 \u45716  \u48512 \u45812 \u49828 \u47084 \u50868  \u47560 \u51020 \u51077 \u45768 \u45796 . \u50500 \u51649 \u51008  \u54844 \u51088 \u44032  \u54200 \u54616 \u47728 , \u49884 \u44036 \u44284  \u44048 \u51221 \u51012  \u50024 \u44032 \u47728  \u50672 \u50528 \u54616 \u44256  \u49910 \u51648 \u45716  \u50506 \u50500 \u54633 \u45768 \u45796 . \'91\u50672 \u50528 \'92 \u51088 \u52404 \u44032  \u54980  \u49692 \u50948 \u50640  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 \u45716  \u49900 \u49324 \u49689 \u44256 \u51077 \u45768 \u45796 . \u50672 \u50528 \u46304  \u51060 \u48324 \u51060 \u46304  \u51116 \u54924 \u46304 . \u50500 \u51649 \u51008  \u54869 \u49888 \u51012  \u47803  \u45236 \u47160 \u44592 \u50640  \u49373 \u44033  \u51473 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47560 \u51648 \u47561 \u51008  \u51677 \u49324 \u46993 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44284 \u44144  \u50672 \u50528 \u50640  \u45824 \u54620  \u49345 \u52376 \u44032  \u51080 \u50612  \u47581 \u49444 \u50668 \u51648 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51648 \u44552 \u52196  \u49345 \u45824 \u48169 \u51008  \u51312 \u50857 \u55176  \u49704 \u50612 \u49436  SNS \u50684 \u53456 \u51012  \u54624  \u49688 \u46020 , \u54841 \u51008  \u47680 \u47532  \u50668 \u54665 \u51012  \u46496 \u45208 \u47728  \u49373 \u44033 \u51012  \u51221 \u47532  \u51473 \u51068  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u44277 \u53685 \u51201 \u51004 \u47196  \u50672 \u50528 \u47484  \'91\u46160 \u47140 \u50892  \u54616 \u45716 \'92 \u49900 \u47532 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u49345 \u45824 \u48169 \u44284  \u51060 \u50612 \u51648 \u44256  \u49910 \u45796 \u47732 , \u50724 \u47004  \u49884 \u44036 \u51060  \u54596 \u50836 \u54616 \u47728 , \u44984 \u51456 \u55176  \u49888 \u47280 \u44048 \u51012  \u49900 \u50612 \u51452 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u44368 \u47448 \u44032  \u48512 \u51313 \u54616 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u48376 \u51064 \u50640 \u44172  \u51665 \u51473 \u54616 \u44592  \u50948 \u54644 , \u49828 \u49828 \u47196 \u47484  \u44256 \u47549 \u49884 \u53412 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51068 \u48512 \u47084  \u50672 \u46973 \u51012  \u48155 \u51648  \u50506 \u44144 \u45208 , \u51652 \u51676  \u49549 \u47560 \u51020 \u51012  \u46300 \u47084 \u45236 \u51648  \u50506 \u45716 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51228  \u49373 \u44033 \u50644  \'91\u47924 \u50616 \u44032  \u45796 \u47480  \u47928 \u51228 \'92\u44032  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u51652 \u47196 \u47484  \u44256 \u48124 \u54616 \u44144 \u45208 , \u49373 \u44228 \u47484  \u44145 \u51221 \u54616 \u45716  \u46321  \u44060 \u51064 \u51201 \u51064  \u47928 \u51228 \u50752  \u50672 \u44288 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45768 , \u51060  \u47784 \u46304  \u44163 \u51012  \'91\u45817 \u49888 \u51032  \u47928 \u51228 \'92\u46972 \u44256  \u48155 \u50500 \u46308 \u51060 \u51648  \u47560 \u49464 \u50836 . \u49345 \u45824 \u48169 \u51032  \u49373 \u44033 \u51060  \u51221 \u47532 \u46104 \u47732 , \u44288 \u44228 \u50640  \u51201 \u44537 \u51201 \u51004 \u47196  \u51076 \u54624  \u44163 \u51077 \u45768 \u45796 .\u52280 \u44256  \u51088 \u47308  : \u51008 \u46164 \u51088  \u52852 \u46300 \u51032  \u50672 \u50528 \u50868  \u51032 \u48120 \u51116 \u54924 \u50640 \u49436  \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u51473 \u47549 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u44284 \u51032  \u51116 \u54924 \u47484  \u49900 \u44033 \u54616 \u44172  \u44256 \u48124 \u54616 \u45716  \u51473 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \u44536 \u47532 \u50892 \u54616 \u44144 \u45208 , \u45796 \u49884  \u50672 \u46973 \u54616 \u47732  \u50612 \u46500  \u45813 \u51109 \u51012  \u48155 \u44172  \u46112 \u51648  \u44145 \u51221 \u54616 \u44144 \u45208 , \u45817 \u49888 \u51060  \u51648 \u44552  \u47924 \u50631 \u51012  \u54616 \u44256  \u51080 \u51012 \u51648  \u49345 \u49345 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u45817 \u49888 \u44284  \u51060 \u50612 \u51648 \u45716  \u44163 \u51012  \u50612 \u45712 \u51221 \u46020  \'91\u46160 \u47140 \u50892 \'92\u54616 \u45716  \u46319 \u54633 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u51060 \u51204  \u50672 \u50528 \u50640 \u49436  \u49345 \u52376 \u47484  \u51077 \u50632 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u54665 \u46041 \u47141 \u51060  \u46504 \u50612 \u51648 \u44592 \u50640  \u50672 \u46973 \u46020  \u50724 \u51648  \u50506 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 .\u51008 \u46164 \u51088  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u51008 \u46164 \u51088  \u52852 \u46300 \u45716  \u51064 \u49373 \u51032  \u48169 \u54693 \u51012  \u44256 \u48124 \u54624  \u46412  \u51088 \u51452  \u45208 \u53440 \u45225 \u45768 \u45796 . \u51648 \u44552  \u45817 \u49888 \u51008  \'91\u51064 \u49373 \u51060  \u51096 \u47803  \u55128 \u47084 \u44032 \u44256  \u51080 \u45796 \u44144 \u45208 \'92 \'91\u51648 \u44552  \u51649 \u50629 \u51060  \u48376 \u51064 \u50640 \u44172  \u47582 \u51648  \u50506 \u45716 \u45796 \u44256 \'92 \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51648 \u44552 \u51008  \u50668 \u47084 \u48516 \u51060  \u54616 \u44256  \u49910 \u51008  \u45824 \u47196 , \u51649 \u44048  \u45824 \u47196  \u54644 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51060 \u48264  \u44592 \u54924 \u50640  \u48376 \u51064 \u51060  \u50612 \u46500  \u49324 \u46988 \u51064 \u51648  \u50508 \u44172  \u46121 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u51080 \u45796 \u47732 , \u51228 \u44032  \u46300 \u47532 \u44256  \u49910 \u51008  \u47568 \u50432 \u51008  \u53356 \u44172  3\u44032 \u51648  \u51077 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u50508 \u51648  \u47803 \u54616 \u45716  \u51068 \u51060 \u45208  \u53804 \u51088 \u50640  \u54632 \u48512 \u47196  \u46832 \u50612 \u46308 \u51648  \u47560 \u49464 \u50836 . \u51452 \u48320 \u51004 \u47196 \u48512 \u53552  \u51312 \u50616 \u51012  \u44396 \u54644 \u50556  \u54620 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51012  \u44284 \u45824 \u54217 \u44032 \u54616 \u47728 , \u45796 \u47480  \u51032 \u44204 \u51012  \u47924 \u49884 \u54616 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u49324 \u54924 \u50640  \u45796 \u49884  \u54633 \u47448 \u54616 \u49464 \u50836 . \u47704 \u53664 \u47484  \u52286 \u46304 , \u46041 \u50500 \u47532 \u50640  \u46308 \u50612 \u44032 \u46304  \u49352 \u47196 \u50868  \u49324 \u46988 \u51012  \u47566 \u51060  \u47564 \u45208 \u48400 \u50556  \u54633 \u45768 \u45796 . \u49828 \u49828 \u47196 \u47484  \u44256 \u47549 \u49884 \u53412 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49464  \u48264 \u51704 , \u50676 \u49900 \u55176  \u44040 \u44256  \u45798 \u50500 \u50772 \u45912  \u44592 \u49696 \u51012  \u50612 \u54596 \u54644 \u50556  \u54620 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51012  \u51096  \u54252 \u51109 \u54616 \u45716  \u44163  \u46608 \u54620  \u49892 \u47141 \u51077 \u45768 \u45796 .\u51008 \u46164 \u51088  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u46024 \u44284 \u45716  \u44144 \u47532 \u44032  \u47676  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51648 \u44552 \u51008  \u47932 \u51656 \u51201 \u51064  \u44163 \u48372 \u45796  \u49457 \u52712 \u44048 , \u54841 \u51008  \u49892 \u47141 \u51012  \u44040 \u44256  \u45798 \u45716 \u45936 \u47564  \u51665 \u51473 \u54644 \u50556  \u54624  \u49884 \u44592 \u51077 \u45768 \u45796 . \u48736 \u47476 \u44172  \u46024 \u51012  \u48268 \u44256  \u49910 \u51008  \u50837 \u44396 \u45716  \u51204 \u54784  \u46020 \u50880 \u51060  \u46104 \u51648  \u50506 \u49845 \u45768 \u45796 . \u49892 \u47141 \u51012  \u44054 \u52628 \u47732  \u46024 \u51008  \u51200 \u51208 \u47196  \u46384 \u46972 \u50724 \u44172  \u46104 \u50612 \u51080 \u49845 \u45768 \u45796 . \u51312 \u50616 \u51216 \u51004 \u47196 \u45716  \'91\u50837 \u49900 \u51012  \u48260 \u47532 \u49464 \u50836 \'92 \u51312 \u47564 \u44036  \u46024 \u51060  \u54665 \u48373 \u51012  \u44032 \u51256 \u45796  \u51452 \u51648  \u50506 \u45716 \u45796 \u45716  \u49324 \u49892 \u51012  \u44648 \u45803 \u44172  \u46121 \u45768 \u45796 .    \
