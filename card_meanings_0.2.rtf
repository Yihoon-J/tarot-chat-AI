   16. \uc0\u53440 \u50892 The Tower \u53440 \u50892  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u50885 \u51109 \u54620  \u53457 \u51060  \u48264 \u44060  1\u48169 \u50640  \u47924 \u45320 \u51256  \u45236 \u47532 \u45716  \u47784 \u49845 \u50696 \u49345 \u52824  \u47803 \u54620  \u53360  \u52649 \u44201 \u51012  \u49345 \u51669 \u51096 \u47803 \u46108  \u49888 \u45392 \u44284  \u49324 \u44256  \u48169 \u49885 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54632 \u50629 \u51201 \u44284  \u47749 \u49457 \u51060  \u47924 \u45320 \u51648 \u45716  \u49345 \u54889 \u48512 \u51221 \u51201  \u51032 \u48120 \u44032  \u44053 \u54620  \u52852 \u46300  \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50696 \u49345 \u52824  \u47803 \u54620  \u53360  \u52649 \u44201 \u53440 \u50892  \u52852 \u46300 \u45716  \u44049 \u51089 \u49828 \u47084 \u50868  \u49324 \u44256 \u45208  \u50696 \u49345 \u52824  \u47803 \u54620  \u52649 \u44201 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u50672 \u50528 \u50868 \u50640 \u49436 \u45716  \'91\u44049 \u51089 \u49828 \u47084 \u50868  \u51060 \u48324  \u53685 \u48372 \'92, \'91\u49892 \u47581 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \'92 \u46321 \u51077 \u45768 \u45796 .\u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u53440 \u50892 \u50640  \u53360  \u48264 \u44060 \u44032  \u52828  \u47784 \u49845 \u51077 \u45768 \u45796 . \u48264 \u44060 \u45716  \u50616 \u51228  \u50612 \u46356 \u49436  \u46504 \u50612 \u51656 \u51648  \u50508  \u49688  \u50630 \u51424 . \u51060 \u45716  \u49324 \u44256 \u47484  \'91\u50696 \u52769 \u54624  \u49688  \u50630 \u50632 \u51020 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u44536 \u51200  \u50868 \u51060  \u50630 \u50632 \u45348 \u50836 . \u53440 \u47196 \u50640 \u49436  \u53440 \u50892  \u52852 \u46300 \u45716  \u45936 \u49828  \u52852 \u46300 \u48372 \u45796 \u46020  \u45908  \u45208 \u49244  \u45149 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51453 \u51020 \u51008  \u45572 \u44396 \u50640 \u44172 \u45208  \u50724 \u44256 , \u50612 \u45712  \u51221 \u46020  \u50696 \u49345 \u51008  \u44032 \u45733 \u54616 \u51648 \u47564 , \u48316 \u46973 \u51008  \u44536 \u47111 \u51648  \u50506 \u44592  \u46412 \u47928 \u51060 \u51424 . \u46384 \u46972 \u49436  \u51453 \u51020  \u52852 \u46300 \u44032  \'91\u45800 \u49692 \u54620  \u51333 \u44208 , \u49352 \u47196 \u50868  \u49884 \u51089 \'92\u51012  \u51032 \u48120 \u54620 \u45796 \u47732 , \u53440 \u50892 \u45716  \'91\u47736 \u47581 \u51060 \u45208  \u51116 \u50521 \'92\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u44396 \u52404 \u51201 \u51004 \u47196  \u51201 \u50857 \u54616 \u47732  :\u44049 \u51089 \u49828 \u47084 \u50868  \u49324 \u44256 \u45208  \u49548 \u49569 \u50640  \u55064 \u47568 \u47540  \u49688  \u51080 \u45796 \u51677 \u49324 \u46993  \u49345 \u45824 \u50640 \u44172  \u51060 \u48120  \u50528 \u51064 \u51060  \u51080 \u45796 \u45716  \u49324 \u49892 \u51012  \u50508 \u44172  \u46108 \u45796 \u45817 \u50672 \u55176  \u48537 \u51012  \u51460  \u50508 \u50520 \u45912  \u45824 \u54617 , \u51649 \u51109 \u50640  \u46504 \u50612 \u51648 \u44172  \u46108 \u45796 \u48127 \u50632 \u45912  \u49324 \u46988 \u51060  \u45236  \u46263 \u45812 \u54868  \u54616 \u45716  \u45236 \u50857 \u51012  \u46307 \u44172  \u46108 \u45796 Comment\u53440 \u50892 \u47484  \u44557 \u51221 \u51201 \u51004 \u47196  \u48376 \u45796 \u47732 , \'91\u51060 \u48264  \u44592 \u54924 \u50640  \u51221 \u49888  \u52264 \u47532 \u44172  \u46108 \u45796 \'92\u46972 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49324 \u46988 \u51008  \u50616 \u51228  \u48320 \u54624 \u44620 \u50836 ? \u48148 \u47196  \u51221 \u49888 \u51201  \u52649 \u44201 \u51012  \u53356 \u44172  \u48155 \u50520 \u51012  \u46412 \u51077 \u45768 \u45796 . \u44536 \u47111 \u51648  \u50506 \u51008  \u51060 \u49345  \u49324 \u46988 \u51008  \u49789 \u44172  \u48320 \u54616 \u51648  \u50506 \u51424 . \u45208 \u49244  \u49845 \u44288 \u51060 \u45208  \u53468 \u46020 \u47484  \u51060 \u48264 \u50640  \u54869 \u49892 \u55176  \u44256 \u52824 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u51109 \u44592 \u51201 \u51004 \u47196  \u48372 \u47732  \u51339 \u51008  \u49480 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u49888 \u47280  \u54841 \u51008  \u50629 \u51201 \u51060  \u47924 \u45320 \u51648 \u45716 \u53440 \u50892  \u52852 \u46300 \u45716  \u49939 \u50500 \u50772 \u45912  \u49888 \u47280  \u54841 \u51008  \u50629 \u51201 \u51060  \u47924 \u45320 \u51648 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u53440 \u50892  \u44845 \u45824 \u44592 \u50640 \u49436  \u50773 \u44288 \u51060  \u46504 \u50612 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50773 \u44288 \u51008  \'91\u47749 \u50696 , \u51649 \u50948 \'92\u51032  \u49345 \u51669 \u51060 \u51424 . \u44536 \u47532 \u44256  \u44263  \u49324 \u46988 \u46308 \u51032  \'91\u47532 \u49828 \u54169 \'92\u51012  \u46907 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51008  \u50773 \u51060 \u45796 . \u45824 \u50864  \u54644 \u51452 \u44192 \u45796 . \u44536  \u50773 \u44288 \u51060  \u46504 \u50612 \u51648 \u45716  \u47784 \u49845 \u51008  \u44263  \u49888 \u47280 \u45208  \u50629 \u51201 \u51060  \u47924 \u45320 \u51664 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u48127 \u50632 \u45912  \u49324 \u46988 \u50640 \u44172  \u48156 \u46321 \u51012  \u52237 \u55176 \u44144 \u45208 \u47568  \u49892 \u49688 \u47484  \u54644 \u49436  \u50672 \u51064 \u51060  \u49892 \u47581 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u54620  \u49692 \u44036 \u51032  \u49892 \u49688 \u47196  \u51649 \u51109 \u50640 \u49436  \u47749 \u50696 \u44032  \u49892 \u52628 \u46104 \u45716  \u49345 \u54889 \u8251  \u53945 \u55176  \u47568  \u49892 \u49688 \u47484  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u53360  \u52649 \u44201 \u51012  \u48155 \u50500  \u45817 \u49888 \u51012  \'91\u49552 \u51208 \'92 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u51096 \u47803 \u46108  \u49888 \u45392 \u44284  \u49324 \u44256 \u48169 \u49885 The Tower \u52852 \u46300 \u45716  \u51096 \u47803 \u46108  \u49888 \u45392 \u44284  \u49324 \u44256 \u48169 \u49885 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u53948 \u53948 \u54644  \u48372 \u50688 \u45912  \u53440 \u50892 \u44032  \u48264 \u44060  1\u48169 \u50640  \u47784 \u46160  \u47924 \u45320 \u51256  \u45236 \u47160 \u49845 \u45768 \u45796 . \u44536  \u51060 \u50976 \u44032  \u47924 \u50631 \u51068 \u44620 \u50836 ? \u44148 \u47932 \u51060  \u52712 \u50557 \u54644 \u49436  \u51068 \u44620 \u50836 ? \u50500 \u45785 \u45768 \u45796 . \u48148 \u47196  \u53440 \u50892 \u44032  \u50672 \u50557 \u54620  \u44592 \u48152  \u50948 \u50640  \u51648 \u50612 \u51276 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u53457 \u51032  \u53356 \u44592 \u50640  \u48708 \u54644  \u46405 \u51008  \u53553 \u50630 \u51060  \u51329 \u51008  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u49345 \u45824 \u48169 \u46020  \u45208 \u47484  \u51339 \u50500 \u54624  \u44163 \u51060 \u46972  \u54869 \u49888 \u54664 \u51648 \u47564 , \u44536  \u44592 \u45824 \u44032  \u54620  \u48264 \u50640  \u47924 \u45320 \u51256  \u45236 \u47548 \u50528 \u51064 \u51060  \u45817 \u50672 \u55176  \u45236  \u44225 \u50640  \u51080 \u51012  \u44163 \u51060 \u46972 \u45716  \u52265 \u44033 \u49789 \u44256  \u48736 \u47476 \u44172  \u46024 \u51012  \u48268 \u47140 \u44256  \u54616 \u45716  \u53468 \u46020 \u44284 \u46020 \u54620  \u50837 \u49900 \u8251  \u53440 \u50892  \u52852 \u46300 \u45716  \u45817 \u49888 \u50640 \u44172  \u44221 \u44256  \u47700 \u49884 \u51648 \u47484  \u45912 \u51256 \u51469 \u45768 \u45796 . \u45720  \u47749 \u49900 \u54616 \u49464 \u50836 . \u51096 \u47803 \u46108  \u49888 \u45392 \u44284  \u49324 \u44256 \u44032  \u45817 \u49888 \u51012  \u47924 \u45320 \u46888 \u47540  \u44163 \u51077 \u45768 \u45796 . \u44263  \u44536  \u49884 \u54744 \u50640  \u50724 \u47476 \u44172  \u46121 \u45768 \u45796 .\u53440 \u50892  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u50672 \u50528  \u48169 \u49885 \u51060 \u45208  \u53468 \u46020 \u47484  \u48148 \u45012 \u50556  \u54633 \u45768 \u45796 . \u44592 \u51316 \u51032  \u51060 \u48120 \u51648 \u47484  \u50756 \u51204 \u55176  \u53448 \u54588 \u54644 \u50556  \u49352 \u47196 \u50868  \u49324 \u46993 \u51012  \u49884 \u51089 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54788 \u51116  \u51096 \u47803 \u46108  \u44032 \u52824 \u44288 \u51012  \u44054 \u44256  \u51080 \u44144 \u45208 , \u50808 \u47784  \u44288 \u47532 \u47484  \u51204 \u54784  \u54616 \u51648  \u50506 \u45716  \u49345 \u53468 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u46892 \u44552 \u50630 \u44144 \u45208  \u50696 \u49345 \u52824  \u47803 \u54620  \u47564 \u45224 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u50724 \u47000  \u51204  \u51082 \u50632 \u45912  \u51060 \u49457 \u50640 \u44172  \u50672 \u46973 \u51060  \u50724 \u44144 \u45208 , \u50864 \u50672 \u52824  \u50506 \u44172  \u47560 \u51452 \u52432  \u50672 \u51060  \u45823 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u50868 \u47749 \u51032  \u51109 \u45212  \u44057 \u51008  \u51068 \u51060  \u48268 \u50612 \u51665 \u45768 \u45796 .\u53440 \u50892  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u47560 \u51020  \u49549 \u50640 \u49436  \u53360  \u48320 \u54868 \u44032  \u49373 \u44596  \u47784 \u50577 \u51077 \u45768 \u45796 . \u50612 \u46500  \u49324 \u44148 \u51004 \u47196  \u51064 \u54644 , \u44536 \u46041 \u50504  \u45817 \u49888 \u50640 \u44172  \u44032 \u51648 \u44256  \u51080 \u50632 \u45912  \'91\u44592 \u45824 \u45208  \u51339 \u51008  \u51060 \u48120 \u51648 \'92\u44032  \u47924 \u45320 \u51256  \u45236 \u47160 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u52964 \u54540 \u51060 \u46972 \u47732  \u51060 \u48324 \u51012  \u44208 \u49900 \u54616 \u44172  \u46104 \u45716  \u49692 \u44036 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u47924 \u50616 \u44032  \u51473 \u50836 \u54620  \u49324 \u49892 \u51012  \u44648 \u45804 \u50520 \u44144 \u45208 , \u54788 \u51116  \u49345 \u54889 \u50640 \u49436  \u48279 \u50612 \u45208 \u44256  \u49910 \u45796 \u45716  \u46907 \u51060 \u47728 , \u49345 \u45824 \u48169 \u51060  \'91\u51060 \u48120 \u51648  \u53448 \u54588 \'92\u47484  \u49884 \u46020 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \'91\u51088 \u51316 \u49900 \u51012  \u47784 \u46160  \u48260 \u47160 \u45796 \'92\u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u54633 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ]\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u51060 \u48324 \u54616 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u53360  \u52649 \u44201 \u51012  \u48155 \u44256  \u51060 \u48324 \u51012  \u44208 \u49900 \u54620  \u46319  \u48372 \u51077 \u45768 \u45796 . \u45796 \u47564  \u50724 \u47000 \u51204 \u48512 \u53552  \u51060 \u50612 \u51652  \u49373 \u44033 \u51008  \u50500 \u45768 \u47728 , \'91\u54616 \u45208 \u51032  \u49324 \u44148 \'92\u51012  \u44228 \u44592 \u47196  \u46496 \u50724 \u47476 \u44172  \u46108  \u49373 \u44033 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u44288 \u44228 \u47484  \u50756 \u51204 \u55176  \u46244 \u50638 \u44256  \u49910 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u48516 \u50948 \u44592 \u45208  \u50672 \u50528  \u49828 \u53440 \u51068  \u46321 \u51012  \u50756 \u51204 \u55176  \u46244 \u48148 \u44984 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 .\u51116 \u54924  \u44032 \u45733 \u49457 \u51008  \u47588 \u50864  \u55148 \u48149 \u54633 \u45768 \u45796 . \u53440 \u50892  \u52852 \u46300 \u45716  \u45800 \u49692 \u54620  \u45149 \u51060  \u50500 \u45772  \'91\u47736 \u47581 \'92\u51032  \u51032 \u48120 \u44032  \u44053 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44288 \u44228 \u44032  \u54924 \u48373 \u46112  \u50668 \u51648 \u44032  \u50504 \u48372 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u46160  \u45224 \u45376 \u44032  \u53440 \u50892 \u50640 \u49436  \u46504 \u50612 \u51648 \u44256  \u51080 \u45716  \u44163 \u52376 \u47100 , \u48148 \u45797 \u51312 \u52264  \u48372 \u51060 \u51648  \u50506 \u45716  \u50612 \u46176  \u49549 \u51004 \u47196  \u44260 \u46160 \u48149 \u51656 \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 .\u53440 \u50892  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u44049 \u51089 \u49828 \u47084 \u50868  \u49324 \u44256 \u47484  \u45824 \u48708 \u54616 \u49464 \u50836 . \u54924 \u49324 \u51032  \u51116 \u51221  \u49345 \u53468 \u44032  \u50612 \u47140 \u50892 \u51648 \u44144 \u45208 , \u51649 \u51109  \u46041 \u47308 \u50752  \u44049 \u51089 \u49828 \u47084 \u50868  \u53944 \u47084 \u48660 \u51060  \u49373 \u44600  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51032  \u54665 \u46041 \u51060  \u53360  \u54588 \u54644 \u47484  \u45180 \u52832  \u49688  \u51080 \u51004 \u48064 \u47196 , \u44316 \u54620  \u50724 \u51648 \u46998 \u46020  \u49340 \u44032 \u50556  \u54633 \u45768 \u45796 . \u51312 \u49900 \u55176  \u54665 \u46041 \u54616 \u49464 \u50836 .\u53945 \u55176  \u45817 \u49888 \u51060  \u49324 \u50629 \u51012  \u50868 \u50689 \u54620 \u45796 \u47732 , \u49324 \u44592 \u45208  \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u48176 \u49888 \u51012  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50808 \u48512 \u51201 \u51064  \u50836 \u51064 \u51004 \u47196  \u49324 \u50629 \u50640  \u50612 \u47140 \u50880 \u51060  \u49373 \u44600  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u51221 \u52824 \u51201  \u49345 \u54889 \u51060 \u45208 , \u44049 \u51089 \u49828 \u47084 \u50868  \u44508 \u51228 \u51077 \u45768 \u45796 . \u49324 \u50629 \u51012  \u54869 \u51109 \u54616 \u44592  \u48372 \u45796 \u45716  \u54788  \u49345 \u54889 \u51012  \u51648 \u53412 \u45716  \u45936  \u51665 \u51473 \u54644 \u50556  \u54616 \u47728 , \u51068 \u51221  \u48512 \u48516  \u48152 \u46300 \u49884  \u54788 \u44552 \u51012  \u54869 \u48372 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u54633 \u44201 \u51012  \u44036 \u51208 \u55176  \u50896 \u54620 \u45796 \u47732  \u47785 \u54364 \u47484  \u45230 \u52628 \u44144 \u45208  \u45432 \u47141 \u51012  \u45720 \u47140 \u50556  \u54633 \u45768 \u45796 . \u45817 \u50672 \u55176  \u48537 \u51012  \u44144 \u46972  \u49373 \u44033 \u54664 \u45912  \u45824 \u54617 , \u51649 \u51109 \u50640 \u49436  \u46504 \u50612 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 . \u53356 \u44172  \u49892 \u47581 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u53945 \u55176  \u44592 \u48376 \u51012  \u51473 \u49900 \u51004 \u47196  \u45796 \u49884  \u44277 \u48512 \u54644 \u50556  \u54633 \u45768 \u45796 .\u53440 \u50892  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u44049 \u51089 \u49828 \u47112  \u53360  \u49552 \u49892 \u51060  \u48156 \u49373 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52572 \u45824 \u54620  \u48372 \u49688 \u51201 \u51004 \u47196  \u51217 \u44540 \u54644 \u50556  \u54633 \u45768 \u45796 . \u45800 \u44592  \u53804 \u51088 \u46020  \u44552 \u47932 \u51077 \u45768 \u45796 . \u50837 \u49900 \u51012  \u48260 \u47532 \u44256  \u51068 \u51221  \u48512 \u48516 \u51008  \u51200 \u52629 \u54644 \u50556  \u54633 \u45768 \u45796 . \u44160 \u49548 \u54632 \u51012  \u50976 \u51648 \u54644 \u50556  \u54624  \u46412 \u51077 \u45768 \u45796 .    \
