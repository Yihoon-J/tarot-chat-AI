   \uc0\u50756 \u46300 3 \u51060 \u48120 \u51648  \u54644 \u49437 \u50756 \u46300 3Three of Wands \u8251  \u44033  \u50836 \u49548 \u50640  \u51648 \u45208 \u52824 \u44172  \u51665 \u51473 \u54616 \u47732  \u54644 \u49437 \u51060  \u55192 \u46308 \u50612 \u51665 \u45768 \u45796 . \u46384 \u46972 \u49436  \u44536 \u47548 \u51060  \u50612 \u46500  \u45656 \u50521 \u49828 \u47484  \u45812 \u44256  \u51080 \u45716 \u51648 \u47484  \u51473 \u51216 \u51004 \u47196  \u51060 \u54644 \u54616 \u49884 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 .\u45572 \u44400 \u44032 \u47484  \u44592 \u45796 \u47532 \u45716  \u47784 \u49845 \u47784 \u54744 \u51012  \u46496 \u45208 \u44592  \u50948 \u54620  \u51456 \u48708 \u47484  \u47560 \u52828  \u47784 \u49845  \u54841 \u51008  \u49892 \u51228 \u47196  \u47784 \u54744 \u51012  \u46496 \u45208 \u50740 \u49457 \u48317  \u48150 \u50640  \u51080 \u45716  \u44163 \u51004 \u47196  \u48372 \u50500  \u49892 \u51228 \u47196  \u44228 \u54925 \u51012  \u51060 \u54665 \u54620  \u47784 \u49845 \u45209 \u44288 \u51201 \u51064  \u48120 \u47000 \u47484  \u44536 \u47532 \u47728  \u48148 \u45796 \u47484  \u44048 \u49345 \u54616 \u45716  \u51473 \u44557 \u51221 \u51201  \u51032 \u48120 \u54644 \u49437  1. \u44228 \u54925 \u51012  \u49892 \u51228 \u47196  \u49892 \u54665 \u54616 \u45716 \u50756 \u46300 3\u51008  \u44228 \u54925 \u51032  \u49892 \u54665 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u47484  \u48372 \u47732 , \u49457 \u48317  \u48150 \u51004 \u47196  \u45208 \u50752  \u46405 \u50640  \u48156 \u51012  \u45236 \u46363 \u51008  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44228 \u54925 \u51012  \u50612 \u45712  \u51221 \u46020  \u51060 \u54665 \u54620  \u54980 , \u48169 \u54693 \u49457 \u51012  \u46384 \u51648 \u44256  \u51080 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300 2\u50752  \u48708 \u44368 \u54644 \u48372 \u47732 , \u51060 \u54644 \u54616 \u44592  \u49789 \u49845 \u45768 \u45796 . (\u50756 \u46300 2 \u54644 \u49437  \u44032 \u51060 \u46300  \u52280 \u44256 )\u50756 \u46300  2, \u49457 \u48317  \u50504 \u50640  \u51080 \u51020  (\u49892 \u54665  X)\u50756 \u46300  3, \u49457 \u48317  \u48150 \u50640  \u51080 \u51020  (\u49892 \u54665  O) \u50756 \u46300  2 \u8594  \u48148 \u45796 \u47484  \u48148 \u46972 \u48372 \u44256  \u51080 \u51004 \u45208  \u50500 \u51649  \u49457 \u48317  \u50504 \u50640  \u51080 \u51020  \u8594  \u44228 \u54925  \u49892 \u54665  X\u50756 \u46300  3 \u8594  \u48148 \u45796 \u47484  \u48148 \u46972 \u48372 \u44256  \u51080 \u44256  \u49457 \u48317  \u48150 \u50640  \u51080 \u51020  \u8594  \u44228 \u54925  \u49892 \u54665  O\u50756 \u46300 2\u44032  \u47784 \u51076 \u50640  \u45208 \u44040 \u44620  \u47568 \u44620 , \u49548 \u44060 \u54021 \u51012  \u45208 \u44040 \u44620  \u47568 \u44620  \'91YES OR NO\'92\u51032  \u45656 \u50521 \u49828 \u47484  \u45812 \u44256  \u51080 \u50632 \u45796 \u47732 , \u50756 \u46300 3\u51008  \u47784 \u51076 , \u49548 \u44060 \u54021 \u50640  \u49892 \u51228 \u47196  \u45208 \u44036  \u46244  \u50612 \u46523 \u44172  \u54665 \u46041 \u54644 \u50556  \u54624 \u51648  \'91HOW TO\'92\u47484  \u44256 \u48124 \u54616 \u45716  \u45656 \u50521 \u49828 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u50696 \u52968 \u45824  \u50672 \u50528 \u50868 \u50640 \u49436 \u45716 \u49892 \u51228 \u47196  \u44288 \u49900  \u44032 \u45716  \u49345 \u45824 \u50640 \u44172  \u50672 \u46973 \u51012  \u49884 \u46020 \u54616 \u44144 \u45208 \u49548 \u44060 \u54021 \u50640  \u45208 \u44032 \u44144 \u45208 SNS \u49345 \u50640 \u49436  \u49345 \u45824 \u48169 \u51032  \u48152 \u51025 \u51012  \u50976 \u46020 \u54616 \u47728 , \u54532 \u47196 \u54596 \u51012  \u48148 \u44984 \u44144 \u45208  \u44172 \u49884 \u47932 \u51012  \u50732 \u47532 \u45716  \u49324 \u46988 \u51116 \u54924 \u47484  \u49373 \u44033 \u47564  \u54616 \u44256  \u51080 \u45796 \u44032 , \u49892 \u51228 \u47196  \u50672 \u46973 \u51012  \u49884 \u46020 \u54616 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u48148 \u45796 \u47484  \u48148 \u46972 \u48372 \u47728  \u44032 \u47564 \u55176  \u49436 \u51080 \u45716  \u47784 \u49845 \u51008  \'91\u49345 \u45824 \u48169 \u51032  \u54588 \u46300 \u48177 \u51060 \u45208  \u48152 \u51025 \u51012  \u44592 \u45796 \u47532 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716  \u51060 \u51649 \u51060 \u45208  \u53748 \u49324 \u47484  \u44048 \u54665 \u54616 \u45716  \u49324 \u46988 , \u49352 \u47196 \u50868  \u49324 \u50629 \u50640  \u46020 \u51204 \u54616 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \'91\u44228 \u54925 \u51060  \u49892 \u51228 \u47196  \u51060 \u54665 \u46104 \u45716  \u49345 \u54889 \'92\u51012  \u49345 \u51669 \u54616 \u44592  \u46412 \u47928 \u50640 , \u51649 \u50629 \u51032  \u55120 \u47492 \u46020  \u44557 \u51221 \u51201 \u51004 \u47196  \u48389 \u45768 \u45796 .\u54644 \u49437  2. \u48120 \u47000 \u50640  \u45824 \u54620  \u51204 \u47581 \u51012  \u44536 \u47532 \u45716 \u50756 \u46300 3\u51008  \u48120 \u47000  \u51204 \u47581 \u51012  \u44536 \u47532 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51060 \u48120  \u51652 \u51204 \u46104 \u44256  \u51080 \u45716  \u44288 \u44228 \u51032  \u44221 \u50864 , \'91\u44208 \u54844 \u51060 \u45208  \u51088 \u45376  \u44228 \u54925 \'92 \u46321  \u44557 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u45716  \u44221 \u50864 \u44032  \u47566 \u49845 \u45768 \u45796 .\u45824 \u54364 \u51201 \u51004 \u47196  \'91\u44208 \u54844 \u51012  \u51456 \u48708 \u54616 \u45716  \u52964 \u54540 \'92 \u50640 \u44172 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u45224 \u51088 \u45716  \u50756 \u46300  3\u44060 \u47484  \u52313 \u44596  \u46244 , \u48148 \u45796 \u47484  \u50612 \u46523 \u44172  \u54756 \u52432 \u45208 \u44032 \u50556  \u54624 \u51648  \u44256 \u48124 \u54616 \u45716  \u44163 \u52376 \u47100  \u48372 \u51060 \u44592 \u46020  \u54616 \u44256 , \u44536 \u51200  \u44221 \u52824 \u47484  \u44048 \u49345 \u54616 \u47728  \u54665 \u48373 \u54620  \u48120 \u47000 \u47484  \u45000 \u44984 \u45716  \u44163 \u52376 \u47100  \u48372 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u49352 \u47196 \u50868  \u50689 \u50669 \u51004 \u47196  \u47784 \u54744 \u51012  \u46496 \u45208 \u45716 \u50756 \u46300 3\u51008  \u49352 \u47196 \u50868  \u50689 \u50669 \u51004 \u47196  \u47784 \u54744 \u51012  \u46496 \u45208 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51060 \u51204 \u44284  \u45796 \u47480  \u49464 \u44228 \u47484  \u44221 \u54744 \u54616 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 .\u45224 \u51088 \u44032  \u49436 \u51080 \u45716  \u44275 \u51008  \u48148 \u45796 \u50752  \u50977 \u51648 \u51032  \u44221 \u44228 \u51077 \u45768 \u45796 . \u51060 \u51204 \u44620 \u51648  \u44536 \u44032  \u50977 \u51648 \u50640 \u49436  \u49332 \u50520 \u45796 \u47732 , \u50526 \u51004 \u47196 \u45716  \u48148 \u45796 \u47484  \u45336 \u50612  \u49352 \u47196 \u50868  \u49464 \u44228 \u50640 \u49436  \u49332 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u50668 \u44592 \u49436  \u49352 \u47196 \u50868  \u49464 \u44228 \u46976  :\u50672 \u50528 \u50808 \u47196 \u50868  \u49556 \u47196  \u49373 \u54876 \u50640 \u49436  \u8594  \u50508 \u53097 \u45804 \u53097  \u52964 \u54540  \u49373 \u54876 \u47196 \u54588 \u44260 \u54620  \u52964 \u54540  \u49373 \u54876 \u50640 \u49436  \u8594  \u51088 \u50976 \u47196 \u50868  \u49556 \u47196  \u49373 \u54876 \u47196 \u51649 \u50629 \u44397 \u45236  \u44592 \u50629 \u50640 \u49436  \u8594  \u54644 \u50808  \u44592 \u50629 \u51004 \u47196 A \u48512 \u49436 \u50640 \u49436  \u8594  B \u48512 \u49436 \u47196 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u49556 \u47196  \u49373 \u54876 \u51012  \u52397 \u49328 \u54616 \u44256  \u52964 \u54540 \u47196 \u49436  \u49352 \u47196 \u50868  \u49334 \u51012  \u49884 \u51089 \u54616 \u47140  \u54620 \u45796 \u44144 \u45208  (\u44536  \u48152 \u45824 \u51068  \u49688 \u46020 ), \u51109 \u44144 \u47532  \u50668 \u54665 \u51012  \u44228 \u54925  \u51473 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716  \u54644 \u50808  \u52712 \u50629 \u51060 \u45208  \u50756 \u51204 \u55176  \u49352 \u47196 \u50868  \u44540 \u47924  \u54872 \u44221 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50668 \u54665  \u51649 \u51333 \u50640 \u46020  \u51096  \u50612 \u50872 \u47532 \u45716  \u52852 \u46300  \u51473  \u54616 \u45208 \u51077 \u45768 \u45796 .\u45224 \u51088 \u45716  \u51088 \u49888 \u44048  \u45336 \u52824 \u45716  \u51088 \u49464 \u47196  \u49436 \u51080 \u49845 \u45768 \u45796 . \u54252 \u51592 \u46020  \u44060 \u48169 \u51201 \u51077 \u45768 \u45796 . \u50526 \u51004 \u47196  \u48268 \u50612 \u51656  \u51068 \u46308 \u51008  \u46160 \u47140 \u50880 \u48372 \u45796  \u55148 \u47581 \u44284  \u49444 \u47128 \u51004 \u47196  \u44032 \u46301 \u54624  \u50696 \u51221 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u51032 \u48120 \u54644 \u49437  1. \u47680 \u50612 \u51648 \u47140  \u54616 \u45716 \u50756 \u46300 3\u51012  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u47680 \u47532  \u46496 \u45208 \u45716  \u49324 \u46988 \u51004 \u47196  \u48372 \u44592 \u46020  \u54633 \u45768 \u45796 .\u44536 \u47548 \u51032  \u49884 \u51216 \u51008  \u45224 \u51088 \u51032  \u46263 \u47784 \u49845 \u51012  \u48708 \u52628 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u44536 \u45716  \u51060 \u48120  \u44032 \u51652  \u50756 \u46300  2\u44060 \u47484  \u46244 \u47196  \u54620  \u52292 , \u49552 \u50640  \u54616 \u45208 \u51032  \u50756 \u46300 \u47564 \u51012  \u51536 \u44256  \u50668 \u54665 \u51012  \u46496 \u45225 \u45768 \u45796 .\u46321  \u46244 \u50640  \u51080 \u45716  \u50756 \u46300  2\u44060 \u45716  \u44284 \u44144  \u52397 \u49328 \u51012  \u50516 \u49884 \u54633 \u45768 \u45796 . \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u44284 \u44144 \u50640  \u49332 \u45912  \u51665 , \u49324 \u46988 , \u44048 \u51221 \u51012  \u48260 \u47532 \u44256  \u47680 \u47532  \u47784 \u54744 \u51012  \u46496 \u45208 \u47140 \u44256  \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  \u50672 \u50528 \u50868 \u50640 \u49436 \u45716  : \u45000 \u45012 \u50772 \u45912  \u51068 \u51060  \u51080 \u50612  \u50672 \u50528 \u47484  \u54252 \u44592 \u54616 \u45716  \u49345 \u54889 , \u49352 \u47196  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u49373 \u44200  \u46496 \u45208 \u47140 \u45716  \u49345 \u54889  \u46321 \u46321 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u46020  : \u50612 \u47160 \u51012  \u51201  \u45000 \u51012  \u51060 \u47336 \u44592  \u50948 \u54644  \u49324 \u54364 \u47484  \u45236 \u45716  \u49324 \u46988 , \u44592 \u51316 \u50640  \u54644 \u50724 \u45912  \u51068 \u51012  \'91\u48260 \u47532 \u45716 \'92 \u49345 \u54889 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u54588 \u46300 \u48177 \u51012  \u44032 \u47564 \u55176  \u44592 \u45796 \u47532 \u45716  \u47784 \u49845 \u50756 \u46300 3\u51060  \'91\u44228 \u54925 \u51032  \u49892 \u54665 \'92\u51012  \u46907 \u54616 \u51648 \u47564 , \u49688 \u46041 \u51201 \u51064  \u51032 \u48120 \u46020  \u54632 \u44760  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45224 \u51088 \u45716  \u50756 \u46300 \u47484  \u46405 \u50640  \u44866 \u50500 \u45459 \u44256  \u48148 \u45796 \u47484  \'93\u44032 \u47564 \u55176 \'94 \u44048 \u49345 \u54616 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51060 \u45716  \u50612 \u46500  \u54665 \u46041 \u51012  \u54620  \u46244 , \u54588 \u46300 \u48177 \u51012  \u44592 \u45796 \u47532 \u44256  \u51080 \u45796 \u45716  \'91\u51008 \u50976 \u51201  \u54364 \u54788 \'92\u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50612 \u46500  \u54588 \u46300 \u48177 (\u48152 \u51025 )\u51012  \u48155 \u44592  \u51204 \u44620 \u51648 , \u50880 \u51649 \u51060 \u51648  \u50506 \u51012  \u44163 \u51060 \u46972 \u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612  :\u54532 \u47196 \u54596  \u49324 \u51652 \u51060 \u45208  \u54532 \u47196 \u54596  \u51020 \u50501 \u51012  \u48148 \u44988  \u46244 \u50640 , \u49345 \u45824 \u48169 \u51060  \u50612 \u46523 \u44172  \u45208 \u50732 \u51648  \u44288 \u52272 \u54616 \u45716  \u47784 \u49845 \u50672 \u46973 \u51012  \u52712 \u54620  \u46244 \u50640  \u49345 \u45824 \u48169 \u51032  \u48152 \u51025 \u51012  \u51648 \u53020 \u48372 \u45716  \u47784 \u49845 SNS \u44172 \u49884 \u47932 \u47196  \u49345 \u45824 \u51032  \u48152 \u51025 \u51012  \u50976 \u46020 \u54616 \u45716  \u47784 \u49845 \u50756 \u46300 3 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u48320 \u54868 \u50640  \u51649 \u47732 \u54620 \u46319 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44536 \u45716  \u53360  \u44208 \u49900 \u51012  \u54664 \u49845 \u45768 \u45796 . \u44536  \u44208 \u49900 \u51060  \u47924 \u50631 \u51064 \u51648 \u45716  \u50508  \u49688  \u50630 \u51004 \u45208 , \u44536 \u46041 \u50504  \u47672 \u47932 \u47084 \u50772 \u45912  \u51109 \u49548 \u47484  \u48260 \u47532 \u44256 , \u49352 \u47196 \u50868  \u49464 \u44228 \u47196  \u48156 \u46027 \u50880 \u54624 \u47564 \u53372  \u53360  \u48320 \u54868 \u51077 \u45768 \u45796 . \u47564 \u50557  \u48372 \u51312  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u44288 \u44228 \u51032  \u54924 \u48373 \u51060 \u45208  \u49352 \u47196 \u50868  \u50672 \u50528 \u47484  \u50948 \u54644  \u55192 \u50420 \u45796 \u45716  \u54364 \u54788 \u51004 \u47196  \u51060 \u54644 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50756 \u46300  3\u48264  \u52852 \u46300 \u45716  \'91\u47924 \u50616 \u44032  \u48260 \u47140 \u50556  \u54616 \u45716  \u49345 \u54889 \'92\u51060  \u50621 \u54784 \u51080 \u45716  \u44221 \u50864 \u44032  \u47566 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u51068 \u51060  \u45320 \u47924  \u48148 \u49240 \u44592  \u46412 \u47928 \u50640  \u50672 \u50528 \u47484  \u54252 \u44592 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 , \u44592 \u51316 \u51032  \u51649 \u50629 \u51012  \u48260 \u47140 \u50556  \u54616 \u45716  \u49345 \u54889  \u46321 \u46321 \u51077 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u48169 \u54889 \u51012  \u44144 \u46317 \u54616 \u45716  20\u45824 \u47484  \u51096  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u46300 3\u51060  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u54644 \u49437 \u51032  \u48169 \u54693 \u49457 \u51008  \u53356 \u44172  3\u44032 \u51648 \u51077 \u45768 \u45796 .\u51648 \u53020 \u48376 \u45796  \u8594  \u49345 \u45824 \u48169 \u51008  \u50612 \u46500  \u54665 \u46041 \u51012  \u51060 \u48120  \u54620  \u49345 \u53468 \u47196 , \u51060 \u51228  \u45817 \u49888 \u51032  \u48152 \u51025 \u51012  \u44592 \u45796 \u47532 \u45716  \u51473 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u44032  \'91\u48150 \u51004 \u47196  \u45208 \u50752  \u46405 \u50640  \u48156 \u51012  \u45236 \u46363 \u51008  \u44163 \u52376 \u47100 \'92 \u47568 \u51060 \u51424 . \u50526 \u51004 \u47196  \u45817 \u49888 \u51060  \u48372 \u51068  \u48169 \u54693 \u49457 \u50640  \u46384 \u46972  \u50526 \u51004 \u47196  \u44228 \u49549  \u45208 \u50500 \u44040 \u51648 , \u48156 \u44600 \u51012  \u46028 \u47540 \u51648  \u49440 \u53469 \u54624  \u50696 \u51221 \u51077 \u45768 \u45796 .\u46496 \u45208 \u47140 \u54620 \u45796  \u8594  \u49345 \u45824 \u48169 \u51008  \u51060 \u51204 \u48512 \u53552  \u54616 \u44256  \u49910 \u50632 \u45912  \u51068 , \u45000 , \u54841 \u51008  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u51080 \u45912  \u49345 \u53468 \u47196 , \u51060 \u51228  \u44536 \u44275 \u51004 \u47196  \u46496 \u45208 \u47140  \u54633 \u45768 \u45796 . \u45817 \u48516 \u44036 \u51008  \u44536 \u44275 \u50640  \u47672 \u47932  \u50696 \u51221 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \u46244 \u47196  \u54620  \u52292 .\u45796 \u44032 \u44036 \u45796  \u8594  \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \u45796 \u44032 \u44032 \u47140  \u54633 \u45768 \u45796 . \u50612 \u46500  \u49885 \u51004 \u47196  \u45796 \u44032 \u44032 \u50556  \u54624 \u51648  \u49373 \u44033 \u54616 \u44256  \u51080 \u51004 \u47728 , \u51452 \u48320  \u52852 \u46300 \u51032  \u55120 \u47492 \u51060  \u44557 \u51221 \u51201 \u51064  \u44221 \u50864 , \u51312 \u47564 \u44036  \u45817 \u49888 \u50640 \u44172  \u50672 \u46973 \u51012  \u54616 \u44192 \u45796 \u45716  \u49888 \u54840 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51064  \u44221 \u50864 , \u51109 \u44592 \u51201 \u51064  \u48120 \u47000 \u47484  \u44256 \u48124 \u54624  \u47564 \u53372  \u50528 \u53963 \u54620  \u49324 \u51060 \u51076 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844 \u51077 \u45768 \u45796 . \'93\u51064 \u49373 \u51032  \u46041 \u48152 \u51088 \'94\u46972 \u4