   \uc0\u52981  \u53304  \u51060 \u48120 \u51648 \u52981  \u53304  \'96 \u47784 \u49457 \u50528  \u45336 \u52824 \u45716  \u50668 \u50773  \u50668 \u50773 \u51060  \u54868 \u47140 \u54620  \u52981 \u51012  \u48148 \u46972 \u48372 \u45716  \u47784 \u49845 \u47784 \u49457 \u50528 \u50752  \u44048 \u49688 \u49457 \u51060  \u45336 \u52824 \u45716  \u49324 \u46988 \u50808 \u47784 \u44032  \u46832 \u50612 \u45212  \u50668 \u49457 , \u50668 \u49457 \u49828 \u47084 \u50868  \u45224 \u51088 \u45236 \u49457 \u51201 \u51060 \u47728  \u51648 \u45208 \u52824 \u44172  \u54732 \u49888 \u54624  \u49688  \u51080 \u51020 \u51012  \u44221 \u44256 \u44221 \u44228 \u49900 \u51060  \u47566 \u44144 \u45208  \u50696 \u48124 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u51060 \u48120 \u51648  \u49345 \u51669 \u46748 \u44753  \u51080 \u45716  \u54868 \u47140 \u54620  \u52981  : \u54413 \u48512 \u54620  \u44048 \u49688 \u49457 , \u49549 \u45236 \u47484  \u44048 \u52628 \u45716 , \u52285 \u51032 \u47141 \u50773 \u44288  : \u45733 \u47141 , \u44428 \u50948 \u54616 \u50560 \u49353  \u46300 \u47112 \u49828  : \u49692 \u49688 \u54632 \u47932 \u50640  \u51222 \u51008  \u46300 \u47112 \u49828  \u45149 \u51088 \u46973  : \u46412 \u47196 \u45716  \u44048 \u51221 \u50640  \u55136 \u48849  \u51222 \u50612 \u48260 \u47540  \u49688  \u51080 \u51020 \u54028 \u46020 \u44032  \u52824 \u45716  \u48148 \u45796  : \u44618 \u51008  \u45236 \u47732 , \u44048 \u49688 \u49457 , \u44048 \u51221  \u44592 \u48373 \u51312 \u44060  \u47784 \u50577  \u48652 \u47196 \u52824  : \u50668 \u49457 \u49457 \u47932 \u44208  \u47924 \u45740  \u47581 \u53664  : \u50668 \u49457 \u49457 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u47784 \u49457 \u50528 \u44032  \u45336 \u52824 \u45716 \u52981  \u53304 \u51008  \u47784 \u49457 \u50528 \u44032  \u45336 \u52824 \u45716  \u49324 \u46988 , \u54841 \u51008  \u45572 \u44400 \u44032 \u47484  \u51339 \u50500 \u54616 \u45716  \u47560 \u51020 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u52981 \u51008  \'91\u49324 \u46988 \u51032  \u47560 \u51020 \u51060 \u45208  \u44048 \u51221 \'92\u51012  \u51032 \u48120 \u54616 \u47728 , \u45458 \u51008  \u44228 \u44553 \u51032  \u53304 (Queen)\u51008  \u44536  \u49549 \u49457 \u51060  \u46748 \u47159 \u54616 \u45796 \u44256  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52981  \u53304 \u51032  \u49324 \u46993 \u51008  \u49457 \u49689 \u54616 \u44256  \u50612 \u47480 \u49828 \u47101 \u49845 \u45768 \u45796 . \u50676 \u51221 \u51201 \u51060 \u44256  \u51676 \u47551 \u54620  \u49324 \u46993 \u48372 \u45796 \u45716 , \'91\u52313 \u44200 \u51452 \u44256  \u54732 \u49888 \u54616 \u45716  \u47784 \u49457 \u50528 \'92\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \u48176 \u47140 \u49900 \u46020  \u46832 \u50612 \u45225 \u45768 \u45796 . \u53440 \u47196 \u50640 \u49436  \'93\u54788 \u47784 \u50577 \u52376 \'94\u47484  \u49345 \u51669 \u54616 \u45716  \u45824 \u54364 \u51201 \u51064  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52981  \u50668 \u50773 \u51008  \u50668 \u49457 \u49457 \u51060  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50668 \u50773 \u51060  \u51077 \u51008  \u47932 \u44208  \u47924 \u45740  \u47581 \u53664 \u50752  \u51312 \u44060  \u48652 \u47196 \u52824 , \u50773 \u51088 \u51032  \u47932 \u44256 \u44592  \u51109 \u49885 \u51008  \u47784 \u46160  \u50668 \u49457 \u51032  \u44048 \u49688 \u49457 \u51012  \u44053 \u51312 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51068 \u48152 \u51201 \u51004 \u47196  \u50808 \u47784 \u44032  \u46832 \u50612 \u45212  \u50668 \u49457 \u51012  \u44032 \u47532 \u53412 \u51648 \u47564 , \u50668 \u49457 \u49828 \u47084 \u50868  \u45224 \u49457 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u47564 \u50557  \u48376 \u51064 \u51032  \u47784 \u49845 \u51060  Queen of Cups\u47196  \u54364 \u54788 \u46096 \u45796 \u47732 , \u50668 \u49457 \u49828 \u47101 \u44172  \u48372 \u51064 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u45817 \u49888 \u50640  \u45824 \u54620  \u50528 \u51221 \u51060  \u45336 \u52841 \u45768 \u45796 . \u50672 \u50528  \u44048 \u51221 \u51060  \u50500 \u45768 \u46972  \u45800 \u49692 \u55176  \u50500 \u45180 \u44144 \u45208  \u46020 \u50752 \u51452 \u44256  \u49910 \u51008  \u47560 \u51020 \u51012  \u45208 \u53440 \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51649 \u50629 \u50868  : \u45572 \u44400 \u44032 \u51032  \u46020 \u50880 \u51012  \u48155 \u44144 \u45208 , \u46020 \u50880  \u51460  \u51068 \u51060  \u49373 \u44592 \u44172  \u46121 \u45768 \u45796 .\u52964 \u54540  : \u49436 \u47196 \u47484  \u49324 \u46993 \u54616 \u47728 , \u46384 \u46907 \u54616 \u44172  \u52313 \u44200 \u51452 \u44256  \u51080 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u54620 \u51901 \u51060  \'93\u51088 \u49885  \u46028 \u48372 \u46319 \'94 \u54665 \u46041 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u50808 \u47784  : \u50668 \u49457 \u49828 \u47101 \u44256  \u51312 \u49888 \u54620  \u51060 \u48120 \u51648 \u54644 \u49437  2. \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54620 \u52981  \u53304 \u51008  \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54620  \u49324 \u46988 , \u46608 \u45716  \u44618 \u51008  \u44048 \u51221 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u52981 \u51060  \u50500 \u51452  \u54868 \u47140 \u54616 \u44172  \u54364 \u54788 \u46108  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \'93\u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \u51060  \u54413 \u48512 \u54616 \u45796 \'94, \'93\u52285 \u51032 \u51201 \u51060 \u45796 \'94\u46972 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50696 \u49696  \u51649 \u51333 \u50640  \u50612 \u50872 \u47532 \u47728 , \u44277 \u44048  \u45733 \u47141 \u46020  \u46832 \u50612 \u45208  \u51452 \u48320  \u54217 \u54032 \u51060  \u51339 \u49845 \u45768 \u45796 .\u46608 \u54620  \u53304 \u51032  \u50743 \u51088 \u46973  \u45149 \u51060  \u47932 \u50640  \u45823 \u50500 \u51080 \u45716 \u45936 \u50836 . \u51060 \u45716  \u45824 \u48512 \u48516 \u51032  \u47928 \u51228 \u47484  \u44592 \u48516 \u51060 \u45208  \u44048 \u51221 \u50640  \u50672 \u44288  \u51648 \u50612  \u49373 \u44033 \u54624  \u49688  \u51080 \u51020 \u51012  \u44221 \u44256 \u54633 \u45768 \u45796 . (\u44053 \u51008  \u49324 \u46988 \u51032  \'93\u44048 \u51221 \'94\u51012  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536  \u44048 \u51221 \u50640  \u55136 \u48849  \u51222 \u50612 \u48260 \u47540  \u49688  \u51080 \u49845 \u45768 \u45796 .) \u49324 \u46993 \u50640  \u48736 \u51648 \u47732  \u50728  \u49888 \u44221 \u51012  \u54028 \u53944 \u45320 \u50640 \u44172 \u47564  \u51665 \u51473 \u54624  \u49688  \u51080 \u44192 \u51004 \u47728 , \u49345 \u52376 \u46020  \u49789 \u44172  \u49789 \u44172  \u48155 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u48516 \u50948 \u44592 \u50640  \u49789 \u44172  \'93\u51204 \u50684 \'94\u46104 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44536 \u47084 \u45208  \u54788 \u49892 \u51201 \u51064  \u47732 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50668 \u50773 \u51008  \u47932 \u44032 \u50640  \u48156 \u51012  \u50732 \u47140 \u45459 \u51648  \u50506 \u44256 , \u50508 \u47197 \u45804 \u47197 \u54620  \u51312 \u50557 \u46028 (\u54788 \u49892 ) \u50948 \u50640  \u48156 \u51012  \u50732 \u47140 \u45459 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47560 \u45285  \u44048 \u49457 \u50640  \u51208 \u50668 \u51652  \u44172  \u50500 \u45768 \u46972 , \u45733 \u47141 \u44284  \u51648 \u54812 \u46020  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u51221 \u47532 \u54644 \u48372 \u47732  :\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u45817 \u49888 \u51012  \u44618 \u44172  \u49373 \u44033 \u54616 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45800 \u49692 \u54620  \'91\u54840 \u44048 , \u54840 \u44592 \u49900 \'92\u48372 \u45796 \u45716  \u45908 \u50865 \u45908  \u44256 \u52264 \u50896 \u51201 \u51064  \u44048 \u51221 \u51077 \u45768 \u45796 . \u50528 \u51221 , \u54732 \u49888 , \u44536 \u47532 \u50880  \u46321 \u46321 \u51077 \u45768 \u45796 .\u49457 \u44201  : \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54616 \u47728 , \u44277 \u44048  \u45733 \u47141 \u51060  \u46832 \u50612 \u45225 \u45768 \u45796 . \u51452 \u48320  \u54872 \u44221 \u50640  \u49789 \u44172  \u50689 \u54693 \u51012  \u48155 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . MBTI\u47196 \u45716  INFJ\u50752  \u44032 \u51109  \u48708 \u49847 \u54633 \u45768 \u45796 .\u51649 \u50629 \u50868  : \u44061 \u44288 \u51201 \u51064  \u54032 \u45800 \u48372 \u45796 \u45716  \'91\u44048 \u51221 \u51060 \u45208  \u51649 \u44288 \'92\u51004 \u47196  \u54032 \u45800 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44048 \u49688 \u49457 \u51060  \u54413 \u48512 \u54616 \u44592  \u46412 \u47928 \u50640  \'93\u50696 \u49696 \u44228 \u45208  \u52285 \u51032 \u49457 \u51012  \u48156 \u55064 \u54616 \u45716  \u51649 \u50629 \'94\u50640  \u50612 \u50872 \u47549 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49549 \u45236 \u47484  \u44048 \u52628 \u45716 \u53304  \u52981 \u51008  \u45236 \u49457 \u51201 \u51064  \u49457 \u44201 , \u54841 \u51008  \u49549 \u45236 \u47484  \u44048 \u52628 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732 , \u50668 \u50773 \u51032  \u52981 \u51060  \u45796 \u47480  \u52981 \u44284  \u45804 \u47532  \u46748 \u44753 \u51004 \u47196  \u45803 \u54784 \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \'91\u48376 \u51064 \u51032  \u49373 \u44033 \u51060 \u45208  \u47560 \u51020 , \u44048 \u51221 \u51012  \u44048 \u52628 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 . \u51677 \u49324 \u46993  \u51204 \u47928 \u44032 \u50640 \u44172  \u47566 \u51060  \u45208 \u50724 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u51068 \u48152 \u51201 \u51064  \u52981 \u51032  \u47784 \u49845 \u52981  \u53304 \u50640 \u49436 \u51032  \u52981  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u45817 \u49888 \u50640 \u44172  \u47568  \u47803 \u54624  \u49324 \u50672 \u51060  \u51080 \u45796 . \u45224 \u50640 \u44172  \u46300 \u47084 \u45236 \u51648  \u47803 \u54616 \u44256  \u45145 \u45145  \u50515 \u44256  \u51080 \u45796 .\u49549 \u51004 \u47196  \u49325 \u54784 \u50556  \u54616 \u45716  \u44618 \u51008  \u49345 \u52376 \u44032  \u51080 \u45796 .\u44169 \u48372 \u44592 \u48372 \u45796  \u55016 \u50476  \u45908  \u47566 \u51008  \u49373 \u44033 \u51012  \u54616 \u44256  \u51080 \u51004 \u47728 , \u54840 \u44048 \u51060 \u45208  \u48152 \u44048 \u51012  \u54000 \u45236 \u51648  \u50506 \u45716  \u49457 \u44201 \u51060 \u45796 .Queen of Cups\u45716  \u45236 \u47732 \u50640  \u44618 \u51008  \u49345 \u52376 \u44032  \u51080 \u45716  \u49324 \u46988 , \u50864 \u50872 \u51613 \u51060  \u51080 \u45716  \u49324 \u46988 , \u50808 \u47196 \u50880 \u51012  \u45712 \u45180 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51060 \u47484  \u53560 \u50612 \u45459 \u51648 \u46020  \u50506 \u50500 , \u51452 \u48320 \u50640 \u49436  \u45576 \u52824 \u52292 \u44592 \u46020  \u55192 \u46308 \u51424 . \u45572 \u44396 \u48372 \u45796  \u50948 \u47196 \u50752  \u44277 \u44048 \u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u48376 \u51064 \u51012  \u46028 \u48372 \u51648  \u47803 \u54616 \u45716 \u52981  \u53304 \u51008  \u48176 \u47140 \u49900 \u51060  \u44053 \u54616 \u44144 \u45208 , \u45224 \u50640 \u44172  \u47582 \u52656 \u51452 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u54637 \u49345  \u45224 \u50640 \u44172  \u49888 \u44221  \u50416 \u44256 , \u47582 \u52656  \u51452 \u45796  \u48372 \u45768  \u48376 \u51064 \u51012  \u46028 \u48372 \u51648  \u47803 \u54616 \u45716  \u44221 \u50864 \u44032  \u45796 \u48152 \u49324 \u51077 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u52981 \u51008  \'93\u49688 \u50857 \u54616 \u44256  \u54252 \u50857 \u54616 \u45716  \u49457 \u44201 \'94\u51012  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644 , \u45224 \u50640 \u44172  \u47582 \u52656 \u51452 \u44256  \u49345 \u45824 \u48169 \u51012  \u51060 \u54644 \u54644 \u51456 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u53304 \u51008  \u51060  \u49457 \u44201 \u51060  \u44537 \u45824 \u54868 \u46104 \u50632 \u45796 \u44256  \u48372 \u49884 \u47732  \u46121 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u45796 \u47480  \u45572 \u44400 \u44032 \u47484  \u49888 \u44221  \u50416 \u45712 \u46972  \u48376 \u51064 \u51012  \u46028 \u48372 \u51648  \u47803 \u54616 \u45716  \u49345 \u53468 \u51088 \u52843  \u44048 \u51221  \u50416 \u47112 \u44592 \u53685 \u51004 \u47196  \u51204 \u46973 \u54624  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54632 \u53440 \u51064 \u50640 \u44172  \u50689 \u54693 \u51012  \u48155 \u44592  \u49772 \u50880 \u51648 \u45208 \u52824 \u44172  \u54140 \u51452 \u45796 \u44032  \u54840 \u44396  \u51105 \u55176 \u44592  \u51339 \u51020 \u45572 \u44400 \u44032 \u47484  \u46020 \u50752 \u51452 