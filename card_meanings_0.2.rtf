   \uc0\u47785 \u52264 \u54644 \u49437  1. \u50500 \u45772  \u44148  \u50500 \u45768 \u45796 \u54644 \u49437  2. \u50696 \u47532 \u54620  \u44048 \u44033 \u51012  \u49324 \u50857 \u54616 \u45716 \u54644 \u49437  3. \u54620 \u45576  \u54036 \u51648  \u50506 \u44256  \u51649 \u51652 \u54616 \u45716 \u54644 \u49437  4. \u48148 \u50868 \u45908 \u47532 \u44032  \u52384 \u51200 \u54620  \u49324 \u46988 \u54644 \u49437  5. \u46608 \u47000 \u48372 \u45796  \u49457 \u49689 \u54620 \u49548 \u46300  \u53304  \u50672 \u50528 \u50868  \u54644 \u49437 \u49548 \u46300  \u53304  \u51649 \u50629 \u50868  \u54644 \u49437 \u49548 \u46300  \u53304  \u44552 \u51204 \u50868  \u54644 \u49437 \u52852 \u46300  \u51060 \u47492  : \u49548 \u46300  \u53304 , Queen of Swords\u47896 \u49324  : \u50628 \u49689 \u54620  \u54364 \u51221 \u51004 \u47196  \u52860 \u51012  \u46308 \u44256 , \u47676  \u44275 \u51012  \u48148 \u46972 \u48372 \u45716  \u50668 \u50773 \u51032  \u47784 \u49845 \u51077 \u45768 \u45796 .\u51452 \u50836  \u53412 \u50892 \u46300  : \u51652 \u49892 , \u49556 \u51649 \u54632 , \u50696 \u47532 \u54632 , \u44592 \u48124 \u54632 , \u46021 \u47549 \u51201 \u51064 , \u49457 \u49689 \u54632 , \u51648 \u54812 \u45208 \u53440 \u45236 \u45716  \u49324 \u46988  : \u51652 \u49892 \u51012  \u52628 \u44396 \u54632 , \u49556 \u51649 \u54620  \u44163 \u51012  \u51339 \u50500 \u54632 , \u48712 \u53960 \u50630 \u44256  \u44860 \u44860 \u54632 , \u54620 \u45576 \u54036 \u51648  \u50506 \u51020 , \u49457 \u49689 \u54632 , \u44592 \u48124 \u54616 \u44172  \u45824 \u52376 \u54632 , \u54200 \u44204 \u51012  \u44054 \u51648  \u50506 \u51020 , \u44592 \u45824 \u52824 \u44032  \u45458 \u51008  \u49324 \u46988 , \u44144 \u51667 \u47568 \u51012  \u45040 \u46763 \u50612 \u48372 \u45716 \u45208 \u53440 \u45236 \u45716  \u49345 \u54889  : \u45257 \u51221 \u54616 \u44172  \u51652 \u49892 \u51012  \u47568 \u54632 , \u48712 \u53960 \u50630 \u44256  \u44860 \u44860 \u54616 \u44172  \u52376 \u47532 \u54632 , \u44592 \u48124 \u54616 \u44172  \u45824 \u52376 \u54632 , \u46021 \u47549 \u51201 \u51004 \u47196  \u48128 \u44256  \u45208 \u44048 , \u51648 \u54812 \u47213 \u44172  \u45824 \u52376 \u54632 , \u50500 \u45772  \u44163 \u51008  \u50500 \u45768 \u46972 \u44256  \u47568 \u54632 \u49549 \u47560 \u51020  : \u49440 \u51012  \u44555 \u44256  \u49910 \u45796 , \u50500 \u45772  \u44148  \u50500 \u45768 \u45796 , \u49556 \u51649 \u54616 \u44172  \u53560 \u50612 \u45480 \u51004 \u47732  \u51339 \u44192 \u45796 , \u50504  \u47582 \u45716  \u48512 \u48516 \u51060  \u51080 \u51004 \u45208  \u51648 \u53020 \u48372 \u44256  \u44208 \u51221 \u54616 \u44192 \u45796 \u48169 \u54644 \u46104 \u45716  \u50836 \u49548  : \u45257 \u51221 \u54632 , \u47196 \u47592 \u54001 \u54616 \u51648  \u50506 \u51020 , \u48512 \u51313 \u54620  \u50668 \u49457 \u49457 , \u51649 \u49444 \u51201 \u51076 , \u44256 \u47549 \u46108 \u51452 \u50836  \u51312 \u50616  : \u49345 \u45824 \u48169 \u50640 \u44172  \u49556 \u51649 \u54616 \u44172  \u53560 \u50612 \u45459 \u51012  \u44163 , \u50696 \u47532 \u54616 \u44256  \u44592 \u48124 \u54616 \u44172  \u45824 \u52376 \u54624  \u44163 , \u51652 \u49892 \u51012  \u51473 \u50836 \u49884  \u50668 \u44600  \u44163 , \u44288 \u45824 \u54632 \u51060  \u54596 \u50836 \u54632 \u54644 \u49437  1. \u50500 \u45772  \u44148  \u50500 \u45768 \u45796 \u49548 \u46300  \u53304 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u51652 \u49900 \u51012  \u52628 \u44396 \u54616 \u47140  \u45432 \u47141 \u54633 \u45768 \u45796 .\u51096 \u47803 \u46108  \u51216 \u51060  \u51080 \u45796 \u47732  \u49345 \u45824 \u48169 \u51060  \u45572 \u44396 \u46304  \u48148 \u47196  \u47568 \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u44592 \u48516 \u48372 \u45796  \u51652 \u49892 \u51060  \u45908  \u51473 \u50836 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u54775 \u44040 \u47532 \u44172  \u54616 \u44144 \u45208  \u50668 \u51648 \u47484  \u51452 \u45716  \u54665 \u46041 \u51008  \u51208 \u45824  \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u44396 \u52404 \u51201 \u51004 \u47196  \u51201 \u50857 \u54616 \u47732  :\u44256 \u48177 \u51012  \u48155 \u50520 \u51004 \u45208  \u47560 \u51020 \u51060  \u50630 \u45716  \u49345 \u53468 \u46972 \u47732 , \u54252 \u51109 \u54616 \u51648  \u50506 \u44256  \'91\u44288 \u49900 \u51060  \u50630 \u45796 \u45716 \'92 \u49885 \u51004 \u47196  \u46385  \u51096 \u46972 \u49436  \u47568 \u54632 \u47588 \u45320 , \u50696 \u51032  \u50630 \u45716  \u54665 \u46041 \u51012  \u48420 \u45796 \u47732 , \u44263 \u48148 \u47196  \u47568 \u54644 \u51452 \u45716  \u49828 \u53440 \u51068 \u46412 \u47196 \u45716  \u51221  \u50630 \u51060  \u48372 \u50668 \u49436  \u50724 \u54644 \u47484  \u49324 \u44592 \u46020  \u54632 \u47560 \u51020 \u51060  \u49885 \u50632 \u45796 \u47732 , \'91\u44428 \u53468 \u44592 \u44032  \u50728  \u44163  \u44057 \u45796 \'92\u44256  \u49556 \u51649 \u54616 \u44172  \u47568 \u54632 \u54840 \u44048 \u51060  \u51080 \u45796 \u47732 , \'91\u45817 \u49888 \u51060  \u51339 \u45796 \u44256 \'92 \u51649 \u49444 \u51201 \u51004 \u47196  \u47568 \u54632 \u46608 \u54620  \u51652 \u49892 \u51012  \u51473 \u50836 \u49884  \u50668 \u44592 \u44592  \u46412 \u47928 \u50640 , \u51060 \u49345 \u54620  \u49548 \u47928 \u50640  \u55064 \u46168 \u47532 \u51648 \u46020  \u50506 \u49845 \u45768 \u45796 . \u54637 \u49345  \u54200 \u44204  \u50630 \u51060  \u49324 \u46988 \u51012  \u48372 \u47140  \u45432 \u47141 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u44160 \u51060  \u51649 \u44033 \u51004 \u47196  \u50732 \u48148 \u47476 \u44172  \u49436 \u51080 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \'91\u54200 \u54693 \u51201 \u51064  \u49324 \u44256 \u44032  \u50630 \u45796 \'92\u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50812 \u51901  \u53304  \u49548 \u46300 , \u50724 \u47480 \u51901  \u53433  \u49548 \u46300  Comment\u49548 \u46300  \u53433 \u51008  \u49548 \u46300  \u53304 \u44284  \u45804 \u47532  \u44160 \u51060  \u44592 \u50872 \u50612 \u51276 \u49845 \u45768 \u45796 . \u51060 \u52376 \u47100  \u49548 \u46300  \u53433 \u51060  \u49345 \u51669 \u54616 \u45716  \u44163 \u51008  \'91\u51088 \u49888 \u47564 \u51032  \u44592 \u51456 , \u46021 \u49440 \u51201 \u51064  \u49457 \u54693 \'92\u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u49548 \u46300  \u53304  \u52852 \u46300 \u45716  \u50500 \u45785 \u45768 \u45796 . \u45224 \u51032  \u51032 \u44204 \u51012  \u44061 \u44288 \u51201 \u51004 \u47196  \u46028 \u50500 \u48380  \u51460  \u50508 \u47728 , \u51339 \u51008  \u51032 \u44204 \u51060 \u46972 \u47732 , \u49345 \u45824 \u48169 \u51060  \u45572 \u44396 \u46304  \u51032 \u44204 \u51012  \u54252 \u50857 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45572 \u44400 \u44032  \u55148 \u47581 \u52268  \u48120 \u47000 \u47484  \u50584 \u44592 \u54616 \u45908 \u46972 \u46020 , \u51060 \u47484  \u54788 \u49892 \u51201 \u51004 \u47196  \u54032 \u45800 \u54620  \u46244  \u48708 \u54032 \u54624  \u51460  \u50500 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 .\u51652 \u49892 \u54632 \u44284  \u49556 \u51649 \u54632 \u51060  \u51473 \u50836 \u54616 \u45796 \u45716  \u47700 \u49884 \u51648 \u47484  \u51452 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51088 \u49888 \u51012  \u54252 \u51109 \u54616 \u45716  \u44163 \u48372 \u45796  \u51652 \u51676  \u49892 \u47141 \u51012  \u53412 \u50864 \u45716  \u51068 \u50640  \u51665 \u51473 \u54644 \u50556  \u54616 \u44256 , \u50528 \u50024  \u52280 \u45716  \u44163 \u48372 \u45796  \u49556 \u51649 \u54616 \u44172  \u47568 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50672 \u50528 \u50640 \u49436 \u46020  \u47560 \u52268 \u44032 \u51648 \u51077 \u45768 \u45796 . \'91\u47560 \u51020 \u51060  \u49885 \u51008  \u44163  \u44057 \u45796 \'92, \'91\u45320 \u50640 \u44172  \u47560 \u51020 \u51060  \u51080 \u45716  \u44163  \u44057 \u45796 \'92 \u45817 \u49888 \u51032  \u51652 \u49900 \u51012  \u49556 \u51649 \u54616 \u44172  \u54364 \u54788 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u50696 \u47532 \u54620  \u44048 \u44033 \u51012  \u49324 \u50857 \u54616 \u45716 \u49548 \u46300  \u53304 \u51008  \u48712 \u53960  \u50630 \u44256  \u44860 \u44860 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51652 \u49900 \u51060 \u45208  \u44144 \u51667 \u47568 \u51012  \u51096  \u45040 \u46763 \u50612 \u48372 \u47728 , \'91\u51068 \u48512 \u47084  \u47784 \u47476 \u45716  \u52377 \'92\u51012  \u54644 \u49436  \u49345 \u45824 \u48169 \u51012  \u53580 \u49828 \u53944 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51096 \u47803 \u54620  \u44172  \u51080 \u45796 \u47732  \u49704 \u44608 \u50630 \u51060  \u53560 \u50612 \u45459 \u45716  \u44172  \u51339 \u51004 \u47728 , \u49345 \u45824 \u48169 \u51008  \u45208 \u49244  \u49548 \u47532 \u47484  \u46307 \u45908 \u46972 \u46020 , \u50724 \u55176 \u47140  \u44536  \u51652 \u49892 \u54632 \u51012  \u45458 \u44172  \u54217 \u44032 \u54624  \u44163 \u51077 \u45768 \u45796 .\u50696 \u47484  \u46308 \u51088 \u47732  :\u50672 \u51064 \u51060  \u47792 \u47000  \u51060 \u49457 \u44284  \u49696 \u51012  \u47560 \u49512 \u45796 \u45716  \u49324 \u49892 \u51012  \u50508 \u44256  \u51080 \u51004 \u45208 , \'93\u50612 \u51228  \u47952 \u54664 \u50612 ?\'94 \u46972 \u45716  \u49885 \u51004 \u47196  \u53580 \u49828 \u53944 \u47484  \u49884 \u46020 . \u49556 \u51649 \u54616 \u44172  \u47568 \u54616 \u47732  \u50857 \u49436 \u54616 \u44256  \u45336 \u50612 \u44048 \u45572 \u44400 \u44032 \u44032  \u44536 \u47484  \u51339 \u50500 \u54616 \u44256  \u51080 \u45796 \u47732  \u51060 \u48120  \u45576 \u52824 \u52312 \u51012  \u44032 \u45733 \u49457  100%\u51060 \u48120 \u51648  \u54644 \u49437  : \u52852 \u46300  \u49549  \u50668 \u50773 \u51008  \u50724 \u47004  \u44592 \u44036  \u50773 \u44397 \u51012  \u53685 \u52824  \u54616 \u47728 , \u52649 \u49888 \u44284  \u48152 \u50669 \u51088 \u47484  \u44396 \u48516 \u54644 \u50772 \u49845 \u45768 \u45796 . \u49548 \u46300  \u53304 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u47924 \u52377 \u51060 \u45208  \u50696 \u47532 \u54633 \u45768 \u45796 . \u49704 \u44200 \u51652  \u51221 \u48372 \u50640  \u45824 \u54620  \u44048 \u51060  \u46832 \u50612 \u45225 \u45768 \u45796 . \u44536 \u45376 \u44032  \u50696 \u47532 \u54616 \u45796 \u44256  \u54644 \u49436 , \'91\u51221 \u51060  \u50630 \u45796 , \u47196 \u48391  \u44057 \u45796 \'92\u45716  \u46907 \u51008  \u50500 \u45785 \u45768 \u45796 . \u44277 \u44048 \u47141 \u46020  \u46832 \u50612 \u45225 \u45768 \u45796 . \u45224 \u51032  \u49836 \u54548 \u51012  \u52649 \u48516 \u55176  \u51060 \u54644 \u54633 \u45768 \u45796 .\u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u52860 \u51012  \u46308 \u51648  \u50506 \u51008  \u50812 \u51901  \u49552 \u51060  \u49552 \u48148 \u45797 \u51012  \u48372 \u51060 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u49345 \u45824 \u48169 \u50640  \u45824 \u54620  \'91\u48176 \u47140 \u50752  \u51316 \u51473 , \u44277 \u44048 \'92\u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u50696 \u47532 \u54620  \u44048 \u44033 \u51012  \u49324 \u50857 \u54644  \u51068 \u51012  \u49457 \u44277 \u51201 \u51004 \u47196  \u51652 \u54665 \u54620 \u45796 \u45716  \u46907 \u46020  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48516 \u49437 , \u52628 \u47200 , \u52968 \u49444 \u54021  \u46321 \u51032  \u50629 \u47924 \u50752  \u51201 \u54633 \u54616 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u54620 \u45576  \u54036 \u51648  \u50506 \u44256  \u51649 \u51652 \u54616 \u45716 \u49548 \u46300  \u53304 \u51008  \u54620 \u45576  \u54036 \u51648  \u50506 \u44256  \u51649 \u51652 \u54616 \u45716  \u49457 \u54693 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49548 \u46300 \u45716  \u54620  \u48264 \u50640  \u54620  \u49324 \u46988 \u47564  \u51453 \u51068  \u49688  \u51080 \u45716  \u47924 \u44592 \u51060 \u47728 , \u53440 \u47196 \u50640 \u49436  \u44208 \u45800 \u51012  \u49345 \u51669 \u54616 \u45716  \u49800 \u53944 \u51077 \u45768 \u45796 . \u50668 \u50773  \u46608 \u54620  \u51221 \u47732 \u51012  \u51025 \u49884 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u54620  \u48264  \u49884 \u51089 \u54620  \u51068 , \u51339 \u50500 \u54616 \u45716  \u52712 \u48120 \u45716  \u45149 \u44620 \u51648  \u54616 \u45716  \u49457 \u54693 \u48148 \u46988 \u45180 \u44032  \u50630 \u45716  \u49324 \u46988 , \u44032 \u51313 \u51012  \u50948 \u54644  \u54732 \u49888 \u54616 \u45716  \u49324 \u46988 \'91\u47680 \u50612 \u51256 \u50556 \u44192 \u45796 \'92\u44256  \u44208 \u49900 \u54664 \u45796 \u47732  \u45149 \u44620 \u51648  \u47680 \u50612 \u51656  \u44163 \u51064 \u45236 \u49900 \u51060  \u44053 \u54644 \u49436  \u47924 \u49832  \u51068 \u51060 \u46304  \u49457 \u44284 \u47484  \u45236 \u45716  \u49324 \u46988 \u47700 \u51060 \u51200  \u50500 \u47476 \u52852 \u45208  \u52852 \u46300  \u51473 \u50640 \u49436 \u45716  \u51204 \u52264  \u52852 \u46300 \u50752  \u44032 \u51109  \u48708 \u49847 \u54620  \u49457 \u44201 \u51012  \u46944 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564  \u44536 \u45376 \u45716  \u50505 \u50500 \u51080 \u44592 \u50640  \u54665 \u46041 \u47141 \u51008  \'91\u49332 \u51677 \'92 \u46504 \u50612 \u51652 \u45796 \u44256  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  4. \u48148 \u50868 \u45908 \u47532 \u44032  \u52384 \u51200 \u54620  \u49324 \u46988 \u49548 \u46300  \u53304 \u51008  \u46021 \u47549 \u51201 \u51064  \u49324 \u46988 , \u54841 \u51008  \u48148 \u50868 \u45908 \u47532 \u44032  \u52384 \u51200 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44592 \u48376 \u51201 \u51004 \u47196  \u49345 \u45824 \u48169 \u50640  \u45824 \u54620  \u44592 \u45824 \u52824 \u44032  \u45458 \u51004 \u47728 , \u48376 \u51064 \u50640 \u44172 \u46020  \u50628 \u44201 \u54620  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u47564 \u50557  \u44536 (\u44536 \u45376 )\u50752  \u51060 \u50612 \u51648 \u44256  \u49910 \u45796 \u47732 , \u44536 \u51200  \u44036 \u53469  \u45817 \u54616 \u45716  \u49688 \u48150 \u50640  \u50630 \u49845 \u45768 \u45796 .\u49548 \u46300  \u53304 \u51008  \u53440 \u47196 \u50640  \u45208 \u50724 \u45716  \u50668 \u49457  \u51473 \u50640 \u49436  \u44032 \u51109  \u45224 \u49457 \u51201 \u51064  \u49457 \u44201 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44288 \u44228 \u50640  \u51032 \u51316 \u54616 \u44592 \u48372 \u45796 \u45716  \'91\u48376 \u51064  \u44040 \u44256  \u45798 \u44592 \'92\u47484  \u49440 \u53469 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50808 \u47213 \u45796 \u44256  \u50500 \u47924 \u45208  \u47564 \u45208 \u51648  \u50506 \u51004 \u47728 , \u49324 \u46993 \u48372 \u45796  \u51068 \u51060  \u50864 \u49440 \u51060 \u46972 \u47732  \u50612 \u46500  \u50976 \u54841 \u51060  \u51080 \u45908 \u46972 \u46020  \u45149 \u44620 \u51648  \u51068 \u47564  \u54624  \u44163 \u51077 \u45768 \u45796 .\u46412 \u47928 \u50640  \u50672 \u50528 \u47484  \u54616 \u45908 \u46972 \u46020  \u48376 \u51064 \u47564 \u51032  \u49884 \u44036 , \u48376 \u51064 \u47564 \u51032  \u44277 \u44036 \u51008  \u46384 \u47196  \u47564 \u46308 \u50612 \u51452 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732  :\u47751  \u49884 \u48512 \u53552  \u47751  \u49884 \u44620 \u51648  \u50672 \u46973 \u51012  \u54616 \u51648  \u50506 \u44192 \u45796 \u45716  \u50557 \u49549 \u51060  \u54596 \u50836 \u54632 \u47588 \u51068  \u47564 \u45208 \u45716  \u44163 \u48372 \u45796  \u50612 \u51788 \u45796  \u54620  \u48264  \u50620 \u44404  \u48372 \u45716  \u44163 \u51012  \u45908  \u49440 \u54840 \u54632 \u50672 \u51064 \u48372 \u45796  \u48376 \u51064 \u51060  \u50864 \u49440 \u49692 \u50948 \u50640  \u51080 \u51020 \u54644 \u49437  5. \u46608 \u47000 \u48372 \u45796  \u49457 \u49689 \u54620 \u49548 \u46300  \u53304 \u51008  \u46608 \u47000 \u48372 \u45796  \u50612 \u47480 \u49828 \u47101 \u44144 \u45208  \u49457 \u49689 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45231 \u49440  \u54872 \u44221 \u50640  \u51201 \u51025 \u54616 \u45716  \u45733 \u47141 \u46020  \u46832 \u50612 \u45208 \u47728 , \u45824 \u51064  \u44288 \u44228  \u46608 \u54620  \u50896 \u47564 \u54633 \u45768 \u45796 . \'91\u52376 \u49464 \u49696 \'92\u46020  \u51339 \u49845 \u45768 \u45796 . \u46412 \u47928 \u50640  \u51452 \u48320  \u49324 \u46988 \u50640 \u44172  \'91\u49457 \u49689 \u54616 \u45796 , \u50612 \u47480 \u49828 \u47101 \u45796 \'92\u45716  \u47568 \u51012  \u51088 \u51452  \u46307 \u45716  \u54200 \u51077 \u45768 \u45796 .\u49548 \u46300  \u53304  \u52852 \u46300 \u45716  \u50612 \u47140 \u50868  \u49345 \u54889 \u50640  \u52376 \u54616 \u45908 \u46972 \u46020 , \u51648 \u54812 \u47213 \u44172  \u52376 \u47532 \u54624  \u49688  \u51080 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45224 \u46308 \u51008  \u44536 \u47088  \u45817 \u49888 \u51012  \'91\u48127 \u51020 \u51649 \u49828 \u47101 \u45796 \'92\u44256  \u49373 \u44033 \u54624  \u44163 \u51060 \u47728 , \u50526 \u51004 \u47196  \u51312 \u50616 \u51012  \u44396 \u54616 \u45716  \u51068 \u51060  \u51110 \u50500 \u51656  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51060 \u48120 \u51648  \u54644 \u49437  : \u50668 \u50773 \u51008  \u51312 \u51649 \u51032  \u47532 \u45908 \u51077 \u45768 \u45796 . \u49324 \u46988 \u46308 \u51012  \u51060 \u45124 \u45716  \u45936  \u53441 \u50900 \u54620  \u51648 \u54812 \u47484  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47111 \u44592  \u46412 \u47928 \u50640 , \u46384 \u46907 \u54620  \u50948 \u47196 \u50752  \u54788 \u49892 \u51201 \u51064  \u51312 \u50616 \u51012  \u44148 \u45348 \u45716  \u45733 \u47141 \u46020  \u46832 \u50612 \u45225 \u45768 \u45796 . \u50668 \u50773 \u51008  \u50724 \u47004  \u44221 \u54744 \u51012  \u49939 \u50500 \u50772 \u49845 \u45768 \u45796 . \u51088 \u49888 \u48372 \u45796  \u51649 \u50948 \u44032  \u45230 \u45908 \u46972 \u46020 , \u44148 \u49444 \u51201 \u51064  \u51032 \u44204 \u51060 \u46972 \u47732  \u44592 \u44732 \u51060  \u49688 \u50857 \u54633 \u45768 \u45796 . \u44536 \u45376 \u50640 \u44172  \u44032 \u51109  \u51473 \u50836 \u54620  \u44163 \u51008  \'91\u51652 \u49892 \'92\u51077 \u45768 \u45796 .\u49548 \u46300  \u53304  \u50672 \u50528 \u50868  \u54644 \u49437 \u49548 \u46300  \u53304 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u47588 \u50864  \u46021 \u47549 \u51201 \u51064  \u49457 \u54693 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45224 \u51012  \u48148 \u46972 \u48372 \u45716  \u44592 \u51456  \u46608 \u54620  \u50628 \u44201 \u54616 \u47728 , \u48376 \u51064 \u50640 \u44172 \u46020  \u44536 \u47092  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u51060 \u49345 \u51452 \u51032 \u47484  \u52628 \u44396 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50500 \u47924 \u45208  \u47564 \u45208 \u44144 \u45208  \u50668 \u44592 \u51200 \u44592  \u50668 \u51648 \u47484  \u51452 \u45716  \u49457 \u44201 \u51008  \u50500 \u45785 \u45768 \u45796 . \u46384 \u46972 \u49436  \u45796 \u49548  \u51221 \u51060  \u50630 \u44144 \u45208 , \u52384 \u48317 \u51012  \u52824 \u45716  \u46319 \u54620  \u45712 \u45196 \u51012  \u48155 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54620  \u48264  \u49324 \u46993 \u50640  \u48736 \u51648 \u47732 , \u44536  \u44048 \u51221 \u51008  \u47588 \u50864  \u44618 \u49845 \u45768 \u45796 . \u51473 \u44036 \u50640  \u48148 \u46988  \u54588 \u45716  \u51068 \u46020  \u44537 \u55176  \u46300 \u47932  \u44163 \u51077 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u49345 \u45824 \u48169 \u51012  \u47560 \u52824  \u51088 \u49888 \u51032  \'91\u49548 \u50872 \u47700 \u51060 \u53944 \'92\u52376 \u47100  \u50668 \u44592 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51032  \u48148 \u50868 \u45908 \u47532  \u45236 \u50640  \u46308 \u50612 \u50728  \u49324 \u46988 \u50640 \u44176  \u47588 \u50864  \u44288 \u45824 \u54633 \u45768 \u45796 . \u45800  1\u44032 \u51648 , \u44288 \u44228 \u50640  \u51080 \u50612  \'91\u48127 \u51020 \u44284  \u49888 \u47280 \'92\u44032  \u44032 \u51109  \u51473 \u50836 \u54616 \u45796 \u44256  \u49373 \u44033 \u54616 \u44592 \u50640  \u44144 \u51667 \u47568 \u51008  \u50857 \u45225 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 .\u44536 (\u44536 \u45376 )\u45716  \u48176 \u50872  \u51216 \u51060  \u47566 \u51008  \u49324 \u46988 \u50640 \u44172  \u45132 \u47532 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45768  \u44536 \u51032  \u47560 \u51020 \u51012  \u50619 \u44256  \u49910 \u45796 \u47732 , \u54217 \u49548  \u49457 \u49689 \u54620  \u53468 \u46020 \u47484  \u48372 \u50668 \u51452 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 . \'91\u46609 \u46609 \u54620  \u49324 \u46988 \'92 \u48372 \u45796 \u45716  \'91\u51648 \u54812 \u47196 \u50868  \u49324 \u46988 \'92\u51012  \u45908  \u51339 \u50500 \u54616 \u47728 , \u48376 \u51064 \u51032  \u44032 \u52824 \u44288 \u44284  \u45817 \u49888 \u51032  \u44032 \u52824 \u44288 \u51060  \u48708 \u49847 \u54616 \u47732 , \u44553 \u49549 \u46020 \u47196  \u44032 \u44620 \u50892 \u51656  \u44163 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u53440 \u54801 \u51060  \u50504 \u46104 \u44144 \u45208 , \u45130 \u51076 \u50630 \u51060  \u47560 \u52272 \u51060  \u49373 \u44596 \u45796 \u47732  \u44288 \u44228 \u47484  \'91\u52860 \u44057 \u51060 \'92 \u45130 \u50612 \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44032 \u51109  \u53360  \u51109 \u50528 \u47932 \u51008  \'91\u44144 \u51667 \u47568 \'92\u51077 \u45768 \u45796 .\u49548 \u46300  \u53304 \u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u50640 \u44172  \u53360  \u54840 \u44048 \u51008  \u50630 \u50612  \u48372 \u51077 \u45768 \u45796 . \u48317 \u51012  \u52824 \u44256  \u51080 \u45796 \u45716  \u45712 \u45196 \u51060  \u46317 \u45768 \u45796 . \u50500 \u51649 \u51008  \u45817 \u49888 \u51012  \u51060 \u49457 \u51060  \u50500 \u45772  \'91\u49324 \u46988 \'92\u51004 \u47196  \u45824 \u54616 \u45716  \u51473 \u51077 \u45768 \u45796 . \u52572 \u44540  \u49345 \u45824 \u48169 \u44284  \u47680 \u50612 \u51276 \u45796 \u47732 , \'91\u44032 \u52824 \u44288  \u47928 \u51228 \'92\u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u49436 \u47196  \u50864 \u49440 \u49884 \u54616 \u45716  \u44032 \u52824 \u44288 \u51060  \u45796 \u47476 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44536 \u51032  \u47560 \u51020 \u51012  \u46028 \u47532 \u44256  \u49910 \u45796 \u47732 , \u50612 \u47480 \u49828 \u47084 \u50868  \u47784 \u49845 , \u45236 \u49892  \u51080 \u45716   \u44032 \u52824 \u44288 \u51012  \u46300 \u47084 \u45236 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u46021 \u47549 \u51201 \u51064  \u49334 \u51012  \u50976 \u51648 \u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50672 \u50528 \u48372 \u45796 \u45716  \u48376 \u51064 \u51032  \u49334 \u51012  \u50864 \u49440 \u49884  \u54644 \u50556  \u54633 \u45768 \u45796 . \u50672 \u50528 \u47484  \u54616 \u45908 \u46972 \u46020  \u52964 \u47532 \u50612 \u45716  \u44984 \u51456 \u55176  \u48156 \u51204 \u49884 \u53020 \u50556  \u54633 \u45768 \u45796 . \u50672 \u50528 \u44032  \u45320 \u47924  \u54788 \u49892 \u51201 \u51004 \u47196  \u44592 \u50872 \u44144 \u45208  \u47196 \u47592 \u54001 \u54632 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u44221 \u44256 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u49548 \u46300  \u53304 \u51008  \u45208 \u49240 \u44172  \u51089 \u50857 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u53304  \u49548 \u46300 \u45716  \u51060 \u49457 \u51032  \u49345 \u51669 \u51060 \u47728 , \u44048 \u51221 \u51060  \u45224 \u50500 \u51080 \u45908 \u46972 \u46020  \u44536  \u44048 \u51221 \u51012  \u51060 \u49457 \u51004 \u47196  \u52384 \u51200 \u55176  \u45130 \u50612 \u45240 \u45796 \u45716  \u46907 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47564 \u50557  \u50672 \u50528  \u46020 \u51473  \'91\u44144 \u51667 \u47568 \'92\u51012  \u54644 \u49436  \u54756 \u50612 \u51276 \u45796 \u47732 , \u45908 \u50865 \u45908  \u51116 \u54924 \u47484  \u44592 \u45824 \u54616 \u51648  \u50506 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u50672 \u51064  \u49324 \u51060 \u50640  \u48708 \u48128 \u51060  \u51080 \u50612 \u49436 \u45716  \u50504 \u46108 \u45796 \u44256  \u49373 \u44033 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u48148 \u46988 \u48372 \u45796  \u44144 \u51667 \u47568 \u51012  \u45908  \u49899 \u50612 \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u54788 \u51116  \u49556 \u47196 \u46972 \u47732 , \u45576 \u51060  \u45320 \u47924  \u45458 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47480  \u49324 \u46988 \u46308 \u50640 \u44172 \u46020 , \u49900 \u51648 \u50612  \u48376 \u51064 \u50640 \u44172 \u46020  \u45320 \u47924  \u50628 \u44201 \u54620  \u44592 \u51456 \u51012  \u46308 \u51060 \u48128 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51312 \u44552  \u45576 \u51012  \u45230 \u52628 \u44256 , \u44288 \u45824 \u54644 \u51656  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 . \u49464 \u49345 \u50640  \u50756 \u48317 \u54620  \u49324 \u46988 \u51008  \u50630 \u49845 \u45768 \u45796 . \u49457 \u49689 \u54632 \u44284  \u45817 \u49888 \u51032  \u51064 \u49373 \u44288 \u51008  \u52649 \u48516 \u55176  \u44053 \u54620  \u47588 \u47141 \u51077 \u45768 \u45796 . \u50612 \u47480 \u49828 \u47084 \u50880 \u51012  \u47924 \u44592 \u47196 , \u49345 \u45824 \u48169 \u51032  \u47560 \u51020 \u51012  \u49324 \u47196 \u51105 \u50500 \u48372 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 .\u49548 \u46300  \u53304  \u51649 \u50629 \u50868  \u54644 \u49437 \u49548 \u46300  \u53304 \u51008  \u51649 \u50629 \u50868 \u50640 \u49436  \u47588 \u50864  \u51339 \u49845 \u45768 \u45796 . \u51648 \u49457 \u44284  \u50696 \u47532 \u54632 , \u47749 \u54869 \u54620  \u48708 \u51204 \u51012  \u49345 \u51669 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u46041 \u47308 \u47196 \u48512 \u53552  \u47566 \u51008  \u51064 \u51221 \u51012  \u48155 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 . \u54841 \u51008  \u45817 \u49888 \u50640 \u44172  \'91\u47704 \u53664 \'92\u44032  \u52286 \u50500 \u50732  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u45716  \u44048 \u51221 \u51201 \u51064  \u51648 \u50896 \u49104 \u47564  \u50500 \u45768 \u46972  \u54788 \u49892 \u51201 \u51064  \u51312 \u50616 \u44620 \u51648  \u44148 \u45348 \u51460  \u44163 \u51077 \u45768 \u45796 . \u45224 \u51032  \u51032 \u44204 \u51012  \u44288 \u45824 \u54616 \u44172  \u49688 \u50857 \u54616 \u46104 , \u48376 \u51064 \u51032  \u46907 \u51012  \u44413 \u55176 \u51648  \u47560 \u49464 \u50836 . \u51060 \u47532 \u51200 \u47532  \u55064 \u46168 \u47532 \u45716  \u47784 \u49845 \u51008  \u45817 \u49888 \u50640 \u44172  \u50612 \u50872 \u47532 \u51648  \u50506 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u50696 \u47532 \u54620  \u44048 \u44033 \u44284  \u44053 \u51064 \u54620  \u47704 \u53448 \u51004 \u47196  \u49884 \u54744 \u51060 \u45208  \u47732 \u51217 \u51012  \u46504 \u51648  \u50506 \u44256  \u48372 \u44172  \u46121 \u45768 \u45796 . \u45796 \u47564  \u48376 \u51064 \u50640 \u44172  \u45320 \u47924  \u50628 \u44201 \u54616 \u44172  \u44404 \u50612  \u49828 \u53944 \u47112 \u49828 \u47484  \u47566 \u51060  \u48155 \u51012  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54217 \u49548 \u50640  \u49828 \u53944 \u47112 \u49828  \u44288 \u47532 \u47484  \u52384 \u51200 \u55176  \u54644 \u50556  \u54633 \u45768 \u45796 .\u49548 \u46300  \u53304  \u44552 \u51204 \u50868  \u54644 \u49437 \u51452 \u48320 \u50640 \u49436  \u45817 \u49888 \u50640 \u44172  \u51068 \u51012  \u47585 \u44592 \u45716  \u49324 \u46988 \u51060  \u47566 \u50500 , \u46024 \u51060  \u45130 \u51060 \u51648  \u50506 \u44172  \u46121 \u45768 \u45796 . \u44536  \u44284 \u51221 \u50640 \u49436  \u45817 \u49888 \u51032  \u45733 \u47141 \u51060  \u45328 \u47532  \u51064 \u51221 \u48155 \u44172  \u46121 \u45768 \u45796 . \u50739 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u51068 , \u53804 \u51088 \u44032  \u51080 \u45796 \u47732  \u44284 \u44048 \u55176  \u44208 \u45800 \u45236 \u47532 \u49464 \u50836 . \u51312 \u44552 \u47564  \u44288 \u49900 \u51012  \u44032 \u51648 \u47732 , \u48512 \u47484  \u53356 \u44172  \u44404 \u47540  \u49688  \u51080 \u45716  \u50868 \u51060  \u51080 \u49845 \u45768 \u45796 .    \
