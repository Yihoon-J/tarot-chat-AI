   \uc0\u52981 2 \'96 \u44368 \u47448  \u52981 2 \u44536 \u47548 \u51032  \u51032 \u48120 \u45224 \u45376 \u44032  \u52981 \u51012  \u51452 \u44256  \u48155 \u45716  \u47784 \u49845 \u49373 \u44033 \u51060  \u51096  \u53685 \u54616 \u45716  \u44288 \u44228 \u44048 \u51221 \u51032  \u48184 \u47088 \u49828 \u44032  \u51096  \u47582 \u45716  \u49345 \u53468 \u51312 \u49900 \u49828 \u47084 \u50880 \u51060 \u45208  \u45576 \u52824 \u47484  \u48372 \u44256  \u51080 \u45796 \u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49373 \u44033 \u51060  \u51096  \u53685 \u54616 \u45716 \u52981 2\u45716  \u49373 \u44033 \u51060  \u51096  \u53685 \u54616 \u45716  \u49324 \u46988 , \u45576 \u45458 \u51060 \u44032  \u48708 \u49847 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640  \u51201 \u50857 \u54644 \u48376 \u45796 \u47732  :\u47568 \u51060  \u51096  \u53685 \u54620 \u45796 \u45208 \u50752  \u49373 \u44033 , \u51077 \u51109 \u51060  \u48708 \u49847 \u54616 \u45796 \u45208 \u50752  \u51096  \u47582 \u45716  \u49324 \u46988 \u51064  \u44163  \u44057 \u45796 \u52981 \u51008  \u50864 \u47532 \u51032  \'91\u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u45376 \u45716  \u49436 \u47196 \u51032  \u52981 \u51012  \u44368 \u54872 \u54616 \u44256  \u51080 \u45716 \u45936 \u50836 . \u51060 \u45716  \u44263  \'91\u44048 \u51221 \u51060 \u45208  \u49373 \u44033 \u51012  \u44368 \u47448 \u54616 \u44256  \u51080 \u45796 \'92\u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45224 \u45376 \u51032  \u53412 \u45716  \u48708 \u49847 \u54616 \u44256  \u49552 \u46020  \u47582 \u45823 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46160  \u45224 \u45376 \u51032  \u47672 \u47532  \u50948 \u50640 \u45716  \'91\u54217 \u54868 , \u44368 \u50