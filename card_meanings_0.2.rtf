   12. \uc0\u47588 \u45804 \u47536  \u45224 \u51088 The Hanged Man \u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u49828 \u49828 \u47196  T\u51088 \u54805  \u45208 \u47924 \u50640  \u47588 \u45804 \u47536  \u47784 \u49845 \u49345 \u54889 \u51032  \u51221 \u52404  \u48143  \u44256 \u45212 \u51012  \u49345 \u51669 \u49345 \u54889 \u51012  \u48155 \u50500 \u46308 \u51064 \u45796 \u45716  \u51032 \u48120 \u44557 \u51221 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u45716  \u47560 \u51020 \u52265 \u44033 \u50640  \u48736 \u51256 \u51080 \u44144 \u45208  \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u45716  \u49324 \u46988  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49345 \u54889 \u51012  \u48155 \u50500 \u46308 \u51060 \u45716 \u54665 \u47592  \u52852 \u46300 \u45716  \u49345 \u54889 \u51012  \u48155 \u50500 \u46308 \u51060 \u44256  \u47560 \u51020 \u51012  \u52264 \u48516 \u55176  \u54616 \u44192 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50672 \u50528 \u50868 \u50640 \u49436 \u45716  \'91\u45817 \u49888 \u50640 \u44172  \u47582 \u52656 \u51452 \u44192 \u45796 \'92\u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u44256  \u44592 \u45796 \u47532 \u44144 \u45208 , \u45817 \u49888 \u51032  \u48152 \u51025 \u51012  \u51648 \u53020 \u48380  \u46412 \u46020  \u45208 \u50741 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u45208 \u47924 \u50640  \u44144 \u44984 \u47196  \u47588 \u45804 \u47140  \u51080 \u51648 \u47564 , \u54364 \u51221 \u51008  \u54200 \u50504 \u54616 \u47728  \u44536  \u50612 \u46496 \u54620  \u51200 \u54637 \u46020  \u50630 \u49845 \u45768 \u45796 . \u45796 \u47532 \u47484  \u44848  \u47784 \u49845 \u51008  \u50668 \u50976 \u47196 \u50880 \u44620 \u51648  \u45712 \u44788 \u51648 \u45716 \u44400 \u50836 . \u50672 \u50528 \u50868 \u51004 \u47196  \u50696 \u49884 \u47484  \u46308 \u47732  : \u50696 \u47484  \u46308 \u50612  \u45817 \u49888 \u51060  \'93\u50836 \u51608  \u48148 \u49240 \u45768 \u44620  \u51068 \u51452 \u51068 \u50640  1\u48264 \u47564  \u47564 \u45208 \u51088 \'94\u46972 \u44256  \u54664 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51060  \'93\u44536 \u47092  \u49688  \u51080 \u51648 , \u45236 \u44032  \u47582 \u52656 \u50556 \u51648 \'94\u46972 \u44256  \u49373 \u44033 \u54620 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u54665 \u47592  \u52852 \u46300 \u44032  \'91\u49688 \u50857 \u54620 \u45796 \'92\u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716  : \u51221 \u49888 \u51201 \u51064  \u49457 \u51109 \u51012  \u54616 \u44172  \u46108 \u45796 \u45716  \u51032 \u48120 \u44032  \u51080 \u49845 \u45768 \u45796 . \u54252 \u50857 \u47141 \u51060  \u45720 \u44256 , \u50612 \u47480 \u51060  \u46104 \u50612 \u44036 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50504 \u46104 \u45716  \u44148  \u50504 \u46104 \u45208  \u48372 \u45796 ~ \u54616 \u45796  \u48372 \u47732  \u50616 \u51232 \u44032 \u45716  \u46104 \u44192 \u51648 ~\u8251  \u52404 \u45392 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51593  \u44060 \u49440 \u54624  \u51032 \u51648 \u44032  \u50630 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u51116 \u54924 \u50868 \u51060  \u45208 \u49244  \u52852 \u46300  \u51473  \u54616 \u45208 \u51064 \u45936 , \u51060 \u48324 \u51012  \u48155 \u50500 \u46308 \u51060 \u47728  \u50500 \u47924 \u47088  \u50529 \u49496 \u51012  \u52712 \u54616 \u51648  \u50506 \u44192 \u45796 \u45716  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u44557 \u51221 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u45716 \u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300 \u45716  \u44557 \u51221 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u45716  \u49324 \u46988 , \u55192 \u46308 \u50612 \u46020  \u48157 \u51008  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u47588 \u45804 \u47536  \u45224 \u51088 \u45716  \u44256 \u47928 \u51012  \u45817 \u54616 \u45716  \u44163 \u52376 \u47100  \u48372 \u51077 \u45768 \u45796 . \u47672 \u47532 \u50640 \u45716  \u54588 \u44032  \u50144 \u47532 \u44256 , \u45796 \u47532 \u44032  \u45130 \u50612 \u51656  \u46319 \u51060  \u50500 \u54532 \u44192 \u51424 . \u44536 \u47084 \u45208  \u44536  \u50752 \u51473 \u50640 \u46020  \u45432 \u46976  \u54980 \u44305 \u51012  \u45236 \u48708 \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44844 \u51064  \u45796 \u47532 \u45716  \u44536 \u51032  \u51221 \u49888 \u47141 \u51012  \u45908 \u50865 \u45908  \u48512 \u44033 \u49884 \u53429 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48389 \u49884 \u45796  :Q. \u49352 \u47196  \u46308 \u50612 \u44032 \u44172  \u46112  \u51649 \u51109 \u50640 \u49436  \u51096  \u51201 \u51025 \u54624  \u49688  \u51080 \u51012 \u44620 \u50836 ?A. \u45348 . \u45817 \u48516 \u44036  \u55192 \u46308 \u44192 \u51648 \u47564 , \u47560 \u51020 \u51012  \u44403 \u44172 \u47673 \u50612  \u48157 \u44256  \u44557 \u51221 \u51201 \u51004 \u47196  \u49373 \u54876 \u54616 \u44172  \u46121 \u45768 \u45796 .Q. \u54756 \u50612 \u51652 \u51648  1\u45804  \u46096 \u45716 \u45936 , \u49345 \u45824 \u48169 \u51008  \u51648 \u44552 \u52196  \u50612 \u46500  \u49373 \u44033 \u51012  \u54616 \u44256  \u51080 \u51012 \u44620 \u50836 ?A. \u45320 \u47924  \u55192 \u46308 \u50612 \u54616 \u47728 , \u45817 \u49888 \u51012  \u44536 \u47532 \u50892 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u52572 \u45824 \u54620  \u48157 \u44172  \u45796 \u45768 \u47140 \u44256  \u45432 \u47141 \u54616 \u45716  \u51473 \u51077 \u45768 \u45796 . \u47925 \u47925 \u55176  \u54624  \u51068 \u51012  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47588 \u45804 \u47536  \u45224 \u51088 \u44032  \u51312 \u50616 \u51216 \u51004 \u47196  \u45208 \u50732  \u44221 \u50864 , \u47560 \u51020 \u44032 \u51664 \u51012  \u44256 \u52432 \u47673 \u50612 \u50556  \u54620 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u54788 \u51116  \u49884 \u47144 \u51012  \u44202 \u44256  \u51080 \u45796 \u47732 , \u51060  \u44221 \u54744 \u51004 \u47196  \u44648 \u45804 \u51020 \u51012  \u50619 \u44172  \u46104 \u45768  \u52572 \u45824 \u54620  \u44557 \u51221 \u51201 \u51004 \u47196  \u49373 \u44033 \u54616 \u46972 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u8251  \u45208 \u47924 \u50640 \u45716  \u51086 \u51060  \u47924 \u49457 \u54616 \u44172  \u50864 \u44144 \u51256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u48120 \u47000 \u50640  \u55148 \u47581 \u51060  \u51080 \u45796 \u45716  \u44163 \u51012  \u50516 \u49884 \u54633 \u45768 \u45796 . \u53440 \u47196  \u52852 \u46300 \u50640 \u49436  \u51086 \u49324 \u44480  \u45804 \u47536  \u47561 \u45824 \u45716  \u48156 \u51204  \u44032 \u45733 \u49457 , \u55148 \u47581 , \u49373 \u47749 \u47141 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u49324 \u44256 \u44032  \u50976 \u50672 \u54620 \u47588 \u45804 \u47536  \u49324 \u46988  \u52852 \u46300 \u45716  \u50976 \u50672 \u54620  \u49324 \u44256 , \u49352 \u47196 \u50868  \u44288 \u51216 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u49464 \u49345 \u51012  \u44144 \u44984 \u47196  \u48372 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u46356 \u51088 \u51064 \u51060 \u45208  \u51020 \u50501 , \u47924 \u50857  \u46321  \u52285 \u51032 \u49457 \u51060  \u54596 \u50836 \u54620  \u48516 \u50556 \u50640 \u49436  \u46832 \u50612 \u45212  \u49457 \u44284 \u47484  \u44144 \u46160 \u44172  \u46121 \u45768 \u45796 .\u44263  \u44256 \u51221 \u44288 \u45392 \u51012  \u44648 \u44256  \u49352 \u47196 \u50868  \u50500 \u51060 \u46356 \u50612 \u47484  \u46496 \u50732 \u47532 \u44172  \u46121 \u45768 \u45796 .\u50864 \u50672 \u54620  \u44228 \u44592 \u47196  \u44648 \u45804 \u51020 \u51012  \u50619 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \u49324 \u44256 \u44032  \u44221 \u51649 \u46104 \u50612  \u51080 \u51004 \u45768  \u54620 \u44152 \u51020  \u47932 \u47084 \u49436 \u49436  \u49345 \u54889 \u51012  \u50976 \u50672 \u54616 \u44172  \u48148 \u46972 \u48372 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 .\u54644 \u49437  4. \u49440 \u53469 \u44428 \u51060  \u51080 \u45716 \u54665 \u47592  \u52852 \u46300 \u45716  \u45817 \u49888 \u50640 \u44172  \u49440 \u53469 \u44428 \u51060  \u51080 \u45796 \u45716  \u47700 \u49884 \u51648 \u47484  \u51452 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u48167 \u51460 \u51012  \u50616 \u51228 \u46304  \u54400  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46160  \u49552 \u44284  \u54620  \u48156 \u51008  \u51088 \u50976 \u47213 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45149 \u44620 \u51648  \u47588 \u45804 \u47140  \u51453 \u51020 \u51012  \u53469 \u54616 \u45716  \u44163 \u46020 , \u48167 \u51460 \u51012  \u54400 \u50612  \u54200 \u50504 \u54644 \u51648 \u45716  \u44163 \u46020  \u44536 \u50640 \u44172  \u45804 \u47160 \u49845 \u45768 \u45796 .\u54620 \u51901  \u48156 \u51060  \u54400 \u47140 \u51080 \u45796  \u45824 \u48512 \u48516 \u51032  \u50500 \u54548 \u51008  \u45817 \u49888 \u51032  \u54644 \u49437 \u51060  \u47564 \u46308 \u50612 \u45253 \u45768 \u45796 . \u51060 \u48324 \u51012  \'91\u54844 \u51088  \u45224 \u44200 \u51656  \u46160 \u47140 \u50880 \'92\u51004 \u47196  \u48155 \u50500 \u46308 \u51060 \u45716  \u49324 \u46988 \u51060  \u51080 \u45716  \u48152 \u47732 , \'91\u54665 \u48373 \u54620  \u51088 \u50976 \'92\u47196  \u48155 \u50500 \u46308 \u51060 \u45716  \u49324 \u46988 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54788 \u49345  \u44536  \u51088 \u52404 \u48372 \u45796 , \u45817 \u49888 \u51032  \u47560 \u51020 \u44032 \u51664 \u51060  \u45908  \u51473 \u50836 \u54633 \u45768 \u45796 . \u50500 \u54536  \u44592 \u50613 \u51012  \u45824 \u45516 \u47728 , \u49828 \u49828 \u47196 \u47484  \u48167 \u51460 \u47196  \u47926 \u44256  \u51080 \u51652  \u50506 \u50520 \u45716 \u51648  \u49373 \u44033 \u54644 \u48380  \u46412 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49345 \u54889 \u51032  \u51221 \u52404 , \u44256 \u49373 \u44600 \u51060  \u50676 \u47536 \u45796 \u47588 \u45804 \u47536  \u49324 \u46988  \u52852 \u46300 \u45716  \u49345 \u54889 \u51060  \u51221 \u52404  \u46104 \u47728 , \u44256 \u49373 \u44600 \u51060  \u50676 \u47536 \u45796 \u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u53945 \u55176  \u51649 \u50629 \u50868 \u44284  \u44552 \u51204 \u50868 \u50640 \u49436 \u45716  \u51339 \u51008  \u47532 \u46377 \u51060  \u44144 \u51032  \u48520 \u44032 \u45733 \u54616 \u45796 \u44256  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47564 \u50557  \u49352 \u47213 \u44172  \u49324 \u50629 \u51012  \u49884 \u46020 \u54620 \u45796 \u47732 , \u44256 \u49373 \u44600 \u51060  \u50676 \u47532 \u45768  \u47560 \u51020 \u51032  \u51456 \u48708 \u47484  \u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49373 \u44033 \u48372 \u45796  \u45908  \u55192 \u46308  \u44163 \u51077 \u45768 \u45796 .\u50672 \u50528 \u50868 \u46020  \u47560 \u52268 \u44032 \u51648 \u47196 , \u44288 \u44228 \u50640  \u51652 \u51204 \u51060  \u50630 \u44144 \u45208  \u49345 \u45824 \u48169 \u51012  \u44592 \u50557  \u50630 \u51060  \u44592 \u45796 \u47532 \u45716  \u47784 \u49845 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50668 \u50976 \u44032  \u50504  \u46076  \u54665 \u46041 \u54616 \u44592  \u55192 \u46304  \u49324 \u46988 , \u54841 \u51008  \u54665 \u46041 \u47141  \u51088 \u52404 \u44032  \u46504 \u50612 \u51648 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50896 \u52824  \u50506 \u45716  \u44221 \u54744 \u50640 \u46020  \u48176 \u50880 \u51060  \u51080 \u50612 \u50836 .\u47000 \u54140  \u44608 \u54616 \u50728 \u54644 \u49437  2. \u52265 \u44033 \u50640  \u48736 \u51256  \u51080 \u45716 \u53440 \u47196  \u54665 \u47592 \u51008  \u47924 \u50616 \u44032  \u52265 \u44033 \u50640  \u48736 \u51256  \u51080 \u44144 \u45208 , \u49345 \u54889 \u51012  \u51228 \u45824 \u47196  \u47803  \u48372 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u47564 \u50557  \u52852 \u46300 \u44032  \u45572 \u44400 \u44032 \u51032  \'91\u49900 \u47532  \u49345 \u53468 \'92\u47484  \u45208 \u53440 \u45240 \u45796 \u47732  :\u49345 \u45824 \u48169 \u50640 \u44172  \u50672 \u46973 \u51060  \u48152 \u46300 \u49884  \u50732  \u44163 \u51060 \u46972  \u44592 \u45824 \u54616 \u45716  \u51473 \u54665 \u46041  \u54616 \u45208 \u54616 \u45208 \u50640  \u51032 \u48120  \u48512 \u50668  \u54616 \u47728 , \u54840 \u44048  \u54841 \u51008  \u48708 \u54840 \u44048 \u51004 \u47196  \u52265 \u44033 \u47784 \u46304  \u44172  \u45236  \u50584 \u44592 \u51064 \u46319  \u52265 \u44033 \u51221 \u45817 \u54620  \u48708 \u54032 \u51012  \u51656 \u53804 \u49900 \u51004 \u47196  \u50780 \u44257 \u54620 \u45796 \u44144 \u45208 \u44221 \u44033 \u49900 \u51060  \u48512 \u51313 \u54620  \u49345 \u53468 \u47560 \u45285  \u51339 \u44172 \u47564  \u48372 \u44256  \u51080 \u45796 \u47732 , \u55148 \u47581  \u54924 \u47196 \u47484  \u46028 \u47532 \u51648  \u47568 \u44256  \'91\u44061 \u44288 \u51201 \u51004 \u47196 \'92 \u48372 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44048 \u51060 \u45208  \u45712 \u45196 \u50640  \u51032 \u51316 \u54644 \u49436 \u45716  \u50504 \u46121 \u45768 \u45796 . \u50672 \u50528 \u50640 \u49436 \u45716  \'91\u49892 \u51228 \u47196  \u49345 \u45824 \u48169 \u51060  \u50612 \u46500  \u54665 \u46041 \u51012  \u54664 \u45716 \u51648 \'92, \u51649 \u50629 \u50640 \u49436 \u45716  \'91\u45936 \u51060 \u53552 \u45208  \u49457 \u44284 \u47484  \u50948 \u51452 \u47196 \'92 \u48400 \u50556  \u54633 \u45768 \u45796 .\u54644 \u49437  3. \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u45716 \u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300 \u45716  \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50696 \u52968 \u45824  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640 \u49436  \u45208 \u50772 \u51012  \u44221 \u50864  :\u50508 \u47732 \u49436 \u46020  \u47924 \u49884 \u54616 \u44256  \u51080 \u45796 \u47676 \u51200  \u45796 \u44032 \u44032 \u45716  \u44148  \u51088 \u51316 \u49900 \u51060  \u49345 \u54620 \u45796 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \u51068 \u51012  \u44536 \u47476 \u52824 \u44256  \u51080 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u49828 \u49828 \u47196  \u55192 \u46304  \u44600 \u51012  \u51088 \u52376 \u54620  \u49480 \u51077 \u45768 \u45796 . \u51068 \u51032  \u48169 \u49885 \u51060 \u45208  \u53468 \u46020 \u47484  \u44256 \u52832  \u54596 \u50836 \u44032  \u51080 \u44192 \u49845 \u45768 \u45796 .\u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u49324 \u44256 \u44032  \u50976 \u50672 \u54616 \u47728 , \u47924 \u50631 \u51060 \u46304  \u45209 \u44288 \u51201 \u51004 \u47196  \u48372 \u47140  \u54633 \u45768 \u45796 . \u51200 \u45716  \u51228 \u51068  \u47676 \u51200  \'93\u49828 \u54256 \u51648 \u48165 \'94\u51060  \u46496 \u50724 \u47493 \u45768 \u45796 . \u55192 \u46304  \u51068 \u46020  \u47925 \u47925 \u55176  \u54644 \u45236 \u47728 , \u55148 \u49373 \u51221 \u49888 \u46020  \u46832 \u50612 \u45225 \u45768 \u45796 . \u45796 \u47564 , \u44048 \u51004 \u47196  \u54032 \u45800 \u54616 \u45716  \u49457 \u44201  \u53459 \u50640  \'91\u51068 \u51032  \u49457 \u44277 \u47456 \'92\u51008  \u45230 \u51008  \u54200 \u51060 \u47728 , \u46412 \u47196 \u45716  \u45576 \u52824  \u50630 \u44172  \u48372 \u51068  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u54665 \u47592  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  3\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 \u45716  \u51088 \u51316 \u49900 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \u44288 \u49900 \u51060  \u51080 \u51004 \u45208 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \u50672 \u46973 \u54616 \u51648  \u50506 \u44256  \u51080 \u49845 \u45768 \u45796 . \'93\u45572 \u44032  \u47676 \u51200  \u50672 \u46973 \u54616 \u45208  \u48372 \u51088 \'94 \u46160  \u48264 \u51704 \u45716  \u48516 \u45432 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u48516 \u45432 \u47484  \u51064 \u45236 \u54616 \u47728  \u52280 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44263  \u44536  \u48516 \u45432 \u44032  \u54253 \u48156 \u54624  \u49688 \u46020  \u51080 \u44192 \u44400 \u50836 . \'93\u50612 \u46356  \u54620  \u48264  \u54644 \u48400 \'94 \u47560 \u51648 \u47561 \u51004 \u47196  \u51677 \u49324 \u46993 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u50640 \u44172  \u54840 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u51648 \u47564 , \u47560 \u51020 \u51012  \u54364 \u54788 \u54616 \u51648  \u47803 \u54616 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44536 \u51200  \u50500 \u54536  \u47560 \u51020 \u51012  \u44985 \u44985  \u45572 \u47476 \u47728  \u52280 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51064 \u45236 \u49900 \u51008  \u52649 \u48516 \u54616 \u44592 \u50640  \u44536  \u47560 \u51020 \u51008  \u50724 \u47000  \u44040  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \'91\u44288 \u44228 \u47484  \u44060 \u49440 \u54616 \u47140 \u45716  \u51032 \u51648 \u44032  \u50630 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 . \u46168  \u47784 \u46160  \u48176 \u47140 \u54616 \u47728  \u47582 \u52656 \u44032 \u45716  \u44172  \u50500 \u45768 \u46972 , \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u51228 \u45716  \u48148 \u45068 \u50612 \u50556  \u54633 \u45768 \u45796 . \u54788 \u51116  \u44032 \u51109  \u51339 \u51008  \u54644 \u44208 \u52293 \u51008  \'91\u45824 \u54868 \'92\u51077 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ]\u51116 \u54924 \u50868 \u51008  \u48512 \u51221 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51060 \u48324 \u51012  \u52404 \u45392 \u54664 \u44144 \u45208 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \u50672 \u46973 \u54616 \u51648  \u50506 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u45817 \u49888 \u50640  \u45824 \u54620  \u44536 \u47532 \u50880 \u51008  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \'91\u44256 \u53685 \u49828 \u47084 \u50868  \u47560 \u51020 \'92\u51060  \u52852 \u46300 \u51032  \u44536 \u47548 \u51004 \u47196  \u45208 \u53440 \u45228 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \u51116 \u54924 \u47484  \u50896 \u54620 \u45796 \u47732 , \u47676 \u51200  \u50672 \u46973 \u51012  \u49884 \u46020 \u54644 \u50556  \u54633 \u45768 \u45796 . \u44032 \u51109  \u53360  \u51109 \u50528 \u47932 \u51008  \u51088 \u51316 \u49900 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51008  \u51648 \u44552  \u51088 \u51316 \u49900 \u51012  \u45236 \u49464 \u50864 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u49440 \u53469 \u51032  \u44592 \u47196 \u50640  \u45459 \u51060 \u44144 \u45208 , \u48120 \u47000 \u44032  \u48520 \u54869 \u49892 \u54620  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51648 \u44552 \u51008  \u47924 \u53553 \u45824 \u44256  \u49440 \u53469 \u54616 \u44592 \u48372 \u45796 \u45716 , \u51104 \u49884  \u49772 \u50612 \u44032 \u47728  \u49373 \u44033 \u51012  \u51116 \u51221 \u48708 \u54624  \u53440 \u51060 \u48141 \u51077 \u45768 \u45796 . \'91\u54665 \u46041 \u51012  \u48372 \u47448 \u54616 \u45716  \u44163 \'92 \u46608 \u54620  \u49440 \u53469 \u51032  \u51068 \u51333 \u51077 \u45768 \u45796 . \u45817 \u48516 \u44036  \u51068 \u51652 \u51060  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u50696 \u49696 \u44032 \u45208  \u46356 \u51088 \u51060 \u45320 , \u51089 \u44032  \u46321  \u52285 \u51032 \u49457 \u51060  \u54596 \u50836 \u54620  \u49324 \u46988 \u50640 \u44176  \u51339 \u51008  \u49548 \u49885 \u51077 \u45768 \u45796 . \u53360  \u44648 \u45804 \u51020 \u51012  \u50619 \u44144 \u45208  \u51064 \u51221 \u48155 \u44172  \u46121 \u45768 \u45796 . \u45796 \u47532 \u47484  \u44844 \u47728  \'93\u45236 \u44032  \u51060 \u47088  \u49324 \u46988 \u51060 \u50556 \'94 \u54616 \u45716  \u44163  \u44057 \u51648  \u50506 \u45208 \u50836 ?\u47564 \u50557  \u51312 \u50616 \u51216 \u51012  \u44396 \u54616 \u44256  \u51080 \u45796 \u47732 , \'91\u49457 \u51109 \u54624  \u54596 \u50836 \u44032  \u51080 \u45796 , \u50500 \u51649  \u48512 \u51313 \u54616 \u45796 \'92\u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u51068 \u51032  \u48169 \u54693 \u49457 \u44284  \u44592 \u48376 \u51201 \u51064  \u51088 \u49464 \u47484  \u51116 \u44160 \u53664 \u54644 \u50556  \u54633 \u45768 \u45796 .\u54633 \u44201 \u50868 \u46020  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51328  \u45908  \u44061 \u44288 \u51201 \u51004 \u47196  \u51088 \u49888 \u51032  \u49892 \u47141 \u51012  \u54032 \u45800 \u54644 \u50556  \u54633 \u45768 \u45796 .\u47588 \u45804 \u47536  \u45224 \u51088  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204  \u55120 \u47492 \u51060  \u51221 \u52404 \u46108  \u49345 \u54889 \u51077 \u45768 \u45796 . \u49345 \u54889 \u51008  \u51109 \u44592 \u51201 \u51004 \u47196  \u55128 \u47084 \u44040  \u46319  \u48372 \u51077 \u45768 \u45796 . \u51088 \u50976 \u47213 \u44172  \u46024 \u51012  \u50416 \u44144 \u45208  \u53804 \u51088 \u54616 \u44592  \u50612 \u47140 \u50872  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49440 \u53469 \u51012  \u48372 \u47448 \u54644 \u50556  \u54633 \u45768 \u45796 . \u51648 \u52636 \u51012  \u52572 \u45824 \u54620  \u51460 \u51060 \u44256 , \u53804 \u51088 \u45716  \u52572 \u45824 \u54620  \u49340 \u44032 \u50556  \u54633 \u45768 \u45796 .    \
