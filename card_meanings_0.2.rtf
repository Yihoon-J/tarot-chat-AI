\u51012  \u52286 \u50500  \u45208 \u49436 \u44172  \u46108 \u45796 \u52964 \u54540 \u47196 \u49436 \u51032  \u49334 \u51060  \u45149 \u45212 \u45796  \u8594  \u50864 \u47532 \u44032  \u55124 \u55176  \u46496 \u50732 \u47532 \u45716  \u51060 \u48324 \u52852 \u46300 \u47484  \u48372 \u47732 , \u51200 \u49849 \u49324 \u51088 \u44032  \'91\u44040  \u46412  \u46108  \u49324 \u46988 \'92\u46308 \u51012  \u52376 \u47532 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48177 \u47560  \u48145 \u50640 \u45716  \u45721 \u51008  \u54889 \u51228 \u44032  \u51453 \u50612 \u51080 \u51424 . \u44536 \u47111 \u51648 \u47564  \u46041 \u49884 \u50640  \u55148 \u47581 \u51201 \u51064  \u50836 \u49548 \u46020  \u48372 \u51077 \u45768 \u45796 . \u51200  \u46244 \u50640 \u45716  \u53468 \u50577 \u51060  \u46496 \u50724 \u47476 \u44256  \u51080 \u51004 \u47728 , \u50612 \u47536  \u50773 \u51088 \u44032  \u49352 \u47213 \u44172  \u53468 \u50612 \u45212  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \u47784 \u46160  \'91\u49352 \u47196 \u50868  \u49884 \u51089 \'92\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51452 \u51032  \u44618 \u44172  \u48400 \u50556  \u54624  \u51216 \u51008  \u53356 \u44172  2\u44032 \u51648 \u51077 \u45768 \u45796 .\u52395  \u48264 \u51704 , \u47924 \u51312 \u44148  \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732  \u50504 \u46121 \u45768 \u45796 .\u51200 \u49849 \u49324 \u51088 \u45716  \u50500 \u47924 \u47088  \u44032 \u52824  \u54032 \u45800 \u51012  \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ] \u52852 \u46300  \u49549  \u54644 \u44264 \u51008  \u45800 \u49692 \u55176  \u54624  \u51068  \u54616 \u45716  \'91\u44277 \u47924 \u50896 \'92 \u52196 \u51032  \u54252 \u51648 \u49496 \u51077 \u45768 \u45796 . \u52860 \u47196  \u47924 \u51109 \u54616 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u51593 , \u45208 \u49244  \u47576 \u51012  \u47673 \u44256  \u50728  \u44172  \u50500