   \uc0\u52981 8- \u54252 \u44592  \u52981 8 \u44536 \u47548 \u51032  \u51032 \u48120 \u45224 \u51088 \u44032  \u52981  8\u44060 \u47484  \u54252 \u44592 \u54616 \u44256  \u46496 \u45208 \u45716  \u49345 \u54889 \u54252 \u44592 \u45208  \u46020 \u54588 \u47484  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u44288 \u49900 \u49324 \u44032  \u48148 \u45068 \u44144 \u45208  \u49892 \u47581 \u54664 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u51020 \u44557 \u51221 \u51201 \u51064  \u51032 \u48120 \u47196 , \u45908  \u45208 \u51008  \u48120 \u47000 \u47484  \u50948 \u54644  \u51104 \u49884  \u46496 \u45208 \u45716  \u49345 \u54889 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u54252 \u44592 \u54616 \u45716 \u52981 8\u51008  \u54252 \u44592 \u47484  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u44536 \u46041 \u50504  \u50528 \u51221 \u51060 \u45208  \u44048 \u51221 \u51012  \u50143 \u50500 \u50772 \u45912  \'91\u51068 , \u49324 \u46988 \'92\u51012  \u54252 \u44592 \u54620 \u45796 \u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u52981 \u51008  \u50528 \u51221 \u51060 \u45208  \u44048 \u51221 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u45224 \u51088 \u45716  \u44536 \u46041 \u50504  \u51096  \u49939 \u50500  \u50732 \u47536  \u52981  8\u44060 \u47484  \u54252 \u44592 \u54620  \u52292 , \u51200  \u47680 \u47532  \u46496 \u45208 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u44536 \u45716  \u51648 \u54049 \u51060 \u50640  \u51032 \u51316 \u54644 \u49436  \u44152 \u50612 \u44032 \u44256  \u51080 \u45716 \u45936 \u50836 . \u50784 \u51648  \u47784 \u47476 \u44172  \u55192 \u44200 \u50892  \u48372 \u51077 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51004 \u47196  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u51060 \u48324  \'96 \u44536 \u46041 \u50504  \u50672 \u51064 \u50640 \u44172  \u44054 \u44256  \u51080 \u50632 \u45912  \u44048 \u51221 \u51060 \u45208  \u50528 \u51221 \u51012  \u48260 \u47532 \u44256  \u44032 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u46496 \u45208 \u47140  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51677 \u49324 \u46993 \u51012  \u44536 \u47564 \u46160 \u45716  \'96 \u44536 \u46041 \u50504  \u44048 \u51221  \u54364 \u54788 , \u50528 \u51221  \u54364 \u54788 \u51012  \u50676 \u49900 \u55176  \u54664 \u51004 \u45208 , \u51060 \u51228  \u51648 \u52432 \u49436  \u54252 \u44592 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u51652 \u51204  \u50630 \u45716  \u44288 \u44228  \'96 \u50756 \u49457 \u46104 \u51648 \u46020  \u47803 \u54616 \u44256 , \u51652 \u51204 \u46020  \u50630 \u45716  \u44288 \u44228 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u52981 \u51008  \u50504 \u51221 \u51201 \u51004 \u47196  \u49939 \u50668  \u51080 \u51648 \u47564 , \u44536  \u47784 \u50577 \u51008  \u48520 \u50756 \u51204 \u54644  \u48372 \u51077 \u45768 \u45796 . \u47560 \u52824  \u51060 \u48744  \u48736 \u51652  \u54840 \u46993 \u51060  \u44057 \u44400 \u50836 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u51088 \u48156 \u51201 \u51004 \u47196  \u54924 \u49324 \u47484  \u46496 \u45208 \u44144 \u45208  \u51648 \u44552 \u44620 \u51648  \u47800 \u45812 \u50500 \u50772 \u45912  \u48516 \u50556 , \u44040 \u44256  \u45798 \u51008  \u44592 \u49696 \u51012  \u54252 \u44592 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50808 \u48512 \u51201 \u51004 \u47196  \'91\u49324 \u44148 \'92\u51060  \u53552 \u51648 \u44592 \u48372 \u45796 \u45716  \'91\u51060 \u51228  \u54624 \u47564 \u53372  \u54664 \u45796 . \u51648 \u44217 \u45796 \'92\u46972 \u45716  \u51064 \u49885 \u51060  \u49373 \u44220 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u50756 \u49457 \u44620 \u51648  2\u44060 \u48150 \u50640  \u45224 \u51648  \u50506 \u50520 \u51004 \u48064 \u47196  \'91\u48260 \u54000 \u46972 \'92\u45716  \u51312 \u50616 \u51012  \u46300 \u47532 \u44256  \u49910 \u49845 \u45768 \u45796 .\u49549 \u47560 \u51020 \u51004 \u47196  \u45208 \u50732  \u44221 \u50864 , \u50500 \u51649  \u44048 \u51221 \u51008  \u47566 \u51060  \u45224 \u50500 \u51080 \u51648 \u47564  \u50612 \u51796  \u49688  \u50630 \u51060  \u46028 \u50500 \u49440 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45804 \u51032  \u54364 \u51221 \u52376 \u47100 , \u44536 \u45716  \u50864 \u50872 \u54644  \u48372 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u46020 \u54588 \u54616 \u45716 Eight of Cups\u45716  \u44040 \u46321 \u51012  \u54924 \u54588 \u54616 \u44144 \u45208 , \u44048 \u51221 \u51201 \u51064  \u49345 \u54889 \u51012  \u46020 \u54588 \u54620 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u46412  \u46020 \u54588 \u54616 \u47140 \u45716  \u49345 \u54889 \u51008  \'91\u44592 \u49256 , \u44048 \u46041 \'92 \u48372 \u45796 \u45716  \'91\u49836 \u54548 , \u48516 \u45432 , \u51396 \u52293 \u44048 \'92 \u46321 \u46321  \u48512 \u51221 \u51201 \u51064  \u44048 \u51221 \u44284  \u44032 \u44637 \u49845 \u45768 \u45796 . \u52981 \u51008  \u52509  8\u44060 , \u50612 \u46360 \u44032  \u48520 \u50756 \u51204 \u54620  \u54805 \u53468 \u47196  \u49939 \u50668 \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :\u50724 \u47000 \u46108  \u52964 \u54540 \u51060  \u49912 \u50868  \u49345 \u54889 , \u51060 \u47484  \u54644 \u44208 \u54616 \u47140  \u54616 \u44592  \u48372 \u45796 \u45716  \u46020 \u54588 \u54616 \u45716  \u48169 \u48277 \u51012  \u53469 \u54632 \u50724 \u47004  \u44592 \u44036  \u50612 \u49353 \u54620  \u49345 \u54889 , \u50612 \u49353 \u54620  \u49345 \u54889 \u51012  \u54400 \u50612 \u48372 \u47140  \u54616 \u44592  \u48372 \u45796 \u45716  \u44536 \u45285  \u47924 \u49884 \u54616 \u45716  \u48169 \u48277 \u51012  \u53469 \u54632 \u49912 \u50872  \u44163  \u44057 \u51004 \u47732  \u44536 \u45285  \u54924 \u54588 \u54644 \u48260 \u47532 \u45716  \u49457 \u54693 \u51060 \u46412  \u45224 \u51088 \u44032  \u53469 \u54620  \u46020 \u54588 \u45716  \u50620 \u47480  \u46020 \u47581 \u52432 \u50556 \u51648  \u48372 \u45796  \u50640 \u55092 \'85 \u50584 \u44592 \u54644 \u49436  \u47952 \u54616 \u44192 \u45768  \u51032  \u45656 \u50521 \u49828 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \u45712 \u47551 \u45712 \u47551  \u52404 \u45392 \u54616 \u46319  \u44152 \u50612 \u44032 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u44288 \u49900 \u49324 \u44032  \u48148 \u45072 \u52981 8\u51008  \u44288 \u49900 \u49324 \u44032  \u48148 \u45068 \u44144 \u45208  \u49352 \u47196 \u50868  \u44600 \u51012  \u52286 \u51004 \u47140 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u51593 , \'91\u49373 \u44033 \u51060 \u45208  \u44048 \u51221 \u51012  \u51648 \u48176 \u54664 \u45912  \u54056 \u53556 \'92\u51012  \u48260 \u47536 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52981  8\u44060 \u45716  \'91\u45224 \u51088 \u44032  \u52264 \u44257 \u52264 \u44257  \u49939 \u50500 \u50772 \u45912  \u49373 \u44033 \'92, \u48148 \u45797 \u51032  \u44053 \u51008  \'91\u50724 \u47000 \u46108  \u44048 \u51221 \'92\u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45224 \u51088 \u45716  \u51060  \u46168 \u51012  \u47784 \u46160  \u48260 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :\u50724 \u47019 \u46041 \u50504  \u51339 \u50500 \u54616 \u45912  \u49324 \u46988 \u51012  \u51228 \u52824 \u44256  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51060  \u49352 \u47213 \u44172  \u49373 \u44596  \u47784 \u49845 \u55141 \u48120 \u47484  \u51075 \u44256 , \u45796 \u47480  \u51649 \u50629 \u51012  \u52286 \u45716  \u49345 \u54889 \u44288 \u49900 \u49324 \u44032  \u50672 \u50528 \u50640 \u49436  \u51068 \u47196  \u48148 \u45072  \u49345 \u54889 \u50672 \u51064 \u51012  \u46496 \u45208 \u44256  \u49352 \u47196 \u50868  \u49324 \u46988 \u51012  \u52286 \u51004 \u47140  \u54632 \u50724 \u47019 \u46041 \u50504  \u44288 \u49900 \u51012  \u44032 \u51256 \u50772 \u45912  \u52712 \u48120 \u47484  \u48260 \u47548 \u50724 \u47019 \u46041 \u50504  \u44040 \u44256  \u45798 \u50500 \u50772 \u45912  \u44592 \u49696 \u51012  \u54252 \u44592 \u54616 \u45716  \u49345 \u54889 \u49345 \u45824 \u48169 \u50640 \u44172  \u47924 \u50616 \u44032  \'91\u51064 \u49885 \u51032  \u51204 \u54872 \'92\u51060  \u49373 \u44596  \u46319  \u48372 \u51077 \u45768 \u45796 .\u54644 \u49437  4. \u49892 \u47581 \u49828 \u47084 \u50868 \u52981 8\u51008  \u50612 \u46360 \u44032  \u49892 \u47581 \u49828 \u47101 \u44144 \u45208 , \u54728 \u51204 \u54620  \u44048 \u51221 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u52981 \u51008  \u51096  \u51221 \u47148 \u46104 \u50612  \u51080 \u51648 \u47564 , \u50612 \u46356  \u54616 \u45208 \u44032  \u48736 \u51256 \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \u44048 \u51221 \u51201 \u51064  \u47564 \u51313 \u44048 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u44163 \u51012  \u46907 \u54633 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u51201 \u50857 \u54633 \u45768 \u45796  :\u50668 \u51088 \u52828 \u44396 (\u54841 \u51008  \u45224 \u51088 \u52828 \u44396 )\u44032  \u51080 \u51020 \u50640 \u46020  \u50808 \u47196 \u50880 \u51012  \u45712 \u45180 \u45716  \u49345 \u53468 \u52649 \u48516 \u54620  \u49324 \u46993 \u51012  \u48155 \u51648  \u47803 \u54644  \u46496 \u45208 \u45716  \u49345 \u54889 \u49457 \u52712 \u44048 \u51060 \u45208  \u49100 \u46319 \u54632 \u51060  \u48512 \u51313 \u54620  \u51649 \u50629 \u50696 \u49345 \u48372 \u45796  \u49884 \u53360 \u46181 \u54620  \u48152 \u51025 \u50640  \u49892 \u47581 \u49828 \u47084 \u50880 \u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \'93\u45817 \u49888 \u51012  \u51652 \u51221 \u51004 \u47196  \u54665 \u48373 \u54616 \u44172  \u54616 \u45716  \u51068 \u51008  \u47924 \u50631 \u51064 \u51648 , \u49457 \u52712 \u44048 \u51012  \u50612 \u46356 \u49436  \u45712 \u45188  \u49688  \u51080 \u45716 \u51648  \u46104 \u46028 \u50500 \u48372 \u46972 \'94\u46972 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u45908  \u45208 \u51008  \u48120 \u47000 \u47484  \u50896 \u54616 \u45716 8\u52981 \u51008  \u45908  \u45208 \u51008  \u48120 \u47000 \u47484  \u50948 \u54644 , \u51068 \u49884 \u51201 \u51004 \u47196  \u46496 \u45208 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u52852 \u46300  \u51060 \u48120 \u51648 \u47484  \'91\u45224 \u51088 \u51032  \u46020 \u54588 \'92\u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u51648 \u47564 , \'91\u52981 \u51012  \u52286 \u44592  \u50948 \u54644  \u46496 \u45208 \u45716  \u49345 \u54889 \'92\u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u51452 \u48320 \u51008  \u44256 \u50836 \u54616 \u44256 , \u47932 \u51008  \u51200  \u50500 \u47000 \u47196  \u48736 \u51652  \u49345 \u54889 \u51077 \u45768 \u45796 . \u51593 , \u49352 \u47196 \u50868  \u44275 \u51012  \u53456 \u54744 \u54616 \u44592 \u50640  \u51339 \u51008  \u49884 \u44592 \u51077 \u45768 \u45796 . \u46608  \u45224 \u51088 \u51032  \u48744 \u44036 \u49353  \u50743 \u51008  \u50676 \u51221 \u44284  \u50676 \u51032 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u46412 \u47928 \u50640  \'91\u46020 \u54588 \'92\u44032  \u50500 \u45772  \'91\u53456 \u54744 \'92\u51060 \u46972 \u45716  \u49884 \u44033 \u46020  \u51316 \u51116 \u54633 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ] \u50696 \u47484  \u46308 \u50612  :\u51109 \u44592 \u51201 \u51064  \u44288 \u44228 \u47484  \u50948 \u54644 , \u51104 \u49884  \u49884 \u44036 \u51012  \u44054 \u51088 \u44256  \u47568 \u54616 \u45716  \u49345 \u54889 \u48376 \u51064 \u51032  \u54665 \u48373 \u51012  \u50948 \u54644 , \u45796 \u47480  \u51649 \u51109 \u51004 \u47196  \u51060 \u51649  \u54616 \u45716  \u49345 \u54889 \u54868 \u47484  \u44032 \u46972 \u50505 \u55176 \u44592  \u50948 \u54644 , \u51104 \u49884  \u55092 \u49885 \u51012  \u52712 \u54616 \u45716  \u49345 \u54889 \u44536 \u47084 \u45208 , \u50668 \u51221 \u51060  \u49692 \u53444 \u54644  \u48372 \u51060 \u51652  \u50506 \u49845 \u45768 \u45796 . \u51200  \u47680 \u47532  \u51080 \u45716  \u49328 \u51060  \u44536 \u44032  \u46020 \u45804 \u54624  \u50669 \u44221 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u48148 \u48372  \u52852 \u46300 \u50640  \u51080 \u45716  \u49444 \u49328 \u44284  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 . \u52981 8 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44552 \u51204 \u51201 \u51064  \u48372 \u49345 \u48372 \u45796 \u45716  \'91\u49457 \u52712 \u44048 , \u47564 \u51313 \u44048 \'92\u51012  \u50864 \u49440 \u49884 \u54616 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u45130 \u51076 \u50630 \u51060  \u48156 \u51204 \u51012  \u52628 \u44396 \u54616 \u47728 , \u54637 \u49345  \'91\u50780  \u51060 \u44163 \u51012  \u54616 \u45716 \u51648 , \u51060 \u44172  \u45208 \u50640 \u44172  \u50612 \u46500  \u51032 \u48120 \u51064 \u51648 \'92 \u46104 \u46028 \u50500 \u48372 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u51032 \u48120  \u50630 \u45716  \u50672 \u50528 \u47484  \u44361 \u51109 \u55176  \u49899 \u50612 \u54616 \u44592 \u50640 , \'91\u44048 \u51221 \u51201 \u51064  \u49548 \u53685 \u51060 \u45208  \u44368 \u47448 \'92\u44032  \u50630 \u51012  \u44221 \u50864 , \u51060 \u48324 \u51012  \u53685 \u48372 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u52981 8\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u44048 \u51221 \u51201 \u51004 \u47196  \'91\u44144 \u47532 \u47484  \u45712 \u45180 \u44256  \u51080 \u45796 \'92\u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u51593 , \u49548 \u53685 \u51008  \u46104 \u51648 \u47564  \u50612 \u46360 \u44032  \u50504  \u47582 \u50520 \u44256 , \u54840 \u44048 \u51012  \u54364 \u54788 \u54664 \u51648 \u47564  \u44536 \u47564 \u53372  \u48155 \u51648  \u47803 \u54664 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172  \u49892 \u47581 \u54644 \u49436  \u46496 \u45208 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u50500 \u51649  \u44048 \u51221 \u51008  \u47566 \u51060  \u45224 \u50500 \u51080 \u51648 \u47564 , \u50612 \u51796  \u49688  \u50630 \u51060  \u46496 \u45208 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972 , \u49352 \u47196 \u50868  \u49324 \u46988 \u51060  \u45576 \u50640  \u46308 \u50612 \u50724 \u44144 \u45208  \u44145 \u51221 \u49828 \u47084 \u50868  \u49345 \u54889 \u51060  \u49352 \u47213 \u44172  \u49373 \u44220 \u45796 \u45716  \u54644 \u49437 \u46020  \u44032 \u45733 \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824  \u51068  \u46412 \u47928 \u50640  \u50672 \u50528 \u47484  \u54252 \u44592 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u44048 \u51221 \u51201 \u51064  \u50976 \u45824 \u44048 , \u45817 \u49888 \u50640  \u45824 \u54620  \u44592 \u45824 \u44032  \u44648 \u51276 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u49549 \u51004 \u47196  \'91\u51060 \u48324 \u51012  \u44208 \u49900 \u54664 \u51012 \'92 \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51068  \u44221 \u50864 , \u48120 \u47000 \u47484  \u50948 \u54644  \'91\u51104 \u49884  \u44144 \u47532 \u47484  \u46160 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 . \u48120 \u50912 \u45912  \u49324 \u46988 \u46020  \u50724 \u47000  \u46504 \u50612 \u51256  \u51080 \u45796  \u48372 \u47732  \u44536 \u47532 \u50892 \u51648 \u45716  \u49692 \u44036 \u51060  \u51080 \u51094 \u50500 \u50836 ? \u51104 \u49884  \u49345 \u45824 \u48169 \u50640 \u44172  \'91\u54844 \u51088  \u51080 \u51012  \u49884 \u44036 \'92\u51012  \u51456 \u45796 \u44256  \u49373 \u44033 \u54616 \u49464 \u50836 .\u51116 \u54924 \u50868 \u51008  \u45208 \u49257 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u51060 \u51228  \u45459 \u50500 \u51452 \u44592 \u47196  \u44208 \u49900 \u54620  \u47784 \u50577 \u51077 \u45768 \u45796 . \u44628 \u45140 \u54620  \u44208 \u45800 \u51060  \u50500 \u45768 \u46972 , \u50612 \u51796  \u49688  \u50630 \u51060  \'91\u45459 \u50500 \u51452 \u45716 \'92 \u49345 \u54889 \u51077 \u45768 \u45796 . \u45656 \u50521 \u49828 \u47484  \u49444 \u47749 \u54616 \u51088 \u47732 , \u50528 \u51648 \u51473 \u51648  \u53412 \u50864 \u45912  \u50556 \u49373  \u46041 \u47932 \u51012  \u50612 \u51796  \u49688  \u50630 \u51060  \u48169 \u49373 \u54616 \u45716  \u45712 \u45196 \u51060 \u46980 \u44620 \u50836 . \u50528 \u51221 \u46020  \u44048 \u51221 \u46020  \u47566 \u51060  \u45224 \u50500 \u51080 \u51648 \u47564 , \u49436 \u47196 \u47484  \u50948 \u54644  \u51060 \u48324 \u51012  \u53469 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51116 \u54924 \u47484  \u50896 \u54620 \u45796 \u47732 , \u47676 \u51200  \u45796 \u44032 \u44032 \u49464 \u50836 . \u47566 \u51060  \u48320 \u54664 \u44256 , \u44060 \u49440 \u54616 \u47140 \u45716  \u51032 \u51648 \u46020  \u51080 \u51020 \u51012  \u48372 \u50668 \u51452 \u50612 \u50556  \u54633 \u45768 \u45796 .\u52981 8 \u51649 \u50629 \u50868  \u54644 \u49437 \u51652 \u51676  \u51339 \u50500 \u54616 \u45716  \u51068 \u51012  \u54693 \u54644  \u45208 \u50500 \u44032 \u49464 \u50836 . \u52981 8\u51008  \u45817 \u49888 \u51060  \'91\u49457 \u52712 \u44048 \u44284  \u54665 \u48373 \'92 \u47784 \u46160 \u47484  \u51075 \u50632 \u51020 \u51012  \u44221 \u44256 \u54633 \u45768 \u45796 . \u45817 \u49888 \u51008  \u50780  \u44536  \u51068 \u51012  \u54616 \u44256  \u51080 \u45208 \u50836 ? \u44536 \u44172  \u51221 \u47568  \u45817 \u49888 \u51060  \u50896 \u54616 \u45912  \u49334 \u51064 \u44032 \u50836 ? \u51648 \u44552  \u51649 \u51109 \u51060  \'91\u50504 \u51221 \u44048 \'92\u51012  \u51460 \u51648 \u45716  \u47792 \u46972 \u46020 , \'91\u54665 \u48373 \'92\u51012  \u44032 \u51256 \u45796 \u51452 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 . \u46020 \u51204 \u54616 \u49464 \u50836 . \u51648 \u44552 \u51060  \u48148 \u47196  \u52572 \u51201 \u51032  \u49884 \u44592 \u51077 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u44592 \u45824 \u54620  \u47564 \u53372 \u51032  \u51216 \u49688 \u44032  \u45208 \u50724 \u51648  \u50506 \u50500  \u49892 \u47581 \u54616 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44277 \u48512 \u54616 \u45716  \u48169 \u49885 \u51012  \u48148 \u45012 \u48372 \u49464 \u50836 . \u50724 \u47000 \u46108  \u54617 \u49845  \u54056 \u53556 \u51012  \u48260 \u47532 \u44256 , \u49352 \u47196 \u50880 \u51012  \u52628 \u44396 \u54616 \u46972 \u45716  \u51312 \u50616 \u51077 \u45768 \u45796 .\u52981 8 \u44552 \u51204 \u50868  \u54644 \u49437 \u49352 \u47196 \u50868  \u53804 \u51088  \u48169 \u49885 \u51012  \u46020 \u51077 \u54644 \u50556  \u54633 \u45768 \u45796 . \u50676 \u49900 \u55176  \u45432 \u47141 \u54664 \u51648 \u47564 , \u48324  \u49548 \u46301 \u51060  \u50630 \u50632 \u45912  \u53804 \u51088 \u47484  \u44040 \u50500 \u50638 \u51012  \u49884 \u44592 \u51077 \u45768 \u45796 . \u54252 \u44592 \u44032  \u55192 \u46308  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u48320 \u54868 \u47484  \u50896 \u54620 \u45796 \u47732  \'91\u44277 \u46308 \u51064  \u53457 \'92\u46020  \u47924 \u45320 \u46888 \u47540  \u51460  \u50508 \u50500 \u50556  \u54633 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u47581 \u49444 \u51060 \u51648  \u47568 \u44256  \u49352 \u47213 \u44172  \u45208 \u50500 \u44032 \u49464 \u50836 \u47924 \u51032 \u48120 \u54616 \u44172  \u48152 \u48373 \u46104 \u45716  \u51068 \u49345 \u50640  \u51648 \u52452 \u45796 \u47732 \'85 \u51648 \u44552 \u51060  \u48148 \u47196  \u48320 \u54868 \u47484  \u47784 \u49353 \u54624  \u46412 \u51077 \u45768 \u45796 . \u51060 \u48120  \u45817 \u49888 \u51008  \'93\u45236 \u44032  \u50780  \u51060 \u47084 \u44256  \u51080 \u51648 ? \u51060 \u44148  \u47956 \u44032  \u51096 \u47803 \u46096 \u51094 \u50500 \'94 \u46972 \u44256  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51089 \u51008  \u51068 \u48512 \u53552  \u54616 \u45208 \u54616 \u45208  \u49884 \u51089 \u54644 \u48372 \u49464 \u50836 . \u51060 \u51649 \u51060 \u45208  \u53748 \u49324 \u44032  \u50612 \u47140 \u50868  \u49345 \u54889 \u51060 \u46972 \u47732 , \u48148 \u44992  \u49688  \u51080 \u45716  \u44163 \u48512 \u53552  \u48148 \u45012 \u45208 \u44032 \u49464 \u50836 .\u51064 \u44036 \u44288 \u44228 \u50640 \u49436 \u46020 , \u47581 \u49444 \u51076 \u51008  \u45908  \u53360  \u49345 \u52376 \u47484  \u45224 \u44600  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50857 \u44592 \u47484  \u45236 \u50612  \u44208 \u51221 \u54616 \u49464 \u50836 . \u49352 \u47196 \u50868  \u49884 \u51089 \u51012  \u51456 \u48708 \u54644 \u46020  \u51339 \u51008  \u46412 \u51077 \u45768 \u45796 .    \
\
   \uc0\u52981 9 \u51060 \u48120 \u51648 \u52981 9 \'96 \u47564 \u51313 \u44048  \u52981 \u51060  \u51652 \u50676 \u46108  \u51109 \u49885 \u45824  \u50526 \u50640  \u45224 \u51088 \u44032  \u51088 \u46993 \u49828 \u47112  \u50505 \u50500 \u51080 \u51020 \u54788 \u51116  \u49345 \u54889 \u50640  \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u45716  \u49345 \u53468 \u47924 \u50616 \u44032  \u51088 \u46993 \u49828 \u47101 \u45796 \u45716  \u49900 \u47532 \u44256 \u51665 \u49828 \u47084 \u50868  \u49324 \u46988 \u51060 \u45208  \u50724 \u47564 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u51088 \u44592 \u51473 \u49900 \u51201 \u51064  \u49324 \u44256 \u47484  \u54588 \u54616 \u46972 \u45716  \u44221 \u44256 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u45716 \u52981 9\u45716  \u47564 \u51313 \u49828 \u47084 \u50868  \u49345 \u53468 \u47484  \u44032 \u47532 \u53429 \u45768 \u45796 . \u52981 \u51008  \'91\u49373 \u44033 \u51060 \u45208  \u44048 \u51221 \'92\u51012  \u51032 \u48120 \u54616 \u45716 \u45936 \u50836 . 9\u44060 \u51032  \u52981 \u51008  \u51340 \u50864 \u45824 \u52845 \u51004 \u47196  \u45208 \u46976 \u55176  \u51652 \u50676 \u46104 \u50612  \u51080 \u49845 \u45768 \u45796 . \u45224 \u51088 \u45716  \u54036 \u51697 \u51012  \u45184  \u52292  \u47564 \u51313 \u49828 \u47084 \u50868  \u54364 \u51221 \u51004 \u47196  \u52981 \u51012  \u51088 \u46993 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47564 \u51313 \u50640 \u45716  \u53356 \u44172  2\u44032 \u51648  \u45656 \u50521 \u49828 \u44032  \u45812 \u44200 \u51080 \u49845 \u45768 \u45796 .\u50668 \u47084  \u48169 \u47732 \u50640 \u49436  \u47564 \u51313 \u49828 \u47101 \u45796  \'96 \u51593 , \u51221 \u49888 \u51201 \u51064  \u47564 \u51313  \u49104 \u47564  \u50500 \u45768 \u46972 , \u47932 \u51656 \u51201 \u51004 \u47196 \u46020  \u47564 \u51313 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u49548 \u53685 \u46020  \u51096  \u46104 \u44256  \u44221 \u51228 \u51201 \u51004 \u47196  \u50668 \u50976 \u47196 \u50868  \u50672 \u50528 \u47484  \u51600 \u44600  \u49688  \u51080 \u44172  \u46121 \u45768 \u45796 . \u51452 \u48320 \u50640 \u49436 \u46020  \'93\u47564 \u45208 \u44600  \u51096 \u54664 \u45796 \'94\u44256  \u51064 \u51221 \u48155 \u44592 \u50640 , \u48324  \u48152 \u45824  \u50630 \u51060  \u44208 \u54844 \u44620 \u51648  \u49692 \u53444 \u54616 \u44172  \u51060 \u50612 \u51656  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u44060 \u51064 \u51201 \u51064  \u47564 \u51313 \u46020 \u46020  \u45458 \u44256  \u50672 \u48393 \u46020  \u44316 \u52270 \u51008  \u51649 \u51109 \u50640  \u46308 \u50612 \u44032 \u44172  \u46121 \u45768 \u45796 .\u47564 \u51313 \u44048 \u51060  \u51201 \u45817 \u54616 \u45796  \'96 \u46385  \u51201 \u45817 \u54620  \u50728 \u46020 \u47196  \u49380 \u50892 \u44592 \u47484  \u53952 \u50632 \u51012  \u46412  \u45712 \u45196 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49549 \u47560 \u51020 \u51012  \u51656 \u47928 \u54664 \u45796 \u47732 , \'93\u45908 \u46020  \u47568 \u44256  \u45916 \u46020  \u47568 \u44256  \u51648 \u44552 \u51060  \u51339 \u45796 \'94\u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54788 \u51116  \u49345 \u53468 \u47484  \u50976 \u51648 \u54616 \u44256  \u49910 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 .\u45796 \u47564 , \u51060  \u47564 \u51313 \u44048 \u51008  \u51068 \u49884 \u51201 \u51068  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732 , \u45224 \u51088 \u45716  \u46385 \u46385 \u54620  \u45208 \u47924  \u51032 \u51088 \u50640  \u50505 \u50500 \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u45716  \u44208 \u44397  \u48520 \u54200 \u54632 \u51012  \u45712 \u45180 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 . Comment\u45208 \u47924  \u51032 \u51088 \u45716  \u51088 \u49688 \u49457 \u44032 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45208 \u47924  \u51032 \u51088 \u50640 \u45716  \u46321 \u51012  \u44592 \u45832  \u49688  \u51080 \u45716  \u46321 \u48155 \u51060 \u44032  \u50630 \u49845 \u45768 \u45796 . \u47924 \u50616 \u44032 \u50640  \u51032 \u51316 \u54616 \u51648  \u50506 \u45716 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . 3\u48264  \u50668 \u54889 \u51228  \u52852 \u46300  \u49549  \u50668 \u54889 \u51228 \u45716  \u48744 \u44036  \u53216 \u49496 \u50640  \u44592 \u45824 \u44256  \u51080 \u45716 \u45936 , \u51060 \u45716  \u45572 \u44400 \u44032 \u51032  \u45733 \u47141 \u50640  \u51032 \u51316 \u54620 \u45796 \u45716  \u51032 \u48120 \u44032  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u51088 \u46993 \u49828 \u47084 \u50868 \u52981 9\u45716  \u51088 \u46993 \u49828 \u47084 \u50880 , \u51088 \u48512 \u49900 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \'93\u45236 \u44032  \u51060 \u47088  \u49324 \u46988 \u51060 \u50556 \'94\u46972 \u45716  \u45656 \u50521 \u49828 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \u45224 \u51088 \u45716  \u54036 \u51697 \u51012  \u45180 \u47728 , \u45796 \u47532 \u46020  \u45331 \u44172  \u48268 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u52981 \u51032  \u51652 \u50676 \u46020  \u45576 \u50668 \u44200 \u48380 \u47564  \u54633 \u45768 \u45796 . \u47560 \u52824  \u45572 \u44400 \u44032 \u50640 \u44172  \u48372 \u50668 \u51452 \u47140 \u45716  \u46319 , \u54028 \u46976 \u49353  \u51109 \u49885 \u45824  \u50948 \u50640  \u51221 \u49457 \u49828 \u47112  \u47784 \u49492 \u45480 \u49845 \u45768 \u45796 . \u51088 \u46993 \u49828 \u47084 \u50880 \u51012  \u49549 \u47560 \u51020 \u50640  \u51201 \u50857 \u54616 \u47732  :\u52964 \u54540  \'96 \u45236  \u50528 \u51064 \u51060  \u51060 \u47088  \u49324 \u46988 \u51060 \u50556 . \u45224 \u46308 \u50640 \u44172  \u51088 \u46993 \u54616 \u44256  \u49910 \u51008  \u49900 \u47532 \u49556 \u47196  \'96 \u45208  \u51060  \u49324 \u46988 \u54620 \u53580  \u50672 \u46973  \u50772 \u45796 ? \u45236 \u44032  \u51060 \u47088  \u49324 \u46988 \u51060 \u50556 . \u54841 \u51008  \u45236 \u44032  \u51060 \u47088  \u49324 \u46988 \u51060 \u45768 \u44620  \u45328  \u44844 \u49892  \u49688  \u51080 \u44192 \u51648 ?\u45796 \u47564 , \u52981 9\u45716  \u51088 \u46993 \u49828 \u47084 \u50880  \u49104 \u47564  \u50500 \u45768 \u46972 , \'91\u44284 \u49884 \'92\u54620 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50528 \u51064 \u51012  \'91\u44284 \u49884 \u54616 \u44592  \u50948 \u54620  \u49688 \u45800 \'92 \u51593  \u53944 \u47196 \u54588 \u50752 \u51060 \u54532 \u52376 \u47100  \u45824 \u54624  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44256 \u51665 \u49828 \u47084 \u50868 \u52981 9\u45716  \u44256 \u51665 \u49828 \u47084 \u50868  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45224 \u51088 \u45716  \u53945 \u51060 \u54616 \u44172  \u46769 \u46769 \u54616 \u44256  \u44256 \u51665 \u49828 \u47084 \u50868  \u47784 \u49845 \u51004 \u47196  \u54364 \u54788 \u46096 \u49845 \u45768 \u45796 . \u46608 \u54620  \u54036 \u51697 \u51012  \u45180 \u47728 , \u48169 \u50612 \u51201 \u51064  \u53468 \u46020 \u47484  \u46300 \u47084 \u45236 \u44256  \u51080 \u45716 \u45936 \u50836 . \u51593 , \u45236  \u49373 \u44033 \u51012  \'93\u45149 \u44620 \u51648  \u44256 \u49688 \u54616 \u44192 \u45796 \'94\u45716  \u51032 \u48120 \u44032  \u44053 \u54633 \u45768 \u45796 .\u46608  \u45224 \u51088 \u45716  \u45458 \u51008  \u50948 \u52824 \u50640  \u52981 \u51012  \u50732 \u47140 \u45459 \u50520 \u49845 \u45768 \u45796 . \u44536 \u47564 \u53372  \'91\u51088 \u49888 \u51032  \u49373 \u44033 \u51060 \u45208  \u44048 \u51221 \'92\u51060  \u45796 \u47480  \u47924 \u50631 \u48372 \u45796  \u50864 \u50948 \u50640  \u51080 \u51020 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \'93\u48148 \u45068 \u44600  \u50896 \u54620 \u45796 \u44256 ? \u44536 \u47084 \u47732  \u45320 \u44032  \u50880 \u51649 \u50668 \u50556 \u51648 \'94\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51088 \u48512 \u49900 \u51060 \u45208  \u51088 \u44592 \u50528 \u44032  \u45320 \u47924  \u44053 \u54644 \u49436 , \u45224 \u51032  \u51032 \u44204 \u51012  \u49688 \u50857 \u54616 \u51648  \u47803 \u54616 \u45716  \u49324 \u46988 \u51088 \u44592  \u51096 \u47803 \u51012  \u51064 \u51221 \u54616 \u51648  \u50506 \u45716  \u49324 \u46988 \u47676 \u51200  \u50672 \u46973 \u51012  \u54624  \u49373 \u44033 \u51060  \u50630 \u45716  \u49324 \u46988 \u54644 \u49437  2. \u50724 \u47564 \u54616 \u44256  \u51088 \u44592 \u51473 \u49900 \u51201 \u51064 \u52981 9\u45716  \u50724 \u47564 \u54616 \u44256  \u51088 \u44592 \u51473 \u49900 \u51201 \u51064  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52981 9\u44060 \u45716  \u47784 \u46160  \'91\u45224 \u51088 \u47484  \u51473 \u49900 \u51004 \u47196 \'92 \u48176 \u50676 \u46104 \u50612  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u47784 \u46304  \u49373 \u44033 \u44284  \u44048 \u51221 \u51060  \u48376 \u51064  \u51473 \u49900 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44172 \u45796 \u44032  \u51789 \u48268 \u54620  \u45796 \u47532 \u45716  \u47924 \u50616 \u44032  \'91\u44284 \u49884 \u54616 \u44256  \u49910 \u51008 \'92 \u50837 \u44396 \u44620 \u51648  \u45236 \u48708 \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u45224 \u51032  \u44592 \u48516 \u51008  \u49373 \u44033 \u52824  \u50506 \u44256  \u47568 \u54644 \u48260 \u47548 \u44537 \u45800 \u51201 \u51068  \u44221 \u50864 , \u45208 \u47476 \u49884 \u49884 \u49828 \u53944  \u49457 \u54693 \u51012  \u48372 \u51076 \u50773 \u51088 \u48337 \u51060 \u45208  \u44277 \u51452 \u48337 \u51060  \u49900 \u54620  \u49324 \u46988 \u52981 9 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u51088 \u44592 \u50528 \u44032  \u44053 \u54616 \u44256 , \u51088 \u51316 \u49900 \u51060  \u49468  \u49457 \u44201 \u51077 \u45768 \u45796 . \u47924 \u50616 \u44032  \u44284 \u49884 \u54616 \u47140 \u45716  \u49457 \u44201 \u46020  \u51080 \u50612  \u45224 \u46308 \u50640 \u44172  \'91\u51116 \u49688  \u50630 \u45796 \'92\u44256  \u48708 \u52656 \u51648 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52981 9\u45716  \u48372 \u53685  \u51088 \u49688 \u49457 \u44032 \u54805  \u49324 \u46988 \u50640 \u44172  \u47566 \u51060  \u46321 \u51109 \u54633 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536  \u49457 \u44284 \u44032  \u44536 \u47111 \u44172  \u53356 \u51648 \u45716  \u50506 \u51008 \u45936 , \u54172 \u53440 \u53364 \u51060  \u50500 \u45768 \u46972  \u52981 \u51060  \u46321 \u51109 \u54664 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \'93\u51088 \u52845 \'94 \u51088 \u49688 \u49457 \u44032 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u51064 \u51221  \u50837 \u44396 \u44032  \u44053 \u54644  \u45130 \u51076 \u50630 \u51060  \u50808 \u48512 \u50640  \u51088 \u49888 \u51012  \u50612 \u54596 \u54616 \u45716  \u49457 \u44201 \u51060 \u47728 , \u49334 \u51060  \u48376 \u51064  \u51473 \u49900 \u51060 \u46972  \'91\u51452 \u52404 \u51201 \'92\u51060 \u46972 \u45716  \u51109 \u51216 \u51060  \u51080 \u51648 \u47564 , \u51088 \u52843  \u45796 \u47480  \u49324 \u46988  \u51032 \u44204 \u44620 \u51648  \u47924 \u49884 \u54624  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .\u52981 9\u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  2\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u51648 \u44552 \u51060  \u46385  \u51201 \u45817 \u54616 \u45796 \u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51060  \u44048 \u51221 \u51012  \u51060 \u45824 \u47196  \u50976 \u51648 \u54616 \u44256  \u49910 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47805 \u44544 \u47805 \u44544 \u54616 \u45768  \u51201 \u45817 \u54620  \u49444 \u47128 \u51012  \u45712 \u45180 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u44256 , \'91\u45817 \u49888 \u50640 \u44172  \u44288 \u49900 \u51060  \u50630 \u51004 \u45768  \u45796 \u44032 \u50724 \u51648  \u47568 \u44256  \u44536 \u45824 \u47196  \u51080 \u50612 \u46972 \'92 \u46972 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46160  \u48264 \u51704 \u45716 , \u51088 \u46993 \u49828 \u47084 \u50880 \u51060 \u45208  \u44284 \u49884 \u54616 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u48376 \u51064  \u52828 \u44396 \u46308 \u50640 \u44172  \u45208  \u50584 \u54620 \u53580  \u50672 \u46973  \u50772 \u45796 ? \u50836 \u49352  \u51088 \u44984  \u52824 \u45824 \u45716  \u44144  \u44057 \u50500 . \u46972 \u44256  \u46496 \u48268 \u47532 \u44144 \u45208 , \u45817 \u49888 \u50640 \u44172  \u45236 \u44032  \u51060 \u47088  \u49324 \u46988 \u51060 \u45796  \u51064 \u51221 \u48155 \u44256  \u49910 \u51008  \u50837 \u44396 \u44032  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49345 \u45824 \u48169 \u51060  \u45817 \u49888 \u51012  \u51088 \u46993 \u49828 \u47084 \u50892 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50808 \u47784 \u45208  \u49457 \u44201 , \u50612 \u46356 \u44032  \u47560 \u51020 \u50640  \u46300 \u45716 \u51648 \u45716  \u51221 \u54869 \u55176  \u50508  \u49688  \u50630 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536 \u47088  \u45817 \u49888 \u50640 \u44172  \'93\u49324 \u46993 \u48155 \u45716 \u45796 \u45716  \u44163 \'94\u51012  \u47588 \u50864  \u47564 \u51313 \u49828 \u47101 \u44172  \u50668 \u44592 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45817 \u49888 \u51012  \u52828 \u44396 \u46308 \u50640 \u44172  \'91\u44284 \u49884 \u54616 \u44256  \u49910 \u51008  \u50857 \u46020 \'92\u47196  \u50424  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u53944 \u47196 \u54588  \u50752 \u51060 \u54532 , \u53944 \u47196 \u54588  \u54728 \u51592 \u48264 \u46300  \u51221 \u46020  \u46104 \u45716  \u54252 \u51648 \u49496 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u54788 \u51116  \u49345 \u54889 \u50640  \u47564 \u51313 \u54616 \u47728  \u50880 \u51649 \u51068  \u49373 \u44033 \u51060  \u50630 \u50612  \u48372 \u51077 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \u47676 \u51200  \u50672 \u46973 \u54620 \u45796 \u47732 , \'93\u45236 \u44032  \u51060 \u47092  \u51460  \u50508 \u50520 \u50612 \'94 \u46972 \u47728  \u50724 \u47564 \u54616 \u44172  \u44404  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45796 \u47564  \u44060 \u51064 \u51201 \u51004 \u47196 , \u51060  \u47564 \u51313 \u44048 \u51060  \u50724 \u47000 \u44032 \u51648 \u45716  \u50506 \u51012  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51652 \u51221 \u54620  \u47564 \u51313 \u48372 \u45796 \u45716  \u51221 \u49888  \u49849 \u47532 \u50640  \u44032 \u44637 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45817 \u48516 \u44036  \u44592 \u45796 \u47140 \u48372 \u49464 \u50836 .\u52981 9 \u51649 \u50629 \u50868  \u54644 \u49437 \u49457 \u52712 \u44048 \u44284  \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u44536 \u46041 \u50504  \u45432 \u47141 \u54644 \u50772 \u45912  \u51068 \u46308 \u51060  \'91\u48376 \u51064  \u44592 \u51456 \u50640 \u49436 \'92 \u49457 \u44277 \u54616 \u44172  \u46121 \u45768 \u45796 . \u48120 \u47144 \u46020  \u50630 \u44256 , \u54980 \u54924 \u46020  \u50630 \u49845 \u45768 \u45796 . \u54665 \u50868 \u46020  \u46384 \u46972 \u51452 \u45716  \u49884 \u44592 \u46972 , \u47924 \u50631 \u51012  \u54616 \u46304  \u51096  \u54400 \u47536 \u45796 \u45716  \u45712 \u45196 \u51012  \u48155 \u51012  \u44163 \u51077 \u45768 \u45796 .\u45796 \u47564  \u51060  \u54665 \u48373 \u44048 \u51008  \u51068 \u49884 \u51201 \u51068  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51648 \u44552 \u51032  \u49457 \u52712 \u47484  \'91\u50976 \u51648 \u729 \u48156 \u51204 \'92 \u54616 \u45716  \u44163 \u51060  \u44032 \u51109  \u51473 \u50836 \u54616 \u47728 , \u54788 \u51116 \u50640  \u50504 \u51452 \u54616 \u51648  \u47560 \u49464 \u50836 .\u52712 \u50629 \u50868 \u44284  \u54633 \u44201 \u50868 \u46020  \u47784 \u46160  \u51339 \u51008  \u54200 \u51077 \u45768 \u45796 . \u51452 \u48320 \u50640 \u49436  \u51064 \u51221 \u48155 \u45716  \u45824 \u54617 \u51060 \u45208  \u51649 \u51109 \u50640  \u46308 \u50612 \u44032 \u44172  \u46121 \u45768 \u45796 . \u48376 \u51064  \u44592 \u51456 \u50640 \u49436 \u46020  \u47564 \u51313 \u49828 \u47084 \u50872  \u44163 \u51077 \u45768 \u45796 . \u51060 \u47564 \u54616 \u47732  \u46096 \u45796 . \u51060 \u51221 \u46020 \u47732  \u51339 \u51648 . \u54200 \u50504 \u54620  \u47560 \u51020 \u51004 \u47196  \u51600 \u44144 \u50868  \u49884 \u44592 \u47484  \u48372 \u45236 \u49464 \u50836 .\u52981 9 \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u46020  \u51339 \u51008  \u49884 \u44592 \u51077 \u45768 \u45796 . Nine of Cups\u45716  \'93\u49548 \u50896 \u51060  \u49457 \u52712 \u46108 \u45796 \'94\u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51648 \u44552 \u44620 \u51648  \u45432 \u47141 \u54664 \u45912  \u53804 \u51088 , \u45000 \u45012 \u50772 \u45912  \u47785 \u54364 \u44032  \u44263  \u51060 \u47364 \u51648 \u44172  \u46121 \u45768 \u45796 . \u44536 \u47196  \u51064 \u54644  \u53360  \u49457 \u52712 \u44048 \u44284  \u54665 \u48373 \u51012  \u45712 \u45180 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u51088 \u44592  \u47564 \u51313 \u50640  \u48736 \u51648 \u51648  \u47560 \u49464 \u50836 \u51648 \u44552  \u44288 \u44228 \u44032  \u49692 \u53444 \u54616 \u44256  \u47564 \u51313 \u49828 \u47101 \u45208 \u50836 ? \u44536 \u47084 \u45208  \'93\u49345 \u45824 \u48169 \u46020  \u44536 \u47111 \u44172  \u45712 \u45180 \u45716 \u51648 \'94\u45716  \u46608  \u45796 \u47480  \u47928 \u51228 \u51077 \u45768 \u45796 . \u47784 \u46160  \u51096  \u54400 \u47532 \u44256  \u51080 \u45716 \u45936 , \u52981  9\u48264  \u52852 \u46300 \u44032  \u46321 \u51109 \u54664 \u45796 \u47732  \'93\u48376 \u51064 \u47564  \u44536 \u47111 \u44172  \u45712 \u45180 \u45716  \u44148  \u50500 \u45772 \u51648 \'94 \u49373 \u44033 \u54644 \u48400 \u50556  \u54633 \u45768 \u45796 . \u51228 3\u51088 \u51032  \u49884 \u49440 \u51004 \u47196  \u48376 \u51064 \u51012  \u46028 \u50500 \u48372 \u49464 \u50836 . \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u51312 \u50616 \u51012  \u44396 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 .\u51312 \u50616  2. \u53132 \u46973 \u47564 \u51012  \u52628 \u44396 \u54616 \u51648  \u47560 \u49464 \u50836 \u54788 \u51116 \u51032  \u47564 \u51313 \u44048 \u50640  \u52712 \u54644 , \u54616 \u47336 \u54616 \u47336  \u53440 \u46973 \u54620  \u49373 \u54876 \u51012  \u48372 \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51088 \u52843  \u54224 \u51064 \u51060  \u46112  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 .    \
\
   \uc0\u52981 10 \u51060 \u48120 \u51648 \u52981 10 \'96 \u54665 \u48373 \u54620  \u44032 \u51313  \u48512 \u48512 \u44032  \u47924 \u51648 \u44060 \u47484  \u48148 \u46972 \u48372 \u47728  \u54632 \u44760  \u52629 \u48373 \u54616 \u45716  \u49345 \u54889 \u54665 \u48373 \u44048 \u51012  \u45712 \u45180 \u45716  \u49345 \u53468 \u44032 \u51313 \u44057 \u51060  \u54200 \u50504 \u54620  \u49324 \u46988 \u51012  \u49345 \u51669 \u54788  \u49345 \u54889  \u44536 \u45824 \u47196  \u47560 \u47924 \u47532  \u46108 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u51020 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u54665 \u48373 \u44048 \u51012  \u45712 \u45180 \u45716 \u52981 10\u51008  \u54665 \u48373 \u44048 \u51012  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u48512 \u48512 \u45716  \u54036 \u51697 \u51012  \u45180 \u44256  \u49888 \u45208 \u45716  \u54252 \u51592 \u47484  \u52712 \u54633 \u45768 \u45796 . \u51200  \u47680 \u47532 \u50644  \u47924 \u51648 \u44060 \u44032  \u46496 \u51080 \u51004 \u47728 , \u50500 \u51060 \u46308 \u46020  \u50694 \u50640 \u49436  \u52644 \u51012  \u52628 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51204 \u48152 \u51201 \u51004 \u47196  \u52629 \u51228  \u44057 \u51008  \u48516 \u50948 \u44592 \u44032  \u45712 \u44788 \u51665 \u45768 \u45796 . 10 of Cups\u45716  \u47932 \u51656 \u51201 \u51064  \u54413 \u50836 \u47196 \u50880 \u48372 \u45796 , \u51064 \u44036 \u44288 \u44228 \u50640 \u49436  \u45712 \u45180 \u45716  \u54665 \u48373 \u44048 \u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 . \u51593 , \u54844 \u51088 \u47564  \u54665 \u48373 \u54620  \u44172  \u50500 \u45768 \u44256 , \u45796 \u44057 \u51060  \u54665 \u48373 \u54620  \u49345 \u53468 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u44172 \u45796 \u44032  \u52981  10\u44060 \u45716  \u47924 \u51648 \u44060  \u50948 \u50640  \'93\u51088 \u50672 \u51201 \u51004 \u47196 \'94 \u45459 \u50668 \u51080 \u45716 \u45936 \u50836 . \u51060 \u45716  \u44536  \u54665 \u48373 \u44048 \u51060  \u51064 \u50948 \u51201 \u51004 \u47196  \u47564 \u46308 \u50612 \u51648 \u45716  \u44172  \u50500 \u45768 \u46972 , \u51088 \u50672 \u49828 \u47112  \u47564 \u46308 \u50612 \u51664 \u51012  \u46907 \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u51088 \u47732  :\u54532 \u47196 \u51229 \u53944 \u44032  \u49457 \u44277 \u51201 \u51004 \u47196  \u45149 \u45208 \u49436 , \u44288 \u47144 \u46108  \u49324 \u46988 \u46308 \u51060  \u45796 \u44057 \u51060  \u52629 \u54616  \u54028 \u54000 \u47484  \u51600 \u44592 \u45716  \u49345 \u54889 \u44032 \u51313 \u45180 \u47532  \u49548 \u48149 \u54616 \u44172  \u55184 \u47553  \u50668 \u54665 \u51012  \u46496 \u45208 \u45716  \u49345 \u54889 , \u44536  \u49549 \u50640 \u49436  \u45712 \u45180 \u45716  \u49548 \u49548 \u54620  \u54665 \u48373 \u49548 \u44060 \u54021 \u51060  \u49457 \u44277 \u51201 \u51004 \u47196  \u45149 \u45208 \u49436 , \u54665 \u48373 \u54616 \u44172  \u45936 \u51060 \u53944 \u47484  \u51600 \u44592 \u45716  \u49345 \u54889 \u48152 \u46300 \u49884  \u53360  \u46024 \u51012  \u48268 \u44144 \u45208 , \u45824 \u48149 \u51012  \u53552 \u46888 \u47536 \u45796 \u45716  \u51032 \u48120 \u45716  \u50500 \u45785 \u45768 \u45796 . \u45796 \u47564 , \u50612 \u46500  \u49324 \u44148 \u51004 \u47196  \u51064 \u54644  \'93\u49457 \u52712 \u44048 , \u54665 \u48373 \u44048 , \u49100 \u46319 \u54632 \'94 \u46321  \u51221 \u49888 \u51201 \u51064  \u47564 \u51313 \u44048 \u51012  \u45712 \u45180 \u44172  \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .Comment\u54172 \u53440 \u53364 10\u51060  \u51060  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u47484  \u46916 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u54172 \u53440 \u53364 10\u51060  \u44221 \u51228 \u51201 \u51004 \u47196  \u54413 \u50836 \u47196 \u50868  \u49345 \u53468 \u47484  \u45208 \u53440 \u45240 \u45796 \u47732 , Ten of Cups\u45716  \u51221 \u49888 \u51201 \u51064  \u54413 \u50836 \u47196 \u50880 \u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 . \u52981 \u51008  \'93\u44048 \u51221 \'94\u51012  \u49345 \u51669 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u52628 \u44032 \u47196  \u44536 \u46308 \u51060  \u48512 \u50976 \u54644  \u48372 \u51060 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u44032 \u51313 \u44057 \u51060  \u54200 \u50504 \u54620 10\u52981 \u51008  \u44032 \u51313  \u44057 \u51008  \u54200 \u50504 \u54632 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51200  \u47680 \u47532  \u48372 \u51060 \u45716  \u51665 \u51008  \'93\u50504 \u51221 \u44048 \u44284  \u54200 \u50504 \u54632 , \u49548 \u49549 \u44048 \'94\u51032  \u49345 \u51669 \u51060 \u47728 , \u50500 \u51060  2\u47749 \u51008  \u44536  \u49324 \u46993 \u51032  \u50756 \u49457 \u52404 \u46972 \u44256  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52964 \u54540 \u51060 \u46972 \u47732  \u44208 \u54844 \u51060 \u45208  \u46041 \u44144 \u47484  \u50557 \u49549 \u54616 \u45716  \u49884 \u44592 \u44032  \u46112  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u52981 \u51008  \u52509  10\u44060 \u51077 \u45768 \u45796 . \u44033 \u44033 \u51032  \u52981 \u51008  \u44048 \u51221 \u51012  \u51032 \u48120 \u54616 \u51424 . \u51060 \u45716  \u44263  \'93\u49688 \u47566 \u51008  \u44048 \u51221 \u46308 \u51012  \u44277 \u50976 \u54664 \u45796 \'94\u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50724 \u47000 \u46108  \u48512 \u48512 \u44032  \u45432 \u51012 \u51012  \u48148 \u46972 \u48372 \u47728 , \u50584 \u44592 \u54616 \u45716  \u51109 \u47732 \u51012  \u49345 \u49345 \u54644 \u48372 \u49464 \u50836 . \u46160  \u49324 \u46988 \u51008  \u55148 \u45432 \u50528 \u46973 \u51012  \u54632 \u44760  \u44202 \u51004 \u47728  \u49688 \u47566 \u51008  \u51064 \u49373 \u51012  \u49332 \u50500 \u50772 \u51012  \u44163 \u51077 \u45768 \u45796 .\u52964 \u54540 \u51032  \u44221 \u50864 , \u51060 \u48120  \u49436 \u47196 \u50640  \u45824 \u54620  \u49888 \u47280 , \u50976 \u45824 \u44048 \u51060  \u49939 \u50688 \u44592  \u46412 \u47928 \u50640  \'93\u49345 \u45824 \u44032  \u47924 \u50631 \u51012  \u54616 \u46304  \u48520 \u50504 \u54616 \u51648  \u50506 \u45796 \'94\u45716  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51060 \u48324 \u44284 \u45716  \u44144 \u47532 \u44032  \u47680 \u47728 , \u44208 \u54844 \u44620 \u51648  \u49373 \u44033 \u54624  \u49688  \u51080 \u45716  \u44288 \u44228 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51004 \u47196  \u46321 \u51109 \u54624  \u44221 \u50864 , \u45817 \u49888 \u51012  \u48127 \u44256  \u54200 \u50504 \u54616 \u44172  \u45824 \u54624  \u49688  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50612 \u49353 \u54616 \u51648 \u46020  \u50506 \u49845 \u45768 \u45796 . \u47560 \u52824  \u44032 \u51313 \u52376 \u47100  \u47568 \u51060 \u51424 . \u45796 \u47564  \u48512 \u51221 \u51201 \u51064  \u47732 \u51060  \u51080 \u45796 \u47732 , \'93\u49444 \u47128 \u51060 \u45208  \u46504 \u47548 \'94\u51060  \u50630 \u45796 \u45716  \u51032 \u48120 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52981 10\u51060  \u50612 \u46500  \u51064 \u47932 \u51012  \u44032 \u47532 \u53416 \u45796 \u47732 , \u44032 \u51221 \u51201 \u51060 \u47728  \u51221 \u49888 \u51201 \u51004 \u47196  \u49457 \u49689 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u50644 \u46377 \u52981 10\u51008  \u54788  \u49345 \u54889 \u51060  \u51333 \u47308 \u46104 \u44256 , \u50644 \u46377 \u51012  \u47582 \u51060 \u54620 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51593 , \'93\u54788  \u49345 \u53468 \u50640 \u49436  \u45908  \u51060 \u49345  \u48156 \u51204 \u54616 \u51648  \u50506 \u45716 \u45796 , \u54841 \u51008  \u52376 \u51020 \u48512 \u53552  \u45796 \u49884  \u49884 \u51089 \u54620 \u45796 \'94\u45716  \u46907 \u51060 \u51424 . \u49688 \u48708 \u54617 \u51201 \u51004 \u47196  10\u51008  \u50756 \u49457 \u44284  \u50728 \u51204 \u54632 \u51012  \u49345 \u51669 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u52981 \u51032  \u48176 \u50676 \u46020  \u48736 \u51664 \u50630 \u51060  \u44873 \u44873  \u52292 \u50892 \u51256  \u51080 \u49845 \u45768 \u45796 .\u51060 \u48120  \u47784 \u46304  \u44163 \u51060  \u50756 \u49457 \u46104 \u50612 , \u51648 \u47336 \u54632 \u44284  \u47588 \u45320 \u47532 \u51608 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . 21\u48264  \u49464 \u44228  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :\u45908  \u51060 \u49345  \u48156 \u51204 \u54616 \u51648  \u47803 \u54616 \u44256 , \u52964 \u47532 \u50612 \u44032  \u51221 \u52404 \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50040 \u51012  \u53440 \u44256  \u51080 \u45716 \u45936  \u52981 10\u51060  \u46321 \u51109 \u54664 \u45796 \u47732 , \u51060 \u48120  \u49436 \u47196 \u47484  \'91\u44032 \u51313 \u52376 \u47100 \'92 \u45824 \u54616 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640 , \u49444 \u47128 \u51060  \u45908  \u51060 \u49345  \u45712 \u44788 \u51648 \u51648  \u50506 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50640  \u49457 \u44277 \u54616 \u51648  \u47803 \u54616 \u44256 , \u44536 \u45824 \u47196  \u50644 \u46377 \u51012  \u47582 \u51060 \u54616 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u45817 \u49888 \u51008  \u54980 \u47144 \u54632 \u51012  \u45712 \u45180 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u52981 10 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44032 \u51221 \u51201 \u51060 \u47728  \u54200 \u50504 \u54620  \u48516 \u50948 \u44592 \u47484  \u54413 \u44592 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u54632 \u44760  \u51080 \u51004 \u47732  \u50612 \u49353 \u54616 \u51648  \u50506 \u44256  \u51088 \u50672 \u49828 \u47112  \u54665 \u46041 \u54616 \u44172  \u46121 \u45768 \u45796 . \u54665 \u48373 \u54616 \u44256  \u49324 \u46993  \u47566 \u51008  \u54872 \u44221 \u50640 \u49436  \u51088 \u46992 \u51012  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \'93\u44032 \u51313 \u44288 \u44228 \'94\u47484  \u44361 \u51109 \u55176  \u51473 \u50836 \u49884  \u50668 \u44592 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u44263  \u50896 \u54616 \u45716  \u54028 \u53944 \u45320 \u47484  \u47564 \u45208 \u44172  \u46112  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u49436 \u47196  \u51665 \u52265 \u54616 \u44144 \u45208  \u51032 \u49900 \u54616 \u51648  \u50506 \u44256  \u54200 \u50504 \u54620  \u49324 \u51060 \u44032  \u46121 \u45768 \u45796 . \u44208 \u54844 \u51012  \u49373 \u44033 \u54624  \u47564 \u53372  \u51096  \u47582 \u45716  \u44288 \u44228 \u51077 \u45768 \u45796 . \u51204 \u48152 \u51201 \u51004 \u47196  \u46160  \u48516  \u45796  \u49457 \u49689 \u54620  \u49324 \u46988 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 . \u46608 \u54620  \u48520 \u49692 \u54620  \u47785 \u51201 \u46020  \u50630 \u50612  \u48372 \u51077 \u45768 \u45796 . \u49692 \u49688 \u54620  \u49324 \u46993 \u51060 \u51424 . \'93\u44032 \u51313  \u44057 \u51008  \u46041 \u47308 \'94\u47484  \u47564 \u45208 \u44172  \u46108 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 .\u52981 10\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u54665 \u48373 \u54632 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45817 \u49888 \u44284  \u54632 \u44760 \u54616 \u45716  \u44172  \u44536  \u47924 \u50631 \u48372 \u45796  \u44592 \u49240 \u45796 \u44256  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u51060  \u54665 \u48373 \u51008  \'93\u46160 \u44540 \u45824 \u45716  \u49444 \u47128 \'94\u48372 \u45796 \u45716  \'93\u51600 \u44144 \u50880 \'94\u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \u44208 \u54844 \u44620 \u51648  \u49373 \u44033 \u54616 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45817 \u49888 \u50640 \u44172  \u54200 \u50504 \u54632 \u51012  \u45712 \u45180 \u44256  \u51080 \u51012  \u49104 , \u50672 \u50528  \u44048 \u51221 \u51008  \u50630 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u45128 \u45128 \u54620  \u50976 \u45824 \u44048 \u44284  \u52828 \u48128 \u44048 \u51060  \u49373 \u44220 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44208 \u54844 \u51012  \u49373 \u44033 \u54624  \u47564 \u53372  \u44618 \u51008  \u49324 \u51060 \u44032  \u46104 \u50632 \u49845 \u45768 \u45796 . \u52981 10\u51008  \'93\u44032 \u51313 \'94\u51012  \u45824 \u54364 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . 10\u44060 \u51032  \u52981 \u51008  \u48516 \u45432 , \u49836 \u54548 , \u54665 \u48373 , \u50672 \u48124  \u46321 \u46321  \u48373 \u51105 \u54620  \u44048 \u51221 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51060 \u45716  \u44263  \'91\u54620  \u52964 \u54540 \'92\u47196 \u49436  \u49688 \u47566 \u51008  \u44592 \u48373 \u51012  \u44202 \u50632 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50526 \u51004 \u47196  \u49789 \u44172  \u51060 \u48324 \u54616 \u51648 \u45716  \u50506 \u51012  \u44163 \u51060 \u47728 , \u47924 \u50631 \u48372 \u45796  \u51221 \u49888 \u51201 \u51004 \u47196 \u46020  \u49457 \u49689 \u54644 \u51276 \u51020 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u48152 \u48152 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972  \u53356 \u44172  2\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u51116 \u54924 \u50640  \u49457 \u44277 \u54616 \u44256 , \u45908 \u50865 \u45908  \u45128 \u45128 \u54620  \u49324 \u51060 \u44032  \u46120 \u51012  \u46907 \u54633 \u45768 \u45796 . \u52981 \u51008  \u52509  10\u44060 , \u44536 \u47564 \u53372  \u45817 \u49888 \u44284  \u51221 \u49436 \u51201 \u51064  \u50976 \u45824 \u44048 \u51060  \u47566 \u51060  \u49939 \u50688 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44208 \u54844 \u46020  \u49373 \u44033 \u54616 \u44172  \u46121 \u45768 \u45796 . \u52852 \u46300 \u51032  \u48516 \u50948 \u44592 \u46020  \u54665 \u48373 \u54644  \u48372 \u51077 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u48373 \u51105 \u54620  \u44048 \u51221 (\u52981  10\u44060 )\u51012  \u49549  \u49884 \u50896 \u54616 \u44172  \u53560 \u50612 \u45459 \u51008  \u46244 , \u47560 \u51020 \u51012  \u51221 \u47532 \u54620 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . 10\u51008  \u50644 \u46377 \u51012  \u46907 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51060 \u48264  \u51068 \u47196 , \u45817 \u49888 \u44284  \u49345 \u45824 \u48169 \u51032  \u47560 \u51020  \u47784 \u46160  \u54620 \u52789  \u45908  \u54980 \u47144 \u54624  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u52981 10 \u51649 \u50629 \u50868  \u54644 \u49437 \u51649 \u50629 \u50868 \u50640 \u49436  \u52981 10\u51008  \u47588 \u50864  \u51339 \u51008  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51452 \u48320  \u46041 \u47308 \u50752 \u51032  \u54868 \u54633  \u49104 \u47564  \u50500 \u45768 \u46972 , \u54665 \u48373 \u44048 , \u49100 \u46319 \u54632 , \u47564 \u51313 \u44048 \u51012  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u44592 \u48516  \u51339 \u51008  \u49884 \u44592 \u44032  \u46104 \u44192 \u49845 \u45768 \u45796 .\u53945 \u51221  \u51649 \u50629 \u51012  \u49440 \u53469 \u54644 \u50556  \u54624 \u51648  \u47581 \u49444 \u50688 \u45796 \u47732 , \u45817 \u49888 \u51060  \u49373 \u44033 \u54620  \u51649 \u50629 \u51012  \u53469 \u54616 \u49464 \u50836 . \'93\u54616 \u44592  \u49899 \u51008  \u51068 \'94\u51060  \u50500 \u45768 \u46972 , \u51600 \u44144 \u50868  \'93\u49334 \u51032  \u51068 \u48512 \'94\u44032  \u46112  \u44163 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u49457 \u52712 \u44048 \u44284  \u47564 \u51313  \u47784 \u46160  \u45712 \u45188  \u49688  \u51080 \u45716  \u52380 \u51649 \u51077 \u45768 \u45796 .\u51649 \u50629 \u51201 \u51064  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u51080 \u45796 \u47732 , \u47676  \u48120 \u47000 \u47484  \u48148 \u46972 \u48372 \u44256 , \u44984 \u51456 \u55176  \u54624  \u49688  \u51080 \u45716  \u51649 \u50629 \u51012  \u49440 \u53469 \u54616 \u49464 \u50836 . \u44536 \u44172  \u45817 \u49888 \u51032  \u54665 \u48373  \u49104 \u47564  \u50500 \u45768 \u46972  \u44032 \u51313 \u51032  \u54665 \u48373 \u44620 \u51648  \u48372 \u51109 \u54644 \u51460  \u44163 \u51077 \u45768 \u45796 .\u54633 \u44201 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u47564 \u51313 \u54624 \u47564 \u54620  \u44208 \u44284 \u47484  \u50619 \u44172  \u46108 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u54980 \u47144 \u54632 \u44284  \u49100 \u46319 \u54632 \u51012  \u45712 \u45180 \u44172  \u46112  \u50696 \u51221 \u51077 \u45768 \u45796 . \u45796 \u47564 , \'91\u44061 \u44288 \u51201 \u51064  \u44208 \u44284 \'92\u45716  \u51221 \u54869 \u55176  \u50508  \u49688  \u50630 \u49845 \u45768 \u45796 . \u48372 \u49884 \u45796 \u49884 \u54588  \u45224 \u51088 \u50752  \u50668 \u51088 \u44032  \u48512 \u50976 \u54644  \u48372 \u51060 \u51648 \u45716  \u50506 \u49845 \u45768 \u45796 . \u44592 \u48516 \u44284  \u49345 \u44288  \u50630 \u51060  \u44061 \u44288 \u51201 \u51004 \u47196  \u51339 \u51008  \u44208 \u44284 \u45716  \'91\u54172 \u53440 \u53364  \u52852 \u46300 \'92\u50640 \u49436  \u46321 \u51109 \u54633 \u45768 \u45796 .\u52981 10 \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u51116 \u51221 \u51201 \u51064  \u47785 \u54364  \u49104 \u47564  \u50500 \u45768 \u46972 , \u44060 \u51064 \u51201 \u51064  \u49457 \u52712 \u44048 , \u47564 \u51313 \u46020 \u44032  \u45458 \u51008  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51060 \u45716  \u51452 \u48320  \u49324 \u46988 \u46308 \u44284  \u44288 \u47144 \u51060  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u49548 \u51473 \u55176  \u50668 \u44592 \u49464 \u50836 \u51452 \u48320  \u49324 \u46988 \u46308 \u44284 \u51032  \u50976 \u45824 \u44048 \u51008  \u49334 \u51012  \u51060 \u44200 \u45244  \u49688  \u51080 \u45716  \u55192 \u51077 \u45768 \u45796 . \u44536  \u50976 \u45824 \u44048 \u51012  \u49548 \u51473 \u55176  \u50668 \u44592 \u49464 \u50836 . \u48708 \u47197  \u45817 \u49888 \u51060  \u51068 \u53552 \u50640  \u51080 \u45908 \u46972 \u46020 , \u46041 \u47308 \u50752  \u49345 \u49324 \u47484  \u44032 \u51313 \u52376 \u47100  \u49373 \u44033 \u54644 \u50556  \u54633 \u45768 \u45796 . \'91\u51064 \u44036 \u44288 \u44228 \'92\u45716  \u54665 \u48373 \u50640  \u51080 \u50612  \u45824 \u45800 \u55176  \u51473 \u50836 \u54620  \u50836 \u49548 \u51077 \u45768 \u45796 . \u46024 \u51060  \u50500 \u45768 \u46972  \u51064 \u44036 \u44288 \u44228 \u50640 \u46020  \u45576 \u51012  \u46028 \u47140 \u48372 \u49464 \u50836 .    \
\
   \uc0\u54168 \u51060 \u51648  \u52981  \u51060 \u48120 \u51648 \u54168 \u51060 \u51648  \u52981  \'96 \u49692 \u49688 \u54620  \u47560 \u51020  \u50612 \u47536  \u49548 \u45380 \u51060  \u47932 \u44256 \u44592 \u47484  \u44592 \u49244  \u54364 \u51221 \u51004 \u47196  \u52432 \u45796 \u48372 \u44256  \u51080 \u51020 \u49692 \u49688 \u54620  \u54840 \u44048 \u51060 \u45208  \u49324 \u46993 \u51012  \u51032 \u48120 \u50689 \u44048 \u51060 \u45208  \u50500 \u51060 \u46356 \u50612 \u44032  \u46496 \u50724 \u47476 \u45716  \u49345 \u54889 \u51600 \u44144 \u50868  \u49345 \u49345 \u51012  \u54616 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u44277 \u49345 \u51060 \u45208  \u47581 \u49345  \u46321  \u48512 \u51221 \u51201 \u51064  \u51032 \u48120 \u47196 \u46020  \u50416 \u51076 \u51060 \u48120 \u51648  \u49345 \u51669 \u47932 \u44256 \u44592  : \u49352 \u47196 \u50868  \u50500 \u51060 \u46356 \u50612 , \u52285 \u51032 \u49457 , \u44396 \u52404 \u51201 \u51064  \u47560 \u51020 \u52981  : \u44048 \u51221 , \u47560 \u51020 , \u49324 \u46993 \u54028 \u46020  : \u44048 \u51221 \u51032  \u44592 \u48373 \u50672 \u44867 \u51060  \u44536 \u47140 \u51652  \u50743  : \u49692 \u49688 \u54620  \u49324 \u46993 , \u49692 \u49688 \u54620  \u47560 \u51020 \u47784 \u51088  : \u49352 \u47196 \u50868  \u50500 \u51060 \u46356 \u50612 , \u50689 \u44048 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49692 \u49688 \u54620  \u54840 \u44048 \u51012  \u44032 \u51652 \u54168 \u51060 \u51648  \u52981 \u51008  \u49692 \u49688 \u54620  \u54840 \u44048 \u51012  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u44536 \u44163 \u46020  \u47561 \u50672 \u54620  \u54840 \u44048 \u51060  \u50500 \u45768 \u46972 , \u50612 \u45712  \u51221 \u46020  \u48169 \u54693 \u49457 \u51060  \u51105 \u55180  \u54840 \u44048 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49548 \u45380 \u51008 , \u52981  \u49549 \u50640  \u47932  \u45824 \u49888  \u47932 \u44256 \u44592 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u53440 \u47196 \u50640 \u49436  \u47932 \u51008  \u49373 \u44033 \u51060 \u45208  \u44048 \u51221 \u51012  \u51032 \u48120 \u54616 \u45716 \u45936 \u50836 . \u51060 \u47532 \u51200 \u47532  \u55120 \u47476 \u44256  \u48320 \u54616 \u45912  \u47932 \u51060  \'93\u47932 \u44256 \u44592 \'94\u47196  \u50612 \u46500  \u54805 \u53468 \u47484  \u44054 \u52632  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44263  \u47560 \u51020 \u51060  \u50612 \u45712  \u51221 \u46020  \u51221 \u54644 \u51276 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49548 \u45380 \u51032  \u54364 \u51221 \u51008  \u48157 \u49845 \u45768 \u45796 . \u50500 \u47560  \u49688 \u51469 \u51020 , \u55120 \u47943 \u54632 , \u49100 \u46319 \u54632 , \u49444 \u47128  \u46321 \u51032  \u44048 \u51221 \u51060 \u44192 \u51424 . \u47932 \u44284  \u47932 \u44256 \u44592 \u51032  \u52264 \u51060 \u47484  \u51221 \u47532 \u54644  \u48372 \u50520 \u49845 \u45768 \u45796  :\u47932 \u47932 \u44256 \u44592 \u49345 \u51669 \u54616 \u45716  \u44163 \u47560 \u51020 , \u44048 \u51221 , \u54840 \u44048 , \u49324 \u46993 \u47560 \u51020 , \u44048 \u51221 , \u54840 \u44048 , \u49324 \u46993 \u53945 \u51669 \u44396 \u52404 \u51201 \u51004 \u47196  \u54364 \u54788 \u54616 \u44592  \u55192 \u46310 \u44396 \u52404 \u51201 \u51004 \u47196  \u54364 \u54788 \u54624  \u49688  \u51080 \u51020  \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796  :\u51077 \u45909 \u48512 \u51221 \u44592 \u47484  \u48279 \u50612 \u45208 \u44256 , \u54869 \u49892 \u54616 \u44172  \u51339 \u50500 \u54616 \u45716  \u47560 \u51020 \u51060  \u49373 \u44608 \u47561 \u50672 \u55176  \u44316 \u52270 \u45796 \u44256  \u49373 \u44033 \u54664 \u45796 \u47732 , \u51060 \u51228  \u51339 \u50500 \u54616 \u45716  \u44048 \u51221 \u510