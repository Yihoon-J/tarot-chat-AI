    \uc0\u49548 \u46300 3 \u44536 \u47548 \u51032  \u51032 \u48120 \u49345 \u52376 \u47484  \u48155 \u45716  \u49345 \u54889 \u50808 \u47196 \u50880 \u51012  \u45712 \u45180 \u45716  \u49324 \u46988 \u54872 \u49345 \u51060  \u44648 \u51648 \u44144 \u45208  \u51096 \u47803 \u46108  \u49345 \u54889 \u51060  \u46300 \u47084 \u45208 \u44172  \u46120 \u52395 \u45576 \u50640  \u48152 \u54620  \u49345 \u54889 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54632 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49345 \u52376 \u47484  \u48155 \u45716 \u49548 \u46300 3\u51008  \u49345 \u52376  \u51077 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49345 \u54889 \u51008  \u47749 \u54869 \u54633 \u45768 \u45796 . \u49900 \u51109 \u51032  \u51221 \u51473 \u50521 \u50640  \u44160  3\u44060 \u44032  \u44866 \u54784 \u51080 \u51004 \u47728 , \u48708 \u44396 \u47492 \u51060  \u44032 \u46301  \u45180 \u50612 \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564  \u49548 \u46300 \u45716  \u44033 \u44592  \u45796 \u47480  \u48169 \u54693 \u50640 \u49436  3\u44060 \u45208  \u44866 \u54784 \u51080 \u44592  \u46412 \u47928 \u50640 , \u50500 \u54548 \u51060  \u44284 \u44144 \u50640 \u46020  \u51080 \u50632 \u44256 , \u44536  \u50500 \u54548 \u51012  \u44648 \u45803 \u44172  \u46104 \u45716  \u49884 \u51216 \u51060  \u51648 \u44552 \u51060 \u46972 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52852 \u46300 \u51032  \u54644 \u49437 \u51008  \u47588 \u50864  \u49772 \u50868  \u54200 \u51077 \u45768 \u45796 . \u45436 \u46976 \u51032  \u50668 \u51648  \u50630 \u51060  \'91\u49345 \u52376 \'92\u47484  \u44032 \u47532 \u53412 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u47751  \u44032 \u51648  \u46356 \u53580 \u51068 \u54620  \u49324 \u54637 \u51060  \u51080 \u49845 \u45768 \u45796 .\u51221 \u47568 \u47196  \'91\u53360  \u51068 \u51060  \u51068 \u50612 \u45208 \u45716 \'92 \u49345 \u54889 \u51060  \u50500 \u45776  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49324 \u49548 \u54620  \u47568 \u45796 \u53820 , \u51312 \u44536 \u47560 \u54620  \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u44284 \u44144 \u48512 \u53552  \u49345 \u52376 \u44032  \u51080 \u50632 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732 , \u49345 \u45824 \u48169 \u51032  \u47568 \u53804 \u44032  \u50724 \u47000 \u51204 \u48512 \u53552  \u48324 \u47196 \u50688 \u51648 \u47564  \u50612 \u45712  \u49692 \u44036  \u54869 \u49892 \u55176  \u51064 \u51648 \u54616 \u44172  \u46104 \u50612 , \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u51077 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640 \u49436 \u45716 , \u51060 \u48324  \u54980  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640 \u49436  \u45208 \u50724 \u44592 \u46020  \u54616 \u44256 , \u49324 \u49548 \u54620  \u45796 \u53820 \u51004 \u47196  \u49345 \u52376  \u48155 \u44172  \u46104 \u45716  \u49345 \u54889 , \u51060 \u48324 \u51012  \u50612 \u45712  \u49692 \u44036  \'91\u49892 \u44048 \'92\u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50500 \u47924 \u47532  \u52572 \u49440 \u51012  \u45796 \u54644 \u46020 , \u49324 \u46988 \u51008  \u49345 \u52376  \u51077 \u51012  \u46416  \u50612 \u51796  \u49688  \u50630 \u51060  \u49345 \u52376 \u47484  \u51077 \u44592  \u47560 \u47144 \u51060 \u51648 . \u49345 \u49892 \u51032  \u49884 \u45824  \'96 \u47924 \u46972 \u52852 \u48120  \u54616 \u47336 \u53412 \u54644 \u49437  2. \u50808 \u47196 \u50868 \u49548 \u46300 3\u51008  \u50808 \u47196 \u50868  \u49324 \u46988 , \u54841 \u51008  \u50808 \u47196 \u50880 \u51012  \u45712 \u45180 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732 , \u44032 \u44620 \u50868  \u49884 \u51068  \u45236 \u50640  \u51060 \u48324 \u51012  \u44221 \u54744 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u44160 \u51008  3\u44060 \u48150 \u50640  \u44866 \u54784 \u51080 \u51648  \u50506 \u51004 \u48064 \u47196  \u44536  \u44256 \u53685 \u51008  \'91\u50612 \u45712  \u51221 \u46020  \u52280 \u51012 \u47564 \u54616 \u45796 \'92\u44256  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 .\u44060 \u51064 \u51201 \u51004 \u47196 \u45716 , \'91\u51064 \u44036 \u51032  \u44540 \u50896 \u51201  \u50808 \u47196 \u50880 \'92\u51004 \u47196  \u54644 \u49437 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u50528 \u51064 \u51060  \u51080 \u51648 \u47564  \u50808 \u47196 \u50868  \u44592 \u48516 , \u49324 \u46988 \u46308  \u49324 \u51060 \u50640  \u51080 \u50612 \u46020  \u44277 \u54728 \u54620  \u44592 \u48516  \u47568 \u51060 \u51424 . \u51064 \u44036 \u51008  \u50896 \u47000  \u54844 \u51088  \u49332 \u50500 \u44032 \u45716  \u51316 \u51116 \u51076 \u51012  \u44648 \u45803 \u44172  \u46121 \u45768 \u45796 .\u51064 \u44036 \u51008  \u45572 \u44396 \u45208  \u50808 \u47196 \u50880 \u51012  \u44032 \u51648 \u44256  \u49332 \u50500 \u44049 \u45768 \u45796 . \u45796 \u47564 , \u47800 \u51060  \u54620 \u44032 \u54624  \u46412 , \u54844 \u51088  \u51080 \u51012  \u46412 , \u44536  \u50808 \u47196 \u50880 \u51060  \u48520 \u54788 \u46319  \u48128 \u47140 \u50724 \u45716  \u44163 \u51060 \u51424 . \u49828 \u49828 \u47196 \u47484  \'91\u50808 \u47196 \u50868  \u49324 \u46988 , \u51096 \u47803  \u49332 \u50500 \u44032 \u45716  \u49324 \u46988 \'92\u51004 \u47196  \u45209 \u51064 \u52237 \u51648  \u47560 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 . \u44536 \u48372 \u45796 \u45716  \u54788 \u51116 \u51032  \u51600 \u44144 \u50880 \u50640  \u51665 \u51473 \u54644 \u50556  \u54624  \u46412 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u48176 \u49888  \u45817 \u54616 \u45716 \u49548 \u46300 3\u51008  \u48176 \u49888 \u51012  \u45817 \u54616 \u45716  \u49324 \u46988 , \u54841 \u51008  \u48176 \u49888  \u45817 \u54664 \u45796 \u44256  \u45712 \u45180 \u45716  \u47560 \u51020 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50696 \u47484  \u46308 \u50612 , \u51060 \u48324  \u54980 \u50640  \'91\u48176 \u49888  \u45817 \u54664 \u45796 \'92\u44256  \u45712 \u45180 \u45716  \u49324 \u46988 , \u48127 \u50632 \u45912  \u52828 \u44396 \u50640 \u44172  \u46263 \u45812 \u54868  \u45817 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u44536 \u47196 \u51064 \u54644  \u47560 \u51020 \u51032  \u49345 \u52376 \u47484  \u48155 \u44192 \u49845 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732 , \u44160  3\u44060 \u44032  \u49900 \u51109 \u50640  \u44866 \u54804 \u45716 \u45936  \u48169 \u54693 \u51060  \u45796  \u45796 \u47493 \u45768 \u45796 . \u51593  \u51060  \u47568 \u51008  \u53440 \u51064 \u51060  \u44866 \u50520 \u44256 , \u44536 \u44163 \u46020  \u50668 \u47084  \u49324 \u46988 \u51060  \u44866 \u50520 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u51060 \u50752  \u50976 \u49324 \u54620  \u52852 \u46300 \u47196  \u49548 \u46300  10\u51060  \u51080 \u49845 \u45768 \u45796 . \u49548 \u46300 10\u46020  \u48176 \u49888 \u51012  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u49548 \u46300 10\u51008  \u48376 \u51064 \u51060  \u51096 \u47803 \u54644 \u49436  \u48376 \u51064 \u51060  \u46244 \u53685 \u49688  \u47582 \u45716  \u49345 \u54889 \u51060 \u46972 \u47732 , \u49548 \u46300 3\u51008  \u48376 \u51064 \u51060  \u51096 \u47803 \u54620  \u44172  \u46385 \u55176  \u50630 \u51020 \u50640 \u46020  \u48176 \u49888  \u45817 \u54616 \u45716  \u49345 \u54889 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u45908 \u50865 \u45908  \u49345 \u52376 \u44032  \u53360  \u47784 \u49845 \u51077 \u45768 \u45796 .\u54869 \u51109 \u46108  \u54644 \u49437 \u54644 \u49437  1. \u52395 \u45576 \u50640  \u48152 \u54620 \u49548 \u46300 3\u51008  \u52395 \u45576 \u50640  \u48152 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49548 \u50948  \u49900 \u53237 \u51060 \u46972 \u44256  \u47568 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u49345 \u49324 \u48337 \u51012  \u51032 \u48120 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . (\u51060  \u54644 \u49437 \u51008  \u52964 \u48036 \u45768 \u54000  \u51076 \u49345 \u51012  \u44540 \u44144 \u47196  \u54620  \u45236 \u50857 \u51077 \u45768 \u45796 )\u54644 \u49437  2. \u54872 \u49345 \u51060  \u44648 \u51648 \u45716  / \u54364 \u47732 \u51004 \u47196  \u46300 \u47084 \u45208 \u45716 \u49548 \u46300 3\u51008  \u54872 \u49345 \u51060  \u44648 \u51648 \u44144 \u45208 , \u54364 \u47732 \u51004 \u47196  \u46300 \u47084 \u45208 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . (\u50808 \u44397  \u53440 \u47196  \u54252 \u47100  Aeclectic Tarot\u50640 \u49436  \u48156 \u52684 \u54620  \u45236 \u50857 \u51012  \u44033 \u49353 \u54664 \u49845 \u45768 \u45796 .\u50896 \u48376  \u44172 \u49884 \u47932  \u47553 \u53356 )\u49789 \u44172  \u49444 \u47749 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796 . \u50724 \u47000  \u51204 \u48512 \u53552  \u47800  \u54620 \u44396 \u49437 \u51060  \u44228 \u49549  \u50500 \u54048 \u45796 \u44256  \u44032 \u51221 \u54644 \u48389 \u49884 \u45796 . \u44536 \u51204 \u44620 \u51648 \u45716  \u50896 \u51064 \u51012  \u50508  \u49688  \u50630 \u45912  \u49345 \u54889 \u51060 \u50632 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45912  \u50612 \u45712  \u45216  \u48337 \u50896 \u50640  \u44052 \u45716 \u45936  \u53360  \u48337 \u51060  \u51080 \u45796 \u45716  \u44161 \u45768 \u45796 . \u51312 \u44552 \u47564  \u45908  \u45734 \u50632 \u51004 \u47732  \u51453 \u50632 \u51012  \u44144 \u46972 \u44396 \u50836 . \u45817 \u49888 \u51008  \u53360  \u52649 \u44201 \u51012  \u48155 \u51020 \u44284  \u46041 \u49884 \u50640  \u50504 \u46020 \u44048 \u51060  \u46308 \u50632 \u51012  \u44163 \u51077 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u50896 \u51064 \u51012  \u50508 \u50520 \u51004 \u45768 \u44620 \u50836 . \u51060 \u44172  \u49548 \u46300 3\u51060  \u44032 \u47532 \u53412 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u49345 \u52376 \u47484  \u48155 \u51648 \u47564  \u50896 \u51064 \u46020  \u50508 \u44172  \u46104 \u45716  \u49345 \u54889  \u47568 \u51060 \u51424 . \u50724 \u55176 \u47140  \u50504 \u46020 \u44048 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796  :\u44228 \u50557 \u49436 \u50640  \u47928 \u51228 \u44032  \u51080 \u45796 \u45716  \u49324 \u49892 \u51012  \u49352 \u47213 \u44172  \u50508 \u44172  \u46104 \u44144 \u45208 \u50672 \u51064 \u51032  \u50808 \u46020  \u49324 \u49892 \u51012  \u54869 \u49888 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u50640 \u44172  \u49892 \u47581 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u49548 \u46300 3 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u47560 \u51020 \u51032  \u49345 \u52376 \u44032  \u44032 \u46301 \u54633 \u45768 \u45796 . \u46608 \u54620  \u49324 \u46993 \u50640  \u45824 \u54620  \u50500 \u54548  \u46412 \u47928 \u50640  \'91\u49352 \u47196 \u50868  \u49324 \u46993 \'92\u50640  \u51665 \u51473 \u54616 \u51648  \u47803 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44536 \u50640 \u44172  \u51080 \u50612  \u49324 \u46993 \u51008  \'91\u46160 \u47140 \u50880 \'92\u44536  \u51088 \u52404 \u51077 \u45768 \u45796 . \u45224 \u50640 \u44172  \u51221 \u51012  \u51096  \u48537 \u51060 \u45716  \u47564 \u53372 , \u48176 \u49888  \u45817 \u54664 \u51012  \u46412  \u49892 \u47581 \u46020  \u53356 \u44256  \u49345 \u52376 \u46020  \u50724 \u47000 \u44049 \u45768 \u45796 .\u49548 \u46300 3\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u49345 \u52376 \u47484  \u48155 \u51008  \u46319  \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824  \u51060 \u48324  \u54980  \u48176 \u49888  \u45817 \u54620  \u44592 \u48516 , \u49324 \u46993 \u51060  \u49885 \u51008  \u44163  \u44057 \u50500  \u45712 \u45180 \u45716  \u50500 \u54548 , \u50724 \u47004  \u51677 \u49324 \u46993 \u51004 \u47196  \u51064 \u54644  \u47560 \u51020 \u51060  \u55192 \u46308 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49345 \u45824 \u45716  \'91\u49324 \u46993 \u54616 \u45716 \'92 \u54665 \u46041 \u51012  \u46160 \u47140 \u50892 \u54616 \u45716   \u46319 \u54633 \u45768 \u45796 . \u53944 \u46972 \u50864 \u47560 \u44032  \u49939 \u51060 \u44256  \u49939 \u50668  \u47560 \u51020 \u51032  \u48317 \u51012  \u52824 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49324 \u46993 \u51012  \u54869 \u49888  \u48155 \u51648  \u47803 \u54620 \u45796 \u47732 (\u50696 \u52968 \u45824  \u48128 \u45817 ), \u49345 \u45824 \u48169 \u51012  \'91\u48176 \u49888 \u51088 \'92\u47196  \u45209 \u51064  \u52237 \u51012  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51060 \u46972 \u47732 , \u52395 \u45576 \u50640  \u48152 \u54664 \u44144 \u45208 , \u50756 \u51204 \u55176  \u44866 \u54784 \u48260 \u47536  \u47560 \u51020 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u51312 \u47564 \u44036  \u44040 \u46321 \u51060 \u45208  \u49884 \u47144 \u51012  \u44202 \u44172  \u46112  \u44163  \u