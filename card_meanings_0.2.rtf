   4. \uc0\u54889 \u51228 The Emperor \u54889 \u51228  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u51204 \u51137 \u53552 \u50640  \u51649 \u51217  \u45208 \u50752  \u51452 \u48320 \u51012  \u44048 \u49884 \u54616 \u45716  \u51473 \u44428 \u50948  \u51080 \u44256 , \u45224 \u49457 \u49457 \u51060  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u49345 \u51669 \u54217 \u49548  \u48520 \u50504 \u44048 \u44284  \u49828 \u53944 \u47112 \u49828 \u50640  \u49884 \u45804 \u47532 \u45716  \u49345 \u53468 \u49345 \u45824 \u48169 \u51012  \u53685 \u51228 \u54616 \u47140 \u45716  \u49900 \u47532 \u52293 \u51076 \u44048 \u51060  \u46832 \u50612 \u45212  \u49324 \u46988  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44428 \u50948  \u51080 \u45716 \u54889 \u51228  \u52852 \u46300 \u45716  \u44428 \u50948  \u51080 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51649 \u50629 \u50868 \u51060  \u51339 \u51008  \u52852 \u46300  \u51473  \u54616 \u45208  \u51077 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u48744 \u44036 \u49353 \u51008  \u44428 \u50948 \u50752  \u45733 \u47141 \u51012  \u49345 \u51669 \u54616 \u45716  \u45824 \u54364 \u51201 \u51064  \u49353 \u44628 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u54889 \u51228 \u46020  \u48744 \u44053  \u50743 \u51004 \u47196  \u44536 \u51032  \u44428 \u50948 \u47484  \u45208 \u53440 \u45236 \u44256  \u51080 \u49845 \u45768 \u45796 . \u47532 \u46377 \u50640 \u49436 \u45716  \u44396 \u52404 \u51201 \u51004 \u47196  :\u49324 \u54924 \u51201 \u51004 \u47196  \u45458 \u51008  \u50948 \u52824 \u50640  \u51080 \u44256 , \u49457 \u44277 \u54620  \u49324 \u46988 \u51012  \u45824 \u48320 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51649 \u50629 \u50640 \u49436 \u46020  \u45224 \u51012  \u47532 \u46300 \u54616 \u45716  \u51649 \u50948 \u50640  \u50724 \u47476 \u44144 \u45208 , \u46041 \u47308 \u50640 \u44172  \u51064 \u51221 \u48155 \u45716  \u51068 \u51060  \u49373 \u44609 \u45768 \u45796 .\u52852 \u47532 \u49828 \u47560  \u51080 \u44256 , \u50612 \u47480 \u49828 \u47084 \u50868  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u47532 \u46300 \u54616 \u45716  \u50672 \u50528 \u50640  \u51061 \u49689 \u54616 \u44144 \u45208 , \u49892 \u51228 \u47196  \u50672 \u49345 \u51068  \u49688  \u51080 \u49845 \u45768 \u45796 .\u48376 \u51064 \u51060  \'91\u44049 \'92\u51060 \u46972 \u45716  \u51064 \u49885 \u46020  \u50612 \u45712  \u51221 \u46020  \u51080 \u44592 \u50640 , \u51088 \u51316 \u49900 \u51060  \u49464 \u44144 \u45208 , \'91\u49345 \u45824 \u48169 \u48372 \u45796  \u50948 \u50640  \u51080 \u45796 \'92\u45716  \u50864 \u50900 \u44048 \u51012  \u44032 \u51656  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u50672 \u50528 \u50640 \u49436 \u46020  \u47560 \u52268 \u44032 \u51648 . \u50672 \u50528 \u50640 \u49436 \u46020  \u49345 \u45824 \u48169 \u51012  \'91\u45572 \u47476 \u47140 \'92 \u54616 \u47728 , \u44032 \u45140 \u50473  \u46021 \u49440 \u51201 \u51004 \u47196  \u44404  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49688 \u51649 \u51201 \u51064  \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u52293 \u51076 \u44048  \u51080 \u45716 \u54889 \u51228  \u52852 \u46300 \u45716  \u52293 \u51076 \u44048  \u51080 \u45716  \u49324 \u46988  \u54841 \u51008  \u44288 \u44228 \u50640  \u52293 \u51076 \u44048 \u51012  \u45712 \u45180 \u45716  \u49900 \u47532 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45824 \u54364 \u51201 \u51004 \u47196 , \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u45716  \u52964 \u54540 \u50640 \u44172  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008 (\u48372 \u53685  \u45224 \u51088 ) \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u51068 \u51333 \u51032  \'91\u51032 \u47924 \'92\u46972 \u44256  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51088 \u49888 \u51032  \u50528 \u51064 \u51012  \'91\u51088 \u49885 \'92\u52376 \u47100  \u52293 \u51076 \u51256 \u50556  \u54620 \u45796 \u45716  \u49373 \u44033 \u51060 \u51424 . \u44288 \u44228 \u47484  \u44032 \u48333 \u44172  \u50668 \u44592 \u51648  \u50506 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u49345 \u45824 \u48169 \u51012  \u44208 \u54844  \u49345 \u45824 \u47196  \u49373 \u44033 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u49828 \u53944 \u47112 \u49828  \u48155 \u45716  \u51068 \u51060  \u51080 \u50612 \u46020 , \u55192 \u46304  \u45236 \u49353  \u50630 \u51060  \u44032 \u51313 \u51012  \u52293 \u51076 \u51648 \u47140 \u45716  \u47560 \u51064 \u46300 \u44032  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51060 \u46412 , \u50948 \u47196 \u45208  \u44277 \u44048 \u52376 \u47100  \u51221 \u49888 \u51201 \u51064  \u51648 \u50896 \u48372 \u45796 \u45716  \u47932 \u51656 \u51201 \u51064  \u51648 \u50896 \u51012  \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u45208  \u55192 \u46308 \u50612 . \u44536 \u47000 ? \u46024  \u51460 \u53580 \u45768 \u44620  \u47579 \u51080 \u45716  \u44144  \u49324 \u47673 \u50612 .\u51649 \u50629 \u50640 \u49436 \u46020 , \u48376 \u51064 \u51060  \u47585 \u51008  \u50629 \u47924 \u45716  \'91\u45149 \u44620 \u51648  \u52293 \u51076 \u51648 \u44256  \u54616 \u45716  \u44221 \u54693 \'92\u51060  \u51080 \u49845 \u45768 \u45796 . \'91\u51452 \u51064  \u51032 \u49885 \'92 \u51004 \u47196  \u48148 \u45012  \u47568  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u54217 \u54032 \u46020  \u51339 \u44256  \u49849 \u51652 \u46020  \u48736 \u47480  \u54200 \u51077 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48520 \u50504 \u44048 \u51012  \u45712 \u45180 \u45716 \u53440 \u47196  \u54889 \u51228 \u45716  \u48520 \u50504 \u44048 \u51012  \u45712 \u45180 \u44144 \u45208 , \u45224 \u51012  \u48127 \u51648  \u50506 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54889 \u51228 \u45716  \u49552 \u49892 \u50640  \u45824 \u54620  \u48520 \u50504 \u44048 \u51060  \u53360  \u49324 \u46988 \u51077 \u45768 \u45796 . \u51088 \u49888 \u51060  \u44032 \u51652  \u51648 \u50948 , \u51116 \u49328 \u51012  \u51075 \u51648  \u50506 \u46020 \u47197  \u54637 \u49345  \u49888 \u44221 \u51012  \u44260 \u46160 \u49464 \u50892 \u50556  \u54616 \u51424 . \u48152 \u50669 \u51088 \u45716  \u50630 \u45716 \u51648 , \u51201 \u51008  \u50504  \u52432 \u46308 \u50612 \u50724 \u45716 \u51648 \'85\u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u54637 \u49345  \u51068 \u50640  \u49884 \u45804 \u47532 \u47728  \u49457 \u44284 \u47484  \u44145 \u51221 \u54616 \u45716  \'91\u51076 \u50896 \u51060 \u45208  \u44256 \u50948 \u51649 , \u49324 \u50629 \u44032 \'92\u50640 \u44172 \u49436  \u45208 \u53440 \u45208 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50756 \u48317 \u51452 \u51032  \u49457 \u54693 \u51060  \u51080 \u50612 \u49436 , \u51312 \u44552 \u47564  \u51068 \u51060  \u50612 \u44555 \u45208 \u46020  \u48520 \u50504 \u44048 \u51012  \u45712 \u45197 \u45768 \u45796 .\u50672 \u51064 \u50640  \u45824 \u54644  \u45130 \u51076 \u50630 \u51060  \u51032 \u49900 \u54616 \u44592 \u46020  \u54616 \u47728 , \u53945 \u51221  \u49324 \u44148 \u51012  \u48380  \u46412  \'91\u48708 \u44288 \u51201 \u51004 \u47196  \u48372 \u45716  \u44221 \u54693 \'92\u51060  \u51080 \u49845 \u45768 \u45796 .\u50672 \u51064 \u44284  \u54756 \u50612 \u51648 \u45716  \u44163 \u51012  \u44537 \u46020 \u47196  \u48520 \u50504 \u54644  \u54633 \u45768 \u45796 . \u44169 \u51004 \u47196 \u45716  \u44540 \u50628 \u54620  \u52377 \u51012  \u54644 \u46020 , \u49324 \u49892  \u50672 \u51064 \u51012  \u50628 \u52397  \u50500 \u45180 \u45716  \u51473 \u51077 \u45768 \u45796 .\u54840 \u44048  \u51080 \u45716  \u49345 \u45824 \u44032  \u46496 \u45208 \u44032 \u51652  \u50506 \u51012 \u44620 , \u45796 \u47480  \u49324 \u46988 \u44284  \u47564 \u45208 \u51648 \u45716  \u50506 \u51012 \u44620  \u54637 \u49345  \u48520 \u50504 \u54644  \u54633 \u45768 \u45796 . \u51665 \u52265 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 .\u47784 \u54744 \u51012  \u44048 \u49688 \u54616 \u44592 \u48372 \u45796  \u48372 \u49688 \u51201 \u51064  \u49440 \u53469 \u51012  \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u47532 \u49828 \u53356 \u44032  \u53360  \u53804 \u51088 \u48372 \u45796 \u45716 , \u51200 \u52629 \u51060 \u45208  \u50696 \u44552 \u51012  \u49440 \u54840 \u54616 \u45716  \u54200 \u51077 \u45768 \u45796 . \u51649 \u50629 \u50868 \u50640 \u49436  \u54889 \u51228  \u52852 \u46300 \u45716 , \u47588 \u50864  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54616 \u51648 \u47564 , \'91\u50619 \u45716  \u47564 \u53372  \u48520 \u50504 \u44048 \u46020  \u52964 \u51652 \u45796 \'92\u45716  \u45800 \u51216 \u51060  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u53685 \u51228 \u54616 \u47140 \u45716 \u54889 \u51228  \u52852 \u46300 \u45716  \u51088 \u49888  \u46907 \u45824 \u47196  \u53685 \u51228 \u54616 \u47140 \u45716  \u49900 \u47532 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \'91\u53685 \u51228 \u54616 \u45716  \u44163 \'92\u50640  \u51665 \u52265 \u54616 \u45716  \u49324 \u46988 , \u50689 \u50612 \u47196 \u45716  Control Freak\u51012  \u44032 \u47532 \u53420  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54889 \u51228 \u45716  \u51452 \u48320 \u51012  \u55184 \u45140  \u44144 \u47532 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51088 \u49888 \u51032  \u48512 \u54616 \u46308 \u51060  \u51096  \u51068 \u54616 \u44256  \u51080 \u45716 \u51648  \'91\u44048 \u49884 \'92\u54616 \u45716  \u44163 \u52376 \u47100  \u48372 \u51077 \u45768 \u45796 . \u50696 \u52968 \u45824  \u47532 \u46377 \u50640 \u49436 \u45716  :\u51088 \u44592  \u51473 \u49900 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u44284  \u50504  \u47582 \u45716  \u48512 \u48516 \u51060  \u51080 \u45796 \u47732 , \u48376 \u51064 \u51012  \u44256 \u52824 \u44592 \u48372 \u45796  \u49345 \u45824 \u48169 \u51012  \u44256 \u52824 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \'91\u54253 \u44400 \'92\u52376 \u47100  \u44404  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50672 \u46973 \u51012  \u44053 \u50836 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45572 \u44396 \u50752  \u50612 \u46356 \u47484  \u44032 \u45716 \u51648  \u51068 \u51068 \u51060  \u48372 \u44256  \u48155 \u50500 \u50556  \u54633 \u45768 \u45796 .\u47924 \u50616 \u44032 \u47484  \u51648 \u51201 \u54616 \u44256 , \u44032 \u47476 \u52824 \u47140 \u45716  \u49457 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u44256 \u51665 \u51060  \u49472  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u48376 \u51064  \u49373 \u44033 \u51060  \u45149 \u44620 \u51648  \u47582 \u45796 \u44256  \u49373 \u44033 \u54616 \u50668 , \u51096 \u47803 \u51012  \u51064 \u51221 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 .\'91\u48128 \u45817 \'92\u51012  \u54616 \u45716  \u49345 \u45824 \u48169 \u50640 \u44172  \u48516 \u45432 \u47484  \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51088 \u49888 \u51032  \u46907 \u45824 \u47196  \u46104 \u51648  \u50506 \u45716 \u45796 \u44256  \u49373 \u44033 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u44172 \u49884 \u47932 \u51060 \u45208  \u54532 \u47196 \u54596 \u51012  \'91\u44048 \u49884 \'92\u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54889 \u51228 \u44032  \u44032 \u51652  \u44428 \u47141 \u44284  \u51648 \u50948 \u45716  \u44536 \u44032  \u49828 \u49828 \u47196  \u51137 \u52712 \u54644  \u50728  \u44163 \u51077 \u45768 \u45796 . \'91\u51088 \u49688 \u49457 \u44032 \'92\u46972 \u44256  \u54624  \u49688  \u51080 \u51424 . \u44536 \u47532 \u44256  \u44536 \u45716  \u51088 \u49888 \u51060  \u51648 \u44552 \u44620 \u51648  \u51060 \u47364 \u50772 \u45912  \u44163 \u46308 \u51012 , \'91\u51648 \u53412 \u44256 , \u46608  \u50976 \u51648 \'92\u54616 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u51593 , \'91\u53685 \u51228 \u54616 \u47140  \u54620 \u45796 \'92\u45716  \u46907 \u51077 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732 , \u54889 \u51228 \u44032  \u54889 \u44552  \u44396 \u49836 \u51012  \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \'91\u48372 \u51452 \'92\u46972 \u44256  \u48520 \u47549 \u45768 \u45796 . \u51068 \u48152 \u51201 \u51064  \u48372 \u51452 \u50640 \u45716  \u49901 \u51088 \u44032 \u44032  \u51080 \u50612 , \'91\u49888 \u51004 \u47196 \u48512 \u53552  \u45236 \u47140 \u50728  \u44428 \u47141 \'92\u51012  \u49345 \u51669 \u54616 \u51648 \u47564 , \u51060 \u48120 \u51648  \u49549  \u48372 \u51452 \u50640 \u45716  \u49901 \u51088 \u44032 \u44032  \u50630 \u49845 \u45768 \u45796 . \u51060 \u45716  \u44263  \'91\u51088 \u49688 \u49457 \u44032 \'92\u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u50997 \u53685 \u49457  \u50630 \u51060  \u47784 \u46304  \u48512 \u48516 \u51012  \u53685 \u51228 \u54616 \u47140  \u46308  \u49688  \u51080 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51032  \u51068 \u51008  \u45824 \u52649  \u54616 \u45716  \u48277 \u51060  \u50630 \u49845 \u45768 \u45796 . \u49324 \u49548 \u54620  \u46356 \u53580 \u51068 \u50640  \u51665 \u52265 \u54616 \u45796  \u48372 \u45768 , \u49828 \u53944 \u47112 \u49828 \u44032  \u49900 \u54620  \u49884 \u44592 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .  \u47560 \u51020 \u51032  \u50668 \u50976 \u50752  \u44288 \u45824 \u54632 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u54889 \u51228  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u50528 \u51221  \u54364 \u54788 \u51060  \u49436 \u53812 \u47084  \u50672 \u50528 \u50640  \u45231 \u49440  \u49324 \u46988 \u51068  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u45224 \u51088 \u46972 \u47732 , \u45224 \u51473  \u45224 \u44256  \u44277 \u45824  \u44400 \u45824  \u53580 \u53356 \u47484  \u48159 \u51008  \u49324 \u46988 . \u50668 \u51088 \u46972 \u47732 , \u47924 \u46749 \u46749 \u54616 \u44256  \u51312 \u50857 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50672 \u50528 \u50640  \u51080 \u50612  \u49345 \u45824 \u48169 \u51012  \'91\u53685 \u51228 \u54616 \u47140 \u45716  \u49900 \u47532 \'92\u44032  \u51080 \u49845 \u45768 \u45796 . \u50672 \u51064 \u51012  \'91\u50500 \u47000 \u47196  \u48372 \u44256 \'92 \u44032 \u45140  \u46021 \u49440 \u51201 \u51004 \u47196  \u44404  \u49688 \u46020  \u51080 \u51648 \u47564 , \u45236  \u49324 \u46988 \u50640 \u44172 \u45716  \u51032 \u47924 \u50752  \u52293 \u51076 \u51012  \u45796 \u54616 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u44048 \u51221  \u54364 \u54788 \u50640  \u50612 \u47140 \u50880 \u51012  \u44202 \u44256 , \u44277 \u44048 \u47141 \u46020  \u48512 \u51313 \u54620  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u49912 \u50912 \u51012  \u46412 \u46020  \u48120 \u50504 \u54620  \u44048 \u51221 \u51008  \u51080 \u51004 \u45208 , \u51077 \u51004 \u47196  \u49324 \u44284 \u54616 \u51648  \u47803 \u54616 \u45716  \u49828 \u53440 \u51068 . \u51008 \u44540 \u49836 \u51789  \u52313 \u44200 \u51452 \u45716  \u52772 \u45936 \u47112 \u51201  \u47732 \u47784 \u46020  \u50687 \u48372 \u51077 \u45768 \u45796 . \u50508 \u44256 \u48372 \u47732  \u47560 \u51020 \u51060  \u50668 \u47532 \u50668 \u47532 \u54620  \u49324 \u46988 \u51077 \u45768 \u45796 . \u47924 \u50631 \u51060 \u46304  \u54844 \u51088  \u52293 \u51076 \u51648 \u47140 \u45716  \u47784 \u49845 \u51012  \u48372 \u51060 \u44592 \u46020  \u54616 \u45716 \u45936 , \u50696 \u52968 \u45824  \u49885 \u45817 \u51012  \u54844 \u51088  \u50508 \u50500 \u48376 \u45796 \u44144 \u45208 , \u50672 \u51064 \u51032  \u44148 \u44053 \u51012  \u45824 \u49888  \u52404 \u53356 \u54616 \u45716  \u51068 \u51077 \u45768 \u45796 . \u44536 \u46041 \u50504  \u55192 \u46308 \u44172  \u49332 \u50500 \u50728  \u48372 \u49345  \u49900 \u47532 , \u54844 \u51088  \u45224 \u44200 \u51656  \u46160 \u47140 \u50880  \u46412 \u47928 \u50640  \u51032 \u49900 \u48337 \u51060 \u45208  \u51665 \u52265 \u51060  \u51080 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u45800 \u51216 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u48157 \u51008  \u48516 \u50948 \u44592 \u50752 \u51032  \u49324 \u46988 \u44284  \u44417 \u54633 \u51060  \u51339 \u51004 \u47728 , \'91\u48376 \u51064 \u51012  \u51096  \u49436 \u54252 \u53944  \u54644 \u51460  \u49688  \u51080 \u45716  \u49324 \u46988 \'92\u44284  \u50612 \u50872 \u47549 \u45768 \u45796 .\u54889 \u51228  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51012  \u53685 \u51228 \u54616 \u44144 \u45208 , \u50864 \u50948 \u50640  \u49436 \u44256  \u49910 \u51008  \u47560 \u51020 \u51060  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u45796 \u47480  \u51060 \u49457 \u51012  \u47803  \u47564 \u45208 \u44172  \u54616 \u44144 \u45208 , \u50672 \u46973 \u51012  \'91\u48372 \u44256 \'92 \u48155 \u50500 \u50556  \u54620 \u45796 \u45716  \u49373 \u44033 \u51077 \u45768 \u45796 . \u44169 \u51004 \u47196  \u45236 \u49353 \u51008  \u50504  \u54616 \u51648 \u47564 , \u45817 \u49888 \u51012  \u51075 \u45716  \u44163 \u50640  \u45824 \u54620  \'91\u46160 \u47140 \u50880 \'92\u51060  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u44536 \u44163  \u46412 \u47928 \u50640  \u51032 \u49900 \u44284  \u48520 \u50504 \u51060  \u52964 \u51656  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u51208 \u45824  \u44032 \u48333 \u44172  \u50668 \u44592 \u51648  \u50506 \u49845 \u45768 \u45796 . \u52293 \u51076 \u44048 \u44284  \u48512 \u45812 \u44048 \u51012  \u46041 \u49884 \u50640  \u45712 \u45180 \u47728  \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u48376 \u51064 \u51060  \'91\u50948 \u50640  \u51080 \u45796 \'92\u44256  \u49373 \u44033 \u54616 \u45716  \u46319  \u54633 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u45817 \u49888 \u51012  \u47532 \u46300 \u54644 \u50556  \u54616 \u47728 , \u52293 \u51076 \u51256 \u50556  \u54620 \u45796 \u44256  \u49373 \u44033 \u54633 \u45768 \u45796 . \u54000 \u45716  \u50504  \u45236 \u51648 \u47564 , \u45817 \u49888 \u51060  \u46496 \u45208 \u44040 \u44620 \u48400  \u54637 \u49345  \u48520 \u50504 \u54620  \u47560 \u51020 \u49104 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u47924 \u50631 \u51012  \u54616 \u44256  \u45796 \u45768 \u45716 \u51648  \'91\u50684 \u53456 \'92\u54624  \u44032 \u45733 \u49457 \u51060  \u45458 \u51004 \u47728 , \u45236  \u44163 \u51004 \u47196  \u47564 \u46308 \u44256  \u49910 \u51008  \u49548 \u50976 \u50837  \u46608 \u54620  \u45712 \u45180 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u50672 \u46973 \u51012  \u54616 \u51648  \u50506 \u50500 \u46020 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \'91\u45236 \u49353 \'92\u51008  \u50504  \u54616 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u44536 \u51032  \u47560 \u51020  \u49549 \u51008  \u48520 \u50504 \u44284  \u51032 \u49900  \u44536  \u51088 \u52404 \u51077 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u44208 \u54844 \u51012  \u49373 \u44033 \u54616 \u44592  \u51339 \u51008  \u44288 \u44228 \u51077 \u45768 \u45796 . \u49436 \u47196  \u44033 \u51088 \u51032  \u52293 \u51076 \u51012  \u45796 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564  \u50672 \u50528 \u44032  \u51648 \u47336 \u54644 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50528 \u51221  \u54364 \u54788 \u46020  \u51201 \u44256 , \u49345 \u45824 \u48169 \u51012  \u51068 \u51333 \u51032  \'91\u48708 \u51592 \u45768 \u49828  \u44288 \u44228 \'92\u52376 \u47100  \u46385 \u46385 \u54616 \u44172  \u45824 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44048 \u51221  \u44368 \u47448 , \u44536 \u47532 \u44256  \u49548 \u53685 \u46020  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 . \u50672 \u51064  \u49324 \u51060 \u44032  \u51068 \u51333 \u51032  \'91\u44049 \u51012  \u44288 \u44228 \'92\u47196  \u48320 \u51656 \u46112  \u49688  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45320  \u50612 \u46356  \u44052 \u50612 ? \u50780  \u50584 \u44592  \u50504  \u54644 ? \u54841 \u51008  \u50780  \u45208 \u54620 \u53580  \u54728 \u46973  \u50504  \u47585 \u50520 \u50612 ? \u49324 \u46993 \u51008  \u48176 \u47140 \u50640 \u49436  \u52636 \u48156 \u54620 \u45796 \u45716  \u44163 \u51012  \u51060 \u54644 \u54644 \u50556  \u54616 \u47728 , \u45817 \u48516 \u44036  \u49353 \u45796 \u47480  \u45936 \u51060 \u53944 \u44032  \u54596 \u50836 \u54644  \u48372 \u51064 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436  \u54889 \u51228  \u52852 \u46300 \u45716  \u45208 \u49240 \u44172  \u51089 \u50857 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44032 \u51109  \u53360  \u47928 \u51228 \u51216 \u51008  \'91\u51088 \u51316 \u49900 \u44284  \u44256 \u51665 \'92\u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \'91\u51652 \u51676 \u47196 \'92 \u45817 \u49888 \u51060  \u46496 \u45208  \u44040  \u44620 \u48400  \u54637 \u49345  \u48520 \u50504 \u54644 \u54616 \u51648 \u47564 , \u51088 \u51316 \u49900  \u46412 \u47928 \u50640  \u50672 \u46973 \u54616 \u51648  \u50506 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u49549 \u51004 \u47196 \u45716  \u50808 \u47196 \u50880 \u51012  \u49900 \u54616 \u44172  \u45712 \u45180 \u44256  \u51080 \u49845 \u45768 \u45796 .\u54889 \u51228  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u53360  \u49457 \u44277 \u51012  \u44144 \u46160 \u47728 , \u48376 \u51064 \u51032  \u51060 \u47492 \u51012  \u50508 \u47532 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u44256 \u50948 \u51649 \u50640  \u50724 \u47476 \u44144 \u45208 , \u51088 \u49888 \u51032  \u50948 \u52824 \u47484  \u44277 \u44256 \u55176  \u54624  \u44592 \u54924 \u44032  \u49373 \u44609 \u45768 \u45796 . \u45796 \u47564 , \u50619 \u45716  \u44163 \u51060  \u51080 \u51004 \u47732  \u51075 \u45716  \u44163 \u46020  \u51080 \u45716  \u48277 . \u45458 \u51008  \u50948 \u52824 \u50640  \u50732 \u46972 \u44032 \u45716  \u47564 \u53372 , \u49828 \u53944 \u47112 \u49828 \u50752  \u50517 \u48149 \u46020  \u49900 \u54644 \u51656  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620 , \u48376 \u51064 \u46020  \u47784 \u47476 \u44172  \u51649 \u51109  \u45236 \u50640 \u49436  \u44256 \u50517 \u51201 \u51064  \u48516 \u50948 \u44592 \u47484  \u47564 \u46308  \u49688 \u46020  \u51080 \u51004 \u45768 , \'91\u44848 \u45824 \'92\u44032  \u46104 \u51648  \u50506 \u46020 \u47197  \u51312 \u49900 \u54644 \u50556 \u44192 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u53945 \u55176  \'91\u44397 \u44032 \u44032  \u51452 \u44288 \u54616 \u45716  \u49884 \u54744 \'92\u50640 \u49436  \u51339 \u51008  \u49457 \u44284 \u44032  \u51080 \u51012  \u50696 \u51221 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u49688 \u45733 , \u44277 \u47924 \u50896  \u49884 \u54744 , \u44277 \u44592 \u50629  \u47732 \u51217  \u46321 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u47784 \u46304  \u44163 \u51012  \u50756 \u48317 \u55176  \u51456 \u48708 \u54644 \u50556  \u54620 \u45796 \u45716  \u49373 \u44033 \u50640  \u49828 \u53944 \u47112 \u49828 \u44032  \u49900 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54217 \u49548  \u49828 \u53944 \u47112 \u49828  \u44288 \u47532 \u50640  \u55192 \u50024 \u50556 \u44192 \u49845 \u45768 \u45796 .\u54889 \u51228  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u46024 \u51012  \u50500 \u44788 \u50556  \u54616 \u44144 \u45208 , \u49324 \u50629 \u52404 \u47484  \u50868 \u50689 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u50640  \u51339 \u51008  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51064  \u53412 \u50892 \u46300 \u45716  \'91\u53685 \u51228 \'92\u51077 \u45768 \u45796 . \u46308 \u50612 \u50724 \u45716  \u46024 \u44284  \u45208 \u44032 \u45716  \u46024 \u51012  \u50628 \u44201 \u54616 \u44172  \u53685 \u51228 \u54624  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u46024 \u51060  \u48512 \u51313 \u54644 \u51648 \u45716  \u51068 \u46020  \u50630 \u44256 , \u49324 \u52824 \u47484  \u48512 \u47532 \u45716  \u51068 \u46020  \u50630 \u51012  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u45320 \u47924  \u51221  \u50630 \u51060  \u44404  \u49688  \u51080 \u51004 \u45768 , \u48288 \u54400 \u50612 \u50556  \u54620 \u45796 \u45716  \u49888 \u54840 . \u51328  \u45908  \u51201 \u44537 \u51201 \u51004 \u47196  \u50880 \u51649 \u51060 \u47728 , \u53804 \u51088 \u52376 \u47484  \u52286 \u50500 \u48400 \u50556  \u54620 \u45796 \u45716  \u47700 \u49884 \u51648 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u51088 \u49888 \u44048 \u51012  \u44032 \u51648 \u44256  \u51201 \u44537 \u51201 \u51004 \u47196  \u45432 \u47141 \u54616 \u49464 \u50836 \u49345 \u45824 \u48169 \u51012  \u49548 \u44537 \u51201 \u51004 \u47196  \u45824 \u54616 \u44256 , \u50612 \u46360 \u44032  \u48520 \u50504 \u54644  \u54616 \u44256  \u51080 \u45716  \u44148  \u50500 \u45776 \u51648 \u50836 . \u45576 \u52824 \u47564  \u48372 \u44256  \u51080 \u50632 \u45796 \u47732  \u51060 \u51228 \u45716  \u45208 \u49436 \u49436  \u45796 \u44032 \u44032 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51068 \u50640 \u49436 \u46020  \u47560 \u52268 \u44032 \u51648 . \u51648 \u44552 \u48372 \u45796  \u45908  \u51032 \u50837 \u51201 \u51004 \u47196  \u51068 \u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u51312 \u50616  2. \u49345 \u45824 \u48169 \u44284  \u50612 \u50872 \u47532 \u49464 \u50836 \u45817 \u49888 \u51060  \u49345 \u45824 \u48169 \u50640 \u44172  \u47582 \u52656 \u50556  \u54616 \u45716 \u51648 , \u50500 \u45768 \u47732  \u51201 \u44537 \u51201 \u51004 \u47196  \u51060 \u45132 \u50612 \u50556  \u54616 \u45716 \u51648  \u50508 \u50500 \u45244  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 . \u51088 \u44984 \u47564  \u51032 \u44204  \u52649 \u46028 \u51060  \u49373 \u44596 \u45796 \u47732  \u51312 \u44552 \u51008  \u45236 \u47140 \u45459 \u51012  \u54596 \u50836 \u44032  \u51080 \u44192 \u49845 \u45768 \u45796 . \'93\u48176 \u47140 \u54616 \u45716  \u47560 \u51020 \'94\u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648  \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .    \
\
   5. \uc0\u44368 \u54889 The Hierophant \u44368 \u54889  \u52852 \u46300  \u44536 \u47548 \u51032  \u51032 \u48120 \u44368 \u54889 \u51060  \u44040 \u46321 \u51012  \u51473 \u51116 \u54644 \u51452 \u45716  \u47784 \u49845 \u48127 \u51012  \u49688  \u51080 \u45716  \u49324 \u46988 , \u47704 \u53664  \u44057 \u51008  \u49324 \u46988 \u51012  \u45208 \u53440 \u45252 \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u48155 \u45716  \u44277 \u51201 \u51064  \u44288 \u44228 \u47484  \u49345 \u51669 \u44032 \u45140  \u44848 \u45824 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632  \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48127 \u51012  \u49688  \u51080 \u45716 \u44368 \u54889  \u52852 \u46300 \u45716  \u48127 \u51012  \u49688  \u51080 \u45716  \u44288 \u44228 \u50640 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u46608 \u54620  \'91\u51221 \u49888 \u51201  \u51648 \u51452 \'92\u45208  \'91\u50696 \u52769 \u51060  \u49772 \u50892  \u51032 \u49900  \u44032 \u51648  \u50506 \u45716  \u49324 \u46988 \'92\u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732 , \u50724 \u47004  \u44592 \u44036  \u50672 \u46973 \u51060  \u45130 \u44200 \u46020  \u51060  \u49324 \u46988 \u51008  \u51648 \u44552 \u52196  \u51060 \u44152  \u54616 \u44256  \u51080 \u44192 \u44396 \u45208 , \u50504 \u49900 \u54644 \u46020  \u46104 \u44192 \u45796 . \u54841 \u51008  \u51060  \u49324 \u46988 \u51060  \u48148 \u46988 \u51012  \u54588 \u51648 \u45716  \u50506 \u44192 \u45796 .\u44208 \u54844 \u50640  \u51080 \u50612 \u49436  \u47588 \u50864  \u44557 \u51221 \u51201 \u51064  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \u44403 \u44172  \u48127 \u51004 \u47728  \u51032 \u51648 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45128 \u45128 \u54620  \u44288 \u44228 , \u53945 \u55176  \'91\u49888 \u50857 \'92\u51060  \u45458 \u51008  \u44288 \u44228 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u50040 \u50640 \u49436 \u45716  \'91\u47196 \u47592 \u54001 \u54632 , \u51676 \u47551 \u54632 \'92\u51060  \u48512 \u51313 \u54644  \u48512 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54616 \u51648 \u47564 , \u49549 \u47560 \u51020 \u51012  \u53552 \u45459 \u44256  \u50584 \u44592 \u54616 \u44256  \u49910 \u45796 \u45716  \u46907 \u46020  \u51080 \u51004 \u48064 \u47196 \'85 \u47560 \u45285  \u48512 \u51221 \u51201 \u51064  \u52852 \u46300 \u45716  \u50500 \u45785 \u45768 \u45796 .\u51649 \u50629 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u51452 \u48320  \u46041 \u47308 \u51032  \u49888 \u47280 \u47484  \u50619 \u44144 \u45208 , \u48127 \u51012 \u47564 \u54620  \u49324 \u46988 \u51060  \u46321 \u51109 \u54644  \u51068 \u51060  \u49692 \u51312 \u47213 \u44172  \u54400 \u47532 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45236 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45572 \u44400 \u44032 \u50752  \u54801 \u47141 \u54616 \u44144 \u45208  \u44228 \u50557  \u54624  \u51068 \u51060  \u49373 \u44596 \u45796 \u47732 , \u49692 \u51312 \u47213 \u44172  \u51652 \u54665 \u46112  \u44163 \u51077 \u45768 \u45796 .\u51060 \u48120 \u51648  \u54644 \u49437 \u51109 \u48120 \u50743 \u44284  \u48177 \u54633 \u50743 \u51012  \u51077 \u51008  \u49324 \u51228 \u44032  \u51312 \u50616 \u51012  \u44396 \u54616 \u47084  \u50772 \u49845 \u45768 \u45796 . \u44536 \u46308 \u51008  \u50780  \u44368 \u54889 \u50640 \u44172  \u51312 \u50616 \u51012  \u44396 \u54616 \u47084  \u50772 \u51012 \u44620 \u50836 ? \u50780 \u45264 \u54616 \u47732 , \u44536 \u44032  \u48127 \u51012 \u47564 \u54620  \u51316 \u51116 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44536 \u45716  \u45458 \u51008  \u50948 \u52824 \u50640  \u51080 \u51004 \u47728 , \u49888 \u51032  \u46907 \u51012  \u50780 \u44257 \u54616 \u51648  \u50506 \u44256  \u44256 \u49828 \u46976 \u55176  \u51204 \u45804 \u54644 \u51452 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u46384 \u46972 \u49436  \u54637 \u49345  \u50739 \u51008  \u45813 \u47564  \u45236 \u45459 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u47704 \u53664  \u44057 \u51008  \u49324 \u46988 \u44368 \u54889  \u52852 \u46300 \u45716  \u47704 \u53664  \u44057 \u51008  \u49324 \u46988 , \u51648 \u54812 \u47196 \u50868  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44536 \u45716  \u49888 \u51032  \u46907 \u51012  \u49324 \u46988 \u46308 \u50640 \u44172  \u50612 \u46523 \u44172  \u51096  \u51204 \u45804 \u54624 \u51648  \u54637 \u49345  \u44256 \u48124 \u54616 \u47728 , \u50724 \u47004  \u44221 \u54744 \u51012  \u49939 \u50500 \u50772 \u49845 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732  \u50976 \u45212 \u55176  3\u44284  \u50621 \u55180  \u50836 \u49548 \u46308 \u51060  \u47566 \u51008 \u45936 , \u51060 \u45716  \u47784 \u46160  \u51333 \u44368 \u51201  \u44428 \u50948 \u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u54788 \u49892 \u50640 \u49436 \u45716  \u45458 \u51008  \u50948 \u52824 \u50640  \u51080 \u44144 \u45208 , \u45733 \u47141 \u51060  \u46832 \u50612 \u45212  \u49324 \u46988  \u51221 \u46020 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .3\u45800  \u50773 \u44288 \u49552 \u44032 \u46973  3\u44060 3\u45800  \u49828 \u53468 \u54532  Comment3\u45800  \u50773 \u44288 , \u49552 \u44032 \u46973  3\u44060 , 3\u45800  \u49828 \u53468 \u54532 \u45716  \u47784 \u46160  \u51333 \u44368 \u50640 \u49436  \u47568 \u54616 \u45716  \'91\u49340 \u50948 \u51068 \u52404 \'92 \u54841 \u51008  \u44368 \u54889 \u51060  \u51473 \u51116 \u54616 \u45716  \'913\u44060 \u51032  \u49464 \u44228 \'92\u47484  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51648 \u50725  \u46405  \u52380 \u44397 , \u51104 \u51116 \u51032 \u49885  \u51032 \u49885  \u52488 \u51032 \u49885 . \u52852 \u46300  \u49549 \u50640 \u49436  \u48152 \u48373 \u46104 \u45716  3\u51008  \u47784 \u46160  \u44368 \u54889 \u51032  \u51333 \u44368 \u51201  \u44428 \u50948 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 .\u52852 \u54187  \u50948  \u50676 \u49632 \u46020  \u44428 \u50948 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51060 \u45716  \u54616 \u45712 \u45784 \u51060  \u52488 \u45824  \u44368 \u54889 \u51064  \u48288 \u46300 \u47196 \u50640 \u44172  \u51500 \u45796 \u44256  \u50508 \u47140 \u51652  \u52380 \u44397  \u50676 \u49632 \u51077 \u45768 \u45796 . \u51060  \u50676 \u49632 \u44032  \u51080 \u51004 \u47732 , \u52380 \u44397 \u51012  \u46300 \u45208 \u46308  \u49688  \u51080 \u45716  \u51088 \u44201 \u51012  \u50619 \u44172  \u46121 \u45768 \u45796 . \u44368 \u54889 \u50640 \u44172 \u46020  \u52380 \u44397 \u51012  \u45336 \u45208 \u46308  \u49688  \u51080 \u45716  \u44428 \u54620 \u51060  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u48288 \u46300 \u47196 \u51032  \u50676 \u49632 \u45236 \u44032  \u45320 \u50640 \u44172  \u47568 \u54620 \u45796 . \u45320 \u45716  \u48288 \u46300 \u47196 \u45796 . \u45236 \u44032  \u51060  \u48152 \u49437  \u50948 \u50640  \u45236  \u44368 \u54924 \u47484  \u49464 \u50872 \u53552 , \u51200 \u49849  \u49464 \u47141 \u46020  \u44536 \u44152  \u51060 \u44592 \u51648  \u47803 \u54624  \u44163 \u51060 \u45796 . \u46608  \u45208 \u45716  \u45320 \u50640 \u44172  \u54616 \u45720  \u45208 \u46972  \u50676 \u49632 \u47484  \u51452 \u44192 \u45796 . \u46405 \u50640 \u49436  \u50676 \u49632 \u47484  \u47588 \u47732  \u54616 \u45720 \u50640 \u49436 \u46020  \u47588 \u51060 \u44256 , \u46405 \u50640 \u49436  \u50676 \u49632 \u47484  \u54400 \u47732  \u54616 \u45720 \u50640 \u49436 \u46020  \u54400 \u47540  \u44163 \u51060 \u45796 . \u47560 \u53468 \u50724  \u48373 \u51020  16\u51109  18-19\u51208 \u51060  \u54644 \u49437 \u51012  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u45817 \u49888 \u51012  \u50612 \u47480 \u49828 \u47084 \u50868  \u49324 \u46988 , \u47704 \u53664  \u44057 \u51008  \u49324 \u46988 , \u48176 \u50872 \u51216 \u51060  \u47566 \u51008  \u49324 \u46988 \u51060 \u46972 \u44256  \u50668 \u44592 \u45716  \u46319 \u54633 \u45768 \u45796 .\u51649 \u50629 \u50868 \u50640 \u49436 \u45716  : \u49345 \u45812 \u51060 \u45208  \u52968 \u49444 \u54021 , \u54801 \u49345 \u54616 \u45716  \u51649 \u51333 \u50640  \u50612 \u50872 \u47549 \u45768 \u45796 .\u49324 \u54924 \u50640 \u49436 \u45716  \u45458 \u51008  \u51649 \u50948 \u50640  \u51080 \u45716  \u49324 \u46988 , \u45733 \u47141  \u51080 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u50672 \u49345 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u49892 \u51228 \u47196  \'91\u47704 \u53664 \'92\u45208  \'91\u51649 \u51109  \u49345 \u49324 \'92\u47484  \u44032 \u47532 \u53420  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u44536 \u47084 \u45208  \u44368 \u54889 \u51008  \'91\u47932 \u51656 \u51201 \u51064  \u51648 \u50896 \'92\u48372 \u45796  \'91\u51221 \u49888 \u51201 \u51064  \u51648 \u50896 \'92\u51012  \u54644 \u51452 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u47568 \u51008  \u44536 \u47092 \u49912 \u54616 \u45208  \u50508 \u47609 \u51060 \u44032  \u50630 \u51012  \u49688 \u46020  \u51080 \u44256 , \u49892 \u51656 \u51201  \u46020 \u50880 \u51060  \u50630 \u51012  \u46412 \u46020  \u47566 \u49845 \u45768 \u45796 . \u47924 \u50631 \u48372 \u45796  \u54637 \u49345  \'91\u51060 \u49345 \'92\u51012  \u52628 \u44396 \u54616 \u44592 \u50640 , \u54788 \u49892  \u44048 \u44033 \u51060  \u46244 \u46504 \u50612 \u51648 \u51424 .\u54644 \u49437  3. \u51339 \u51008  \u44288 \u44228 \u44032  \u46104 \u46020 \u47197  \u46020 \u50752 \u51452 \u45716 \u44368 \u54889  \u52852 \u46300 \u45716  \u49548 \u44060 \u54021 \u51012  \u49345 \u51669 \u54616 \u45716  \u45824 \u54364  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732  \u44263  \u49548 \u44060 \u54021 \u51060  \u46308 \u50612 \u50724 \u44144 \u45208 , \u54840 \u44048  \u44032 \u45716  \u49345 \u45824 \u50752  \u51096  \u46112  \u49688  \u51080 \u46020 \u47197  \u46020 \u50752 \u51452 \u45716  \u49324 \u46988 \u51060  \u46321 \u51109 \u54624  \u50696 \u51221 \u51077 \u45768 \u45796 . \u49104 \u47564  \u50500 \u45768 \u46972  \u45572 \u44400 \u44032 \u50752  \u49324 \u51060 \u44032  \u47680 \u50612 \u51276 \u44144 \u45208 , \u49436 \u47673 \u49436 \u47673 \u54664 \u45796 \u47732  \u44536  \u44288 \u44228 \u47484  \u45796 \u49884  \'91\u50997 \u54868 \'92 \u49884 \u53020 \u51460  \u51008 \u51064 \u51060  \u45208 \u53440 \u45208 \u44172  \u46121 \u45768 \u45796 .\u51116 \u54924 \u50640 \u49436 \u46020  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u44368 \u54889 \u51008  \'91\u48372 \u51060 \u51648  \u50506 \u45716  \u49888 \u51032  \u47700 \u49884 \u51648 \'92\u47484  \u49324 \u51228 \u50640 \u44172  \u51204 \u45804 \u54616 \u45716  \u50669 \u54624 . \u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732 , \u54756 \u50612 \u51652  \u50672 \u51064 \u51032  \u44048 \u51221 , \u49373 \u44033 \u46308 \u51012  \u44263  \u51204 \u45804  \u48155 \u44172  \u46108 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51649 \u50629 \u50640 \u49436 \u45716 , \u51649 \u51109  \u45236  \u44040 \u46321 \u51012  \u54644 \u44208 \u54644  \u51460  \u49324 \u46988 \u51060  \u46321 \u51109 \u54616 \u44172  \u46121 \u45768 \u45796 .\u44368 \u54889 \u51008  \u49888 \u44284  \u49888 \u51088 \u47484  \'91\u51473 \u44060 \'92 \u54644 \u51452 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u44368 \u54889 \u51008  \u49888 \u51032  \u46907 \u51012  \u51204 \u45804  \u48155 \u50500  \u46160  \u49324 \u51228 \u51032  \u44040 \u46321 \u51012  \u54644 \u44208 \u54644 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620 , \u44368 \u54889  \u46244 \u51032  \u44592 \u46181  2\u44060 \u46020  \u44536 \u44032  \u46832 \u50612 \u45212  \u51473 \u44060 \u51088 \u51076 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 .\u51060 \u45716  \u50668 \u49324 \u51228  \u52852 \u46300  \u49549  \u55121 \u48177  \u44592 \u46181 \u44284  \u48708 \u44368 \u54644 \u48372 \u47732  \u47749 \u54869 \u55176  \u50508  \u49688  \u51080 \u49845 \u45768 \u45796 . \u55121 \u48177  \u44592 \u46181 \u51060  \u49440 \u44284  \u50501 , \u48731 \u44284  \u50612 \u46176 \u52376 \u47100  \'91\u51060 \u51473 \u49457 \'92\u51012  \u49345 \u51669 \u54664 \u45796 \u47732 , \u44368 \u54889  \u52852 \u46300  \u49549  \u54924 \u49353  \u44592 \u46181 \u51008  \u49353 \u44628 \u51060  \u46609 \u44057 \u50500  \'91\u51312 \u54633 , \u50997 \u54633 \'92\u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u52280 \u44256  \u51088 \u47308 \u55121 \u48177  \u44592 \u46181 \u51060 \u51473 \u49457 \u54924 \u49353  \u44592 \u46181 \u50997 \u54633 \u44284  \u51312 \u54868  \u51221 \u47532 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796  :\u49556 \u47196 \u46972 \u47732 , \u49548 \u44060 \u54021 \u51060  \u46308 \u50612 \u50732  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52572 \u44540 \u50640  \u45572 \u44400 \u44032 \u50752  \u49912 \u50912 \u44144 \u45208 , \u54756 \u50612 \u51276 \u45796 \u47732  \u45572 \u44400 \u44032 \u51032  \u46020 \u50880 \u51004 \u47196  \u44288 \u44228 \u44032  \u54924 \u48373 \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u51649 \u51109 \u50640 \u49436 \u45716  \u44040 \u46321 \u51060  \u54644 \u44208 \u46112  \u51669 \u51312 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44277 \u51201 \u51064  \u44288 \u44228 \u47196  \u50668 \u44592 \u45716 \u44368 \u54889  \u52852 \u46300 \u47484  \u45208 \u49240 \u44172  \u54644 \u49437 \u54616 \u47732 , \u44277 \u51201 \u51064  \u44288 \u44228 , \u48708 \u51592 \u45768 \u49828  \u44288 \u44228 , \u54617 \u44368  \u49440 \u54980 \u48176  \u44288 \u44228 \u47484  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51593 , \u47196 \u47592 \u54001 \u54620  \u44048 \u51221 \u51008  \u50630 \u44256  \u54805 \u49885 \u51201 \u51004 \u47196 \u47564  \u45824 \u54620 \u45796 \u45716  \u46907 \u51060 \u51424 .\u45817 \u49888 \u51060  \u50040 \u51012  \u53440 \u44256  \u51080 \u45796 \u47732 , \u50500 \u47560 \u46020  \u49345 \u45824 \u48169 \u51008  :\u48324  \u47560 \u51020 \u51060  \u50630 \u45796 .\u44536 \u45285  \u47751  \u48264  \u47560 \u51452 \u52828  \u54617 \u44368  \u49440 \u54980 \u48176 , \u51649 \u51109  \u46041 \u47308 \u47196  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 .\u47196 \u47592 \u54001 \u54620  \u44048 \u51221 \u51008  \u45712 \u44788 \u51648 \u51648  \u50506 \u45716 \u45796 .\u52852 \u46300  \u49549  \u44368 \u54889 \u44284  \u49324 \u51228 \u45716  \u49436 \u47196  \u52828 \u48516  \u51080 \u45716  \u44288 \u44228 \u44032  \u50500 \u45785 \u45768 \u45796 . \u44536 \u51200  \u46020 \u50880 \u51012  \u51452 \u44256  \u48155 \u45716  \u44277 \u51201  \u44288 \u44228 \u51424 .\u45796 \u47564  \u51649 \u50629 \u50640 \u49436 \u45716  \u51339 \u44172  \u51089 \u50857 \u54633 \u45768 \u45796 . \u44368 \u54889  \u52852 \u46300 \u44032  \'91\u51333 \u44368 \u51032  \u51077 \u47928 , \u44277 \u51201 \u51064  \u50672 \u44208 \'92\u51012  \u49345 \u51669 \u54616 \u48064 \u47196 , \u52712 \u50629  \u49457 \u44277 \u51060 \u45208  \u54633 \u44201 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54644 \u49437  2. \u44256 \u51648 \u49885 \u54620 \u44368 \u54889  \u52852 \u46300 \u45716  \u44256 \u51648 \u49885 \u54620  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u44368 \u54889 \u51008  \u49888 \u51032  \u44508 \u51221 , \u44368 \u47532 , \u48277 \u52825 \u50640  \u50621 \u47588 \u50668  \u51080 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u54637 \u49345  \u50739 \u51008  \u45813 \u47564  \u45236 \u45459 \u44256 , \u44508 \u52825 \u51201 \u51064  \u49373 \u54876 \u51012  \u48152 \u48373 \u54616 \u51424 . \u50612 \u44555 \u45208 \u45716  \u48277 \u46020  \u50630 \u49845 \u45768 \u45796 .\u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u48852 \u54616 \u44256  \u51116 \u48120 \u50630 \u45716 , \u47924 \u48120 \u44148 \u51312 \u54620  \u50672 \u50528 \u47484  \u54616 \u44172  \u46121 \u45768 \u45796 . \u48165  \u52852 \u54168  \u50689 \u54868  \u48165  \u52852 \u54168  \u50689 \u54868 \'85\u50997 \u53685 \u49457  \u48512 \u51313 \u51060  \u53360  \u45800 \u51216 \u51077 \u45768 \u45796 . \u49440 \u51077 \u44204 \u46020  \u44053 \u54616 \u44256 , \u49468 \u49828 \u44032  \u48512 \u51313 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54637 \u49345  \u48376 \u51064 \u51060  \u50739 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44368 \u54889 \u51008  \u49888 \u51032  \u46907 \u51060  \u54637 \u49345  \u50739 \u44256 , \u48376 \u51064 \u51008  \u44536 \u44163 \u51012  \u50504 \u45796 \u44256  \u49373 \u44033 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51060  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732 , \'91\u44256 \u51648 \u49885 \u54632 , \u44256 \u51665 \u51060  \u49480 , \u51116 \u48120  \u50630 \u51020 , \u49468 \u49828  \u50630 \u51020 , \u48376 \u51064 \u51060  \u54637 \u49345  \u50739 \u45796 \u44256  \u49373 \u44033 \u54632 \'92 \u51221 \u46020 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 .\u51649 \u50629 \u50640 \u49436 \u45716 , \u48156 \u51204 \u51060  \u47688 \u52676 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u49352 \u47196 \u50868  \u44163 \u51012  \u48155 \u50500 \u46308 \u51060 \u51648  \u50506 \u50520 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u50724 \u51648 \u46998 \u51060  \u49900 \u54620 \u49324 \u49324 \u44148 \u44148  \u44036 \u49453 \u51060  \u47566 \u44256 , \u50724 \u51648 \u46998  \u46496 \u45716  \u49324 \u46988 \u51004 \u47196  \u54644 \u49437 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51312 \u50616 \u51060  \u44284 \u54616 \u47732  \'91\u50724 \u51648 \u46998 \'92\u51060  \u46121 \u45768 \u45796 .\u47928 \u51228 \u45716  \u44368 \u54889 \u51060  \'91\u47582 \u45716  \u47568 \'92\u47564  \u54620 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 . \u49440 \u51032 \u51032  \u44144 \u51667 \u47568 \u46020  \u54616 \u51648  \u50506 \u44256 , \u49345 \u45824 \u48169 \u50640 \u44172  \u49345 \u52376 \u47484  \u51452 \u44144 \u45208  \u44403 \u51060  \u54596 \u50836 \u54616 \u51648  \u50506 \u51008  \'91\u51652 \u49892 \'92\u44620 \u51648  \u47568 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u45320  \u50836 \u51608  \u49332  \u47566 \u51060  \u51864 \u44396 \u45208 . \u49332 \u51012  \u51328  \u48772  \u54596 \u50836 \u44032  \u51080 \u44192 \u50612 . \u46769 \u46769 \u54616 \u47732  \u44288 \u51208 \u50640  \u47924 \u47532 \u44032  \u44040  \u49104 \u47564  \u50500 \u45768 \u46972  \u44033 \u51333  \u49457 \u51064 \u48337 \u51032  \u50896 \u51064 \u51060 \'85\u44368 \u54889  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u44368 \u54889  \u52852 \u46300 \u44032  \u45208 \u53440 \u45236 \u45716  \u49324 \u46988 \u51008 , \u54637 \u49345  \u50739 \u51008  \u47568 \u47564  \u54616 \u44592 \u50640  \u49324 \u46988 \u46308 \u51060  \u44536 \u51032  \u51312 \u50616 \u51012  \u44396 \u54616 \u47084  \u47784 \u50668 \u46308 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44536 \u45716  \u55124 \u55176  \u47568 \u54616 \u45716  \u50976 \u44368 \u48372 \u51060 , \u50976 \u44368 \u44152 \u51077 \u45768 \u45796 . \u51676 \u47551 \u54632 \u44284  \u55141 \u48516 \u51012  \u52628 \u44396 \u54616 \u44592 \u48372 \u45796 , \u51221 \u49888 \u51201 \u51064  \u44368 \u47448 \u45208  \u48127 \u51012 \u47564 \u54620  \u44288 \u44228 \u47484  \u49440 \u54840 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u54637 \u49345  \u44057 \u51008  \u54056 \u53556 \u51004 \u47196  \u49332 \u50500 \u44032 , \'91\u47924 \u50631 \u51012  \u54616 \u44256  \u51080 \u51012 \u51648 \'92 \u50696 \u52769 \u51060  \u49772 \u50868  \u49324 \u46988 \u51077 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u49345 \u45824 \u48169 \u51012  \u48520 \u50504 \u54616 \u44172  \u54616 \u51648 \u46020  \u50506 \u49845 \u45768 \u45796 . \u51116 \u48120 \u50630 \u51648 \u47564  \u52265 \u54616 \u44256  \u48127 \u51020 \u51649 \u54620  \u49324 \u46988 . \u51060  \u49324 \u46988 \u44284  \u44208 \u54844 \u54616 \u44172  \u46104 \u47732 , \u54200 \u50504 \u54616 \u44256  \u50504 \u51221 \u51201 \u51064  \u49334 \u51012  \u49332 \u50500 \u44032 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 .\u44368 \u54889  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 ,\u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51068  \u44221 \u50864  :\u49345 \u45824 \u48169 \u51060  \u51328  \u45908  \u51652 \u51648 \u54620  \u44288 \u44228 \u47484  \u50896 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172  \u49888 \u47280 \u44032  \u49373 \u44220 \u51004 \u47728 , \u49549 \u47560 \u51020 \u51012  \u53552 \u45459 \u44256  \u51652 \u51473 \u54620  \u45824 \u54868 \u47484  \u45208 \u45572 \u44256  \u49910 \u50612 \u54633 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172  \u49444 \u47112 \u45716  \u44048 \u51221 \u51008  \u50630 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u45817 \u49888 \u44284  \u51080 \u51004 \u47732  \u50784 \u51648  \u47784 \u47476 \u44172  \u54200 \u50504 \u54616 \u44256 , \u47928 \u51228 \u47484  \u54644 \u44208 \u54624  \u44163 \u47564  \u44057 \u51008  \u45712 \u45196 \u51060  \u46317 \u45768 \u45796 . \u50616 \u51228 \u46304 \u51648  \u52286 \u50500 \u44032 \u49436  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u49910 \u51008  \u49324 \u46988 , \u45236  \u47704 \u53664 , \u50612 \u47480 \u49828 \u47084 \u50868  \u49324 \u46988 \u51004 \u47196  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44208 \u54844 \u54644 \u46020  \u51339 \u51008  \u49324 \u46988 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51068  \u44221 \u50864  :\u44536 \u51200  \u44277 \u51201 \u51064  \u44288 \u44228 \u47196  \u49373 \u44033 \u54616 \u45716  \u46319 \u54633 \u45768 \u45796 . \u50696 \u52968 \u45824  \u54617 \u44368  \u49440 \u54980 \u48176 , \u51649 \u51109  \u46041 \u47308 \u51077 \u45768 \u45796 . \u44368 \u54889  \u52852 \u46300 \u45716  \'91\u49688 \u51649 \u51201  \u44288 \u44228 \'92\u47484  \u45208 \u53440 \u45253 \u45768 \u45796 . \u45817 \u49888 \u51012  \u44536 \u51200  \u50967 \u49324 \u46988 , \u54841 \u51008  \u52313 \u44200 \u51480 \u50556  \u54624  \u50500 \u47019 \u49324 \u46988  \u51221 \u46020 \u47196  \u50668 \u44592 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46412 \u47196 \u45716  \u45817 \u49888 \u51012  \'91\u44848 \u45824 \u49828 \u47101 \u45796 \u44256 \'92 \u49373 \u44033 \u54616 \u45716  \u46319 \u54633 \u45768 \u45796 . \u49324 \u49324 \u44148 \u44148  \u44036 \u49453 \u54616 \u44144 \u45208 , \u50508 \u51648 \u46020  \u47803 \u54616 \u47732 \u49436  \u51312 \u50616  \u54616 \u44144 \u45208 , \u47568 \u51060  \u45320 \u47924  \u47566 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196  \u51648 \u51648 \u54616 \u47728  \u50504 \u51221 \u51201 \u51064  \u44288 \u44228 \u47484  \u47564 \u46308 \u50612 \u45208 \u44036 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u48148 \u46988 \u51060 \u45208  \u48520 \u47452 \u44284 \u45716  \u44144 \u47532 \u44032  \u47680 \u47728 , \u50672 \u50528  \u46412 \u47928 \u50640  \u51068 \u51012  \u49548 \u54848 \u55176  \u54616 \u51648 \u46020  \u50506 \u49845 \u45768 \u45796 . \u49436 \u47196 \u50640 \u44172 \u49436  \u48176 \u50864 \u45716  \u51216 \u46020  \u47566 \u49845 \u45768 \u45796 . \u44208 \u54844 \u54644 \u46020  \u51339 \u51008  \u44288 \u44228 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u46168  \u51473  \u45572 \u44400 \u44032 \u44032  \u45320 \u47924  \'91\u50756 \u48317 \u54620  \u49345 \u54889 \'92\u47564 \u51012  \u50896 \u54624  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u47568 \u47564  \u44536 \u47092 \u49912 \u54616 \u44256  \'91\u54788 \u49892 \u51201 \u51064  \u45824 \u48708 \'92\u47484  \u54616 \u51648  \u50506 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47751  \u49332  \u46412 \u44620 \u51648  \u48730 \u51012  \u44058 \u44256 , \u47751  \u49332  \u46412 \u44620 \u51648  \u51665 \u51012  \u49332  \u44144 \u50556 . \u44536 \u47084 \u47732  \u50864 \u47532 \u45716  \u44208 \u54844 \u54624  \u49688  \u51080 \u44192 \u51648 ? \u50500 \u51060 \u45716  2\u47749 \u47564  \u45235 \u44256  \u45432 \u54980  \u45824 \u48708 \u45716 \'85 \u44536 \u47000 \u49436  \u50620 \u47560  \u47784 \u50520 \u45716 \u45936 ? \u45208 ? 30\u47564 \u50896 .\u51116 \u54924 \u50640 \u49436  \u44368 \u54889  \u52852 \u46300 \u45716  \u44557 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u44368 \u54889  \u52852 \u46300 \u44032  \'91\u49888 \u51032  \u47700 \u49884 \u51648 , \u48372 \u51060 \u51648  \u50506 \u45716  \u47700 \u49884 \u51648 \u47484  \u51204 \u45804 \u54616 \u44172  \u46108 \u45796 \'92\u45716  \u46907 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u53945 \u55176  \u49345 \u45824 \u48169 \u44284  \u52649 \u46041 \u51201 \u51004 \u47196  \u54756 \u50612 \u51276 \u45796 \u47732  \u45908 \u45908 \u50865  \u51339 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51648 \u44552  \u54980 \u54924 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u44284  \u54632 \u44760  \u54664 \u45912  \u49334 \u51060  \u54200 \u50504 \u54616 \u44256  \u50504 \u51221 \u51201 \u51060 \u50632 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51068 \u45800  \u48376 \u51064  \u54624  \u51068 \u50640  \u52649 \u49892 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u51312 \u47564 \u44036  \u50672 \u46973 \u51060  \u50732  \u44163 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u44368 \u54889  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u51339 \u51008  \u47704 \u53664 \u47484  \u52286 \u44592 \u50640  \u51201 \u51208 \u54620  \u46412 \u51077 \u45768 \u45796 . \u44263  \u45817 \u49888 \u50640 \u44172  \u51473 \u50836 \u54620  \u51312 \u50616 \u51012  \u44148 \u45348 \u51452 \u44144 \u45208 , \u51221 \u49888 \u51201 \u51064  \u46020 \u50880 \u51012  \u51460  \u49324 \u46988 \u51060  \u50724 \u44172  \u46121 \u45768 \u45796 . \u51649 \u51109  \u45236  \u44040 \u46321 \u51060  \u51080 \u50632 \u45796 \u47732  \u44263  \u54644 \u44208 \u46104 \u47728 , \u53945 \u50976 \u51032  \u53685 \u52272 \u47141 \u51004 \u47196  \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u51064 \u51221 \u48155 \u45716  \u51068 \u46020  \u49373 \u44609 \u45768 \u45796 .\u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u44368 \u54889  \u52852 \u46300 \u45716  \'91\u44277 \u51201 \u51064  \u50672 \u44208 , \u49352 \u47196 \u50868  \u50689 \u50669 \u50640  \u48156  \u46308 \u51060 \u44172  \u46120 \'92\u51012  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u50896 \u54616 \u45716  \u51649 \u51109 \u50640  \u52712 \u51649 \u54616 \u44144 \u45208 , \u45824 \u54617 \u50640  \u51077 \u54617 \u54616 \u44172  \u46104 \u50612  \u49352 \u47196 \u50868  \u49464 \u44228 \u50640  \u48156 \u51012  \u46308 \u51060 \u44172  \u46121 \u45768 \u45796 .\u44368 \u54889  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u53804 \u51088 \u45208  \u49324 \u50629 \u50640 \u49436 \u45716  \u53360  \u49548 \u46301 \u51060  \u50630 \u49845 \u45768 \u45796 . \u51200 \u52629 \u51060 \u45208  \u50696 \u44552 \u52376 \u47100  \'91\u51204 \u53685 \u51201 \u51064  \u51116 \u53580 \u53356  \u48169 \u49885 \'92\u51060  \u51339 \u49845 \u45768 \u45796 . \u52572 \u44540 \u50640  \u45208 \u50728  \u53804 \u51088  \u48169 \u48277 , \u54841 \u51008  \u49352 \u47196 \u50868  \u53804 \u51088 \u47484  \u49884 \u46020 \u54616 \u45716  \u44163 \u51008  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u44368 \u54889  \u52852 \u46300 \u44032  \'91\u51204 \u53685 , \u44508 \u50984 , \u44508 \u52825 \'92\u44284  \u50672 \u44288 \u51060  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u44592 \u51316 \u50640  \u54616 \u45912  \u54056 \u53556 \u51012  \u50976 \u51648 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u44540 \u47732  \u49457 \u49892 \u54616 \u44172  \u51068 \u54616 \u44256  \u51208 \u50557  \u54624  \u46412 \u51077 \u45768 \u45796 .\u52636 \u52376  : \u44368 \u54889  \u52852 \u46300  \u44552 \u51204 \u50868  \u51312 \u50616  \'96 Sibyl Tarot\u51312 \u50616 \u51216 \u51312 \u50616  1. \u44256 \u51221 \u44288 \u45392 \u51012  \u48260 \u47532 \u49464 \u50836 \u45817 \u50672 \u54616 \u44172  \u50668 \u44220 \u45912  \u49373 \u44033 \u46308 \u51060  \u53952 \u47540  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44060 \u48169 \u51201 \u51064  \u53468 \u46020 \u47196 , \'93\u54217 \u49548 \u50752 \u45716  \u45796 \u47480  \u48169 \u48277 , \u49324 \u44256  \u48169 \u49885 \'94\u51012  \u44032 \u51656  \u54596 \u50836 \u44032  \u51080 \u44192 \u49845 \u45768 \u45796 . \u51032 \u50808 \u47196  \u49789 \u44172  \u51221 \u45813 \u51012  \u52286 \u51012  \u49688  \u51080 \u51012 \u51648 \u46020  \u47784 \u47493 \u45768 \u45796 .\u51339 \u51008  \u49324 \u46988 \u51008  \u51060 \u47000 \u50556  \u54644 , \u51339 \u51008  \u50672 \u51064 \u51008  \u51060 \u47000 \u50556  \u54644 . \u53945 \u51221 \u54620  \'93\u54805 \u53468 \'94\u47484  \u44256 \u51665 \u54616 \u45716  \u44148  \u50500 \u45776 \u51648 \u50836 . \u49345 \u45824 \u48169 \u50640 \u44172 \u46020  \u44060 \u49457 \u51060  \u51080 \u51020 \u51012  \u51316 \u51473 \u54644 \u51452 \u49464 \u50836 .\u51312 \u50616  2. \u51452 \u48320 \u50640  \u46020 \u50880 \u51012  \u52397 \u54616 \u49464 \u50836 \u54217 \u49548  \u48127 \u51012 \u47564 \u54664 \u45912  \u52828 \u44396 , \u44032 \u51313 \u50640 \u44172  \u51312 \u50616 \u51012  \u44396 \u54616 \u45716  \u44148  \u50612 \u46504 \u51648 \u50836 . \u44536 \u46308 \u51008  \u51060 \u48120  \u51221 \u45813 \u51012  \u50508 \u44256  \u51080 \u49845 \u45768 \u45796 .    \
