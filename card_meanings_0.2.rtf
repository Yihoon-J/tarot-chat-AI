   \uc0\u52981 6 \'96 \u54924 \u49345  \u52981 6 \u44536 \u47548 \u51032  \u51032 \u48120 \u50612 \u47536  \u49548 \u45380 \u51060  \u49548 \u45376 \u50640 \u44172  \u44867 \u51012  \u49440 \u47932 \u54616 \u45716  \u47784 \u49845 \u49692 \u49688 \u54620  \u49324 \u46993 \u51012  \u49345 \u51669 \u44284 \u44144 \u47484  \u54924 \u49345 \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u52313 \u44200 \u51452 \u44256  \u49910 \u51008  \u47560 \u51020 \u50612 \u47536  \u49324 \u44256 \u48169 \u49885 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49692 \u49688 \u54620  \u47560 \u51020 \u51012  \u44032 \u51652 \u52981 6\u45716  \u49692 \u49688 \u54620  \u47560 \u51020 , \u51312 \u44148  \u50630 \u45716  \u49324 \u46993 \u51012  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u50668 \u47084 \u48516 \u51060  \u52395 \u49324 \u46993 \u51012  \u46496 \u50732 \u47540  \u46412  \u45712 \u45180 \u45716  \u44048 \u51221 \u44284  \u48708 \u49847 \u54633 \u45768 \u45796 . \u50676 \u51221  \u45336 \u52824 \u45716  \u49324 \u46993 \u48372 \u45796 \u45716  \u48512 \u46300 \u47101 \u44256  \u54217 \u54868 \u47196 \u50868  \u44048 \u51221 \u51060  \u50612 \u50872 \u47549 \u45768 \u45796 . \u52852 \u46300 \u45716  \u51204 \u52404 \u51201 \u51004 \u47196  \u50500 \u47144 \u54616 \u47728 , \u54665 \u48373 \u54620  \u48516 \u50948 \u44592 \u47484  \u54413 \u44592 \u44256  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49548 \u45380 \u51008  \u50612 \u47536  \u50500 \u51060 \u50640 \u44172  \u54616 \u50560  \u44867 \u51012  \u45208 \u45600 \u51452 \u47140  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54616 \u50560  \u44867 \u51008  \'91\u49692 \u49688 \u54632 \'92\u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51593  \'91\u44867 \u51060  \u45812 \u44596  \u52981 \'92\u51008  \'91\u49692 \u49688 \u54620  \u49324 \u46993 \'92\u51012  \u48708 \u50976 \u51201 \u51004 \u47196  \u45208 \u53440 \u45240  \u44163 \u51060