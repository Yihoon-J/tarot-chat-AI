   10. \uc0\u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 Wheel of Fortune \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300  \u44536 \u47548 \u51032  \u51032 \u48120 \u50612 \u51724 \u46304  \u49345 \u54889 \u51008  \u44404 \u47084 \u44036 \u45796 \u44557 \u51221 \u51060  70% \u48512 \u51221 \u51060  30% \u51221 \u46020  \u46104 \u45716  \u49345 \u54889 \u51060 \u45796 \u49688 \u54840 \u49888 \u44284  \u48169 \u54644 \u44988 \u51060  \u51460 \u45796 \u47532 \u44592 \u47484  \u54616 \u44256  \u51080 \u45796 \u51452 \u44592 \u51201 \u51064  \u48320 \u54868 \u47484  \u48152 \u48373 \u54616 \u44172  \u46108 \u45796  (\u49345 \u54889 , \u47560 \u51020 \u51060 )\u48320 \u54868 \u47484  \u48152 \u48373 \u54616 \u47728  \u49457 \u51109 \u54616 \u44172  \u46108 \u45796  \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300  \u54644 \u49437 \u54644 \u49437  1. \u50612 \u51724 \u46304  \u49345 \u54889 \u51008  \u44404 \u47084 \u44036 \u45796 \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u45716  \u49345 \u54889 \u51060  \u50612 \u51724 \u46304  \u44404 \u47084 \u44036 \u45796 \u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u54620  \u48264  \u46028 \u50500 \u44032 \u44592  \u49884 \u51089 \u54620  \u48148 \u53300 \u45716  \u44288 \u49457  \u46412 \u47928 \u50640  \u49789 \u44172  \u47688 \u52628 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51060 \u52376 \u47100 , \u45817 \u49888 \u51060  \u49373 \u44033 \u54616 \u45716  \'91\u44536  \u50868 \u47749 \'92\u46020  \u49789 \u44172  \u47688 \u52628 \u51648  \u50506 \u45716 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u48148 \u53300 \u51032  \u50896 \u46041 \u47141 \u51008  \u46041 \u47932 \u47196  \u54364 \u54788 \u46108  \u49688 \u54840 \u49888  4\u47749 \u51077 \u45768 \u45796 . \u51060  \u49688 \u54840 \u49888 \u46308 \u51008  \u45817 \u49888 \u51032  \u50868 \u47749 \u51012  \u44404 \u47084 \u44032 \u44172  \u54633 \u45768 \u45796 . \u47532 \u46377 \u50640 \u49436 \u45716  \u51060 \u47111 \u44172  \u51201 \u50857 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796  :\u44536 \u47532 \u50868  \u49324 \u46988 \u51012  \u50724 \u47004 \u47564 \u50640  \u47564 \u45208 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44536 \u44172  \u45817 \u49888 \u51060  \u50896 \u54616 \u45912  \u51109 \u47732 \u51008  \u50500 \u45776  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u54616 \u44256  \u49910 \u50632 \u45912  \u49324 \u46988 \u44284  \u50616 \u51232 \u44032  \u45796 \u49884  \u47564 \u45208 \u44172  \u46121 \u45768 \u45796 .\u54633 \u44201 \u54624  \u49688  \u51080 \u51012 \u44620 ? \u8594  \u49345 \u54889 \u51060  \u44404 \u47084 \u44036 \u45796 \u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u51004 \u48064 \u47196 , \u50868  \u51339 \u44172  \u54633 \u44201 \u54624  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u47564 \u50557  \u48520 \u54633 \u44201  \u54620 \u45796 \u47732 , \u51060  \u46608 \u54620  \u45817 \u49888 \u51032  \'91\u50868 \u47749 \'92. \u45908  \u51096 \u46104 \u44592  \u50948 \u54620  \u48145 \u44144 \u47492 \u51060 \u46972 \u44256  \u49373 \u44033 \u54616 \u49884 \u47732  \u46121 \u45768 \u45796 .\u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u50868 \u51032  \u55120 \u47492 \u51060  \u51080 \u51004 \u45768  \u48150 \u51004 \u47196  \u51088 \u51452  \u45208 \u44032 \u46972 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u50868 \u47749 \u51201 \u51064  \u49345 \u45824 \u50752  \u47564 \u45208 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u47532 \u46377 \u51060  \u50612 \u47140 \u50864 \u49884 \u45796 \u47732 , \u45916 \u52985 \u45916 \u52985  \u44144 \u47532 \u47732 \u49436  \u45208 \u50500 \u44032 \u45716  \'91\u51473 \u44256 \u52264 \'92\u47484  \u49373 \u44033 \u54616 \u49884 \u47732  \u46121 \u45768 \u45796 . \u48520 \u50504 \u54616 \u44256 , \u49849 \u52264 \u44048 \u46020  \u45208 \u49240 \u51648 \u47564 , \u50612 \u51724 \u46304  \u50526 \u51004 \u47196  \u45208 \u50500 \u44040  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u44557 \u51221  70 : \u48512 \u51221  30\u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u47484  \u49707 \u51088 \u47196  \u54364 \u54788 \u54616 \u47732 , \u44557 \u51221  70 : \u48512 \u51221  30 \u51004 \u47196  \u48148 \u44992  \u49688  \u51080 \u49845 \u45768 \u45796 .\u52852 \u46300 \u50640 \u45716  \u48169 \u54644  \u50836 \u49548 \u44032  \u51080 \u49845 \u45768 \u45796 . \u48192 , \u44536 \u47532 \u44256  \u44060 \u52376 \u47100  \u49373 \u44596  \u50500 \u45572 \u48708 \u49828 \u44032  \u48148 \u53300 \u44032  \u44404 \u47084 \u44032 \u45716  \u44163 \u51012  \u47561 \u44256  \u51080 \u51424 . \u51060 \u45716  \u44263 , \u45817 \u49888 \u51032  \u46907 \u45824 \u47196  \u49345 \u54889 \u51060  \u44404 \u47084 \u44036 \u45796  \u54644 \u46020 ,100% \u50756 \u48317 \u54620  \u49345 \u53468 \u47196  \u45796 \u44032 \u50724 \u51648 \u45716  \u50506 \u44144 \u45208 \u50756 \u48317 \u55176  \u51060 \u47364 \u51256 \u46020 , \u51652 \u51221  \u45817 \u49888 \u51060  \u50896 \u54616 \u45716  \u44172  \u50500 \u45776  \u49688 \u46020  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u53440 \u47196  \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u45716  \u49352 \u50745 \u51648 \u47560 \u46972 \u45716  \u47568 \u47196  \u48148 \u45012  \u51060 \u54644 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44600 \u55113 \u54868 \u48373 \u51008  \u48320 \u49688 \u44032  \u47566 \u50500 \u49436 , \u50696 \u52769 \u54616 \u44144 \u45208  \u45800 \u51221 \u54616 \u44592  \u55192 \u46308 \u45796 \u45716  \u46907 \u51060 \u51424 . \u51648 \u44552  \u51339 \u50500  \u48372 \u51060 \u45716  \u44163 \u46020  \u54868 \u44032  \u46112  \u49688  \u51080 \u44256 , \u45208 \u48736  \u48372 \u51060 \u45716  \u44163 \u46020  \u48373 \u51060  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u47568 \u51060  \u46020 \u47581 \u44032 \u49436  \u50612 \u51805 \u45768 \u44620 ? \u51221 \u47568  \u50504 \u46104 \u49512 \u49845 \u45768 \u45796 .\u44544 \u50052 \u50836 . \u51060  \u51068 \u51060  \u48373 \u51060  \u46112 \u51648 , \u54868 \u44032  \u46112 \u51648  \u50612 \u52236  \u50508 \u44192 \u49845 \u45768 \u44620 ? \u49352 \u50745 \u51648 \u47560  \u44288 \u47144  \u51068 \u54868  \u20013 \u45817 \u49888 \u51060  \u50896 \u54616 \u45716  \u44163 \u51060  \u51221 \u47568  \u45817 \u49888 \u50640 \u44172  \u51339 \u51008  \u51068 \u51064 \u44032 \u50836 ?\u44536 \u51200  \u50868 \u47749 \u50640  \u47585 \u44592 \u47728 , \u50976 \u50689 \u54616 \u46319  \u49332 \u50500 \u44032 \u49464 \u50836 .\u54644 \u49437  3. \u51452 \u44592 \u51201 \u51064  \u48320 \u54868 \u47484  \u47582 \u51060 \u54620 \u45796 \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u45716  \u45817 \u49888 \u51060  \u49373 \u44033 \u54616 \u45716  \u51068 , \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u46020  \u51452 \u44592 \u51201 \u51004 \u47196  \u48320 \u54620 \u45796 \u45716  \u46907 \u51012  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u47484  \u50864 \u47532 \u45716  \'91\u50868 \u47749 \'92\u51060 \u46972 \u44256  \u48512 \u47493 \u45768 \u45796 .\u48148 \u53300 \u45716  \u45130 \u51076 \u50630 \u51060  \u46028 \u50500 \u44049 \u45768 \u45796 . \u50967 \u48512 \u48516 \u51060  \u50500 \u47000 \u47196 , \u48145 \u48512 \u48516 \u51060  \u46608  \u50948 \u47196  \u50732 \u46972 \u44032 \u51424 . \u49688 \u54840 \u49888  4\u47749 \u44284  \u48169 \u54644 \u44988  2\u47749 \u51060  \u50638 \u52824 \u46973  \u46244 \u52824 \u46973  \u51460 \u45796 \u47532 \u44592 \u47484  \u48152 \u48373 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u52376 \u47100  \u45817 \u49888 \u51032  \u49345 \u54889 \u46020  \u51452 \u44592 \u51201 \u51004 \u47196  \u44557 \u51221 \u44284  \u48512 \u51221 \u51012  \u48152 \u48373 \u54620 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u49464 \u49345 \u51068 \u50640 \u45716  \u48152 \u46300 \u49884  \u44404 \u44257 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51339 \u51008  \u51068 \u44284  \u45208 \u49244  \u51068 \u51008  \u44228 \u49549 \u54644 \u49436  \u49692 \u54872 \u54616 \u47728  \u50668 \u47084 \u48516 \u51012  \u47604 \u46028  \u44163 \u51077 \u45768 \u45796 .\u45824 \u54364 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640 \u49436 \u45716  :\u51228  \u49324 \u50629 \u51008  \u50612 \u46523 \u44172  \u46112 \u44620 \u50836 ? \u8594  \u51339 \u51008  \u49345 \u54889 \u44284  \u50504  \u51339 \u51008  \u49345 \u54889 \u51012  \u45130 \u51076 \u50630 \u51060  \u48152 \u48373 \u54616 \u44172  \u46121 \u45768 \u45796 .\u44536  \u49324 \u46988 \u51032  \u49549 \u47560 \u51020 \u51008 ? \u8594  \u47560 \u51020 \u51060  \u50724 \u46973 \u44032 \u46973 \u54616 \u45768  \u44536 \u51200  \u50868 \u47749 \u50640  \u47585 \u44592 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 .\u44288 \u44228 \u51032  \u55120 \u47492 \u51008  \u50612 \u46523 \u44172  \u46112 \u44620 \u50836 ? \u8594  \u48320 \u54868 \u47484  \u47582 \u51060 \u54616 \u44172  \u46121 \u45768 \u45796 . \u44536 \u47084 \u45208  \u44536  \u48320 \u54868 \u45716  \u45817 \u49888 \u51060  \u52968 \u53944 \u47204 \u54624  \u49688  \u50630 \u49845 \u45768 \u45796 . \u47784 \u46160  \'91\u50868 \u47749 \'92\u50640  \u45804 \u47140 \u51080 \u49845 \u45768 \u45796 .\u51652 \u54665 \u54616 \u45716  \u54532 \u47196 \u51229 \u53944 \u45716  \u50612 \u46523 \u44172  \u46112 \u44620 \u50836 ? \u8594  \u47751  \u48264 \u51032  \u44256 \u48708 \u50752  \u47751  \u48264 \u51032  \u49457 \u44277 \u51060  \u51080 \u49845 \u45768 \u45796 . \u48320 \u54868 \u47484  \u48152 \u48373 \u54616 \u47728  \u44208 \u44397 \u50640 \u45716  \u50756 \u49457 \u46121 \u45768 \u45796 .\u48320 \u54868 \u44032  \u51452 \u44592 \u51201 \u51004 \u47196  \u51068 \u50612 \u45208 \u47732 , \u51201 \u51025 \u46020  \u55192 \u46308 \u44256  \u49828 \u53944 \u47112 \u49828 \u46020  \u49900 \u54644 \u51665 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u55064 \u46168 \u47532 \u51648  \u47568 \u44256  \u48376 \u51064 \u51032  \u49888 \u45392 \u51012  \u51648 \u53020 \u50556  \u54633 \u45768 \u45796 . \u51060  \u47784 \u46304  \u44163 \u51008  \'91\u45817 \u49888 \u51060  \u51096  \u46104 \u44592  \u50948 \u54620 \'92 \u44284 \u51221 \u51068  \u49104 \u51077 \u45768 \u45796 .\u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300  \u50672 \u50528 \u50868  \u54644 \u49437 \u51068 \u48152 \u51201 \u51004 \u47196  \u50868 \u47749 \u51032  \u48148 \u53300 \u45716  \'91\u44288 \u44228 \u50640  \u48320 \u54868 \u44032  \u49373 \u44596 \u45796 \'92\u45716  \u51669 \u51312 \u51077 \u45768 \u45796 . \u44557 \u51221 \u51201 \u51060 \u46304 , \u48512 \u51221 \u51201 \u51060 \u46304  \u48320 \u54868 \u47484  \u48152 \u48373 \u54616 \u47728  \u47561 \u54784 \u51080 \u45912  \u44288 \u44228 \u44032  \'91\u51652 \u52377 \u46108 \u45796 \'92\u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u49556 \u47196 \u46972 \u47732 , \u50864 \u50672 \u55176  \u50868 \u47749 \u51201 \u51064  \u49345 \u45824 \u47484  \u47560 \u51452 \u52824 \u44144 \u45208 , \u51082 \u44256  \u51080 \u45912  \u49324 \u46988 \u50640 \u44172  \u50672 \u46973 \u51060  \u50732  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u54665 \u50868 \u51060  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u48150 \u51004 \u47196  \u51088 \u51452  \u45208 \u44032 \u47732  \u51339 \u49845 \u45768 \u45796 .\u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u47560 \u51020 \u51060  \u50724 \u46973 \u44032 \u46973 \u54616 \u45768  \u50868 \u47749 \u50640  \u47585 \u44592 \u44192 \u45796 \u45716  \u49900 \u47532 \u51077 \u45768 \u45796 . \u51060 \u46412  \'91\u54869 \u49888 \'92\u44284 \u45716  \u44144 \u47532 \u44032  \u47689 \u45768 \u45796 . \u50612 \u51796  \u46412 \u45716  \u45817 \u49888 \u51060  \u48372 \u44256  \u49910 \u44256 , \u50612 \u51796  \u46412 \u45716  \u45817 \u49888 \u51060  \u44536 \u51200  \u54217 \u48276 \u54620  \u52828 \u44396 \u47196  \u48372 \u51077 \u45768 \u45796 . \u51648 \u44552 \u51008  \u50612 \u45712  \u54620  \u51901 \u51004 \u47196  \u44288 \u44228 \u47484  \u44208 \u51221 \u54616 \u44592  \u50612 \u47140 \u50864 \u45768 , \'91\u45817 \u49888 \u51060  \u50612 \u46500  \u54665 \u46041 \u51012  \u52712 \u54616 \u45716 \u51648 \'92, \'91\u45236  \u47560 \u51020 \u51008  \u50612 \u46523 \u44172  \u48320 \u54624 \u51648 \'92 \u44288 \u52272 \u54644 \u48372 \u44256  \u44208 \u47200  \u45236 \u44192 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51068 \u45800  \u44536 \u51032  \u47560 \u51020 \u51008  \u49688 \u46041 \u51201 \u51077 \u45768 \u45796 . \u51201 \u44537 \u51201 \u51004 \u47196  \u45796 \u44032 \u44040  \u49373 \u44033 \u51008  \u50630 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208  \u52852 \u46300  \u49549  \u49688 \u54840 \u49888 \u51008  4\u47749 , \u48169 \u54644 \u44988 \u51008  2\u47749 \u51060 \u48064 \u47196 \'85 \u44288 \u44228 \u45716  \u51068 \u45800  \u44557 \u51221 \u51201 \u51064  \u48169 \u54693 \u51004 \u47196  \u55128 \u47084 \u44040  \u44163 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u44404 \u44257  \u47566 \u51008  \u44288 \u44228 \u47196  \u48372 \u51077 \u45768 \u45796 . \u54217 \u53444 \u54616 \u51648  \u50506 \u51008  \u50672 \u50528 \u47484  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50883 \u44256  \u49912 \u50864 \u44592 \u47484  \u48152 \u48373 \u54616 \u47728 , \u44208 \u44397  \u51221 \u51060  \u46308 \u50612 \u44032 \u44256  \u51080 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \u51068 \u45800 , \u44288 \u44228 \u51032  \u55120 \u47492 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u45817 \u49888 \u46020  \u49345 \u45824 \u48169 \u46020  \u44288 \u44228 \u47484  \u51060 \u50612 \u45208 \u44040 \u51648  \u54869 \u49888 \u51060  \u50630 \u45716  \u49345 \u53468 \u51648 \u47564 , \u49436 \u47196 \u47484  \'91\u44208 \u44397 \u50644  \u54632 \u44760 \u54624  \u49324 \u46988 \'92\u51004 \u47196  \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u48516 \u44036  \u50676 \u47536  \u47560 \u51020 \u51012  \u50976 \u51648 \u54616 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u50724 \u46973 \u44032 \u46973 \u54620  \u47560 \u51020 \u50640  \u54844 \u46976 \u51012  \u44202 \u44256  \u51080 \u45716  \u47784 \u49845 \u51077 \u45768 \u45796 . \'91\u45796 \u49884  \u49884 \u51089 \u54616 \u45716  \u44172  \u47582 \u45716  \u44148 \u51648 \'92 \'91\u51060  \u44288 \u44228 \u44032  \u50612 \u46523 \u44172  \u48156 \u51204 \u54644 \u45208 \u44040 \u51648 \'92 \u44048 \u51312 \u52264 \u46020  \u50630 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u47672 \u47551 \u49549 \u51060  \u48373 \u51105 \u54616 \u45768 , \u44536 \u51200  \'91\u50868 \u47749 \u50640  \u47585 \u44592 \u51088 \u44256 \'92 \u49373 \u44033 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u46160  \u49324 \u46988 \u51032  \u49900 \u51221 \u50640  \u48320 \u54868 \u44032  \u51080 \u45796 \u45716  \u44163 \u51008  \u54869 \u49892 \u54616 \u48064 \u47196 , \u51116 \u54924 \u50868 \u51008  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 . \u44536 \u47532 \u44256  \u50868 \u47749 \u51032  \u49688 \u47112 \u48148 \u53300 \u44032  \u51204 \u54616 \u45716  \u47700 \u49884 \u51648 \u45716  \u45800  \u54616 \u45208  \u51077 \u45768 \u45796 . \'91\u44536 \u45824 \u47196  \u47680 \u50612 \u51648 \u46304 \'92, \'91\u45796 \u49884  \u49884 \u51089 \u54616 \u46304 \'92 \u47784 \u46160  \u54616 \u45720 \u51032  \u46907 \u51077 \u45768 \u45796 