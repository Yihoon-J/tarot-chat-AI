   \uc0\u50640 \u51060 \u49828  \u54172 \u53440 \u53364  \u51060 \u48120 \u51648 \u50640 \u51060 \u49828  \u54172 \u53440 \u53364  \'96 \u51228 \u50504  \u44396 \u47492 \u50640 \u49436  \u45208 \u50728  \u49552 \u51060  \u54172 \u53440 \u53364 \u51012  \u44148 \u45348 \u45716  \u47784 \u49845 \u45576 \u50640  \u48372 \u51060 \u45716  \u44208 \u44284 \u47932 \u51060 \u45208  \u48372 \u49345 \u51012  \u50619 \u44172  \u46104 \u45716  \u49345 \u54889 \u49352 \u47196 \u50868  \u44592 \u54924 \u44032  \u45796 \u44032 \u50740  (\u54788 \u49892 \u51201 \u51064  \u49345 \u54889 \u44284  \u44288 \u47144 )\u49892 \u50857 \u49457 \u51012  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u50504 \u51221 \u51201 \u51064  \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u51060 \u48120 \u51648  \u49345 \u51669 \u44396 \u47492  \u49549  \u49552  : \u49352 \u47196 \u50868  \u44592 \u54924 , \u49692 \u49688 \u54620  \u51104 \u51116 \u47141 \u54172 \u53440 \u53364  : \u47932 \u51656 \u51201 \u51064  \u44592 \u54924 , \u48372 \u49345 , \u44208 \u44284 \u47932 \u48708 \u50725 \u54620  \u51221 \u50896  : \u54413 \u50836 \u47196 \u50880 , \u44221 \u51228 \u51201 \u51064  \u50668 \u50864 \u51221 \u50896 \u51032  \u51077 \u44396  : \u44648 \u45804 \u51020 \u51004 \u47196  \u44032 \u45716  \u50668 \u51221 \u47680 \u47532  \u48372 \u51060 \u45716  \u49328  : \u44648 \u45804 \u51020 \u51012  \u53685 \u54644  \u44537 \u48373 \u54624  \u49688  \u51080 \u45716  \u49884 \u47144 , \u47785 \u54364 \u54645 \u49900  \u54644 \u49437 \u54644 \u49437  1. \u49892 \u51656 \u51201 \u51064  \u44208 \u44284 \u47484  \u50619 \u44172  \u46104 \u45716 \u54172 \u53440 \u53364  \u50640 \u51060 \u49828 \u45716  \u49892 \u51656 \u51201 \u51064  \u44208 \u44284 \u47484  \u50619 \u44172  \u46108 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \u53440 \u47196 \u50640 \u49436  \u54172 \u53440 \u53364 \u51008  \u47932 \u51656 , \u48372 \u49345  \u46321  \u45576 \u50640  \u48372 \u51060 \u45716  \u44163 \u46308 \u51012  \u49345 \u51669 \u54616 \u45716 \u45936 \u50836 . \u44396 \u52404 \u51201 \u51004 \u47196  \u46024 , \u51068 \u51088 \u47532 , \u49892 \u51656 \u51201 \u51064  \u49457 \u44284  \u46321 \u46321 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u50640 \u51060 \u49828  \u52852 \u46300 \u45716  \u47784 \u46160  \u49884 \u51089 \u51032  \u50640 \u45320 \u51648 , \u51104 \u51116 \u47141 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u44172  \u48148 \u47196  ACE\u44032  1\u48264  \u45824 \u49888  \u46321 \u51109 \u54616 \u45716  \u51060 \u50976 \u51077 \u45768 \u45796 . \u49352 \u47196 \u50868  \u49884 \u51089 \u51012  \u50526 \u46160 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640 , \u47924 \u49832  \u44208 \u44284 \u44032  \u45208 \u50732 \u51648 \u45716  \u50508  \u49688  \u50630 \u49845 \u45768 \u45796 .\u44396 \u47492  \u49549  \u49552 \u51012  \u51088 \u49464 \u55176  \u48372 \u47732 , \u46041 \u51204 \u51012  \u51536 \u44256  \u51080 \u45716  \u44163 \u51060  \u50500 \u45768 \u46972  \'93\u44148 \u45348 \u44256 \'94 \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u45432 \u47141 \u50640  \u49345 \u51025 \u54616 \u45716  \u48372 \u49345 \u51012  \u48155 \u44172  \u46108 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50672 \u50528 \u50640 \u49436  \u49892 \u51656 \u51201 \u51064  \u48372 \u49345 \u51012  \u48155 \u44172  \u46108 \u45796 \u45716  \u44148 , \u44263  \u44288 \u44228 \u44032  \u48156 \u51204 \u54616 \u44172  \u46108 \u45796 , \u44208 \u49892 \u51012  \u47610 \u45716 \u45796 \u45716  \u51032 \u48120 \u44032  \u51080 \u49845 \u45768 \u45796 . \u50040 \u50640 \u49436  \u50672 \u50528 \u47196 , \u49548 \u44060 \u54021 \u50640 \u49436  \u50528 \u54532 \u53552 \u47196 , \u50672 \u51064 \u50640 \u49436  \u48512 \u48512 \u47196  \u48156 \u51204 \u54616 \u45716  \u49345 \u54889 \u51060 \u51424 .\u44536 \u47084 \u45208 , \u44845  \u51339 \u51008  \u51216 \u47564  \u51080 \u45716  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u52572 \u44540 \u50640  \u49912 \u50912 \u44144 \u45208  \u44288 \u44228 \u44032  \u49436 \u47673 \u49436 \u47673 \u54644 \u51276 \u45796 \u47732 , \u51060 \u48324 \u51060 \u45208  \u51060 \u54844 \u51012  \u51032 \u48120 \u54624  \u49688  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \'93\u50896 \u51064 \u50640  \u46384 \u47480  \u44208 \u44284 \'94\u44032  \u46028 \u50500 \u50724 \u45716  \u44163 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u49352 \u47196 \u50868  \u44592 \u54924 \u50640 \u51060 \u49828  \u54172 \u53440 \u53364 \u51008  \u49352 \u47196 \u50868  \u44592 \u54924 \u45208  \u51228 \u50504 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u54801 \u50629  \u51228 \u50504 , \u47564 \u45224  \u51228 \u50504 , \u49548 \u44060 \u54021  \u46321 \u46321 \u51077 \u45768 \u45796 . \u51060 \u48120 \u51648 \u47484  \u48372 \u47732  \u49552 \u51060  \u54172 \u53440 \u53364 \u51012  \'93\u44148 \u45348 \u44256 \'94 \u51080 \u49845 \u45768 \u45796 .\u54172 \u53440 \u53364  \u50640 \u51060 \u49828 \u47484  \u51312 \u50616 \u51216 \u51004 \u47196  \u54644 \u49437 \u54620 \u45796 \u47732 , \'93\u47672 \u47551 \u49549 \u51004 \u47196 \u47564  \u49373 \u44033 \u54624  \u46412 \u44032  \u50500 \u45768 \u45796 \'94 \u46972 \u45716  \u46907 \u51060  \u46121 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u44263  \u44592 \u54924 \u44032  \u52286 \u50500 \u50724 \u44256 , \u44536  \u44592 \u54924 \u47484  \u48152 \u46300 \u49884  \u51105 \u50500 \u50556  \u54616 \u44592  \u46412 \u47928 \u51060 \u51424 . \u49352 \u47196 \u50868  \u44592 \u54924 \u44032  \u52286 \u50500 \u50772 \u45796 \u44256  \u49373 \u44033 \u51060  \u46308 \u47732 , \u48152 \u46300 \u49884  \u44396 \u52404 \u51201 \u51064  \u44228 \u54925 \u51012  \u49464 \u50892  \'93\u49892 \u54788 \'94\u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 .\u51116 \u54924 \u50868 \u50640 \u49436 \u45716 , \u53356 \u44172  2\u44032 \u51648  \u54644 \u49437 \u51060  \u44032 \u45733 \u54633 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u49345 \u45824 \u48169 \u51060  \u51339 \u51008  \u49324 \u46988 \u51012  \u49548 \u44060  \u48155 \u50500  \u49352 \u47213 \u44172  \u49884 \u51089 \u54616 \u47140  \u54633 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u45817 \u49888 \u50640 \u44172  \u51116 \u54924  \u50672 \u46973 \u51060  \u50724 \u44172  \u46121 \u45768 \u45796 . \u49552 \u51060  \u44148 \u45348 \u44256  \u51080 \u45716  \u44148  \'93\u46041 \u51204 \'94\u51060 \u48064 \u47196 , \u54788 \u49892 \u51201 \u51004 \u47196  \u53440 \u54801  \u54664 \u44144 \u45208  \u51312 \u44148 \u51060  \u44054 \u52656 \u51256 \u49436  \u50672 \u46973 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u49892 \u50857 \u49457 \u51012  \u46384 \u51648 \u45716 \u50640 \u51060 \u49828  \u50724 \u48652  \u54172 \u53440 \u53364 \u51008  \u49892 \u50857 \u49457 \u51012  \u46384 \u51648 \u45716  \u44288 \u44228 \u50640 \u49436 \u46020  \u46321 \u51109 \u54633 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644 , \u50676 \u51221 \u51201 \u51064  \u54840 \u44048 \u48372 \u45796 \u45716  \'91\u54788 \u49892 \u51201 \u51064  \u51312 \u44148 , \u49345 \u54889 \'92\u50640  \u52488 \u51216 \u51060  \u47582 \u52656 \u51256  \u51080 \u49845 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50504 \u51221 \u51201 \u51064  \u50672 \u50528 \u47484  \u51600 \u44592 \u47732 \u49436  \u44033 \u51088 \u51032  \u48156 \u51204 \u46020  \u52628 \u44396 \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47924 \u51089 \u51221  \u47560 \u51020 \u47564  \u48128 \u50612 \u48537 \u51060 \u45716  \u44288 \u44228 \u44032  \u50500 \u45768 \u46972 \u45716  \u51032 \u48120 \u51424 . \u49436 \u47196  \u50952 \u50952  \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 .\u45796 \u47564 , \u50040 \u50640 \u49436 \u45716  \u45796 \u49548  \u48512 \u51221 \u51201 \u51004 \u47196  \u51089 \u50857 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45800 \u49692  \u48708 \u51592 \u45768 \u49828  \u44288 \u44228 \u47484  \u51032 \u48120 \u54624  \u49688  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u50640 \u51060 \u49828 \u45716  \u52649 \u48516 \u54620  \u51104 \u51116 \u47141 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592 \u50640 , \'93\u51652 \u51676  \u49324 \u46993 \'94\u51004 \u47196  \u48156 \u51204 \u54616 \u44172  \u46112  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51200  \u49324 \u46988 \u51060 \u46993  \u52828 \u54644 \u51648 \u47732  \u47924 \u50616 \u44032  \u50619 \u51012  \u49688  \u51080 \u44192 \u45796 . \u51665 \u50504 \u51060 \u45208  \u54617 \u48268 \u51060  \u51339 \u51004 \u45768  \u47564 \u45208 \u46020  \u51339 \u44192 \u45796 .\u54644 \u49437  4. \u50504 \u51221 \u51201 \u51064 \u54172 \u53440 \u53364  \u50640 \u51060 \u49828 \u45716  \u50504 \u51221 \u51201 \u51064  \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51060 \u48120 \u51648 \u47484  \u48372 \u47732 , \u49552 \u46321 \u51060  \u50500 \u45772  \u49552 \u48148 \u45797 \u51012  \u48372 \u50668 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \'93\u49888 \u47280 \u50752  \u48127 \u51020 \'94\u51032  \u54364 \u49884 \u51077 \u45768 \u45796 . \u48155 \u52432 \u51452 \u44256 , \u49556 \u51649 \u54616 \u44172  \u46300 \u47084 \u45240 \u45796 \u45716  \u45656 \u50521 \u49828 \u51424 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u52964 \u54540  \'96 \u49436 \u47196 \u47484  \u48127 \u45716 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44033 \u51088  \u47924 \u50631 \u51012  \u54616 \u46304  \u51032 \u49900 \u54616 \u51648  \u50506 \u44256  \u54200 \u50504 \u54620  \u49345 \u53468 \u51077 \u45768 \u45796 . \u49436 \u47196 \u44032  \u46496 \u45208 \u51648  \u50506 \u51012  \u44144 \u46976  \u54869 \u49888 \u46020  \u51080 \u49845 \u45768 \u45796 .\u49556 \u47196  \'96 \u45817 \u49888 \u50640 \u44172  \u54200 \u50504 \u54632 \u51012  \u51452 \u45716  \u49324 \u46988 \u51060  \u52286 \u50500 \u50741 \u45768 \u45796 . \u48148 \u46988 \u44592 \u46020  \u50630 \u44256  \u50504 \u51221 \u51201 \u51064  \u49324 \u46988 \u51077 \u45768 \u45796 .\u51649 \u50629  \'96 \u49324 \u50629 \u51060  \u50504 \u51221 \u51201 \u51004 \u47196  \u49457 \u51109 \u54616 \u44144 \u45208 , \u51064 \u44036  \u44288 \u44228 \u50640 \u49436  \u53944 \u47084 \u48660 \u51060  \u51460 \u50612 \u46308 \u44172  \u46121 \u45768 \u45796 .\u45796 \u47564 , \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972 \u49436  \'93\u44288 \u44228 \u50640  \u51648 \u47336 \u54632 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \'94\u45716  \u51032 \u48120 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 .\u50640 \u51060 \u49828  \u54172 \u53440 \u53364  \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u49892 \u47532 \u47484  \u52628 \u44396 \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u44288 \u44228 \u47484  \u47610 \u51012  \u46412 \u46020  \u44033 \u51088 \u51032  \u48156 \u51204 \u51012  \u47588 \u50864  \u51473 \u50836 \u49884  \u50668 \u44609 \u45768 \u45796 . \u50672 \u50528  \u51473 \u50640 , \u52964 \u47532 \u50612 \u47484  \u48156 \u51204 \u49884 \u53020 \u50556  \u54616 \u44256 , \u50612 \u45712  \u51221 \u46020  \u46021 \u47549 \u46108  \u49334 \u51012  \u50976 \u51648 \u54644 \u50556  \u54633 \u45768 \u45796 . \u47560 \u51020 \u48372 \u45796 \u45716  \u51312 \u44148 \u51012  \u51473 \u50836 \u49884  \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u51665 \u50504  \u48176 \u44221 , \u54617 \u48268 , \u51116 \u49328 , \u50808 \u47784  \u46321 \u46321 \u51077 \u45768 \u45796 . \u49324 \u46993  \u54364 \u54788 \u51012  \'91\u44048 \u51221 \'92\u48372 \u45796 \u45716  \'91\u46024 \'92\u51060 \u45208  \'91\u49440 \u47932 \'92\u47196  \u54616 \u45716  \u49828 \u53440 \u51068 \u51060 \u47728 , \u52964 \u54540  \u44036 \u51032  \u48127 \u51020 \u51060  \u44032 \u51109  \u51473 \u50836 \u54616 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u54788 \u51116  \u49556 \u47196 \u46972 \u47732 , \'91\u51312 \u44148 \u51012  \u46384 \u51648 \u47140 \u45716  \u47560 \u51020 \'92\u51060  \u51109 \u50528 \u47932 \u47196  \u51089 \u50857 \u54633 \u45768 \u45796 . \u51339 \u51008  \u49324 \u46988 \u46020  \u45459 \u52824 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47560 \u51020 \u51012  \u48708 \u50864 \u44256  \'93\u49345 \u45824 \u48169 \u51060  \u50612 \u46500  \u49324 \u46988 \u51064 \u51648  \u50508 \u50500 \u44032 \u45716  \u44163 \'94\u50640 \u47564  \u51665 \u51473 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u44263  \u49548 \u44060 \u54021  \u51228 \u50504 \u51060  \u46308 \u50612 \u50728 \u45796 \u45716  \u46907 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50640 \u51060 \u49828  \u54172 \u53440 \u53364 \u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u50640 \u44172  \u50504 \u51221 \u44048 \u51012  \u45712 \u45180 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u44048 \u51221 \u51201 \u51004 \u47196  \u55141 \u48516 \u54616 \u44592  \u48372 \u45796 \u45716  \u54200 \u50504 \u54620  \u47560 \u51020 \u51012  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46412 \u50640  \u46384 \u46972  \u44208 \u54844 \u51012  \u49373 \u44033 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u54788 \u49892 \u51201 \u51064  \u51312 \u44148 \u51012  \u46384 \u51648 \u45716  \u46319  \u54616 \u47728 , \'93\u44552 \u51204 \u51201  \u48176 \u44221 \'94\u51060  \u44288 \u44228  \u49457 \u49324 \u51032  \u53412 \u54252 \u51064 \u53944 \u44032  \u46112  \u44163  \u44057 \u49845 \u45768 \u45796 . \u52572 \u44540  \u54788 \u49892 \u51201 \u51064  \u50584 \u44592 \u47484  \u45208 \u45572 \u44144 \u45208 , \u49440 \u47932  \u48155 \u51008  \u44163 \u51008  \u50630 \u45716 \u51648  \u46496 \u50732 \u47140 \u48372 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u44208 \u51221 \u51201 \u51064  \u54840 \u44048  \u49888 \u54840 \u51077 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54620 \u45796 \u47732 , \u44536 \u51200  \u47932 \u51656 \u51012  \u51452 \u44256  \u48155 \u45716  \u48708 \u51592 \u45768 \u49828  \u44288 \u44228 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51339 \u51008  \u51649 \u51109  \u46041 \u47308 , \u44316 \u52270 \u51008  \u45824 \u54617  \u46041 \u44592  \u46321 \u46321 \u51077 \u45768 \u45796 . \u44288 \u44228 \u50640  \u51080 \u50612  \u51648 \u47336 \u54632 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u49353 \u45796 \u47492 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u47588 \u50864  \u51060 \u49345 \u51201 \u51064  \u44288 \u44228 \u47196  \u48372 \u51077 \u45768 \u45796 . \u49436 \u47196  \u46021 \u47549 \u46108  \u51316 \u51116 \u51076 \u51012  \u51064 \u51221 \u54616 \u47728 , \u48156 \u51204 \u54616 \u45716  \u44288 \u44228 \u47484  \u52628 \u44396 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48127 \u51020 \u46020  \u44618 \u51008  \u49345 \u53468 \u51077 \u45768 \u45796 . \u54788 \u49892  \u44048 \u44033 \u51060  \u46832 \u50612 \u45208 \u45768  \u44208 \u54844 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 . \u45796 \u47564  \u45572 \u44400 \u44032 \u44032  \u51648 \u47336 \u54632 \u51012  \u45712 \u45180 \u44144 \u45208 , \u44288 \u44228 \u50640  \'93\u44032 \u49457 \u48708 \'94\u47484  \u46384 \u51648 \u44172  \u46112  \u49688  \u51080 \u51004 \u45768  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u54637 \u49345  \u48376 \u51064 \u51060  \u49552 \u54644  \u48372 \u44192 \u45796 \u45716  \u47560 \u51020 \u51060  \u54596 \u50836 \u54644  \u48372 \u51077 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u45208 \u49240 \u51648  \u50506 \u49845 \u45768 \u45796 . \u50640 \u51060 \u49828  \u54172 \u53440 \u53364 \u51060  \'93\u50612 \u46500  \u51228 \u50504 \u51060 \u45208  \u44592 \u54924 \'94\u47484  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u52981 \u51060 \u45208  \u50756 \u46300 \u52376 \u47100  \'93\u44048 \u51221 \'94\u50640  \u50621 \u55180  \u52852 \u46300 \u45716  \u50500 \u45785 \u45768 \u45796 . \'93\u54788 \u49892 \'94\u50640  \u50621 \u54784 \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u51088 \u47732 , \u52712 \u51456  \u49373 \u54876 \u51060  \u48148 \u48736 \u49436 , \u46024 \u51060  \u50630 \u50612 \u49436 , \u45817 \u51109  \u54624  \u44172  \u47566 \u50500 \u49436 , \u54788 \u49892 \u51201 \u51064  \u51312 \u44148 \u51060  \u50504  \u47582 \u50500 \u49436 . \u44536  \u48512 \u48516 \u50640  \u45824 \u54620  \'93\u53440 \u54801 \'94\u51060  \u51060 \u47336 \u50612 \u51648 \u47732  \u51116 \u54924 \u47484  \u49884 \u46020 \u54624  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45796 \u47480  \u45572 \u44400 \u44032 \u50640 \u44172  \u49548 \u44060 \u54021  \u51228 \u50504 \u51060 \u45208  \u50672 \u46973 \u51012  \u48155 \u50500 , \u49352 \u47213 \u44172  \u49884 \u51089 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . (\u50640 \u51060 \u49828 \u45716  \u49352 \u47196 \u50868  \u49884 \u51089 , \u44032 \u45733 \u49457 \u51012  \u49345 \u51669 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 )\u50640 \u51060 \u49828  \u54172 \u53440 \u53364  \u51649 \u50629 \u50868  \u54644 \u49437 \u50640 \u51060 \u49828  \u54172 \u53440 \u53364 \u51008  \u52572 \u44256 \u51032  \u51649 \u50629 \u50868 \u51012  \u51088 \u46993 \u54633 \u45768 \u45796 . \u49892 \u51656 \u51201 \u51064  \u44208 \u44284 \u47932 \u51060 \u45208  \u49457 \u44284 \u47484  \u50619 \u45716 \u45796 \u45716  \u46907 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51649 \u51109 \u50640 \u49436 \u45716  \u51339 \u51008  \u51068 \u51088 \u47532 \u47484  \u51228 \u50504  \u48155 \u44144 \u45208 , \u50672 \u48393 \u51060  \u50724 \u47476 \u44172  \u46121 \u45768 \u45796 . \u49324 \u50629 \u50640 \u49436 \u45716  \u51339 \u51008  \u44144 \u47000 \u52376 \u47484  \u52286 \u44144 \u45208  \u47588 \u52636 \u51060  \u53356 \u44172  \u45720 \u50612 \u45216  \u49688  \u51080 \u49845 \u45768 \u45796 .\u54633 \u44201 \u50868 \u46020  \u51339 \u49845 \u45768 \u45796 . \u45432 \u47141 \u51032  \u44208 \u49892 \u51012  \u47610 \u44172  \u46121 \u45768 \u45796 .\u46412 \u50640  \u46384 \u46972 , \u47588 \u45320 \u47532 \u51608 \u50640  \u48736 \u51648 \u44144 \u45208  \u51648 \u47336 \u54632 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50668 \u47084 \u47784 \u47196  \u54872 \u44221 \u51012  \u48148 \u45012 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u44208 \u44284 \u47932 \u47564  \u51923 \u44256  \u51080 \u45796 \u45716  \u44221 \u44256 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44284 \u51221 \u50640 \u49436  \u51116 \u48120 \u47484  \u52286 \u50500 \u48372 \u49464 \u50836 .\u50640 \u51060 \u49828  \u54172 \u53440 \u53364  \u44552 \u51204 \u50868  \u54644 \u49437 \u51648 \u44552 \u44620 \u51648  \u54644 \u50724 \u45912  \u53804 \u51088 \u50640 \u49436  \u53360  \u49457 \u44284 \u44032  \u51080 \u49845 \u45768 \u45796 . \u54841 \u51008  \u45572 \u44400 \u44032 \u50640 \u44172  \u51339 \u51008  \u53804 \u51088 \u47484  \u51228 \u50504  \u48155 \u44144 \u45208 , \u44865 \u46024 \u51060  \u49373 \u44600  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u54645 \u49900 \u51008  \u44536  \u46024 \u51012  \'93\u50976 \u51648 \u54616 \u45716  \u44163 \'94\u50640  \u51080 \u49845 \u45768 \u45796 . \u50640 \u51060 \u49828 \u45716  \u49884 \u51089 \u51012  \u51032 \u48120 \u54616 \u44592  \u46412 \u47928 \u50640 , \u48155 \u51008  \u46024 \u51012  \u50612 \u46523 \u44172  \u50416 \u45712 \u45264 \u50640  \u46384 \u46972 , \u48120 \u47000 \u44032  \u44537 \u47749 \u54616 \u44172  \u44040 \u47549 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u51064 \u45236 \u49900 \u51060  \u54596 \u50836 \u54633 \u45768 \u45796 \u45817 \u49888 \u51060  \u45824 \u48149 \u51060 \u45208  \u49345 \u54889 \u51032  \u44553 \u51204 \u44060 \u47484  \u50896 \u54616 \u44256  \u51080 \u45796 \u47732 , \u54172 \u53440 \u53364  \u50640 \u51060 \u49828 \u45716  \'93\u44984 \u51456 \u55176  \u45432 \u47141 \u54616 \u46972 \'94\u45716  \u47700 \u49884 \u51648 \u47484  \u51204 \u54644 \u51469 \u45768 \u45796 . \u44208 \u44284 \u44620 \u51648  \u44900  \u50724 \u47004  \u49884 \u44036 \u51060  \u44152 \u47532 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45817 \u51109 \u51032  \u49457 \u44284 \u47564 \u51012  \u51923 \u45716 \u45796 \u47732 , \u44552 \u48169  \u54252 \u44592 \u54616 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 . \u48512 \u46356  \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u44256 , \u44284 \u51221  \u54616 \u45208  \u54616 \u45208 \u50640  \u47792 \u51077 \u54644 \u48372 \u49464 \u50836 .\u51312 \u50616  2. \u54788 \u49892 \u51201 \u51004 \u47196  \u49373 \u44033 \u54644 \u48372 \u49464 \u50836 \u51648 \u44552 \u51008  \u54788 \u49892 \u51201 \u51004 \u47196  \u46384 \u51256 \u50556  \u54624  \u46412 \u51077 \u45768 \u45796 . \u44396 \u52404 \u51201 \u51064  \u49892 \u54665  \u44228 \u54925  \u50630 \u51060  \u47924 \u47784 \u54616 \u44172  \u46832 \u50612 \u46300 \u45716  \u44163 \u51008  \u50500 \u45772 \u51648 . \u49324 \u46993 \u47564 \u51004 \u47196  \u44208 \u54844 \u54624  \u49688  \u51080 \u45796 \u44256  \u49373 \u44033 \u54616 \u45716  \u44148  \u50500 \u45772 \u51648 . \u49464 \u49345  \u47928 \u51228 \u51032  95% \u51060 \u49345 \u51008  \u46024  \u47928 \u51228 \u51077 \u45768 \u45796 . \u51328  \u45908  \u54788 \u49892 \u51201 \u51004 \u47196  \u51217 \u44540 \u54616 \u49464 \u50836 .    \
\
   \uc0\u54172 \u53440 \u53364 2 \u51060 \u48120 \u51648 \u54172 \u53440 \u53364 2 \'96 \u51200 \u44544 \u47553  \u54620  \u45224 \u51088 \u44032  \u54172 \u53440 \u53364  2\u44060 \u47484  \u51200 \u44544 \u47553  \u54616 \u45716  \u47784 \u49845 \u49373 \u44033 \u54624  \u47928 \u51228 \u44032  \u47566 \u51008  \u49345 \u54889 \u51060 \u47532 \u51200 \u47532  \u51096  \u51312 \u51208 \u54616 \u45716  \u49324 \u46988 \u51600 \u44144 \u50880 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u46020  \u51080 \u51020 \u51060 \u48120 \u51648  \u49345 \u51669 \u44596  \u47784 \u51088  : \u51116 \u48120 \u47484  \u45712 \u45180 \u45716 , \u49549 \u51076 \u49688 \u47484  \u50416 \u45716 2\u44060 \u51032  \u54172 \u53440 \u53364  : \u51116 \u51221  \u49345 \u54889 \u51032  \u48320 \u54868 , 2\u44032 \u51648  \u50837 \u49900 , 2\u44032 \u51648  \u49440 \u53469 \u51648 \u45433 \u49353  \u48180 \u46300  : \u47924 \u54620 \u54620  \u48320 \u54868 , \u49345 \u54889 \u51032  \u48152 \u48373 \u46160  \u52377 \u51032  \u48176  : \u44256 \u45212 , \u49345 \u54889 \u51032  \u50612 \u47140 \u50880 , \u50948 \u53468 \u47196 \u50880 \u54645 \u49900  \u54644 \u49437 \u54644 \u49437  1. \u49373 \u44033 \u54624  \u47928 \u51228 \u44032  \u47566 \u51008 \u54172 \u53440 \u53364 2\u45716  \u49373 \u44033 \u54624  \u47928 \u51228 \u44032  \u47566 \u51008  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u51068 \u51012  \u50668 \u47084  \u44060  \u54616 \u44144 \u45208  \u44277 \u48512 \u50752  \u50672 \u50528 \u47484  \u46041 \u49884 \u50640  \u54644 \u50556  \u54616 \u45716  \u49324 \u46988 , \u49888 \u44221  \u50024 \u50556  \u54624  \u47928 \u51228 \u46308 \u51060  \u47566 \u51008  \u49345 \u54889 \u51077 \u45768 \u45796 . \u44032 \u51648 \u44256  \u44032 \u50556  \u54624  \u51664 \u51060  \u47566 \u50500  \u48372 \u51077 \u45768 \u45796 .\u49892 \u51228  \u47532 \u46377 \u50640 \u49436 \u45716  :\u47560 \u51020 \u51032  \u50668 \u50976 \u44032  \u50630 \u45796 . \u45817 \u51109  \u44256 \u48124 \u44144 \u47532 \u46020  \u47566 \u45796 .\u47924 \u50616 \u44032 \u47484  \u48152 \u46300 \u49884  \u46041 \u49884 \u45796 \u48156 \u51201 \u51004 \u47196  \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u51060 \u45796 .\u46168  \u45796  \u48260 \u47540  \u49688 \u44032  \u50630 \u45716  \u49345 \u54889 \u51060 \u45796 .\u52852 \u46300  \u49549  \u44536 \u47548 \u51012  \u48372 \u47732 , \u45224 \u51088 \u44032  \u54172 \u53440 \u53364  2\u44060 \u47196  \u51200 \u44544 \u47553 \u51012  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54620  \u48156 \u47196 \u47564  \u49436  \u51080 \u44256  \u47924 \u50616 \u44032  \u50500 \u49836 \u50500 \u49836 \u54644 \u48372 \u51077 \u45768 \u45796 . \u50668 \u44592 \u49436  \u46041 \u51204 \u51008  \'93\u54788 \u49892 \u51201 \u51064  \u47928 \u51228 \u45208  \u44284 \u51228 \'94 \u46321 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51593 , \u47928 \u51228 \u45208  \u44284 \u51228 \u47484  \u50500 \u49836 \u50500 \u49836 \u54616 \u44172  \u54644 \u44036 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51200  \u47680 \u47532 \u50644  \u48176 \u46020  \u51080 \u45716 \u45936 \u50836 . \u45336 \u49892 \u45824 \u45716  \u54028 \u46020  \u50948 \u50640 \u49436  \u50948 \u53468 \u47196 \u50868  \u50668 \u51221 \u51012  \u51060 \u50612 \u45208 \u44032 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060 \u48264 \u50644  \u45433 \u49353  \u48180 \u46300 \u50640  \u51452 \u47785 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796 . \u45433 \u49353  \u48180 \u46300 \u45716  \u47868 \u48708 \u50864 \u49828 \u51032  \u46944  \u54841 \u51008  \u50864 \u47196 \u48372 \u47196 \u49828 \u47484  \u49345 \u51669 \u54616 \u45716  \u44163 \u51004 \u47196  \'93\u47924 \u54620 \u45824 \'94\u50752  \u50672 \u44288 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u51452 \u50612 \u51652  \u47928 \u51228 \u44032  \'93\u44900 \u45208  \u50724 \u47000  \u51060 \u50612 \u51256  \u50772 \u44256 , \u50526 \u51004 \u47196 \u46020  \u48152 \u48373 \u46112  \u50696 \u51221 \'94 \u51060 \u46972 \u45716  \u45656 \u50521 \u49828 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u54172 \u53440 \u53364 2\u44032  \u48169 \u54644  \u50836 \u49548 \u47196  \u51089 \u50857 \u54624  \u44221 \u50864 , \u45320 \u47924  \u47566 \u51008  \u44284 \u51228 \u45208  \u51032 \u47924 \u47484  \u51648 \u44256  \u51080 \u45716  \u49345 \u54889 , \u47928 \u51228 \u50640  \u50517 \u46020  \u45817 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u50668 \u50976 \u47484  \u44032 \u51648 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 . \u50500 \u45768 \u47732  \u50668 \u50976 \u44032  \u50630 \u45796 \u44256  \u49556 \u51649 \u54616 \u44172  \u53560 \u50612 \u45459 \u45716  \u44163 \u46020  \u51339 \u49845 \u45768 \u45796 .\u54644 \u49437  2. \u51060 \u47532 \u51200 \u47532  \u51096  \u51312 \u51208 \u54616 \u45716 \u54172 \u53440 \u53364 2\u45716  \u47924 \u50616 \u44032  \u51312 \u51208 \u54616 \u44144 \u45208  \u46384 \u51648 \u45716  \u49324 \u46988 , \u50997 \u53685 \u49457  \u51080 \u45716  \u49324 \u46988 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300 \u47484  \u48372 \u47732 , \u45224 \u51088 \u44032  \u46041 \u51204 \u51012  \u46504 \u50612 \u46888 \u47532 \u51648  \u50506 \u44592  \u50948 \u54644 , \u51200 \u44544 \u47553 \u51012  \u54616 \u44256  \u51080 \u45716 \u45936 \u50836 . \u45236 \u47140 \u44032 \u47732  \u50732 \u47532 \u44256 , \u50732 \u46972 \u44032 \u47732  \u45236 \u47532 \u45716  \u49885 \u51077 \u45768 \u45796 . \u51060 \u45716  \'93\u54620 \u51901 \u51060  \u44284 \u54616 \u44144 \u45208  \u45336 \u52824 \u51648  \u50506 \u46020 \u47197  \u51312 \u51208 \u54620 \u45796 \'94\u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51208 \u51228  \u52852 \u46300 \u50752  \u48708 \u49847 \u54620  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u51060 \u47484  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51060  \u49324 \u46988 \u46020  \u51339 \u44256 , \u51200  \u49324 \u46988 \u46020  \u51339 \u51648 \u47564  \u54620 \u51901 \u51004 \u47196  \u54869 \u49888 \u54624  \u49688  \u50630 \u45716  \u49345 \u54889 Comment. \u54172 \u53440 \u53364 \u51008  \u54788 \u49892 \u51201 \u51064  \u49549 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u44592  \u46412 \u47928 \u50640 , \u48708 \u44368  \u44592 \u51456 \u46020  \'93\u45733 \u47141 , \u51116 \u47141 , \u54617 \u48268 \'94 \u46321  \u54788 \u49892 \u51201 \u51064  \u50836 \u49548 \u44032  \u46112  \u44163 \u51077 \u45768 \u45796 .\u50672 \u46973 \u51032  \u48712 \u46020 \u49688 \u47484  \u49464 \u49900 \u54616 \u44172  \u51312 \u51208 \u54616 \u45716  \u49345 \u54889 \u45813 \u51109  \u53440 \u51060 \u48141 \u51012  \u49464 \u49900 \u54616 \u44172  \u51312 \u51208 \u54616 \u45716  \u49345 \u54889 \u48128 \u45817  \u51473 \u51064  \u49324 \u46988 \u50672 \u50528 \u50752  \u51068 \u51032  \u48184 \u47088 \u49828 \u47484  \u49464 \u49900 \u54616 \u44172  \u47582 \u52628 \u45716  \u49324 \u46988 \u47564 \u50557  \u51649 \u50629 \u50868 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :\u48320 \u54616 \u45716  \u49345 \u54889 \u50640  \u48736 \u47551 \u48736 \u47551  \u45824 \u52376 \u54616 \u45716  \u49324 \u46988 \u51068 \u51032  \u50864 \u49440 \u49692 \u50948 \u44032  \u48520 \u48516 \u47749 \u54620  \u49324 \u46988 \u50668 \u47084  \u48516 \u50556 \u47484  \u51204 \u48152 \u51201 \u51004 \u47196  \u44288 \u47532 \u54616 \u45716  \u49324 \u46988 \u54172 \u53440 \u53364 2\u45716  \u50997 \u53685 \u49457  \u51080 \u44172  \u49332 \u50500 \u50556  \u54620 \u45796 \u45716  \u51312 \u50616 \u51216 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 . \u47924 \u50616 \u44032  \u44256 \u51665 \u49828 \u47101 \u44172  \u44208 \u51221 \u54616 \u44592  \u48372 \u45796 \u45716 , \u49345 \u54889 \u50640  \u46384 \u46972  \u50997 \u53685 \u49457  \u51080 \u44172  \u45824 \u52376 \u54616 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 .\u54644 \u49437  3. \u51600 \u44144 \u50880 \u51012  \u45712 \u45180 \u45716 \u54172 \u53440 \u53364 2\u45716  \u51116 \u48120 \u47484  \u45712 \u45180 \u45716  \u49324 \u46988 \u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u52852 \u46300  \u49549  \u45224 \u51088 \u45716  \u51200 \u44544 \u47553 \u51012  \u51600 \u44592 \u44256  \u51080 \u49845 \u45768 \u45796 . \u54620  \u48156 \u51012  \u46308 \u50612  \u50668 \u50976 \u47196 \u50880 \u44620 \u51648  \u48372 \u51060 \u45348 \u50836 .\u47564 \u50557  \u54172 \u53440 \u53364 2\u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u44284 \u51032  \u44288 \u44228 \u47484  \u54200 \u54616 \u44172  \u51600 \u44592 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \'93\u51652 \u49900 \u51004 \u47196  \u49324 \u46993 \u54620 \u45796 \'94\u44592  \u48372 \u45796 \u45716 , \u47805 \u44544 \u47805 \u44544 \u54620  \u44048 \u51221 \u51012  \u51600 \u44592 \u45716  \u46319 \u54620  \u45712 \u45196 \u51077 \u45768 \u45796 . \u51116 \u48140 \u45716  \u52828 \u44396 \u50752  \u47564 \u45208 \u45716  \u44592 \u48516 \u51060 \u44592 \u46020  \u54616 \u44396 \u50836 . \u54841 \u51008  \u51060 \u44163  \u51200 \u44163  \u49688 \u47566 \u51008  \u44284 \u51228 \u47484  \u54616 \u47732 \u49436 , \u49457 \u52712 \u44048 \u44284  \u47792 \u51077 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51452 \u47196  \'93\u51068 , \u54617 \u50629 \'94\u44284  \u44288 \u47144 \u51060  \u44618 \u49845 \u45768 \u45796 .\u47564 \u50557  \u45817 \u49888 \u51060  \u54588 \u44260 \u54632 \u44284  \u50864 \u50872 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u45796 \u47732 , \u50526 \u51004 \u47196  \u54876 \u47141  \u45336 \u52824 \u45716  \u51068 \u51060  \u49373 \u44592 \u44172  \u46112  \u44163 \u51060 \u46972 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u45796 \u44032 \u50732  \u46020 \u51204  \u44284 \u51228 \u47484  \u48152 \u44049 \u44172  \u47560 \u51452 \u54616 \u49464 \u50836 .\u54172 \u53440 \u53364 2 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u50997 \u53685 \u49457 \u51060  \u51339 \u51004 \u47728 , \u51064 \u49373 \u51032  \u51116 \u48120 \u47484  \u52628 \u44396 \u54616 \u45716  \u49324 \u46988 \u51077 \u45768 \u45796 . \u49888 \u45392 \u51012  \u48128 \u50612 \u48537 \u51060 \u44592 \u48372 \u45796  \u49345 \u54889 \u50640  \u46384 \u46972  \u50976 \u50672 \u54616 \u44172  \u45824 \u52376 \u54616 \u45716  \u49457 \u54693 \u51077 \u45768 \u45796 . \u48372 \u53685  \u45224 \u50640 \u44172  \u47582 \u52656 \u51452 \u45716  \u49828 \u53440 \u51068 \u51060 \u47728 , \u51652 \u51648 \u54620  \u49345 \u54889 \u51012  \u54588 \u54616 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54788 \u51116  \u47560 \u51020 \u51032  \u50668 \u50976 \u44032  \u50630 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51060 \u44163 \u51200 \u44163  \u49888 \u44221  \u50424  \u44172  \u47566 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45796  \u44032 \u51256 \u44032 \u44256  \u49910 \u51648 \u47564 , \u50612 \u45712  \u51901 \u51008  \u54252 \u44592 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u50672 \u46973 \u50640  \u49548 \u54848 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u46168  \u49324 \u51060 \u47484  \u51116 \u44144 \u45208  \u48128 \u45817 \u51012  \u49884 \u46020 \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u54788 \u51116  \u49556 \u47196 \u46972 \u47732 , \u50672 \u50528  \u54624  \u46412 \u44032  \u50500 \u45776  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u47560 \u51020 \u51032  \u50668 \u50976 \u44032  \u50630 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u52376 \u47532 \u54644 \u50556  \u54624  \u51068 \u51060  \u45320 \u47924 \u45208  \u47566 \u49845 \u45768 \u45796 . \u51648 \u44552 \u51008  \u50672 \u50528 \u45208  \u51068  \u46168  \u51473  \u54616 \u45208 \u45716  \u54252 \u44592 \u54644 \u50556  \u54616 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u51088 \u52843  \u46168  \u45796  \u45459 \u52832  \u49688  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u50668 \u47084 \u47784 \u47196  \u44512 \u54805  \u51105 \u55180  \u44288 \u44228 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51068 \u51060 \u47732  \u51068 , \u50672 \u50528 \u47732  \u50672 \u50528 , \u50977 \u50500 \u47732  \u50977 \u50500  \u47784 \u46160  \u51096  \u54644 \u45236 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48372 \u53685 \u51008  \u47588 \u50864  \u48148 \u49244  \u52964 \u54540 \u50640 \u44172 \u49436  \u51088 \u51452  \u46321 \u51109 \u54633 \u45768 \u45796 .[\u52280 \u44256  \u51088 \u47308 ] \u44032 \u45140 \u51008  \u45320 \u47924  \u47924 \u47532 \u54616 \u51648  \u50506 \u51012  \u54596 \u50836 \u44032  \u51080 \u51004 \u47728 , \u50672 \u46973 \u51060  \u49548 \u54848 \u54644 \u46020  \u45320 \u44536 \u47101 \u44172  \u51060 \u54644 \u54616 \u45716  \u53468 \u46020 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 .\u54172 \u53440 \u53364 2\u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u53356 \u44172  4\u44032 \u51648 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u45817 \u49888 \u50640 \u44172  \u47582 \u52628 \u47140 \u45716  \u47560 \u51020 \u51060  \u51080 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51593 , \u48376 \u49900 \u51012  \u49556 \u51649 \u54616 \u44172  \u46300 \u47084 \u45236 \u44592 \u48372 \u45796 \u45716  \u45817 \u49888 \u51032  \u53468 \u46020 , \u48152 \u51025 \u50640  \u46384 \u46972  \u44144 \u51667 \u47568 \u51012  \u54624  \u49688 \u46020  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44592 \u48376 \u51201 \u51004 \u47196 , \u45817 \u49888 \u50640 \u44172  \u50672 \u50528  \u44048 \u51221 \u51012  \u45712 \u45180 \u44592 \u48372 \u45796 \u45716  \'93\u51116 \u48120 \u50752  \u51600 \u44144 \u50880 \'94\u51012  \u45712 \u45180 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44208 \u54844 \u52376 \u47100  \u51652 \u51648 \u54620  \u48516 \u50948 \u44592 \u45716  \u50500 \u45785 \u45768 \u45796 . \u48128 \u45817 \u51012  \u51032 \u48120 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u46160  \u48264 \u51704 , \u47924 \u50616 \u44032  \u54869 \u49888 \u51060  \u50630 \u51012  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50724 \u45720 \u51008  \u51060  \u49324 \u46988 \u51060  \u51339 \u51008 \u45936 , \u45236 \u51068 \u51008  \u51200  \u49324 \u46988 \u51060  \u51339 \u50500 \u51648 \u45716  \u49345 \u54889  \u47568 \u51060 \u51424 . \u46168  \u49324 \u51060 \u47484  \u51116 \u48372 \u44256  \u44288 \u44228 \u47484  \u51060 \u50612 \u45208 \u44040 \u51648 , \u45130 \u51012 \u51648  \u44208 \u51221 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u44536  \u51473 \u49900 \u50640 \u45716  \'93\u54788 \u49892 \u51201 \u51064  \u50836 \u49548 \'94\u44032  \u45180 \u50612 \u51080 \u49845 \u45768 \u45796 . \u54617 \u48268 \u51060 \u45208  \u51116 \u47141 , \u45733 \u47141  \u46321 \u46321 \u51004 \u47196  \u48708 \u44368 \u54620 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u49464  \u48264 \u51704 , \u54788 \u49892 \u51201 \u51064  \u50668 \u50976 \u44032  \u50630 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u51068 \u51060 \u45208  \u54617 \u50629 \u51012  \u48337 \u54665 \u54644 \u50556  \u54616 \u45716  \u49345 \u54889 \u51060 \u51424 . \u50672 \u50528 \u45716  \u48376 \u51064  \u45733 \u47141  \u48150 \u51060 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45348  \u48264 \u51704 , \u54775 \u44040 \u47536 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51060  \u48376 \u51064 \u51012  \u51339 \u50500 \u54616 \u45716 \u51648  \u50504  \u51339 \u50500 \u54616 \u45716 \u51648  \u54775 \u44040 \u47140 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u51116 \u54924 \u47484  \u54624 \u51648  \u47568 \u51648  \u44256 \u48124 \u54616 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u50500 \u51649  \u54869 \u49888 \u51060  \u50630 \u44144 \u45208  \u47560 \u51020  \u51221 \u47532 \u44032  \u45916  \u46108  \u46319  \u48372 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972  \u44288 \u44228 \u44032  \u44557 \u51221 \u51201 \u51004 \u47196  \u55128 \u47084 \u44040  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54616 \u47732 , \u45800 \u49692 \u55176  \u51068 \u51060 \u45208  \u54617 \u50629 \u51060  \u48148 \u49240 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47560 \u51020 \u51032  \u50668 \u50976 \u44032  \u50630 \u50612 \u48372 \u51077 \u45768 \u45796 .\u54172 \u53440 \u53364 2 \u51649 \u50629 \u50868  \u54644 \u49437 \u54217 \u49548 \u48372 \u45796  \u45908  \u48148 \u49244  \u49884 \u44592 \u47484  \u48372 \u45236 \u49884 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u50668 \u47084  \u54532 \u47196 \u51229 \u53944 \u47484  \u46041 \u49884 \u50640  \u51652 \u54665 \u54616 \u44144 \u45208 , \u53804 \u51105 \u51012  \u54616 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47564 \u50557  \u51649 \u51109 \u50640 \u49436  \u45320 \u47924  \u47566 \u51008  \u51068 \u51012  \u50948 \u51076 \u54620 \u45796 \u47732 , \'93\u51221 \u45817 \u54620  \u48372 \u49345 \u51060 \u45208  \u55092 \u49885 \'94\u51012  \u48152 \u46300 \u49884  \u50836 \u44396 \u54644 \u50556  \u54633 \u45768 \u45796 . \u46412 \u46412 \u47196 , \u50892 \u46972 \u48184 \u51012  \u51648 \u53020 \u50556  \u54620 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 . \u51068 \u44284  \u44032 \u51221  \u47784 \u46160  \u49548 \u54848 \u55176  \u54616 \u51648  \u50506 \u46020 \u47197  \u51312 \u49900 \u54616 \u49464 \u50836 .\u54633 \u44201 \u50868 \u51008  \u48152 \u48152 \u51004 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u54869 \u49892 \u55176  \u44208 \u51221  \u45212  \u49324 \u50504 \u51060  \u50500 \u47924 \u44163 \u46020  \u50630 \u49845 \u45768 \u45796 . \u48376 \u51064 \u51032  \u47560 \u51020 \u44032 \u51664 \u50640  \u45804 \u47160 \u49845 \u45768 \u45796 .\u54172 \u53440 \u53364 2 \u44552 \u51204 \u50868  \u54644 \u49437 \u49373 \u44228 \u47484  \u50976 \u51648 \u54616 \u44592  \u50948 \u54644 , \u50676 \u49900 \u55176  \u51068 \u54616 \u44256  \u44228 \u49888  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45817 \u48516 \u44036 \u51008  \u48520 \u50504 \u54620  \u49345 \u54889 \u50640  \u45459 \u51068  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49352 \u47196 \u50868  \u51649 \u51109 \u51012  \u44396 \u54616 \u44144 \u45208 , \u48730 \u51012  \u44058 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u45716  \u49345 \u54889 \u51077 \u45768 \u45796 . \u51648 \u44552  \u45817 \u49888 \u50640 \u44176  \'93\u51201 \u51025 \u47141 \'94\u51060  \u54596 \u50836 \u54633 \u45768 \u45796 . \u44536 \u51200  \u51648 \u44552 \u51032  \u49334 \u50640  \u51201 \u51025 \u54616 \u47728 , \u45432 \u47141 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 .\u51312 \u50616 \u51216 \u51312 \u50616  1. \u50997 \u53685 \u49457 \u51012  \u48156 \u55064 \u54644 \u48372 \u49464 \u50836 \u44256 \u51665 \u49828 \u47101 \u44172  \u44396 \u45716  \u44148  \u50500 \u45772 \u51648  \u49373 \u44033 \u54644 \u48400 \u50556  \u54633 \u45768 \u45796 . \u49345 \u45824 \u51032  \u51032 \u44204 \u50640 \u46020  \u44480  \u44592 \u50872 \u50612 \u50556  \u54633 \u45768 \u45796 . \u46608 \u54620 , \u49352 \u47196 \u50868  \u54872 \u44221 \u50640  \u45459 \u51060 \u45908 \u46972 \u46020  \'93\u50997 \u53685 \u49457 \'94\u51060  \u54596 \u50836 \u54624  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47480  \u47568 \u47196  \'93\u51201 \u51025 \'94\u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u48376 \u51064 \u51032  \u46907 \u45824 \u47196  \u48148 \u44984 \u47140 \u45716  \u44163 \u48372 \u45796  \u50997 \u53685 \u49457  \u51080 \u44172  \u51201 \u51025 \u54616 \u47140 \u45716  \u53468 \u46020 \u44032  \u54596 \u50836 \u54633 \u45768 \u45796 .\u51312 \u50616  2. \u50864 \u49440 \u49692 \u50948 \u47484  \u51221 \u54616 \u49464 \u50836 \u47784 \u46304  \u44163 \u51012  \u51096  \u54624  \u49688 \u45716  \u50630 \u49845 \u45768 \u45796 . \u44536  \u51473  \u47924 \u50616 \u44032 \u45716  \u48260 \u47140 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172  \u44032 \u51109  \u51473 \u50836 \u54620  \u44163 \u51008  \u47924 \u50631 \u51064 \u44032 \u50836 ? \u51068 , \u54617 \u50629 , \u50672 \u50528  \u51473  \u47568 \u51060 \u51424 . \u50864 \u49440  \u49692 \u50948 \u47484  \u51221 \u54616 \u44256  \u54616 \u45208 \u50640 \u47564  \u47792 \u46160 \u54616 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u44033 \u51088  \u51201 \u45817 \u54620  \u53440 \u51060 \u48141 \u51060  \u51080 \u49845 \u45768 \u45796 .\u51312 \u50616  3. \u44032 \u48316 \u50868  \u47560 \u51020 \u51012  \u44032 \u51648 \u49464 \u50836 \u45320 \u47924  \u51652 \u51648 \u54616 \u44172  \u49373 \u44033 \u54616 \u51648  \u47568 \u44256 , \u44536 \u51200  \'93\u51600 \u44592 \u47140  \u50772 \u45796 \'94\u45716  \u49885 \u51004 \u47196  \u51076 \u54644 \u48372 \u49464 \u50836 . \u44596 \u51109 \u51008  \u49324 \u46972 \u51648 \u44256 , \u51088 \u50672 \u49828 \u47084 \u50868  \u53468 \u46020 \u44032  \u45208 \u50732  \u44163 \u51077 \u45768 \u45796 .    \
\
   \uc0\u54172 \u53440 \u53364 3 \u51060 \u48120 \u51648 \u54172 \u53440 \u53364 3 \'96 \u54801 \u47141  \u45824 \u49457 \u45817 \u51012  \u44277 \u49324 \u54616 \u44592  \u50948 \u54644  \u47784 \u51064  \u49324 \u46988 \u46308 \u44984 \u51456 \u55176  \u48156 \u51204 \u54616 \u45716  \u51068 , \u50672 \u50528 \u47484  \u45208 \u53440 \u45252 \u49436 \u47196  \u54801 \u47141 \u54616 \u44592  \u50948 \u54644  \u47564 \u45208 \u45716  \u49345 \u54889 \u50500 \u51649  \u48120 \u50756 \u49457 \u46108  \u49345 \u54889 , \u44288 \u44228 \u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54632 \u51060 \u48120 \u51648  \u49345 \u51669 \u45824 \u49457 \u45817  : \u44277 \u46041 \u51004 \u47196  \u52628 \u44396 \u54616 \u45716  \u47785 \u54364 \u51312 \u44033 \u46108  \u54172 \u53440 \u53364  3\u44060  : \u50500 \u51649  \u48120 \u50756 \u49457  \u46108  \u49345 \u54889 , \u47785 \u54364 3\u47749 \u51032  \u49324 \u46988  : \u49436 \u47196  \u54801 \u47141 \u54616 \u45716  \u49324 \u46988 \u51312 \u44033 \u44032  : \u50500 \u51649  \u44221 \u54744 \u51060  \u48120 \u49689 \u54620  \u49324 \u46988 , \u49457 \u49892 \u54632 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u49436 \u47196  \u54801 \u47141 \u54616 \u44172  \u46104 \u45716 \u54172 \u53440 \u53364 3\u51008  \u45572 \u44400 \u44032 \u50752  \u54801 \u47141 \u54616 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . 3\u47749 \u51032  \u49324 \u46988 \u51060  \u45824 \u49457 \u45817 \u51012  \u50756 \u49457 \u54616 \u44592  \u50948 \u54644 , \u49436 \u47196  \u51032 \u45436 \u54616 \u45716  \u51473 \u51077 \u45768 \u45796 . \u44536 \u46308 \u51008  \u52828 \u47785 \u51060  \u50500 \u45772  \u48708 \u51592 \u45768 \u49828  \u47785 \u51201 \u51004 \u47196  \u47564 \u45228 \u49845 \u45768 \u45796 . \u54172 \u53440 \u53364  3\u51008  \u44277 \u46041 \u51032  \u47785 \u54364 \u47484  \u54693 \u54644  \u54801 \u47141 \u54616 \u45716  \u49345 \u54889 , \u45824 \u54868 \u54616 \u45716  \u49345 \u54889 , \u51312 \u50616 \u51012  \u44396 \u54616 \u45716  \u49345 \u54889 \u50640  \u51088 \u51452  \u46321 \u51109 \u54633 \u45768 \u45796 . \u54172 \u53440 \u53364 \u51008  \'93\u54788 \u49892 \u51201 \u51064  \u49457 \u44201 \'94\u51012  \u46944 \u44256  \u51080 \u44592 \u50640  \u44048 \u51221 \u51201 \u51064  \u44368 \u47448 \u48372 \u45796 \u45716  \u51652 \u51648 \u54620  \u45824 \u54868 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 .\u50696 \u52968 \u45824  \u50672 \u50528 \u50868 \u50640 \u49436 \u45716 , \u49345 \u45824 \u48169 \u44284  \u51652 \u51648 \u54620  \u45824 \u54868 \u47484  \u54616 \u44172  \u46104 \u44144 \u45208 , \u48120 \u47000  \u44228 \u54925 \u50640  \u45824 \u54644  \u45436 \u51032 \u54624  \u46412  \u45208 \u53440 \u45208 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u50526 \u51004 \u47196  \u47924 \u50631 \u51012  \u54616 \u47728  \u49332  \u44148 \u51648 , \u49457 \u44201  \u52264 \u51060 \u45716  \u50612 \u46523 \u44172  \u44537 \u48373 \u54624  \u44148 \u51648 , \u46024 \u51008  \u50612 \u46523 \u44172  \u48268  \u44163 \u51064 \u51648 , \u45000 \u51008  \u47924 \u50631 \u51064 \u51648  \u46321 \u46321 \u51077 \u45768 \u45796 . \u50756 \u46300 5\u52376 \u47100  \'93\u49912 \u50880 \'94\u51012  \u45208 \u53440 \u45236 \u44592 \u48372 \u45796 \u45716  \'93\u54801 \u47141 , \u51032 \u45436 \'94\u50640  \u44032 \u44620 \u50868  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u51328  \u45908  \u54252 \u44292 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u49345 \u45824 \u48169 \u51060  \u45208 \u50640 \u44172  \u48148 \u46972 \u45716  \u44163 \u51060  \u51080 \u45796 \u47732  : \u51652 \u51648 \u54620  \u45824 \u54868 , \u51032 \u45436 , \u54788 \u49892 \u51201 \u51064  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u49910 \u51020 \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51008  : \u50500 \u51649  \u48708 \u51592 \u45768 \u49828  \u44288 \u44228 , \u47924 \u50616 \u44032  \u51312 \u50616 \u51012  \u44396 \u54616 \u44256  \u49910 \u51008  \u49324 \u46988 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716  : \u49324 \u50629 \u51060 \u45208  \u51649 \u51109 \u50640 \u49436  \u49352 \u47196 \u50868  \u54028 \u53944 \u45320 \u50752  \u51068 \u51012  \u51652 \u54665 \u54616 \u44172  \u46120 , \u49436 \u47196  \u54801 \u47141 \u54616 \u50668  \u51068 \u51060  \u51096  \u47560 \u47924 \u47532  \u46104 \u45716  \u49345 \u54889 \u51312 \u50616 \u51216 \u51008  : \u54844 \u51088 \u49436  \u54616 \u51648  \u47568 \u44256  \u45572 \u44400 \u44032 \u50752  \u44057 \u51060  \u54644 \u50556  \u54632 \u54644 \u49437  2. \u44984 \u51456 \u55176  \u48156 \u51204 \u54616 \u45716 \u54172 \u53440 \u53364 3\u51008  \u44984 \u51456 \u55176  \u48156 \u51204 \u54616 \u45716  \u49345 \u54889 , \u49436 \u49436 \u55176  \u50676 \u47588 \u47484  \u47610 \u44172  \u46104 \u45716  \u49345 \u54889 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u52852 \u46300  \u49549  \u51312 \u44033 \u44032 \u45716  \'93\u51210 \u51008  \u45224 \u49457 \'94\u51004 \u47196  \u54364 \u54788 \u46096 \u49845 \u45768 \u45796 . \u45432 \u46976 \u49353  \u50526 \u52824 \u47560 \u46020  \u47924 \u50616 \u44032  \'93\u48120 \u49689 \u54620  \u45712 \u45196 , \u44860 \u49688 \u47484  \u48512 \u47532 \u51648  \u50506 \u45716  \u45712 \u45196 \'94\u51012  \u51469 \u45768 \u45796 . \u44536 \u45716  \u51060 \u48264  \u51068 \u51012  \u53685 \u54644  \u45908 \u50865  \u45908  \u49457 \u51109 \u54616 \u44256 , \u48176 \u50864 \u44172  \u46112  \u44163 \u51077 \u45768 \u45796 . \u51060 \u45716  \u50500 \u51649  \u44592 \u49696 \u51201 \u51004 \u47196 \u46020 , \u51064 \u44036 \u44288 \u44228 \u50640  \u51080 \u50612 \u49436 \u46020  \u45916  \u50756 \u49457 \u46096 \u51020 \u51012  \u45208 \u53440 \u45236 \u45716  \u48708 \u50976 \u51201  \u54364 \u54788 \u51077 \u45768 \u45796 . \u50500 \u51649  \u50612 \u47532 \u44256  \u48120 \u49689 \u54616 \u45796 \u45716  \u46907 \u51060 \u51424 .\u44172 \u45796 \u44032  \u49457 \u45817 \u50640  \u51312 \u44033  \u46108  \u54172 \u53440 \u53364 \u51008  \u50500 \u51649  \u52832 \u54644 \u51648 \u51648  \u50506 \u50520 \u49845 \u45768 \u45796 . \u51060 \u45716  \'93\u44277 \u46041 \u51032  \u47785 \u54364 \u44032  \u50500 \u51649  \u48120 \u50756 \u49457 \u46104 \u50632 \u44256 , \u54788 \u51116  \u44984 \u51456 \u55176  \u48156 \u51204 \u54616 \u44256  \u51080 \u51020 \'94\u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u51060 \u51228  \u49353 \u52832 \u47564  \u54616 \u47732  \u46104 \u44592  \u46412 \u47928 \u50640 , \'93\u47785 \u54364 \u44032  \u44144 \u51032  \u50756 \u49457 \u46104 \u50632 \u51020 \'94\u51012  \u50508  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51060 \u47484  \u51649 \u50629 \u50868 \u50640  \u51201 \u50857 \u54616 \u47732  :\u50668 \u47084 \u47784 \u47196  \u44592 \u49696 \u51201 \u51004 \u47196  \u48156 \u51204 \u54616 \u45716  \u49884 \u44592 \u44032  \u46112  \u44163 \u51060 \u45796 .\u51068 \u51060  \u51221 \u52404  \u46104 \u51648  \u50506 \u44256 , \u44984 \u51456 \u55176  \u51652 \u54665 \u46104 \u44256  \u51080 \u45796 . \u47672 \u51648  \u50506 \u50500  \u44208 \u49892 \u51012  \u47610 \u44172  \u46112  \u45216 \u51060  \u50728 \u45796 .\u54617 \u50629 \u50640 \u49436 \u46020  \u44984 \u51456 \u55176  \u48156 \u51204 \u54616 \u44256  \u51080 \u51004 \u45768 , \u47672 \u51648  \u50506 \u50500  \u51339 \u51008  \u51216 \u49688 \u47484  \u50619 \u44172  \u46108 \u45796 .\u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54620 \u45796 \u47732  :\u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020  : \u44288 \u44228 \u47484  \u44984 \u51456 \u55176  \u48156 \u51204 \u49884 \u53412 \u44256  \u49910 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732  : \u44288 \u44228 \u44032  \u51312 \u44552  \u45908  \u44618 \u51008  \u49688 \u51456 \u51004 \u47196  \u48156 \u51204 \u54616 \u44172  \u46108 \u45796 . \u44208 \u54844 \u51012  \u45208 \u53440 \u45244  \u49688 \u46020  \u51080 \u45796 .\u49345 \u45824 \u48169 \u51008  \u44288 \u44228 \u47484  \u52380 \u52380 \u55176 , \u44536 \u47532 \u44256  \u44984 \u51456 \u55176  \u48156 \u51204 \u49884 \u53412 \u44256  \u49910 \u51008  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45817 \u49888  \u46608 \u54620  \u44536  \u54168 \u51060 \u49828 \u50640  \u47582 \u52656 \u44032 \u47732  \u51339 \u51012  \u44163 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u50612 \u45712  \u51901 \u51060  \u51068 \u48169 \u51201 \u51004 \u47196  \u47532 \u46300 \u54616 \u44256  \u45132 \u50612 \u44032 \u45716  \u44172  \u50500 \u45768 \u46972 , \u55192 \u51012  \u54633 \u52432  \u48120 \u47000 \u47484  \u47564 \u46308 \u44256  \u49910 \u45796 \u45716  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u52280 \u44256  \u51088 \u47308  : \u53440 \u47196  \u52852 \u46300  \'93\u54172 \u53440 \u53364 \u51032  3\u8243 \u51032  \u51032 \u48120 , \u54644 \u49437 , \u49345 \u51669  \'96 \u53440 \u47196  \u54616 \u52824 \u48288 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48120 \u50756 \u49457 \u46108 \u54172 \u53440 \u53364 3\u51008  \u50500 \u51649  \u48120 \u50756 \u49457  \u46096 \u51020 \u51012  \u44221 \u44256 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50526 \u49436  \u47568 \u54664 \u46319 , \u54172 \u53440 \u53364 \u51008  \u50500 \u51649  \u49353 \u52832 \u46104 \u51648  \u50506 \u51008  \u49345 \u53468 \u51077 \u45768 \u45796 . \u51060 \u45824 \u47196  \u51068 \u51012  \u51473 \u45800 \u54616 \u44172  \u46104 \u47732 , \u49457 \u45817 \u51008  \u44208 \u53076  \u50756 \u49457 \u46104 \u51648  \u50506 \u49845 \u45768 \u45796 . \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u50500 \u51649  \u48512 \u51313 \u54620  \u44172  \u47566 \u51004 \u45768  \u44984 \u51456 \u55176  \u45432 \u47141 \u54644 \u50556  \u54620 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u54633 \u44201 \u50868 \u46020  \u45824 \u52404 \u47196  \u51339 \u51004 \u45208  \u46263 \u49900 \u51060  \u48512 \u51313 \u54616 \u50668  \u49892 \u54056 \u54616 \u44172  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51060 \u52196  \u54616 \u47732  \u46096 \u51648  \u46972 \u45716  \u49373 \u44033 \u51008  \u51217 \u50612 \u46160 \u50612 \u50556  \u54633 \u45768 \u45796 .\u50672 \u50528 \u50868 \u50640 \u49436 \u45716  :\u52964 \u54540 \u51060 \u46972 \u47732  : \u50500 \u51649  \u46168  \u49324 \u51060 \u50640  \u54644 \u44208 \u54616 \u51648  \u47803 \u54620  \'93\u44284 \u51228 \'94\u44032  \u45224 \u50500  \u51080 \u51020 . \u44288 \u44228 \u47484  \u48156 \u51204 \u49884 \u53412 \u44592  \u50948 \u54644 \u49440  \u44536  \u47928 \u51228 \u47484  \u44845  \u51674 \u44256  \u45336 \u50612 \u44032 \u50556  \u54632 .\u50040 \u51060 \u46972 \u47732  : \u51652 \u51648 \u54620  \u45824 \u54868 \u44032  \u44845  \u54596 \u50836 \u54644  \u48372 \u51076 . \u50672 \u51064 \u51004 \u47196  \u46104 \u44592  \u51204  \u44144 \u52432 \u50556  \u54624  \u54596 \u49688  \u50836 \u49548 \u51076 . \u47564 \u50557  \u49457 \u44553 \u54616 \u44172  \u51652 \u51204  \u49884 \u53040 \u45796 \u44036 , \u50620 \u47560  \u50504  \u44032  \u44648 \u51648 \u44172  \u46112  \u49688  \u51080 \u51020 .\u51473 \u44036 \u50640  \u45817 \u49888 \u51060  \u49552 \u51012  \u46524 \u47732 , \u51068 \u51060 \u45208  \u50672 \u50528 \u44032  \u44536  \u49692 \u44036 \u48512 \u53552  \u51221 \u52404  \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 . \u44984 \u51456 \u55176  \u45432 \u47141 \u54616 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 .\u54172 \u53440 \u53364 3 \u50672 \u50528 \u50868  \u54644 \u49437 \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u51032 \u49324 \u49548 \u53685  \u45733 \u47141 \u51060  \u46832 \u50612 \u45208 \u47728  \u49324 \u46988 \u51012  \u47784 \u51004 \u45716  \u55192 \u51060  \u51080 \u49845 \u45768 \u45796 . \u51060  \u52852 \u46300 \u45716  \u51452 \u47196  \'93\u50612 \u46500  \u51060 \u54644  \u44288 \u44228 \u44032  \u50621 \u55180  \u49324 \u46988 \'94\u51012  \u45208 \u53440 \u45236 \u45716  \u44221 \u50864 \u44032  \u47566 \u49845 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644  \u46024 \u51004 \u47196  \u50670 \u51064  \u49324 \u46988 \u51060 \u46972 \u45716  \u46907 \u51060 \u51424 . \u50696 \u52968 \u45824  \u51649 \u51109  \u46041 \u47308 , \u54617 \u44368  \u49440 \u54980 \u48176 , \u44144 \u47000 \u52376  \u49324 \u46988  \u46321 \u46321 \u51077 \u45768 \u45796 . \u44148 \u49444 \u51060 \u45208  \u51064 \u53580 \u47532 \u50612 \u52376 \u47100  \u51452 \u47196  \u49552 \u44592 \u49696 \u51012  \u50836 \u54616 \u45716  \u51649 \u51333 \u51012  \u45208 \u53440 \u45244  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54172 \u53440 \u53364 3\u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u44288 \u44228 \u47484  \u44984 \u51456 \u55176  \u48156 \u51204 \u49884 \u53412 \u44256  \u49910 \u51008  \u44163  \u44057 \u49845 \u45768 \u45796 . \u51652 \u46020 \u47484  \u54620  \u48264 \u50640  \u54029 \u54029  \u45208 \u44032 \u47140 \u45716  \u49373 \u44033 \u51008  \u50630 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u50500 \u51649 \u51008  \u51060 \u49457 \u51201 \u51064  \u44048 \u51221 \u48372 \u45796 \u45716  \'93\u46041 \u47308 \'94\u47196 \u49436  \u47560 \u51020 \u51060  \u53360  \u49345 \u53468 \u51077 \u45768 \u45796 . \u47568 \u46020  \u51096  \u53685 \u54616 \u44256 , \u50612 \u46360 \u44032  \u51312 \u50616 \u51012  \u44396 \u54624 \u47564 \u54620  \u49324 \u46988 \u51004 \u47196 \u46020  \u48372 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \'93\u51652 \u51648 \u54632 \'94\u51012  \u45208 \u53440 \u45236 \u45716  \u52852 \u46300 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u48512 \u51221 \u51201 \u51060 \u46972 \u47732 , \u44536 \u51200  \'93\u50612 \u46500  \u47932 \u51656 \'94\u51012  \u52313 \u44200 \u51452 \u44144 \u45208 , \u48155 \u