u46748 \u47159 \u54620 \u51221 \u51032  \u52852 \u46300 \u45716  \u52712 \u54693 \u51060 \u45208  \u51452 \u44288 \u51060  \u46748 \u47159 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45216 \u52852 \u47196 \u50868  \u52489 \u51004 \u47196  \u51649 \u51109 \u50640 \u49436 \u46020  \u49457 \u44284 \u44032  \u51339 \u51008  \u54200 \u51060 \u47728 , \u45224 \u50640 \u44172  \u55064 \u46168 \u47532 \u51648  \u50506 \u50500  \u44032 \u49828 \u46972 \u51060 \u54021 \u46020  \u51096  \u45817 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 .\u52852 \u46300 \u51032  \u51060 \u47492 \u51008  \'91\u51221 \u51032 \'92\u51060 \u51648 \u47564 , \u50500 \u51060 \u47084 \u45768 \u54616 \u44172 \u46020  \'91\u51452 \u44288 \u51201 \u51064 \'92 \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50864 \u47532 \u44032  \u49373 \u44033 \u54616 \u45716  \'91\u51221 \u51032 \u51032  \u49324 \u46020 \'92 \u51060 \u47088  \u45712 \u45196 \u51060  \u50500 \u45785 \u45768 \u45796 . \u50780 \u45264 \u54616 \u47732  \u52852 \u46300  \u49549  \u48277 \u44288 \u51008  \u45576 \u51012  \u46888 \u44256  \u51080 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u51060 \u45716  \'91\u54032 \u45800 \u50640  \u51452 \u44288 \u51060  \u45812 \u44596 \u45796 \'92\u45716  \u51032 \u48120 \u47484  \u45812 \u44256  \u51080 \u49845 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u50672 \u50528 \u50868 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u51060 \u49345 \u54805 \u51060  \u46748 \u47159 \u54644 \u49436  \'91\u44036 \u53469 \'92 \u45817 \u54616 \u45716  \u49688 \u48150 \u50640  \u50630 \u45796 \u51060 \u48120  \u45817 \u49888 \u51060  \'91\u44036 \u53469 \'92 \u45817 \u54664 \u45796 \u47732 , \u44536 \u51032  \u44620 \u45796 \u47196 \u50868  \u52712 \u54693 \u51012  \u47564 \u51313  \u49884 \u53040 \u51012  \u44163 \u51060 \u45796 \u45813 \u51221 \u45320  \u49457 \u54693 \u51060  \u50612 \u45712  \u51221 \u46020 \u45716  \u51080 \u45796 \u49324 \u46988 \u51012  \u48380  \u46412  \u49440 \u51077 \u44204 \u51060  \u51080 \u51012  \u44032 \u45733 \u49457 \u46020  \u45458 \u49845 \u45768 \u45796 . \u51060 \u44148  \u50696 \u49884 \u51077 \u45768 \u45796 .\u47928 \u49888 \u51060  \u51080 \u45716  \u49324 \u46988 \u51012  \u49899 \u50612 \u54620 \u45796 \u49696  \u51339 \u50500 \u54616 \u45716  \u49324 \u46988 \u51012  \u48324 \u47196  \u51339 \u50500 \u54616 \u51648  \u50506 \u45716 \u45796 \u53945 \u51221  \u51649 \u50629 \u51012  \u49899 \u50612 \u54616 \u44144 \u45208 , \u44288 \u49345 \u51004 \u47196  \u49324 \u46988 \u51012  \u54032 \u45800 \u54620 \u45796 \u51221 \u51032  \u52852 \u46300 \u51032  \u47784 \u54000 \u48652 \u45716  \u46356 \u52992 \u51077 \u45768 \u45796 . \u46356 \u52992 \u45716  \u51221 \u51032 \u51032  \u50668 \u49888 \u51004 \u47196 , \u50724 \u47019 \u46041 \u50504  \u44277 \u51221 \u54632 \u51032  \u49345 \u51669 \u51004 \u47196  \u51088 \u47532  \u51105 \u50500  \u50772 \u49845 \u45768 \u45796 . \u45796 \u47564  \u44536 \u45376 \u45716  \u52852 \u46300  \u49549  \u48277 \u44288 \u44284  \u45804 \u47532  \u45576 \u44032 \u47532 \u44060 \u47484  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51060  \u45576 \u44032 \u47532 \u44060 \u45716  \'91\u51452 \u44288 \u44284  \u44048 \u51221 \u50640  \u55081 \u50424 \u47532 \u51648  \u50506 \u51004 \u47728 , \u51312 \u44552 \u51060 \u46972 \u46020  \u50724 \u54644 \u47484  \u49324 \u51648  \u50506 \u44192 \u45796 \u45716  \u51032 \u51648 \u51032  \u54364 \u47749 \'92\u51077 \u45768 \u45796 . \u44263  \'91\u44061 \u44288 \u49457 \'92\u51012  \u49345 \u51669 \u54616 \u51424 .\u54644 \u49437  3. \u49100 \u47536 \u45824 \u47196  \u44144 \u46160 \u45716 \u51221 \u51032  \u52852 \u46300 \u45716  \u49100 \u47536 \u45824 \u47196  \u44144 \u46164 \u45796 \u45716  \u51032 \u48120 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47480  \u47924 \u50616 \u44032 \u44032  \'91\u45180 \u50612 \u46308 \u51648 \'92 \u50506 \u45716 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 .\u52852 \u46300  \u49549  \u48277 \u44288 \u51008  \u44160 \u51012  \u46308 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44160 \u51008  \u44208 \u45800 \u51060 \u45208  \u54032 \u45800 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u48277 \u44288 \u51060  \u50612 \u46500  \'91\u54032 \u45800 \'92\u51012  \u45236 \u47536 \u45796 \u47732 , \u44208 \u44284 \u45716  YES OR NO\u47196  \u46385 \u46385  \u45208 \u45600  \u46504 \u50612 \u51656  \u44163 \u51077 \u45768 \u45796 . \u47749 \u54869 \u54616 \u44172  \u44208 \u47200 \u45225 \u45768 \u45796 .\u51593 , \u50526 \u51004 \u47196  \u50619 \u44172  \u46112  \u44208 \u44284 \u44032  \'91\u45817 \u49888 \u51032  \u54032 \u45800 \'92\u50640  \u51032 \u54644  \u44208 \u51221 \u46108 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u50864 \u47532 \u45716  \u51060 \u44163 \u51012  \u51088 \u50629 \u51088 \u46301 , \u49828 \u49828 \u47196  \u48520 \u47084 \u50728  \u51116 \u50521 , \u45432 \u47141 \u51008  \u48176 \u49888 \u54616 \u51648  \u50506 \u45716 \u45796 \u46972 \u44256  \u48512 \u47493 \u45768 \u45796 .\u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u44284 \u44144 \u50640  \u50612 \u46500  \u51096 \u47803 \u51012  \u54664 \u44256 , \u48120 \u47000 \u50640  \u51221 \u51032  \u52852 \u46300 \u44032  \u46516 \u45796 \u47732 , \u44263  \u44288 \u44228 \u44032  \u45149 \u45212 \u45796 \u44256  \u54644 \u49437 \u54624  \u49688  \u51080 \u45796 \u54633 \u44201 \u50868 \u51012  \u47932 \u50632 \u51012  \u46412 , \'91\u49892 \u47141 \u45824 \u47196  \u48376 \u45796 \'92\u44256  \u45813 \u48320 \u54624  \u49688  \u51080 \u45796 \u51068 \u51060  \u51096  \u50504 \u46108 \u45796 \u47732 , \u50868 \u51060  \u45208 \u49240 \u45796 \u44592 \u48372 \u45796 \u45716  \u45817 \u49888 \u51032  \u53468 \u46020  \u54841 \u51008  \u51068 \u51032  \u48169 \u49885 \u50640  \u47928 \u51228 \u44032  \u51080 \u45796 \u45716  \u46907 \u51060 \u45796 \u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u48376 \u51064  \u49888 \u45392 \u45824 \u47196  \u54616 \u45716 \u51221 \u51032  \u52852 \u46300 \u45716  \u48376 \u51064  \u49888 \u45392 \u45824 \u47196  \u54032 \u45800 \u54616 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u44256 \u51665 \u51060  \u49464 \u44144 \u45208  \u50997 \u53685 \u49457 \u51060  \u48512 \u51313 \u54616 \u45796 \u45716  \u45656 \u50521 \u49828 \u51077 \u45768 \u45796 .\u52852 \u46300 \u51032  \u44536 \u47548 \u51012  \u48372 \u47732  : \u48277 \u44288  \u46244 \u50640 \u45716  \u48521 \u51008  \u52380 \u47561 \u51060  \u51080 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u52380 \u47561 \u51008  \u53468 \u50577 \u48731 \u51012  \u44032 \u47532 \u44256  \u51080 \u51424 . \u51060 \u45716  \u44263  \'91\u50808 \u48512  \u49345 \u54889 \u51060 \u45208  \u51032 \u44204 \u51012  \u52264 \u45800 \u54616 \u44256  \u51080 \u45796 \'92\u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u49345 \u45824 \u48169 \u51060  \u47560 \u51020 \u50640  \u46308 \u51648  \u50506 \u51004 \u47732 , \'91\u45208 \u46993  \u51060 \u50612 \u51656  \u49688  \u50630 \u45716  \u49324 \u46988 \'92\u51060 \u46972 \u44256  \u48120 \u47532  \u44208 \u47200 \u51012  \u45236 \u47140 \u48260 \u47548 . \'91\u50504 \u46104 \u45716  \u51060 \u50976 \u47484  \u47564 \u46308 \u50612 \u49436 \'92 \u54633 \u47532 \u54868 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u51020 . \u48708 \u47197  \u51339 \u51008  \u47732 \u51012  \u48156 \u44204 \u54616 \u45908 \u46972 \u46020 .\u51452 \u48320 \u50640 \u49436  \'91\u51339 \u51008  \u49324 \u46988 \'92\u51060 \u46972 \u44256  \u54616 \u45908 \u46972 \u46020 , \u46308 \u51008  \u52292  \u47564  \u52292 ~\u54644 \u49437  2. \u51060 \u44163 \u51200 \u44163  \u51116 \u48372 \u44256  \u46384 \u51648 \u45716 \u51221 \u51032  \u52852 \u46300 \u45716  \u51060 \u44163 \u51200 \u44163  \u51116 \u48372 \u44256  \u46384 \u51648 \u45716 , \u54841 \u51008  \u48708 \u44368 \u54616 \u45716  \u49345 \u54889 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51060  \u49549 \u47560 \u51020 \u51012  \u47932 \u50632 \u45796 \u47732 , \'91\u45796  \u54140 \u51460  \u47564 \u53372 \u51032  \u54840 \u44048 \'92\u51008  \u50630 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44228 \u49328 \u51201 \u51064  \u49324 \u46988 , \u45908 \u52824 \u54168 \u51060  \u54616 \u44144 \u45208  \u46301 \u49892  \u46384 \u51648 \u45716  \u44288 \u44228 \u50640 \u49436 \u46020  \u51088 \u51452  \u46321 \u51109 \u54633 \u45768 \u45796 .\u45656 \u50521 \u49828 \u47484  \u49789 \u44172  \u51060 \u54644 \u54616 \u47140 \u47732  : \u47732 \u51217 \u44288 \u51060  \u49436 \u47448 \u47484  \u44160 \u53664 \u54616 \u45716  \u49345 \u54889 \u51012  \u46496 \u50732 \u47532 \u47732  \u46121 \u45768 \u45796 . \u45208 \u51060 , \u54617 \u48268 , \u50808 \u47784 , \u44221 \u47141  \u46321 \u46321 \u51012  \u44860 \u44860 \u55176  \u44160 \u53664 \u54620  \u46244 , \u44592 \u51456 \u50640  \u48512 \u54633 \u54616 \u47732  OK. \u50500 \u45768 \u47732  OUT.\u44396 \u52404 \u51201 \u51004 \u47196  \u47532 \u46377 \u50640  \u51201 \u50857 \u54644 \u48372 \u47732  :\u49556 \u51649 \u55176  \u47568 \u54644 \u49436  \u50808 \u47784 \u45716  \u45236 \u44032  \u45908  \u50948  \u50500 \u45772 \u44032 ? \u54617 \u48268 \u46020  \u45236 \u44032  \u45908  \u44316 \u52270 \u51008 \u45936 \u51648 \u44552  \u45236 \u44032  \u50672 \u50528 \u47484  \u54644 \u46020  \u46104 \u45208 ? \u50672 \u50528 \u45716  \u45208 \u54620 \u53580  \u49324 \u52824  \u44057 \u51008 \u45936 \'85 \u45320 \u47924  \u44256 \u48124 \u46108 \u45796 \u51060 \u44172  \u45236 \u44032  \u51096 \u47803 \u54644 \u49436  \u54756 \u50612 \u51652  \u44148 \u44032 ? \u45236 \u44032  \u48380  \u46416  \u44116  \u47928 \u51228 \u44032  \u45908  \u52984 \u50612 \u45236 \u44032  \u51060 \u48264  \u45804 \u50640 \u45716  \u48165 \u51012  5\u48264  \u49344 \u51004 \u45768 \u44620 , \u44116 \u46020  \u48165 \u51012  5\u48264  \u49324 \u50556  \u46076 . 2\u48264 \u47564  \u45908  \u50619 \u50612  \u47673 \u51004 \u47732  \u46104 \u44192 \u45796 \u51649 \u50629 \u50868 \u50640 \u49436 \u45716 , \u44592 \u54924 \u48708 \u50857 \u51012  \u46384 \u51256 \u48400 \u50556  \u54620 \u45796 \u45716  \u51312 \u50616 \u51216 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 .\u51221 \u51032  \u52852 \u46300  \u50672 \u50528 \u50868  \u54644 \u49437 \u51221 \u51032  \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u54217 \u46321 \u54616 \u47728  \u49436 \u47196  \u48176 \u47140 \u54616 \u45716  \u44288 \u44228 \u47484  \u51339 \u50500 \u54633 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u54620 \u51901 \u51060  \u51068 \u48169 \u51201 \u51004 \u47196  \u46024 \u51012  \u45236 \u44144 \u45208 , \u50672 \u46973 \u54616 \u45716  \u44288 \u44228 \u47484  \u49899 \u50612 \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44592 \u48652 \u50532 \u53580 \u51060 \u53356 \u45208  \u45908 \u52824 \u54168 \u51060 \u47484  \u52628 \u44396 \u54616 \u45716  \u49457 \u44201 \u53459 \u50640 , \u44032 \u45140  \'91\u51221 \u51060  \u50630 \u44144 \u45208 , \u45320 \u47924  \u51900 \u51092 \u54616 \u45796 \'92\u44256  \u45712 \u45188  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 . \u44032 \u51109  \u53360  \u45800 \u51216 \u51004 \u47196 \u45716  \'91\u50997 \u53685 \u49457  \u48512 \u51313 , \u44256 \u51221 \u44288 \u45392 \'92\u51060 \u47728  \u49464 \u49345 \u51012  \u50976 \u50672 \u54616 \u44172  \u48148 \u46972 \u48380  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u51221 \u51032  \u52852 \u46300 \u44032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \'91\u50672 \u50528 \u51032  \u49549 \u46020 \u47484  \u47582 \u52628 \u44192 \u45796 , \u47560 \u51020 \u51012  \u51312 \u51208  \u54616 \u44192 \u45796 \'92\u46972 \u45716  \u46907 \u46020  \u51080 \u51648 \u47564 , \u44592 \u48376 \u51201 \u51004 \u47196  \u45817 \u49888 \u50640 \u44172  \'91\u53360  \u54840 \u44048 \'92\u51008  \u50630 \u50612  \u48372 \u51077 \u45768 \u45796 . \u51060 \u44163 \u51200 \u44163  \u51116 \u44256  \u46384 \u51656  \u47564 \u53372 \u51032  \u44288 \u44228 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u51452 \u47196  \'91\u44277 \u51201 \u51064  \u44288 \u44228 \'92\u50640 \u49436  \u51088 \u51452  \u46321 \u51109 \u54616 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u45817 \u49888 \u50640 \u44172  \u50696 \u51032 \u47484  \u44054 \u52628 \u44256 , \u48176 \u47140 \u54616 \u47140 \u45716  \u47560 \u51020 \u51060  \u50687 \u48372 \u51077 \u45768 \u45796 . \u45817 \u49888 \u51012  \'91\u46041 \u46321 \u54620  \u51064 \u44201 \u52404 \'92\u47196 \u49436  \u51316 \u51473 \u54644 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196 \u47484  \u51316 \u51473 \u54644 \u51452 \u47728  \u44512 \u54805  \u51105 \u55180  \u44288 \u44228 \u47484  \u47564 \u46308 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \'91\u44049 -\u51012  \u44288 \u44228 \'92\u45208  \'91\u50612 \u47532 \u49689 \u54620  \u50672 \u50528 \'92\u50752 \u45716  \u44144 \u47532 \u44032  \u47689 \u45768 \u45796 . \u52628 \u44032 \u47196  \u49345 \u45824 \u48169 \u51008  \u51060  \u44288 \u44228 \u47484  \'91\u50724 \u54588 \u49500 \'92\u47196  \u47564 \u46308 \u44256  \u49910 \u51008  \u44163  \u44057 \u49845 \u45768 \u45796 . \u45796 \u49884  \u47568 \u54644  \u44208 \u54844 \u54616 \u44256  \u49910 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u49436 \u47196  \u48708 \u49847 \u54620  \u47732 \u46020  \u47566 \u50500  \u44208 \u54844 \u54644 \u46020  \u51339 \u51008  \u49324 \u51060 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u46168  \u49324 \u51060 \u44032  \u45320 \u47924  \u44228 \u49328 \u51201 \u51004 \u47196  \u46104 \u51648  \u50506 \u46020 \u47197  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u49552 \u54644  \u48372 \u47728  \u50672 \u50528  \u54616 \u44192 \u45796 \u45716  \u47560 \u51020 \u51012  \u44032 \u51648 \u49884 \u44600  \u48148 \u46989 \u45768 \u45796 .\u51116 \u54924 \u50868 \u51008  \u48512 \u51221 \u51201 \u51077 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \'91\u51060 \u48324 \u51012  \u52712 \u49548 \u54616 \u51648  \u50506 \u44192 \u45796 \'92\u45716  \u51077 \u51109 \u51012  \u52712 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \'93\u45320 \u44032  \u51096 \u47803 \u54664 \u51004 \u45768  \u51060 \u48324 \u51008  \u45817 \u50672 \u54620  \u44144 \u50556 \'94 \'93\u50864 \u47532 \u45716  \u50612 \u52264 \u54588  \u50504  \u46112  \u49324 \u51060 \u50688 \u50612 \'94 \u50612 \u46360 \u44032  \u54633 \u47532 \u54868 \u54616 \u45716  \u47560 \u51020 \u46020  \u46027 \u48372 \u51077 \u45768 \u45796 . \u54841 \u50668 \u45208  \u45796 \u49884  \u47564 \u45208 \u45908 \u46972 \u46020 , \u44536  \u50696 \u54980 \u45716  \u51339 \u51648  \u50506 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51060  \u50612 \u46500  \u48169 \u49885 \u51004 \u47196  \u51060 \u48324 \u51012  \u53685 \u48372 \u54664 \u45716 \u51648 , \u49457 \u44201 \u51060  \u48148 \u45068 \u50632 \u45716 \u51648  \u46321 \u46321 \u51012  \u45130 \u51076 \u50630 \u51060  \u46384 \u51656  \u44163 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u51221 \u51032  \u52852 \u46300  \u51649 \u50629 \u50868  \u54644 \u49437 \u54665 \u46041 \u50640  \u46384 \u47480  \u44208 \u44284 \u47484  \u50619 \u44172  \u46104 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612  \u47792 \u47000  \u47924 \u50616 \u44032 \u47484  \u48764 \u46028 \u47160 \u45796 \u47732  \u44536 \u45824 \u47196  \u46308 \u53685 \u45208 \u47728 , \u50676 \u49900 \u55176  \u51068 \u54664 \u45796 \u47732  \u44536 \u47564 \u53372 \u51032  \u48372 \u49345 \u51012  \u44256 \u49828 \u46976 \u55176  \u46028 \u47140 \u48155 \u44172  \u46121 \u45768 \u45796 . \u47564 \u50557  \u45817 \u49888 \u51060  \u51649 \u50629 \u51201 \u51004 \u47196  \u47928 \u51228 \u47484  \u44202 \u44256  \u51080 \u45796 \u47732  \'91\u51068 \u51032  \u48169 \u49885 \u51060 \u45208  \u53468 \u46020 \'92\u50640  \u47928 \u51228 \u44032  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u50868 \u51060  \u45208 \u48736 \u49436 , \u49884 \u44592 \u44032  \u45208 \u48736 \u49436 \u44032  \u50500 \u45785 \u45768 \u45796 . \u51228 \u44032  \u46300 \u47532 \u44256  \u49910 \u51008  \u51312 \u50616 \u51008  1\u44032 \u51648 , \u49345 \u54889 \u50640  \u47582 \u44172  \u54665 \u46041 \u54616 \u49464 \u50836 . \u50997 \u53685 \u49457 \u51060  \u48512 \u51313 \u54644 \u48372 \u51077 \u45768 \u45796 .\'91\u50892 \u46972 \u48184 \'92\u51012  \u52313 \u44200 \u50556  \u54620 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54633 \u45768 \u45796 . \u52572 \u44540  \u51068 \u50640  \u49548 \u54848 \u54664 \u45796 \u47732  \'91\u51068 \u51012  \u50676 \u49900 \u55176  \u54644 \u50556  \u54620 \u45796 \'92\u45716  \u51032 \u48120 \u44256 , \u50672 \u50528 \u45208  \u52712 \u48120 \u50640  \u49548 \u54848 \u54664 \u45796 \u47732  \'91\u51068 \u51012  \u51460 \u50668 \u50556  \u54620 \u45796 \'92\u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u54633 \u44201 \u50868 \u51008  \u48152 \u48152 \u51077 \u45768 \u45796 . \u49100 \u47536 \u45824 \u47196  \u44144 \u46164 \u45796 \u45716  \u46907 \u51060  \u51080 \u49845 \u45768 \u45796 . \u49892 \u49688 \u47484  \u54616 \u44144 \u45208  \u50619 \u50612  \u44152 \u47532 \u45716  \u51068 \u51008  \u50630 \u51012  \u44163 \u51077 \u45768 \u45796 .\u51221 \u51032  \u52852 \u46300  \u44552 \u51204 \u50868  \u54644 \u49437 \u49548 \u48708 \u50752  \u51648 \u52636 \u51012  \u44512 \u54805  \u51080 \u44172  \u51312 \u51208 \u54644 \u50556  \u54620 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u47588 \u45804  \u46308 \u50612 \u50724 \u44256  \u45208 \u44032 \u45716  \u46024 \u51060  \u50620 \u47560 \u51064 \u51648  \u50508  \u54596 \u50836 \u44032  \u51080 \u51004 \u47728 , \'91\u44284 \u44048 \u54620  \u53804 \u51088 \'92\u47484  \u44048 \u54665 \u54616 \u45716  \u49884 \u44592 \u45716  \u50500 \u45785 \u45768 \u45796 . \'91\u48260 \u45716  \u46024 \u51060  \u45720 \u50612 \u45212  \u47564 \u53372  \u50432 \u50432 \u51060 \u46020  \u52964 \u51276 \u45796 \'92\u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .    \
