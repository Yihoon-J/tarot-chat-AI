    \uc0\u50756 \u46300  \u53304  \u44536 \u47548 \u51032  \u51032 \u48120 \u50676 \u51221 \u51201 \u51060 \u44256  \u51471 \u45824  \u51080 \u45716  \u49324 \u46988 \u50808 \u47784 \u44032  \u47588 \u47141 \u51201 \u51064  \u50668 \u49457 \u51012  \u49345 \u51669 \u50808 \u48512 \u50640  \u44288 \u49900 \u51060  \u47566 \u51004 \u45208 , \u54665 \u46041 \u51060  \u51228 \u54620 \u51201 \u51064  \u49324 \u46988 \u50724 \u51648 \u46998 \u51060  \u51080 \u45716  \u49324 \u46988 \u51201 \u44537 \u51201 \u51004 \u47196  \u45796 \u44032 \u44032 \u44256  \u49910 \u51008  \u47560 \u51020 \u44557 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u51088 \u49888 \u44048 , \u51471 \u45824 \u44032  \u51080 \u45716 \u50756 \u46300  \u53304 \u51008  \u51088 \u49888 \u44048 \u51060  \u45336 \u52824 \u47728 , \u51088 \u44592  \u51452 \u44288 \u51060  \u46748 \u47159 \u54620  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 . \u51200 \u45716  \u44060 \u51064 \u51201 \u51004 \u47196  \'9230\u45824  \u52488 \u48152  \u52964 \u47532 \u50612 \u50864 \u47676 \'92\u51060  \u49373 \u44033 \u45208 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u52852 \u46300 \u47484  \u48372 \u47732  : \u50773 \u48708 \u45716  \u50526  \u47784 \u49845 \u51012  \u51088 \u49888  \u51080 \u44172  \u48372 \u50668 \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50756 \u46300 \u47484  \u49688 \u51649 \u51004 \u47196  \u46308 \u50612 , \u47560 \u52824  \u48376 \u51064 \u51032  \u50689 \u50669 \u51012  \u51648 \u53412 \u45716  \u46319 \u54620  \u47784 \u49845 \u51077 \u45768 \u45796 . \u44536 \u45376 \u51032  \u54252 \u51592  \u46608 \u54620  \u44060 \u48169 \u51201 \u51060 \u47728  (\u51789 \u48268 ) \u51088 \u49888 \u44048 \u51060  \u45336 \u52841 \u45768 \u45796 .\u53304  \u50756 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u44288 \u44228 \u50640  \u45824 \u54620  \u48520 \u50504 \u44048 \u51060  \u51201 \u51008  \u54200 \u51077 \u45768 \u45796 . \u45224 \u50640 \u44172  \u51032 \u51316 \u54616 \u44144 \u45208  \u49552 \u51012  \u46496 \u48268 \u47532 \u45716  \u51068 \u46020  \u50630 \u49845 \u45768 \u45796 . \u46412 \u47928 \u50640  \u49892 \u51228 \u47196  \u45208 \u51060 \u44032  \u47566 \u44144 \u45208 , \u46608 \u47000 \u48372 \u45796  \u50612 \u47480 \u49828 \u47084 \u50868  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u51221 \u49436 \u46020  \u50504 \u51221 \u51201 \u51060 \u47728 , \u45572 \u44400 \u44032 \u47484  \u47532 \u46300 \u54616 \u45716  \u45936  \u51061 \u49689 \u54633 \u45768 \u45796 .\u51060 \u47484  \u47532 \u46377 \u50640  \u51201 \u50857 \u54616 \u47732  :\u51649 \u50629 \u50868  \u8594  \u47532 \u45908  \u54841 \u51008  \u51452 \u46020 \u51201 \u51004 \u47196  \u51068 \u51012  \u52376 \u47532 \u54616 \u45716  \u49324 \u46988 , \u44144 \u52840 \u50630 \u51060  \u51032 \u44204 \u51012  \u44060 \u51652 \u54616 \u45716  \u49324 \u46988 , \u51088 \u49888 \u51032  \u49888 \u45392 \u51012  \u44413 \u55176 \u51648  \u50506 \u44256  \u50896 \u54616 \u45716  \u51068 \u51012  \u54616 \u44172  \u46112  \u44163 \u50672 \u50528 \u50868  \u8594  \u50672 \u50528 \u47484  \u51452 \u46020 \u54616 \u45716  \u49324 \u46988 , \u49345 \u45824 \u48169 \u50640 \u44172  \u55064 \u46168 \u47532 \u51648  \u50506 \u45716  \u50672 \u50528 , \u44032 \u49828 \u46972 \u51060 \u54021 \u51012  \u51096  \u45817 \u54616 \u51648  \u50506 \u51020 \u48512 \u45817 \u54620  \u51068 \u51012  \u45817 \u54664 \u45796 \u47732 , \u54624  \u47568  \u45796 \u54616 \u47728  \u49912 \u50880 \u46020  \u47560 \u45796 \u54616 \u51648  \u50506 \u44592 \u50640 , \u50668 \u47084 \u47784 \u47196  \u49884 \u50896 \u49884 \u50896 \u54620  \u49457 \u44201 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50668 \u49457 \u46308 \u50640 \u44172  \u49440 \u47581 \u51032  \u45824 \u49345 \u51060  \u46112 \u47564 \u54620  \u52880 \u47533 \u53552 \u45348 \u50836 .\u54644 \u49437  2. \u47588 \u47141 \u51201 \u51064 \u50756 \u46300  \u53304 \u51008  \u51452 \u47196  \u47588 \u47141 \u51201 \u51064  \u49324 \u46988 , \u54841 \u51008  \u48157 \u44256  \u45209 \u52380 \u51201 \u51064  \u49324 \u46988 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u50668 \u50773  \u48145 \u50640  \u51080 \u45716  \u44160 \u51008  \u44256 \u50577 \u51060 \u45716  \'91\u44288 \u45733 \u51201 \u51064  \u47588 \u47141 \'92\u51012 , \u50668 \u50773 \u51032  \u48268 \u50612 \u51652  \u45796 \u47532 \u44032  \'91\u49457 \u50837 \'92\u51012  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u50640 \u49436  \u46321 \u51109 \u54664 \u51012  \u46412 \u45716 , \'91\u45817 \u49888 \u51060  \u47588 \u47141 \u51201 \u51004 \u47196  \u45712 \u44788 \u51652 \u45796 \'92\u44256  \u54644 \u49437 \u54616 \u45716 \u45936 , \u49345 \u45824 \u48169 \u51008  \u51064 \u44036  \u51088 \u52404 \u47196 \u49436 \u51032  \u54840 \u44048  \u46608 \u45716  \u49457 \u50837 \u51012  \u54408 \u44256  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 . \u54617 \u47141 , \u51649 \u50629 , \u44221 \u51228 \u51201  \u51312 \u44148  \u46321  \'91\u44228 \u49328 \u51201 \u51064  \u47560 \u51020 \'92\u51012  \u46384 \u51648 \u51648  \u50506 \u49845 \u45768 \u45796 . \u44536 \u48372 \u45796 \u45716  \'91\u53945 \u50976 \u51032  \u48157 \u51008  \u47588 \u47141 , \u47800 \u51667 , \u51088 \u49888 \u44048  \u51080 \u45716  \u53468 \u46020 \'92\u50640 \u49436  \u48708 \u47215 \u46108  \u47560 \u51020 \u51060 \u46972  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51008  \u44396 \u52404 \u51201 \u51004 \u47196  :\u54617 \u47141 , \u51649 \u50629 , \u44221 \u51228 \u51201  \u51312 \u44148 \u48372 \u45796 \u45716  \u44536  \u49324 \u46988  \u51088 \u52404 \u51032  \u47588 \u47141 , \u48516 \u50948 \u44592 \u47484  \u48372 \u45716  \u49828 \u53440 \u51068 \u46609 \u44057 \u51060  \u48157 \u44256  \u45209 \u52380 \u51201 \u51064  \u49324 \u46988 \u51012  \u51339 \u50500 \u54616 \u45208 , \u48176 \u50872  \u51216  \u47566 \u51008  \u51652 \u51473 \u54620  \u49324 \u46988 \u46020  \u51339 \u50500 \u54632 \u50672 \u51064  \u49324 \u51060 \u50640 \u49436  \u50756 \u46300  \u53304 \u51060  \u46321 \u51109 \u54664 \u45796 \u47732 , \u54788 \u51116  \u54876 \u44592 \u52264 \u44256  \u55141 \u48516 \u46104 \u45716  \u44288 \u44228 \u51076 \u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u49436 \u47196 \u44032  \u45320 \u47924  \u47588 \u47141 \u51201 \u51004 \u47196  \u48372 \u51060 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 .\u54644 \u49437  3. \u51652 \u52712 \u51201 \u51064 \u53304  \u50756 \u46300 \u45716  \u51201 \u44537 \u51201 \u51060 \u44256  \u50556 \u47581  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 . \u50948 \u54744 \u51012  \u44048 \u49688 \u54616 \u44144 \u45208  \u54876 \u46041 \u51201 \u51064  \u51068 \u51012  \u51339 \u50500 \u54616 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u44032  \u44557 \u51221 \u51201 \u51068  \u44221 \u50864 , \'91\u51201 \u44537 \u51201 \u51004 \u47196  \u45796 \u44032 \u44032 \u44256  \u49910 \u51008  \u49900 \u47532 \'92\u47484  \u45208 \u53440 \u45236 \u44592 \u46020  \u54633 \u45768 \u45796 . \u46907 \u51012  \u54869 \u51109 \u54644 \u49436  \u54644 \u49437 \u54644 \u48372 \u47732 ,\u51649 \u50629 \u50868 \u50640 \u49436 \u45716  :\u50504 \u51221 \u51201 \u51064  \u51649 \u51333 \u48372 \u45796  \u49324 \u50629 \u50640  \u50612 \u50872 \u47532 \u45716  \u49457 \u54693 \u50892 \u46972 \u48296 \u48372 \u45796 \u45716  \u49457 \u51109 \u51060  \u51473 \u50836 \u50668 \u44592 \u51200 \u44592  \u50880 \u51649 \u51060 \u47728  \u48176 \u50872  \u49688  \u51080 \u45716  \u44275 \u51012  \u49440 \u54840 \u44208 \u44284 \u48372 \u45796 \u45716  \u44284 \u51221 \u51012  \u51473 \u50836 \u49884 \u54616 \u45716  \u49457 \u54693 \u50672 \u50528 \u47484  \u54620 \u45796 \u47732  :\u45936 \u51060 \u53944  \u53076 \u49828 \u47196 , \u48372 \u46300  \u44172 \u51076 \u51060 \u45208  \u48169 \u53448 \u52636 , \u52404 \u54744 \u54805  \u51204 \u49884 \u54924  \u44057 \u51008  \u49888 \u52404  \u54876 \u46041 \u51012  \u49440 \u54840 \u48376 \u51064 \u44284  \u48708 \u49847 \u54620  \u50556 \u47581  \u51080 \u45716  \u49324 \u46988 \u51012  \u51339 \u50500 \u54632 \u48376 \u51064 \u51060  \u47676 \u51200  \u45796 \u44032 \u44032 \u45716  \u49828 \u53440 \u51068 \u50977 \u52404 \u51201  \u44288 \u44228 \u47484  \u51473 \u50836 \u49884 \u54616 \u45716  \u44221 \u54693 \u48520 \u51008  \'91\u50676 \u51221 \'92\u51012  \u49345 \u51669 \u54616 \u45716  \u50836 \u49548 \u51077 \u45768 \u45796 . \u50773 \u48708 \u44032  \u46308 \u44256  \u51080 \u45716  \u50756 \u46300 , \u44536 \u47532 \u44256  \u54644 \u48148 \u46972 \u44592 , \u50773 \u51340 \u50640  \u51109 \u49885 \u46108  \u49324 \u51088 \u45716  \u47784 \u46160  \u48520 \u51032  \u49549 \u49457 \u51012  \u46916 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49884 \u49440 \u46020  \u50808 \u48512 \u47484  \u54693 \u54616 \u50668 , \u54620  \u44275 \u50640  \u47672 \u47932 \u47084 \u51080 \u51648  \u50506 \u44256  \u45130 \u51076 \u50630 \u51060  \u48156 \u51204 \u54616 \u47140 \u45716  \u49457 \u54693 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 . \u52852 \u46300 \u44032  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008  \u50676 \u51221 \u51201 \u51060 \u44256  \u54868 \u45128 \u54616 \u51648 \u47564 , \u44536 \u47564 \u53372  \u51656 \u53804 \u49900 \u46020  \u47566 \u44256 , \u49849 \u48512 \u50837 \u51060  \u44053 \u54620  \u49828 \u53440 \u51068 \u51060 \u44592 \u46020  \u54633 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51649 \u50629 \u50868 \u51012  \u47932 \u50632 \u45796 \u47732 , \u50676 \u51221 \u51012  \u44054 \u44256  \u51652 \u52712 \u51201 \u51004 \u47196  \u51068 \u50640  \u51076 \u54632 \u51012  \u46907 \u54633 \u45768 \u45796 . \u51068 \u50640  \u51600 \u44144 \u50880 \u51012  \u45712 \u45180 \u47728 , \u51032 \u50837 \u46020  \u45336 \u52432 \u49436  \u46832 \u50612 \u45212  \u49457 \u44284 \u47484  \u44144 \u46168  \u49688  \u51080 \u45716  \u49884 \u44592 \u51077 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732 , \u50676 \u51221 \u51201 \u51060 \u44256  \u46888 \u44144 \u50868  \u49324 \u46993 \u51012  \u54616 \u44256  \u51080 \u45796 \u45716  \u51032 \u48120 \u51077 \u45768 \u45796 . \u45796 \u47564  \u49912 \u50872  \u46412 \u46020  \u53356 \u44172  \u53356 \u44172  \u48520 \u44057 \u51060  \u49912 \u50864 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 . \u44592 \u47484  \u51312 \u44552  \u51453 \u51068  \u54596 \u50836 \u44032  \u51080 \u49845 \u45768 \u45796 .\u48512 \u51221 \u51201  \u54644 \u49437 \u54644 \u49437  1. \u44048 \u51221 \u51201 \u51064 \u53304  \u50756 \u46300 \u45716  \u45796 \u47480  \u50668 \u50773 \u46308 \u44284  \u45804 \u47532 , \u45796 \u49548  \u44048 \u51221 \u51201 \u51060 \u44256  \u54028 \u51060 \u54021  \u45336 \u52824 \u45716  \u49324 \u46988 \u51012  \u44032 \u47532 \u53429 \u45768 \u45796 .\u50756 \u46300 \u45716  \u48520 \u51032  \u49549 \u49457 \u51012  \u44032 \u51648 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48520 \u51008  \u51020 \u49885 \u51012  \u47564 \u46308 \u44256 , \u47800 \u51012  \u46384 \u46907 \u54616 \u44172  \u54644 \u51452 \u45716  \u46321  \'91\u50640 \u45320 \u51648 \u44032  \u54413 \u48512 \u54620 \'92 \u50836 \u49548 \u51077 \u45768 \u45796 . \u45796 \u47564 , \u51060 \u47484  \u51096 \u47803  \u49324 \u50857 \u54664 \u51012  \u44221 \u50864 \u50644  \'91\u51452 \u52404 \u54624  \u49688  \u50630 \u51060  \u52964 \u51652 \u45796 \u45716 \'92 \u45800 \u51216 \u46020  \u51080 \u51424 .\u48520 \u51008  \u51452 \u50948  \u54872 \u44221 \u50640  \u46384 \u46972  \u49789 \u44172  \u48148 \u45068 \u44592  \u46412 \u47928 \u50640 , \u44048 \u51221  \u44592 \u48373 \u51060  \u49900 \u54616 \u47728 , \u45796 \u54792 \u51656 \u51064  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u46020  \u54633 \u45768 \u45796 .\u54868 \u44032  \u45208 \u47732 , \u49345 \u45824 \u50640 \u44172  \u51649 \u49444 \u51201 \u51064  \u47568 \u51012  \u45236 \u48201 \u44144 \u45208 , \u44277 \u44201 \u51201 \u51064  \u53468 \u46020 \u47484  \u48372 \u51076 \u53356 \u44172  \u49912 \u50864 \u51648 \u47564 , \u44536 \u47564 \u53372  \u54868 \u46020  \u48744 \u47532  \u54400 \u47532 \u45716  \u49828 \u53440 \u51068 \u51060 \u49457 \u51201 \u51004 \u47196  \u44208 \u51221 \u54616 \u44592 \u48372 \u45796 \u45716  \u44048 \u51221 \u50640  \u52824 \u50864 \u52828  \u44208 \u51221 , \u52649 \u46041 \u51201 \u51064  \u44208 \u51221 \u51012  \u45236 \u47548 \u51656 \u53804 \u49900 \u51060  \u44053 \u54644 \u49436 , \u50672 \u51064 \u51060  \u45796 \u47480  \u49324 \u46988 \u51060 \u46993  \u44865 \u45285  \u44865 \u45285  \u54616 \u45716  \u44163 \u51012  \u44361 \u51109 \u55176  \u49899 \u50612 \u54632 \u54756 \u50612 \u51648 \u47732  \u49696  \u47560 \u49884 \u47728  \u50837 \u51012  \u54140 \u48531 \u45716  \u49828 \u53440 \u51068 , \u44536 \u47564 \u53372  \u47566 \u51060  \u50872 \u44592 \u46020  \u54632 \u51649 \u50629 \u50868 \u50640 \u49436 \u46020 , \u44060 \u51064 \u51201 \u51064  \u44048 \u51221  \u46412 \u47928 \u50640  \u51096  \u46104 \u45912  \u51068 \u46020  \u44536 \u47476 \u52832  \u49688  \u51080 \u45796 \u45716  \u44221 \u44256  \u47700 \u49884 \u51648 \u47196  \u54644 \u49437 \u54633 \u45768 \u45796 . \u44048 \u51221 \u51012  \u45236 \u47140 \u45459 \u44256  \u51060 \u49457 \u51201 \u51064  \u54032 \u45800 \u51060  \u54596 \u50836 \u54620  \u49884 \u51216 \u51077 \u45768 \u45796 .\u54644 \u49437  2. \u52280 \u44204 \u54616 \u45716 \u50756 \u46300  \u50668 \u50773 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u45796 \u47480  \u51068 \u50640  \u52280 \u44204 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u54844 \u51088 \u49436 \u46020  \u51096  \u49332 \u51648 \u47564 , \'91\u49324 \u46988 \u46308 \u51060  \u47924 \u50631 \u51012  \u54616 \u45716 \u51648 \'92\u46020  \u44288 \u49900 \u51060  \u47566 \u49845 \u45768 \u45796 . \u48708 \u50976 \u47484  \u46308 \u51088 \u47732 , \u44256 \u44553  \u49849 \u50857 \u52264 \u47484  \u50868 \u51204 \u54616 \u47732 \u49436 , \u54844 \u51107 \u47568 \u47196  \u45224 \u51012  \u50837 \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u45224 \u46308 \u51060  \u51096  \u49324 \u45716  \u47784 \u49845 \u51012  \u48372 \u47732 , \'91\u51656 \u53804 \u49900 \u51060  \u54253 \u48156 \'92\u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u50668 \u44592 \u49436  \u54252 \u51064 \u53944 \u45716 , \u50756 \u46300  \u53433 \u51060  \u52280 \u44204  + \u51649 \u51217  \u54665 \u46041 \u51012  \u51032 \u48120 \u54664 \u45796 \u47732 ,\u50756 \u46300  \u53304 \u51008  \u52280 \u44204  + \u44036 \u51217  \u54665 \u46041 \u51012  \u51032 \u48120 \u54620 \u45796 \u45716  \u51216 \u51077 \u45768 \u45796 .\u44396 \u52404 \u51201 \u51004 \u47196  \u48708 \u44368 \u54616 \u51088 \u47732  :  \u50756 \u46300  \u53433  : \u51649 \u51217  \u45208 \u49444  \u51456 \u48708 \u44032  \u46104 \u50612  \u51080 \u45796 \u50756 \u46300  \u53304  : \u51649 \u51217  \u45208 \u49436 \u51648 \u45716  \u50506 \u51020 \u51116 \u54924 \u50640 \u49436  \u50756 \u46300  \u53304 \u51008  : \u50892 \u45209  \u48148 \u49240 \u44256  \u54876 \u46041 \u51201 \u51064  \u49324 \u46988 \u51012  \u44032 \u47532 \u53412 \u44592 \u50640 , \u51116 \u54924 \u47484  \u44618 \u44172  \u49373 \u44033 \u54616 \u51648 \u45716  \u50506 \u44192 \u49845 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u52280 \u44204 \u51008  \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u47564 \u50557  \u52852 \u52852 \u50724 \u53665  \u54532 \u47196 \u54596 \u51012  \u48148 \u44984 \u44144 \u45208 , SNS \u44172 \u49884 \u47932 \u51012  \u50732 \u47160 \u51012  \u44221 \u50864 , \'91\u50668 \u44592  \u46608  \u44052 \u45348 , \u45208 \u46993  \u44052 \u45912  \u44275 \u51064 \u45936 \'92, \'91\u54648 \u46300 \u54256  \u52992 \u51060 \u49828  \u50500 \u51649 \u46020  \u50504  \u48148 \u45032 \u45348 . \u50500 \u51649  \u47560 \u51020 \u51060  \u51080 \u45796 \u45716  \u44148 \u44032 ?\'92 \'91\u54756 \u50612 \u49828 \u53440 \u51068  \u51652 \u51676  \u48324 \u47196 \u45796 . \u54620  \u48264  \u50672 \u46973  \u54644 \u48380 \u44620 ?\'92\u54644 \u49437  3. \u44428 \u50948 \u51201 \u51064 \u44032 \u45140  \u44428 \u50948 \u51201 \u51060 \u44256  \u46021 \u49440 \u51201 \u51004 \u47196  \u54665 \u46041 \u54616 \u50668 , \u51088 \u49888 \u46020  \u47784 \u47476 \u44172  \'91\u51652 \u49345 \u51667 \'92\u51012  \u54665 \u54624  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51648 \u44552  \u50864 \u47532  \u50528 \u50500 \u48736 \u44032  \u54868 \u44032  \u47566 \u51060  \u45228 \u50612 \u50836 \'85\u50756 \u46300  \u53304 \u51008  \u48148 \u50868 \u45908 \u47532 \u44032  \u54869 \u49892 \u54633 \u45768 \u45796 . \u54036 \u51060  \u50504 \u51004 \u47196  \u44413 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u45236  \u49324 \u46988 \u50640 \u44172 \u45716  \u45908 \u50630 \u51060  \u46384 \u46907 \u54616 \u44256  \u54732 \u49888 \u51201 \u51060 \u51648 \u47564 , \u45224 \u50640 \u44172 \u45716  \u50628 \u44201 \u54616 \u47728  \u44428 \u50948 \u51201 \u51004 \u47196  \u44404  \u49688  \u51080 \u44192 \u49845 \u45768 \u45796 .\u47532 \u46377 \u50640 \u49436 \u45716  :\u44032 \u45140  \u51652 \u49345 \u52376 \u47100  \u48372 \u51060 \u51648 \u47564 , \u44536 \u47000 \u46020  \u45236  \u49324 \u46988 \u54620 \u53580 \u45716  \u46384 \u46907 \u54620  \u49457 \u44201 \u51649 \u51109 \u50640 \u49436  \u45208 \u47484  \'91\u44848 \u45824 \'92\u47196  \u48380  \u49688 \u46020 \u54841 \u51008  \u44428 \u50948 \u51201 \u51064  \u49345 \u49324 \u44032  \u46321 \u51109 \u54624  \u49688 \u46020  \u51080 \u45796 . \u45236  \u54200 \u51060  \u46104 \u47732  \u50668 \u47084 \u47784 \u47196  \u51339 \u51004 \u45768 , \u50500 \u48512 \u46020  \u54596 \u50836 \u54624  \u46319 \u50756 \u46300  \u53304  \u50672 \u50528 \u50868  \u54644 \u49437 \u50756 \u46300  \u53304 \u51060  \u44032 \u47532 \u53412 \u45716  \u49324 \u46988 \u51008 , \u50676 \u51221 \u51201 \u51060 \u44256  \u51652 \u52712 \u51201 \u51064  \u47784 \u49845 \u51004 \u47196  \u47784 \u46304  \u51068 \u50640  \u52572 \u49440 \u51012  \u45796 \u54633 \u45768 \u45796 . \u49884 \u50896 \u49884 \u50896 \u54616 \u44256  \u51649 \u49444 \u51201 \u51064  \u49457 \u44201 \u51060 \u46972 , \u44144 \u51667 \u47568 \u51012  \u44361 \u51109 \u55176  \u49899 \u50612 \u54633 \u45768 \u45796 . \u49899 \u51004 \u47732  \u49899 \u45796 . \u51339 \u51004 \u47732  \u51339 \u45796 . \u46972 \u44256  \u54869 \u49892 \u55176  \u54364 \u54788 \u54644 \u51452 \u45716  \u44172  \u51339 \u49845 \u45768 \u45796 . \u51452 \u48320  \u49324 \u46988 \u46308 \u50640 \u44172  \u48157 \u44256  \u54876 \u44592 \u52268  \u48516 \u50948 \u44592 \u47484  \u51204 \u54616 \u47728 , \u48376 \u51064  \u46608 \u54620  \u51452 \u48320  \u48516 \u50948 \u44592 \u50640  \u51096  \u47932 \u46300 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u46020 \u51204 \u44284  \u47784 \u54744 \u51012  \u51339 \u50500 \u54616 \u45716  \u52964 \u47532 \u50612 \u50864 \u47676 , \u54841 \u51008  \u50676 \u51221 \u51201 \u51064  \u49324 \u50629 \u44032 \u50640 \u44172 \u49436  \u47566 \u51060  \u45208 \u50724 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208  \u51648 \u45208 \u52824 \u44172  \u44048 \u51221 \u51201 \u51064  \u49457 \u44201 \u51060  \u45800 \u51216 \u51060  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u45236  \u49324 \u46988 \u50640 \u44172 \u45716  \u54620 \u50630 \u51060  \u51096  \u52313 \u44200 \u51452 \u51648 \u47564 , \u45224 \u50640 \u44176  \u44032 \u45140  \u51652 \u49345 \u52376 \u47100  \u44404 \u47728 , \u44428 \u50948 \u51201 \u51064  \u53468 \u46020 \u47484  \u48372 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51312 \u50616 \u51216 \u51004 \u47196 \u45716 , \u51060 \u49457 \u51201 \u51004 \u47196  \u52264 \u48516 \u54616 \u44172  \u54665 \u46041 \u54644 \u50556  \u54616 \u47728 , \u49324 \u44148 \u51012  \u44061 \u44288 \u51201 \u51004 \u47196  \u48380  \u44163 . \u49457 \u44201 \u51060  \u44553 \u54620  \u47732 \u46020  \u51080 \u51004 \u45768 , \u51064 \u45236 \u49900 \u51012  \u44032 \u51648 \u46972 \u45716  \u51312 \u50616 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53304 \u51060  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732 , \u45817 \u49888 \u51060  \u47588 \u47141  \u51080 \u44172  \u48372 \u51060 \u47728  \u51201 \u44537 \u51201 \u51004 \u47196  \u45796 \u44032 \u44032 \u44256  \u49910 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 . \u51060 \u46412  \u44228 \u49328 \u51201 \u51064  \u49457 \u54693 \u51008  \u51201 \u49845 \u45768 \u45796 . \u51593  \u46024 \u51060 \u45208  \u51649 \u50629 , \u54617 \u48268 \u51012  \u48372 \u44256  \u45796 \u44032 \u44032 \u45716  \u44172  \u50500 \u45768 \u46972 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44536 \u48372 \u45796 \u45716  \'91\u50620 \u44404 \u51060 \u45208  \u47800 \'92 \u54841 \u51008  \'91\u51204 \u52404 \u51201 \u51064  \u48516 \u50948 \u44592 \'92 \u46412 \u47928 \u51068  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51060  \u50612 \u46500  \u54665 \u46041 \u51012  \u54644 \u46020  \u54200 \u51012  \u46308 \u50612 \u51460  \u44163 \u51077 \u45768 \u45796 . \u51060 \u46412  \u54632 \u44760  \u45208 \u49436 \u44592 \u48372 \u45796 \u45716  \u46244 \u50640 \u49436  \u50676 \u51221 \u51201 \u51004 \u47196  \u51025 \u50896 \u54616 \u47728 , \u51648 \u51648 \u54616 \u45716  \u49828 \u53440 \u51068 \u51077 \u45768 \u45796 . \u52852 \u46300  \u49549  \u50668 \u50773 \u51008  \u54637 \u49345  \u50808 \u48512 \u47484  \u48372 \u44256  \u51080 \u44592 \u50640 , \u50684 \u53456 \u51012  \u47566 \u51060  \u54620 \u45796 \u45716  \u46907 \u51004 \u47196 \u46020  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u48512 \u51221 \u51201 \u51004 \u47196  \u54644 \u49437 \u54624  \u44221 \u50864 , \'91\u45208 \u54620 \u53580  \u44284 \u54616 \u44172  \u46308 \u51060 \u45824 \u45716  \u49324 \u46988 \'92 \u54841 \u51008  \'91\u51648 \u45208 \u52824 \u44172  \u50724 \u51648 \u46998  \u46496 \u45716  \u49324 \u46988 \'92\u51004 \u47196  \u48372 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u46608 \u54620  \u44288 \u44228 \u50640  \u51109 \u50528 \u47932 \u51060  \u51080 \u45796 \u47732 , \'91\u45320 \u47924  \u44592 \u44032  \u49464 \u44256 , \u54364 \u54788 \u51060  \u51649 \u49444 \u51201 \u51060 \u44592  \u46412 \u47928 \'92\u51060 \u46972 \u44256  \u48380  \u49688  \u51080 \u49845 \u45768 \u45796 .\u45817 \u49888 \u51060  \u52964 \u54540 \u51060 \u46972 \u47732 , \u49436 \u47196 \u44032  \u47588 \u47141  \u51080 \u44172  \u48372 \u51060 \u47728 , \u50528 \u53963 \u54620  \u49324 \u46993 \u51012  \u45208 \u45572 \u44256  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \u44048 \u51221  \u54364 \u54788 \u46020  \u54413 \u48512 \u54616 \u47728 , \u50977 \u52404 \u51201 \u51064  \u44288 \u44228 \u46020  \u51096  \u47582 \u45716  \u46319  \u48372 \u51077 \u45768 \u45796 . \u49324 \u51060 \u46020  \u45128 \u45128 \u54633 \u45768 \u45796 . \u45796 \u47564 , \u49912 \u50872  \u46412 \u46020  \u48520 \u44057 \u51060  \u49912 \u50864 \u47728 , \u51649 \u49444 \u51201 \u51064  \u47568 \u46020  \u51088 \u51452  \u45236 \u48201 \u45716  \u44163  \u44057 \u49845 \u45768 \u45796 . \'91\u49912 \u50864 \u45796 \u44032  \u51221 \u46300 \u45716  \u44288 \u44228 \'92 \u54841 \u51008  \'91\u54620  \u47749 \u51060  \u51068 \u48169 \u51201 \u51004 \u47196  \u51256 \u51452 \u45716  \u44288 \u44228 \'92\u47196  \u48156 \u51204 \u54624  \u44032 \u45733 \u49457 \u51060  \u45458 \u49845 \u45768 \u45796 . \u44048 \u51221 \u51012  \u45236 \u47140 \u45459 \u44256 , \u52264 \u48516 \u54632 \u51012  \u50976 \u51648 \u54644 \u50556  \u54620 \u45796 \u45716  \u49888 \u54840 \u51077 \u45768 \u45796 . \u49436 \u47196  \u51312 \u44552 \u47564  \u50577 \u48372 \u54616 \u47732 , \u44208 \u54844 \u44620 \u51648 \u46020  \u44040  \u49688  \u51080 \u45716  \u51064 \u50672 \u51004 \u47196  \u48372 \u51077 \u45768 \u45796 .\u45817 \u49888 \u51060  \u51116 \u54924 \u47484  \u49373 \u44033 \u54616 \u44256  \u51080 \u45796 \u47732 , \u49345 \u45824 \u48169 \u51008  \u48516 \u45432 \u50752  \u50528 \u53963 \u54632 \u51060  \u46041 \u49884 \u50640  \u45712 \u44788 \u51648 \u45716  \u50577 \u44032 \u44048 \u51221 \u51012  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49789 \u44172  \u50696 \u47484  \u46308 \u51088 \u47732 , \u49373 \u44033 \u54644 \u48372 \u45768 \u44620  \u50676  \u48155 \u45348 . \u50416 \u47112 \u44592  \u44057 \u51008  \u45448 . \u44536 \u47000 \u46020  \u44900  \u51339 \u50520 \u45716 \u45936 .. \u45817 \u49888 \u51060  \u47924 \u50631 \u51012  \u54616 \u47728 , \u50612 \u46523 \u44172  \u49332 \u44256  \u51080 \u45716 \u51648  \u44288 \u49900 \u51060  \u47566 \u50500  \u48372 \u51077 \u45768 \u45796 . SNS \u50684 \u53456 \u46020  \u51088 \u51452  \u54624  \u44163 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u51060 \u44163 \u51060  \u51116 \u54924 \u47484  \u44036 \u51208 \u55176  \u50896 \u54620 \u45796 \u45716  \u46907 \u51008  \u50500 \u45785 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51032  \u54665 \u46041 \u51008  \u51228 \u54620 \u51201 \u51060 \u44256 , \u51088 \u51316 \u49900 \u51012  \u44413 \u55176 \u45716  \u49457 \u44201 \u46020  \u50500 \u45768 \u44592  \u46412 \u47928 \u51077 \u45768 \u45796 . \u45572 \u44400 \u44032 \u44032  \u51088 \u51316 \u49900 \u51012  \u45236 \u47140 \u45459 \u50500 \u50556  \u54633 \u45768 \u45796 . \u50612 \u45712  \u54620  \u51901 \u51060  \u51088 \u51316 \u49900 \u51012  \u44413 \u55176 \u44256  \u45796 \u44032 \u49440 \u45796 \u47732 , \u51116 \u54924  \u44032 \u45733 \u49457 \u51060  \u51080 \u45716  \u44288 \u44228 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53304  \u51649 \u50629 \u50868  \u54644 \u49437 \u51068 \u51012  \u52509 \u44292 \u54616 \u44144 \u45208 , \u44060 \u51064 \u51201 \u51064  \u49324 \u50629 \u50640  \u50612 \u50872 \u47532 \u45716  \u52852 \u46300 \u51077 \u45768 \u45796 . \u51452 \u48320  \u49324 \u46988 \u46308 \u51012  \u53685 \u49556 \u54616 \u45716  \u45733 \u47141 \u51060  \u46832 \u50612 \u45208 , \u54016  \u45800 \u50948 \u47196  \u51068 \u54616 \u47732  \u45458 \u51008  \u49457 \u44284 \u47484  \u44144 \u46168  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u46041 \u47308 \u46020  \u51096  \u52313 \u44592 \u45716  \u49828 \u53440 \u51068 \u51060 \u51648 \u47564 , \u46412 \u47196  \u51649 \u49444 \u51201 \u51064  \u47568 \u51012  \u45236 \u48201 \u50612  \u49345 \u52376 \u47484  \u51452 \u44256  \u51080 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 . \'91\u44848 \u45824 \'92 \u54841 \u51008  \'91\u51210 \u51008  \u44848 \u45824 \'92\u47196  \u48372 \u51060 \u44592 \u46020  \u54616 \u47728 , \u44288 \u45824 \u54632 \u51060  \u54596 \u50836 \u54616 \u45796 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u50756 \u46300  \u53304  \u44552 \u51204 \u50868  \u54644 \u49437 \u44552 \u51204 \u50868 \u51008  \u51339 \u49845 \u45768 \u45796 . \u45817 \u49888 \u51032  \u49548 \u49888 \u45824 \u47196  \u54616 \u49464 \u50836 . \u45817 \u49888 \u51060  \u50612 \u46500  \u53804 \u51088 \u47484  \u54616 \u46304 , \u50504 \u51221 \u51201 \u51064  \u49688 \u51061 \u51012  \u44144 \u46160 \u44172  \u46121 \u45768 \u45796 . \u49552 \u49892 \u51060  \u45208 \u46020 , \u48376 \u51064  \u45733 \u47141 \u51004 \u47196  \u52964 \u48260  \u46112  \u49688 \u51456 \u51077 \u45768 \u45796 . \u44536 \u47084 \u45208 , \u51452 \u48320  \u49324 \u46988 \u46308 \u51032  \u47568 \u50640  \u55064 \u46168 \u47532 \u44144 \u45208 , \u53804 \u51088  \u44428 \u50976  \u48155 \u45716  \u44163 \u51012  \u51312 \u49900 \u54644 \u50556  \u54633 \u45768 \u45796 . \u45817 \u49888 \u51008  \'91\u47924 \u49832  \u53804 \u51088 \u44032  \u51339 \u44256  \u45208 \u49244 \u51648 \'92 \u44396 \u48516 \u54624  \u49688  \u51080 \u45716  \'91\u51060 \u49345 \u51201 \u51064  \u50948 \u52824 \'92\u50640  \u51080 \u49845 \u45768 \u45796 . \u52489 \u51012  \u48127 \u51004 \u49464 \u50836 .\u50756 \u46300  \u53304  \u44552 \u51204 \u50868  \u54644 \u49437  \u52628 \u44032  \u51088 \u47308  (Labyrinthos)    \
