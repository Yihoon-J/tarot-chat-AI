   \uc0\u47785 \u52264 \u44032 \u44620 \u50872  \u49688  \u51080 \u45716  \u53945 \u49457 \u47680  \u49688  \u51080 \u45716  \u53945 \u49457 \u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u49444 \u47749 \u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u50672 \u50528 \u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u51649 \u50629 \u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u44552 \u51204 \u45824 \u54364 \u51201 \u51064  YES OR NO \u51656 \u47928 \u44032 \u44620 \u50872  \u49688  \u51080 \u45716  \u53945 \u49457 \u54840 \u44592 \u49900 \u49900 \u49324 \u49689 \u44256 \u54616 \u45716 \u51312 \u49900 \u49828 \u47084 \u50868 \u44417 \u44552 \u54620 \u51665 \u51473 \u54616 \u45716 \u54952 \u44284 \u51201 \u51064 \u51312 \u44552  \u48512 \u51313 \u54620 \u49892 \u50857 \u51201 \u51064 \u54788 \u49892 \u51201 \u51064 \u49352 \u47196 \u50868  \u49884 \u51089 \u51104 \u51116 \u47141  \u51080 \u45716 \u45236 \u49457 \u51201 \u51064 \u47680  \u49688  \u51080 \u45716  \u53945 \u49457 \u51676 \u47551 \u54620 \u47924 \u47784 \u54620 \u50756 \u48317 \u54620 \u55141 \u48516 \u46104 \u45716 \u51032 \u49900 \u54616 \u45716 \u51104 \u51116 \u47141  \u48512 \u51313 \u48708 \u54788 \u49892 \u51201 \u51064 \u50808 \u54693 \u51201 \u51064 \u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u49444 \u47749 \u51060 \u48120 \u51648  \u54644 \u49437 \u54168 \u51060 \u51648  \u50724 \u48652  \u54172 \u53440 \u53364 \u51008  \u49352 \u47196 \u50868  \u49884 \u51089 , \u50689 \u44048 , \u54840 \u44592 \u49900 , \u51312 \u49900 \u49828 \u47084 \u50880 \u51060  \u49438 \u51064  \u52852 \u46300 \u51077 \u45768 \u45796 .\u45331 \u51008  \u46308 \u54032 \u50640  \u49436  \u51080 \u45716  \u52397 \u45380 \u51060  \u50577  \u49552 \u50640  \u54172 \u53440 \u53364 \u51012  \u46308 \u50612  \u50976 \u49900 \u55176  \u49332 \u54196 \u48372 \u44256  \u51080 \u49845 \u45768 \u45796 . \u51452 \u48320  \u49345 \u54889 \u51012  \u51082 \u50612  \u48260 \u47536 \u46319 , \u46041 \u51204 \u50640  \u47588 \u47308 \u46104 \u50632 \u49845 \u45768 \u45796 .\u52397 \u45380 \u51008  \u51060  \u46041 \u51204 \u51060  \u50612 \u46356 \u49436  \u50772 \u44256 , \u50620 \u47560 \u45208  \u48708 \u49916  \u44163 \u51064 \u51648 , \u47784 \u50577 \u51008  \u50612 \u46523 \u44172  \u49373 \u44220 \u45716 \u51648  \u44417 \u44552 \u54644  \u54633 \u45768 \u45796 . \u44536 \u47532 \u44256  \u46608 \u54620  \'91\u44032 \u51256 \u44032 \u46020  \u46112 \u44620 ? \u47564 \u51256 \u46020  \u44316 \u52270 \u51012 \u44620 ?\'92 \u54616 \u45716  \u47581 \u49444 \u51076 \u46020  \u54632 \u44760  \u52286 \u50500 \u50741 \u45768 \u45796 .\u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u50672 \u50528 \u45208 \u53440 \u45236 \u45716  \u49457 \u44201  : \u49888 \u51473 \u54616 \u44256  \u54788 \u49892 \u51201 \u51064 \u54168 \u51060 \u51648  \u50724 \u48652  \u54172 \u53440 \u53364 \u51008  \u47784 \u46304  \u51068 \u50640  \u49888 \u51473 \u54616 \u44256  \u54788 \u49892 \u51201 \u51064  \u49324 \u46988 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u44048 \u51221 \u48372 \u45796 \u45716  \'91\u44228 \u49328 \'92\u51012  \u50526 \u49464 \u50864 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 .\u50696 \u52968 \u45824 , \u47924 \u53553 \u45824 \u44256  \u49345 \u45824 \u48169 \u50640 \u44172  \u46308 \u51060 \u45824 \u44592 \u48372 \u45796 , \u49345 \u45824 \u48169 \u51060  \u50612 \u46523 \u44172  \u48152 \u51025 \u54624 \u51648  \u50696 \u52769 \u54616 \u44256 , \u50672 \u46973 \u51012  \u54644 \u46020  \u51339 \u51012 \u51648 , \u50616 \u51228  \u54644 \u50556  \u54624 \u51648  \u49888 \u51473 \u54616 \u44172  \u54032 \u45800 \u54616 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u45236 \u49457 \u51201 \u51064  \u49457 \u54693 \u46020  \u54620 \u47787 \u54633 \u45768 \u45796 .\u44048 \u51221 \u51201 \u51064  \u51648 \u50896 \u48372 \u45796  \u49892 \u51656 \u51201 \u51064  \u54665 \u46041 \u51004 \u47196  \u49324 \u46993 \u51012  \u54364 \u54788 \u54616 \u45716  \u44221 \u54693 \u51060  \u51080 \u49845 \u45768 \u45796 . \u48165 \u51012  \u49324 \u51452 \u47728  \u50948 \u47196 \u47484  \u44148 \u45352 \u45796 \u46304 \u44032 , \'91\u54788 \u49892 \u51201 \u51064  \u54644 \u44208 \u52293 \'92\u51012  \u51228 \u49884 \u54616 \u45716  \u49457 \u44201 \u51077 \u45768 \u45796 . \u51312 \u50857 \u54616 \u44144 \u45208  \u54617 \u44396 \u51201 \u51064  \u49457 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 .\u50672 \u50528  \u45824 \u49345 \u51012  \u44256 \u47484  \u46412 , \u50808 \u47784 \u48372 \u45796  \u51312 \u44148 \u51012  \u46384 \u51656  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u45208 \u53440 \u45236 \u45716  \u49549 \u47560 \u51020  : \u54840 \u44592 \u49900 \u54172 \u53440 \u53364  \u54168 \u51060 \u51648 \u45716  \u54840 \u44592 \u49900 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u45800 \u49692 \u55176  \u51068 \u50640  \u50676 \u51473 \u54616 \u45716  \u54840 \u44592 \u49900 \u51068  \u49688 \u46020  \u51080 \u44256 , \u45817 \u49888 \u50640  \u45824 \u54620  \u54840 \u44592 \u49900 \u51068  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u51656 \u47928 \u50640  \u46384 \u46972  \'91\u54840 \u44592 \u49900 \u51032  \u45824 \u49345 \'92\u51060  \u45804 \u46972 \u51665 \u45768 \u45796 .\u50668 \u44592 \u49436  \u47568 \u54616 \u45716  \u54840 \u44592 \u49900 \u51060 \u46976 , \u54840 \u44048 \u48372 \u45796  \u44417 \u44552 \u51613 \u50640  \u44032 \u44637 \u49845 \u45768 \u45796 . \'91\u51200  \u49324 \u46988  \u47952 \u51648 ?\'92 \'91\u51200 \u47088  \u47732 \u46020  \u51080 \u50632 \u45208 ?\'92 \'91\u50780  \u50724 \u45720 \u51008  \u44984 \u48120 \u44256  \u50772 \u51648 ?\'92 \u54841 \u51008  \'91\u50724 \u45720  \u47952  \u54616 \u47140 \u45208 ?\'92 \'91\u50528 \u51064  \u49373 \u44220 \u45208 ?\'92\u50752  \u44057 \u51008  \u47560 \u51020 \u51077 \u45768 \u45796 .\u53440 \u47196 \u50640 \u49436  \u54168 \u51060 \u51648 (Page)\u45716  \u49352 \u47196 \u50868  \u49884 \u51089 \u44284  \u51104 \u51116 \u47141 \u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u54756 \u50612 \u51652  \u44288 \u44228 \u44032  \u50500 \u45768 \u46972 \u47732 , \u45817 \u49888 \u44284  \u44536  \u49324 \u46988 \u51032  \u44288 \u44228 \u45716  \u51060 \u51228  \u49884 \u51089 \u51077 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51008  \u47588 \u49324 \u50640  \u49888 \u51473 \u54616 \u44256 , \u54788 \u49892 \u51201 \u51064  \u49457 \u44201 \u51012  \u44054 \u44256  \u51080 \u49845 \u45768 \u45796 . \u45817 \u49888 \u50640 \u44172  \u44417 \u44552 \u51613 \u51060  \u51080 \u50612 \u46020 , \u54000 \u45716  \u45236 \u51648  \u50506 \u51012  \u44163 \u51077 \u45768 \u45796 . \u51452 \u48320  \u52852 \u46300 \u50640  \u46384 \u46972  SNS \u50684 \u53456 \u51060 \u45208 , \u46263 \u51312 \u49324 \u47484  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u49892 \u51228 \u47196  \u54952 \u44284 \u44032  \u51080 \u45716 \u54172 \u53440 \u53364  \u54168 \u51060 \u51648 \u45716  \u49892 \u51228 \u47196  \u54952 \u44284 \u44032  \u51080 \u45716  \u51068 , \u54665 \u46041  \u44536 \u47532 \u44256  \u47932 \u44148 \u51012  \u49345 \u51669 \u54616 \u44592 \u46020  \u54633 \u45768 \u45796 .\u52852 \u46300  \u49549  \u49884 \u51333 \u51008  \u54172 \u53440 \u53364 \u51012  \u48156 \u44204 \u54616 \u44256 , \'91\u51060 \u47111 \u44172  \u54616 \u47732  \u54952 \u44284 \u44032  \u51080 \u44396 \u45208 , \u45908  \u47566 \u51008  \u46041 \u51204 \u51012  \u50619 \u51012  \u49688  \u51080 \u44192 \u44396 \u45208 \'92 \u44592 \u45824 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 .\u47564 \u50557  \u52852 \u46300 \u47484  \u48977 \u44592  \u51204  \u51656 \u47928 \u51060  \'91~\u54616 \u47732  \u50612 \u46504 \u44620 ?\'92 \'91~\u54644 \u46020  \u44316 \u52270 \u51012 \u44620 ?\'92 \u50688 \u45796 \u47732 , \u49892 \u51228 \u47196  \u54952 \u44284 \u44032  \u51080 \u51004 \u45768  \u54616 \u46972 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u47751  \u44032 \u51648  \u51656 \u47928 \u51012  \u44032 \u51221 \u54644 \u48372 \u44192 \u49845 \u45768 \u45796 .\u51648 \u44552  \u50672 \u46973 \u54644 \u46020  \u51339 \u51012 \u44620 ? \u8594  \u50500 \u47560  \u51339 \u51012  \u46319 \u48128 \u45817 \u54644 \u48380 \u44620 ? \u8594  \u54952 \u44284  \u51080 \u51012  \u46319 \u44536 \u45285  \u50672 \u46973  \u54616 \u51648  \u47568 \u44256  \u44592 \u45796 \u47140 \u48380 \u44620 ? \u8594  \u44316 \u52270 \u51012  \u46319 \u54756 \u50612  \u49828 \u53440 \u51068 \u51012  \u48148 \u44984 \u47732 , \u44536  \u49324 \u46988 \u51060  \u51339 \u50500 \u54624 \u44620 ? \u8594  \u50500 \u47560  \u44536 \u47092  \u46319 \u48512 \u51221 \u51201 \u51064  \u47732  : \u47956 \u44032 \u44032  \u48512 \u51313 \u54620 \u54172 \u53440 \u53364  \u54168 \u51060 \u51648 \u44032  \u51109 \u50528 \u47932 \u47196  \u51089 \u50857 \u54624  \u44221 \u50864 , \u54788 \u49892 \u51201  \u51312 \u44148 \u51060  \u51312 \u44552  \u50500 \u49789 \u45796 \u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50696 \u52968 \u45824 , \u54617 \u48268 , \u46024 , \u51665 \u50504  \u45236 \u47141  \u46321 \u46321 \u51077 \u45768 \u45796 .\u46041 \u51204 \u51012  \u50976 \u49900 \u55176  \u46308 \u50668 \u45796 \u48372 \u45716  \u44163 \u52376 \u47100 , \'91\u54788 \u49892 \u51201 \u51064  \u51312 \u44148 \'92\u51012  \u54616 \u45208 \u54616 \u45208  \u54217 \u44032  \u48155 \u44172  \u46112  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 .\u44288 \u44228  \u49457 \u47549  \u51204  (\u50040 )\u50577 \u51901  \u47784 \u46160  \u49345 \u45824 \u50640  \u45824 \u54620  \u54840 \u44592 \u49900 \u51008  \u51080 \u45716  \u49345 \u53468 \u51077 \u45768 \u45796 .\u51676 \u47551 \u54620  \u47196 \u47592 \u49828 \u48372 \u45796  \'91\u54788 \u49892 \u51201 \u51060 \u47728  \u50504 \u51221 \u51201 \u51064 \'92 \u44288 \u44228 \u50640  \u51080 \u49845 \u45768 \u45796 . \u49436 \u47196 \u47484  \u51312 \u49900 \u49828 \u47101 \u44172  \u50668 \u44592 \u45716  \u44221 \u54693 \u46020  \u51080 \u49845 \u45768 \u45796 .\u44048 \u51221 \u51060  \u45336 \u52824 \u45716  \u44288 \u44228 \u45716  \u50500 \u45785 \u45768 \u45796 . \u44536 \u48372 \u45796  \u54788 \u49892 \u51201 \u51060 \u47728 , \u50504 \u51221 \u51201 \u51060 \u46972 \u45716  \u51064 \u49345 \u51012  \u51469 \u45768 \u45796 . \u44536 \u47111 \u44592 \u50640  \u47564 \u45212 \u51648  \u50620 \u47560  \u50504 \u46096 \u51648 \u47564 , \'91\u50724 \u47000 \u46108  \u52964 \u54540 \'92\u51032  \u45712 \u45196 \u46020  \u54413 \u44609 \u45768 \u45796 .\u54172 \u53440 \u53364 \u51008  \'91\u44228 \u49328 \'92, \'91\u54788 \u49892 \u51201 \u51064 \'92\u51060 \u46972 \u45716  \u53412 \u50892 \u46300 \u44032  \u54637 \u49345  \u54252 \u54632 \u46121 \u45768 \u45796 . \u51648 \u44552  \u51060  \u44288 \u44228 \u51032  \u54645 \u49900 \u46020  \'91\u54788 \u49892 \u51201  \u51312 \u44148 \'92\u51077 \u45768 \u45796 .\u50526 \u51004 \u47196  \u45817 \u49888 \u51008  \u50620 \u47560 \u45208  \u48260 \u45716 \u51648 , \u46024 \u51008  \u50620 \u47560 \u45208  \u44054 \u44256  \u51080 \u45716 \u51648 , \u44032 \u52824 \u44288 \u51008  \u50612 \u46500 \u51648  \u54217 \u44032  \u48155 \u44172  \u46121 \u45768 \u45796 .\u49457 \u49892 \u54620  \u49457 \u44201 \u44284  \u44032 \u52824 \u44288 \u51012  \u50612 \u54596 \u54616 \u47732 , \u44288 \u44228 \u47484  \u44557 \u51221 \u51201 \u51004 \u47196  \u51060 \u50612 \u45208 \u44040  \u49688  \u51080 \u49845 \u45768 \u45796 .\u44288 \u44228 \u44032  \u49457 \u47549 \u54620  \u49345 \u54889  (\u52964 \u54540 )\u48120 \u47000  \u51648 \u54693 \u51201 \u51064  \u44288 \u44228 \u47484  \u49345 \u51669 \u54633 \u45768 \u45796 . \u45824 \u54364 \u51201 \u51004 \u47196  \u44208 \u54844 \u51077 \u45768 \u45796 .\u52964 \u54540 \u51060 \u46972 \u47732 , \u51060  \u44288 \u44228 \u47484  \u50526 \u51004 \u47196  \u50612 \u46523 \u44172  \u45132 \u44256  \u45208 \u44040  \u44163 \u51064 \u51648 , \u48120 \u47000  \u44228 \u54925 \u51008  \u50612 \u46523 \u44172  \u46104 \u45716 \u51648 , \u49436 \u47196  \u50504 \u47582 \u45716  \u48512 \u48516 \u51008  \u50612 \u46523 \u44172  \u47582 \u52636 \u51648  \u46321 \u46321 \'85 \u54788 \u49892 \u51201 \u51064  \u45824 \u50504 \u51012  \u47560 \u47144 \u54644 \u50556  \u54633 \u45768 \u45796 .\u47564 \u50557  \u52572 \u44540  \u44288 \u44228 \u50640  \u44512 \u50676 \u51060  \u49373 \u44220 \u45796 \u47732 , \'91\u45817 \u49888 \u50640 \u44172  \u47924 \u50616 \u44032  \u48120 \u49689 \u54616 \u44144 \u45208 , \u47924 \u52293 \u51076 \u54620  \u47784 \u49845 \'92\u51060  \u51080 \u45796 \u45716  \u46907 \u51077 \u45768 \u45796 . \'91\u44048 \u51221  \u44368 \u47448 \'92, \'91\u50528 \u51221  \u54364 \u54788 \'92 \u44057 \u51008  \u44163 \u46308 \u51060  \u50500 \u45785 \u45768 \u45796 . \u48372 \u45796  \u49892 \u51656 \u51201 \u51060 \u47728 , \u54788 \u49892 \u51201 \u51064  \u47928 \u51228 \u51077 \u45768 \u45796 .\u46024 \u51012  \u47561 \u50416 \u44256  \u51080 \u45716 \u51648 , \u45320 \u47924  \u50528 \u44057 \u51008  \u47784 \u49845 \u51012  \u48372 \u51064  \u44148  \u50500 \u45772 \u51648 , \u49373 \u54876  \u48169 \u49885 \u50640 \u49436  \u50504  \u47582 \u45716  \u48512 \u48516 \u51060  \u51080 \u50632 \u45716 \u51648  \u46384 \u51256 \u48400 \u50556  \u54633 \u45768 \u45796 .\u44288 \u44228 \u44032  \u45130 \u44596  \u49345 \u54889  (\u51116 \u54924 )\u49345 \u54889 \u51060  \u44553 \u48320 \u54644 \u49436  \u50672 \u46973 \u51060  \u50724 \u45716  \u52852 \u46300 \u45716  \u50500 \u45785 \u45768 \u45796 . \u49345 \u45824 \u48169 \u51060  \u45817 \u49888 \u50640  \u45824 \u54644  \u44417 \u44552 \u54644  \u54616 \u44256  \u44256 \u48124 \u54616 \u45716  \u47784 \u49845 \u51012  \u45208 \u53440 \u45253 \u45768 \u45796 .\u49345 \u45824 \u48169 \u51008  \u45817 \u49888 \u51060  \u50612 \u46523 \u44172  \u51648 \u45236 \u44256  \u51080 \u45716 \u51648  \u44417 \u44552 \u54644  \u54616 \u47728 , \u51060  \u44288 \u44228 \u47484  \u45796 \u49884  \u51060 \u50612 \u44032 \u47140 \u47732  \u50612 \u46500  \u45432 \u47141 \u51012  \u54644 \u50556  \u54624 \u51648  \'91\u54788 \u49892 \u51201 \u51064  \u44256 \u48124 \'92\u51012  \u54616 \u44256  \u51080 \u51012  \u44032 \u45733 \u49457 \u51060  \u53373 \u45768 \u45796 .\u54665 \u46041 \u47141 \u51008  \u46504 \u50612 \u51648 \u44592 \u50640 , \u50724 \u47004  \u44592 \u44036  \u44592 \u45796 \u47532 \u49492 \u50556  \u54624  \u44163  \u44057 \u49845 \u45768 \u45796 .\u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u51649 \u50629 \u49352 \u47196 \u50868  \u44592 \u54924  : \u44060 \u51064 \u51201 \u51064  \u47785 \u54364 \u45208  \u45000 \u51012  \u46020 \u51204 \u54616 \u44592  \u51339 \u51008  \u46412 \u51077 \u45768 \u45796 . \u51060 \u51649 \u51060 \u45208  \u49352 \u47196 \u50868  \u49324 \u50629 \u51012  \u49884 \u51089 \u54644 \u46020  \u51339 \u49845 \u45768 \u45796 . \u51652 \u51204 \u51060  \u50504 \u46104 \u45912  \u51068 \u51060  \u51080 \u50632 \u45796 \u47732 , \u49345 \u54889 \u51012  \u53440 \u44060 \u54624  \'91\u46028 \u54028 \u44396 \'92\u47484  \u52286 \u51012  \u49688 \u46020  \u51080 \u44192 \u49845 \u45768 \u45796 .\u51312 \u44552 \u51008  \u48120 \u49689 \u54632  : \u45796 \u47564  \u51068  \u52376 \u47532 \u50640  \u51080 \u50612  \u51312 \u44552 \u51008  \u48120 \u49689 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49892 \u49688 \u46020  \u51110 \u50500 \u49436  \u47588 \u49324 \u50640  \u51312 \u49900 \u49828 \u47084 \u50892 \u51648 \u44592 \u46020  \u54633 \u45768 \u45796 .\u51104 \u51116 \u47141 \u51060  \u52649 \u48516 \u54632  : \u44536 \u47084 \u45208 , \u51104 \u51116 \u47141 \u51060  \u52649 \u47564 \u54620  \u49345 \u53468 \u51060 \u48064 \u47196 , \u45817 \u49888 \u51060  \u51648 \u44552  \u44054 \u44256  \u51080 \u45716  \'91\u50556 \u47581 \'92\u51012  \u44984 \u51456 \u55176  \u48128 \u44256  \u44032 \u46972 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 .\u45432 \u47141 \u51032  \u44208 \u49892 \u51012  \u47610 \u44172  \u46120  : \u54168 \u51060 \u51648  \u54172 \u53440 \u53364 \u51008  \'91\u49892 \u51228 \u47196  \u54952 \u44284 \u44032  \u51080 \u45716  \u51068 \'92, \'91\u54788 \u49892 \u51201 \u51064  \u44208 \u44284 \u47484  \u50619 \u44172  \u46120 \'92\u51012  \u49345 \u51669 \u54633 \u45768 \u45796 . \u51204 \u52404 \u51201 \u51064  \u50640 \u45320 \u51648 \u45716  \u44557 \u51221 \u51201 \u51077 \u45768 \u45796 .\u54788 \u49892 \u51201 \u51064  \u44228 \u54925 \u51060  \u54596 \u50836 \u54632  : \u50556 \u47581 \u51060  \u51080 \u44256 , \u51104 \u51116 \u47141 \u46020  \u52649 \u48516 \u54620  \u49345 \u53468 \u51077 \u45768 \u45796 . \u54788 \u49892 \u51201 \u51060 \u44256  \u49892 \u54788  \u44032 \u45733 \u54620  \u44228 \u54925 \u51012  \u49464 \u50892 , \'91\u44984 \u51456 \u55176  \u49892 \u52380 \'92 \u54616 \u46972 \u45716  \u47700 \u49884 \u51648 \u51077 \u45768 \u45796 . \u49884 \u51089 \u51008  \u44144 \u52285 \u54616 \u51648 \u47564 , \u45149 \u51008  \u48120 \u50557 \u54616 \u44172  \u46112  \u44032 \u45733 \u49457 \u46020  \u51080 \u49845 \u45768 \u45796 .\u54168 \u51060 \u51648  \u54172 \u53440 \u53364  \u44552 \u51204 \'91\u44552 \u51204 \u51032  \u55120 \u47492 \u51060  \u50612 \u46523 \u44172  \u46112 \u51648 \'92 \u51656 \u47928 \u54664 \u45796 \u47732 , \'91\u52380 \u52380 \u55176  \u45208 \u50500 \u51652 \u45796 , \u49436 \u49436 \u55176  \u45208 \u50500 \u51652 \u45796 , \u45208 \u50500 \u51656  \u44592 \u48120 \u44032  \u48372 \u51064 \u45796 \'92\u45716  \u46907 \u51004 \u47196  \u54644 \u49437 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u50948 \u54744 \u51012  \u44048 \u49688 \u54616 \u44592 \u48372 \u45796 \u45716  \u49888 \u51473 \u54616 \u44256 , \u49892 \u50857 \u51201 \u51064  \u53804 \u51088 \u44032  \u51339 \u49845 \u45768 \u45796 . \u45817 \u48516 \u44036  \u51064 \u45236 \u49900 \u51