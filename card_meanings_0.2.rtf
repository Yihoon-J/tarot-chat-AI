5 \u52840 \u45824  \u50630 \u51060  \u49688 \u54217 \u51004 \u47196  \u44152 \u47140 \u51080 \u49845 \u45768 \u45796 . \u51060 \u45716  \u54788 \u49892 \u50640 \u49436  \u51068 \u50612 \u45216  \u49688  \u50630 \u45716  \u51068 \u51060 \u51424 . \u51593 , \u51200  \u49548 \u46300 \u46308 \u51008  \u50668 \u51088 \u44032  \u47564 \u46308 \u50612 \u45240  \'91\u49345 \u49345 \'92\u51060 \u46972 \u44256  \u48372 \u49884 \u47732  \u46121 \u45768 \u45796 . \u50696 \u52968 \u45824  \u49345 \u45824 \u48169 \u51032  \u49549 \u47560 \u51020 \u51012  \u45208 \u53440 \u45240 \u45796 \u47732  :\u51068 \u51060  \u49884 \u51089 \u46104 \u51648  \u50506 \u50520 \u51020 \u50640 \u46020 , \u51096 \u47803 \u46112 \u44620 \u48400  \u46160 \u47140 \u50892  \u54616 \u45716  \u49345 \u53468 \u45817 \u49888 \u51060  \u46496 \u45208 \u44040 \u44620 \u48400  \u46160 \u47140 \u50892 \u54616 \u45716  \u47560 \u51020 \u50672 \u51064 \u51060  \u48148 \u46988 \u51012  \u54588 \u44256  \u51080 \u45716  \u44148  \u50500 \u45776 \u51648  \u44145 \u51221 \u54616 \u45716  \u47560 \u51020 \u45572 \u44400 \u44032 \u50640 \u44172  \u49892 \u47581 \u51012  \u50504 \u44200 \u51500 \u45796 \u44256  \u49373 \u44033 \u54616 \u50668 , \u51396 \u52293 \u44048 \u51012  \u45712 \u45180 \u44256  \u51080 \u51020 \u45824 \u48512 \u48516 \u51032  \u44145 \u51221 \u51008  \u50424 \u47784 \u44032  \u50630 \u49845 \u45768 \u45796 . 40%\u45716  \u51208 \u45824  \u51068 \u50612 \u45208 \u51648  \u50506 \u51012  \u51068 , 20%\u45716  \u49324 \u49548 \u54620  \u51068 , 30%\u45716  \u51060 \u48120  \u51068 \u50612 \u45212  \u51068 \u50640  \u44288 \u54620  \u44163 \u51077 \u45768 \u45796 . \u50864 \u47532 \u45716  \u50424 \u47784  \u50630 \u45716  \u51068 \u50640  \u50640 \u45320 \u51648 \u47484  \u45229 \u48708 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44145 \u51221 \u51012  \u45916 \u50612 \u45236 \u49464 \u50836 .\u54644 \u49437  2. \u50724 \u47019 \u46041 \u50504  \u51648 \u49549 \u46104 \u45716  \u44256 \u53